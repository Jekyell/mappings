{
    "id": 304400,
    "collectionNo": 288,
    "name": "虞美人",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "你在看哪里？　消失吧。"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "你难道以为自己能触及吗！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "真是个走运的家伙。"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "好了——你可以去死了。"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "项羽大人托付给我的这把枪，",
                "0_B051": "必能将夏日魔物尽数击灭。",
                "0_B052": "吾之诅咒只为顺应此意！",
                "0_B053": "飞舞绽放疯狂凋零吧，犹如昔日的雏芥子！",
                "0_B054": "夏魔必灭枪舞！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "做好觉悟了吧！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "真不想给你见识。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "搭讪？　这就是所谓的搭讪吧？"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "出现了，夏日的魔物……！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "全都献给项羽大人……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "这舞蹈还令人满意吗。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "别碰我！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "我才没兴奋过度呢……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "啊，不行。还是太热了。"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "明明被托付了这把枪，却还这么不中用……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "你在开玩笑吗？"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "你行你上啊。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "就当是舞蹈练习好了。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "我的思念啊，请传达到——"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "哈，就因为是夏天吗？　你看不起我吗？"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "啊啊，项羽大人——"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "只是想让那位大人好好欣赏而已。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "干脆试着从上方旋转下来好了。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "偶尔也要试试这种才行！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "最后是这个！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "阳光就不能毁灭吗。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "算了，只要把这当做练习也就没办法了。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "吾之舞蹈只为献给项羽大人。",
                "0_B801": "也就是说看到了舞蹈的你们休想活下去！",
                "0_B802": "甚至不允许你们保留临终的记忆。",
                "0_B803": "若吾身一般，如尘土般消散吧！",
                "0_B804": "夏魔必灭枪舞！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "虚拟项羽大人展开完毕。",
                "0_B811": "枪舞模拟开始！",
                "0_B812": "啊啊，项羽大人在对我点头，而且，那是……",
                "0_B813": "微……微笑！？",
                "0_B814": "虞……虞，虞真是太幸福了！"
            }
        },
        "0_G010": {
            "name": "升级 {0}",
            "text": {
                "0_G010": "对了对了，毕竟是夏天嘛。",
                "0_G011": "你当然应该比平时更加孝敬我。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "夏天的开销总是很大呢……",
                "0_G021": "但现在我可没有温柔到会对你手下留情。",
                "0_G022": "你也不要犹豫，尽管向我投放资源吧。 我迟早会回报你的。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我可没打算换衣服。",
                "0_G031": "因为……这套泳装……曾经得到过项羽大人的夸奖……"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "差不多想停手了？",
                "0_G041": "很遗憾，事已至此，已经停不下来了哦。",
                "0_G042": "人类所说的吸血鬼，就是无视人类的意愿吸干人类的存在吧？　我没说错吧？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "唉……愚蠢。真是太愚蠢了。",
                "0_G051": "你明白什么叫愚蠢吗？　就是指老好人的笨蛋啦。",
                "0_G052": "被我这么利用敲诈，怎么还露出心满意足的笑容啦……",
                "0_G053": "……啊啊真是的，我可是对项羽大人一心一意的，才不会顾虑你的心情呢！",
                "0_G054": "事已至此，我可要狠狠差遣你啦！",
                "0_G055": "差遣到你一看到我的脸就会禁不住全身发抖的程度！"
            }
        },
        "0_G110": {
            "name": "升级 {0}",
            "text": {
                "0_G110": "快给我，我知道你囤了不少。",
                "0_G111": "啊。你该不会唯独不舍得给我用吧！？",
                "0_G112": "算歧视！？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "我说你啊……你该不会觉得我换上了泳装就会愿意为你庆祝生日了？",
                "0_H011": "你认真的吗？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "夏天总是伴随着祭典。",
                "0_H021": "也就是说只要发生了祭典，那就是夏天……",
                "0_H022": "难道不是吗？",
                "0_H023": "我可不允许你坐视不理哦。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "我说你啊。就算我再怎么不了解人类的生活，也知道为了享受夏天，必须外出才行。",
                "0_H031": "然后呢，你躲在这里干什么？",
                "0_H032": "故意让我不爽？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我还是一如既往，是只属于项羽大人的存在。",
                "0_H041": "可不会像你们那样飘忽不定。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "你好歹也是御主，有正确使用我的义务。",
                "0_H051": "而现在的我是为了尽享夏日的灵基。",
                "0_H052": "所以你……",
                "0_H053": "没错，从逻辑性角度来说，你就是为了让我能享受夏日而服务的「奴仆」。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？ 那么一目了然的事，还用我特地说出口吗？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "如果你想知道我讨厌的东西，就凝视被我的枪尖刺穿的东西吧。",
                "0_H071": "项羽大人就是为此才把这个托付给我的。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我对那种可疑的圣杯毫无兴趣啦。",
                "0_H081": "而且，我的愿望……",
                "0_H082": "已经基本实现了。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "这杆枪是为了击灭夏日的魔物，从项羽大人那里借来的东西。",
                "0_H091": "虽然我不知道那所谓的魔物是什么，但完全不需要害怕。",
                "0_H092": "我只会让它们发出凄厉的惨叫。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "夏日魔物，那个……",
                "0_H101": "指的是搭讪男，兴奋的心情那类吗？",
                "0_H102": "这么说来，项羽大人会担心我被那些东西侵袭，",
                "0_H103": "也就意味着他非常珍视我吧……",
                "0_H104": "呵呵，呵呵呵呵……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我还以为御主肯定不会……但你偶尔也会做些奇怪的事呢。",
                "0_H111": "就是说你也会被夏日魔物吞噬啊。",
                "0_H112": "如果你胆敢有什么可疑的举动，我可是会直接给你一枪，小心点。",
                "0_H113": "你应该还没天真到觉得我会手下留情吧？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "虽说我对人类的文化毫无兴趣。但跳舞另当别论。",
                "0_H121": "因为小鸟和蜥蜴也会跳舞吧？",
                "0_H122": "所以，那啥……",
                "0_H123": "在心爱的人面前，我还是会模仿着跳跳舞的啦。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "下次想看我认真跳一次舞？",
                "0_H131": "你这蠢货，我不是说过了吗？",
                "0_H132": "我的舞蹈只会献给我心爱的项羽大人。非分之想是会搭上性命的……！",
                "0_H133": "虽然我本想这么说……",
                "0_H134": "但说到底，我们算是从者和御主？　有魔力供给才能更有力地跳舞吧？",
                "0_H135": "如果你只是在远处眺望我向项羽大人献舞的话，也行……",
                "0_H136": "不，我并没有允许哦。只是说如果你擅自偷看我也没办法的意思。",
                "0_H137": "但如果我心情不好，还是可能刺瞎你眼睛的，所以你偷看时务必考虑到这种风险。",
                "0_H138": "还有，千万别把你的感想告诉我。",
                "0_H139": "……事到如今，会很不好意思的啦。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "啊啊，项羽大人……",
                "0_H171": "我能否依照您的愿望，尽情享受夏日呢……？"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "看来你也有一套相当不错的夏装嘛。",
                "0_H181": "不过，远远不及获得了项羽大人夸奖的我哦！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "我说！　那个年轻的卫士长……",
                "0_H191": "不对，就是那家伙啦！",
                "0_H192": "最近说什么『想感受一下非人之物的枪术究竟如何……』死命追着我不放啊。",
                "0_H193": "能不能告诉他我的枪术不是他想的那种呢？"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "阎魔亲，你偶尔也休息一下放松一下自己吧？",
                "0_H201": "……果然。你真是太认真了。",
                "0_H202": "真拿你没办法，我会以这副打扮去拜访你那儿的。",
                "0_H203": "这样一来，阎魔亭也会多少增添几分夏日的风情了吧。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "什么八百比丘尼啊，装糊涂也该有点限度吧！？",
                "0_H211": "你不就是深海里的那个嘛！",
                "0_H212": "话说你到底吃了多少条人鱼啦！？",
                "0_H213": "又不是包子！"
            }
        },
        "0_I010": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 1",
            "text": {
                "0_I010": "哼，你以为我能当得了店员吗？",
                "0_I011": "然而却可以……真令人垂头丧气……"
            }
        },
        "0_I020": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 2",
            "text": {
                "0_I020": "快做决定啦。",
                "0_I021": "只要找到了想要的东西，不顾一切地买不就行了嘛。"
            }
        },
        "0_I030": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 3",
            "text": {
                "0_I030": "给，拿着。反正你很快就会用完了。",
                "0_I031": "我明白。因为我就是这样的。"
            }
        },
        "0_I040": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 4",
            "text": {
                "0_I040": "哈？　赠品？　怎么可能有嘛。",
                "0_I041": "你就尽情哭闹叫唤吧，我甚至不会给你打折。"
            }
        },
        "0_I050": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 5",
            "text": {
                "0_I050": "收下东西时你还真是一脸高兴呢。",
                "0_I051": "……再来光顾不就行了嘛？"
            }
        },
        "0_I060": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 6",
            "text": {
                "0_I060": "好嘞，真遗憾，世间罕见的真祖商店已经关门啦～。",
                "0_I061": "我要赶紧回项羽大人身边了。赶快，收摊收摊！"
            }
        },
        "0_I070": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 7",
            "text": {
                "0_I070": "发生在这里的事似乎很漫长，又似乎很短暂，有种奇妙的感觉。",
                "0_I071": "对于身在永恒中的我来说，或许已经算是常态了……",
                "0_I072": "哎，你也有一样的感觉？",
                "0_I073": "……唔。所谓的夏天，或许就是这样的吧。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "……唉，这次倒是意外倒大霉了……",
                "0_S011": "话说你那眼神什么意思。想说我是问题的元凶吗？",
                "0_S012": "那么早以前的事我哪知道啊。",
                "0_S013": "我的记忆容量可是优先为项羽大人准备的！",
                "0_S014": "总之，这下总算能享受正常的夏季山间假日了。",
                "0_S015": "我打算尽情享受，作为对前辈的孝顺，你也得支援！",
                "0_S016": "明白了吗！？"
            }
        },
        "0_S020": {
            "name": "期间限定加入",
            "text": {
                "0_S020": "话说在前面，这样子并不是因为兴奋过度哦。",
                "0_S021": "因为项羽大人希望我能充分享受夏日。",
                "0_S022": "换言之，这是因爱而生的泳装。",
                "0_S023": "明白了吗？妨碍我就意味着对项羽大人有意见。",
                "0_S024": "而我则毫不留情地一枪刺穿那种人，请知晓一下。"
            }
        }
    },
    "ChrVoice_ids": [
        "304400"
    ]
}
{
    "id": 305000,
    "collectionNo": 329,
    "name": "坂本龙马",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "去吧！ 好嘞！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "哈啊啊啊啊啊！　最后一击！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "噢啦噢啦噢啦！",
                "0_B041": "用这个，击穿！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "为天逆鉾祝福之国津大蛇，",
                "0_B051": "吾欲所成之事仅为汝所知。",
                "0_B052": "白君啊，趁赴绵津见之原！",
                "0_B053": "噢噢噢噢噢噢噢噢！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "干劲十足的阿龙小姐。 记得手下留情哦。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "大家一并放马过来吧～。 那真是太可靠了。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "那么，就将你们一并清洗掉吧。 使劲上咯～！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "偶尔也要表现一下自己优秀的一面呢。 噢，难得那么有干劲呢。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "看来总算是解决了呢，大家都没事吧？ 阿龙小姐还是精力充沛的阿龙小姐。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "成功了哦，胜利的阿龙小姐和平！　龙马也一起来。 这，这样吗？ 耶～！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "永远……在一起……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "没有什么是……毫无意义的……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是这样吗？"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "原来如此啊。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "那就动手吧。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "阿龙小姐，要拔出宝具了哦。 随时都可以，龙马。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "嗯，我们一起走吧。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "吾乃非归顺之神，高千穗的大蛇。同时也是……！"
            }
        },
        "0_B380": {
            "name": "指令卡 4",
            "text": {
                "0_B380": "嗯，那就这么办吧。"
            }
        },
        "0_B390": {
            "name": "指令卡 5",
            "text": {
                "0_B390": "你刚才有说什么吗？"
            }
        },
        "0_B400": {
            "name": "指令卡 6",
            "text": {
                "0_B400": "交给我吧～。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "阿龙小姐！ 交给我吧，嘿呀！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "认真瞄准哦。 责任重大呢。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "哒哒哒哒哒哒！　唔？"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "这是两个人协同工作哦！",
                "0_B441": "你是从哪儿学会这句话的啊！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "我，们是，外星人哦。 不不，是从者吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "上吧，阿龙小姐。 交给我吧，龙马。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "把胆敢妨碍龙马的家伙统统打飞。 谢谢，帮大忙啦。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "呼，我吓得肝都凉了。 唔？　肚子可别着凉了哦。 不是这种意思啦。"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "没用的！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "不要紧，不要紧……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "对不起了，大家……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "这就是我们的宝具……",
                "0_B801": "上吧，阿龙小姐！",
                "0_B802": "嗯，永远陪你在一起！",
                "0_B803": "白龙啊，趁赴绵津见之原！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "耶～，阿龙小姐和平。",
                "0_B811": "应该算是发挥海援队的本领吧。",
                "0_B812": "嗯……",
                "0_B813": "君啊，趁赴绵津见之原！"
            }
        },
        "0_B840": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B840": "阿龙小姐，在此解决吧！",
                "0_B841": "上！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "阿龙小姐超强化了哦。",
                "0_G011": "那真是可喜可贺。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "希望有朝一日能穿着这身打扮航海跑遍全世界的海洋呢。",
                "0_G021": "到时候阿龙小姐当船长，而龙马当水夫吧。",
                "0_G022": "哎呀，要这样啊。",
                "0_G023": "好啦好啦，请尽管吩咐我水手坂本吧，船长。",
                "0_G024": "你看上去很开心呢，龙马。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "这身打扮怎么样？",
                "0_G031": "犹如海援队的船长，不，应该叫captain吧。",
                "0_G032": "毕竟龙马是个最喜欢船或船长的人嘛。",
                "0_G033": "只有这个打从童年起至今一点都没有改变呢。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "嗯，状态不错。",
                "0_G111": "毕竟龙马很弱小嘛，得好好锻炼才行。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝你生日快乐，御主。我其实很想送您一件礼物……",
                "0_H011": "对了，要不要一起出海？",
                "0_H012": "可以，今天比较特别，就允许你坐在阿龙小姐的背上吧。",
                "0_H013": "要抓紧哦。",
                "0_H014": "这太好了，那就拜托了哦，阿龙小姐。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "喂，好像有些吵闹呢。",
                "0_H021": "真的呢，是发生了什么纠纷……",
                "0_H022": "不，是发生了什么愉快的事吧。",
                "0_H023": "那你早点说啊。",
                "0_H024": "喂，人类，和阿龙小姐一起外出吧。",
                "0_H025": "那我们就出发吧。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "好，出门吧。",
                "0_H031": "重要的是先行动起来。向着你的志向前进。",
                "0_H032": "说得对，趁还能做力所能及的事。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我不太习惯和他人缔结主从关系。",
                "0_H041": "我果然还是想和别人成为朋友。没错，和你也一样。",
                "0_H042": "综上所述，握手，能请你多关照吗？",
                "0_H043": "太狡猾了，让阿龙小姐也握握啦，握手握手。",
                "0_H044": "嗯，注意别用力过猛哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "你的目标，我的目标，两者是否一样并不重要。",
                "0_H051": "无论目的为何，我都想助你一臂之力。",
                "0_H052": "所以我想和你不以御主和从者的关系，让我想想……",
                "0_H053": "嗯，能是对等的同盟关系就好了。",
                "0_H054": "可以叫做龙御主同盟呢。",
                "0_H055": "虽然读起来稍微有点奇怪呢。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗……",
                "0_H061": "是啊，还是喜欢大海吧。",
                "0_H062": "小时候就经常眺望着大海发呆嘛。",
                "0_H063": "然后发呆到天黑都没意识到，最终被家里人狠狠训斥哦。",
                "0_H064": "阿龙小姐也喜欢大海哦。",
                "0_H065": "虽然以前并不喜欢……",
                "0_H066": "——嗯，现在喜欢。",
                "0_H067": "这样啊……",
                "0_H068": "谢谢你，阿龙小姐。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西吗，应该还是讨厌悲伤的事吧。",
                "0_H071": "虽然还是有很多无能为力的事，但在我力所能及范围内，还是希望能稍微减少一些悲伤的事。",
                "0_H072": "正是怀着这种想法，我过去和现在才会站在这个位置。",
                "0_H073": "不要那么沮丧嘛，龙马，悲伤的事只要用更为快乐的事打飞就行了嘛。",
                "0_H074": "确实……",
                "0_H075": "哎呀，真是比不过阿龙小姐呢。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "希望圣杯实现的愿望吗？",
                "0_H081": "以前我也说过，我应该没有什么特别的愿望吧。",
                "0_H082": "要不要成立一个公司？",
                "0_H083": "阿龙小姐随时准备成为能干美女秘书哦。",
                "0_H084": "这种事还是自己亲手干比较有意思哦。",
                "0_H085": "对了，要不要和我以及阿龙小姐一起，成立一个新公司呢？",
                "0_H086": "哦，干吧干吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "人不可貌相，阿龙小姐其实非常凶暴，还是别惹怒阿龙小姐为好哦。",
                "0_H091": "哎哟，看来今天脾气不太好呢。",
                "0_H092": "对不起啦，希望你暂时别理她为好哦。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "比起什么都不做，还是趁能行动时动起来比较好。",
                "0_H101": "有志向的话，就更不用说了哦。",
                "0_H102": "怎么了，龙马，说话突然那么正经。",
                "0_H103": "哈哈哈，我自认一直很正经哦。",
                "0_H104": "嗯，这话确实不该由我来说呢。",
                "0_H105": "对，你应该早就明白了。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "其实我还是相当擅长使枪术，或者说，擅长使长兵器哦。",
                "0_H111": "年轻时我还在江户拿过长刀的证书哦。",
                "0_H112": "喂，那段故事阿龙似乎还是第一次听说哦。",
                "0_H113": "咦，我没告诉过你吗？",
                "0_H114": "不过，归根结底，我还是斗不过刀、枪或是火枪。更不用说阿龙小姐了。",
                "0_H115": "哼哼～，你还是很明白的嘛。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "土佐方言吗？",
                "0_H121": "或许是因为用这种口音说话已经很久了，最近稍微有些懈怠吧。",
                "0_H122": "偶尔说说方言也无妨哦。你来山里时也是那种口音吧？",
                "0_H123": "啊哈哈哈，是吗？",
                "0_H124": "那会儿俺还天不怕地不怕嘞，不，应该算啥都不懂的愣头青吧。",
                "0_H125": "听起来像以藏了，还是别这么说了。",
                "0_H126": "哎呀，这话可千万别告诉以藏先生哦。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我呀，没什么大不了的。只是希望大家能够幸福。",
                "0_H131": "没错，离开土佐时的我，内心只有这个念头。",
                "0_H132": "但是啊，我并不知道。",
                "0_H133": "不知道希望大家能够幸福是件多么残酷的事。",
                "0_H134": "龙马……",
                "0_H135": "但我遇见了你……没错，来到这迦勒底，我还是会这么想。",
                "0_H136": "希望大家能够幸福哦。",
                "0_H137": "说得对，希望阿龙小姐、龙马和你都能幸福哦。",
                "0_H138": "说得对，我真的这么想。",
                "0_H139": "我希望你，希望大家都能幸福。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "哟，第六天魔王信长公，不，应该是名副其实的魔王信长公吧。",
                "0_H171": "我本以为自己来这里是为了和信长公战斗，但看来并非如此呢。",
                "0_H172": "感觉龙马经常容易搞错哦。",
                "0_H173": "是，是吗？",
                "0_H174": "没什么，抑止力啊守护者云云的问题只要听个一半就够了。",
                "0_H175": "我们是凭自身意志来这里的。这点肯定是没错的。",
                "0_H176": "反正就算战斗也会赢嘛。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "竟然能与以藏先生并肩战斗，我真的非常高兴啊。",
                "0_H181": "嗯，我真的非常高兴。",
                "0_H182": "……呃，等一下，等一下，不要把手放在刀柄上啦。",
                "0_H183": "真的对不起，一时没注意。",
                "0_H184": "这是所谓的，阿，阿龙小姐一点都不高兴，你千万不要误会了哦。",
                "0_H185": "你是从哪儿学会这种说法的啊。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "原来如此，这个形象才是能最大程度发挥你原本能力的形象啊。",
                "0_H191": "嗯，你毫无疑问是可以被称为抑止守护者阶级的英灵啊。",
                "0_H192": "——这样啊，你是这样达成了啊。",
                "0_H193": "话说小个子的那个看上去很好吃呢。",
                "0_H194": "你要是这么做，结果可不止是吃坏个肚子就能解决的问题哦，还是别吃了好吗。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "新选组三番队队长，斋藤一。",
                "0_H201": "活到了明治时代的新选组吗……",
                "0_H202": "哎呀，我也好想见识一下明治那个时代呢。",
                "0_H203": "……抱歉，龙马。",
                "0_H204": "啊，不，不是这个意思啦。不用介意。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "没想到继信长公之后，连那位谦信公都出现了呀。",
                "0_H211": "照这样下去，像我这种段位的英灵终于要跟不上了吧？",
                "0_H212": "放心吧，阿龙小姐的段位要比那些家伙要高100倍以上哦。",
                "0_H213": "既然这样，那我就放心了。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "旅行者……前往星之大海的英灵吗。",
                "0_H221": "这样啊，人类竟然连遥远的天空彼方都能去了啊。",
                "0_H222": "唔唔唔……对阿龙小姐来说，这个或许确实难了点。",
                "0_H223": "不过去月亮的话，或许还能想想办法哦。",
                "0_H224": "和阿龙小姐去月世界旅行吗，一定会非常开心。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "哟，阿国小姐啊。",
                "0_H231": "虽然我们之间发生了不少事，但今后还请多多关照啦。",
                "0_H232": "你背后那个家伙相当难对付哦。",
                "0_H233": "那个迦勒底公演也很令人期待哦。",
                "0_H234": "要不阿龙小姐也来参加演出吧。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "高杉先生吗？",
                "0_H241": "嗯，他就是这样的男人。",
                "0_H242": "那家伙真是肆意妄为呢。",
                "0_H243": "哈哈，说得太对了。他从以前起就这么任性哦。",
                "0_H244": "没错，简直就是麒麟儿——不，是麒麟哦。"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "喂，龙马，那家伙叫什么来着？",
                "0_H251": "……武市先生吗？",
                "0_H252": "嗯，就是那家伙就是那家伙。",
                "0_H253": "虽然发生了不少事，但我觉得以人类而言，那家伙相当有干劲哦。",
                "0_H254": "说得对……说得没错。",
                "0_H255": "——武市先生，相当出色哦。"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "新选组的山南敬助啊，我生前一直很想找机会和他谈谈。",
                "0_H261": "以那群家伙的同伴而言，这家伙看上去柔柔弱弱的，感觉一打就会折断呢。",
                "0_H262": "千万别干哦，我说真的。",
                "0_H263": "不过说到打，他的剑其实非常柔软且有韧性。",
                "0_H264": "对付起来可不简单哦。"
            }
        },
        "0_M010": {
            "name": "唠唠叨叨龙马千钧一发！！ 8",
            "text": {
                "0_M010": "嗯，就按照这个状态，拜托了哦。",
                "0_M011": "要努力寻找更好的东西哦，人类。"
            }
        },
        "0_M020": {
            "name": "唠唠叨叨龙马千钧一发！！ 9",
            "text": {
                "0_M020": "哦！　找到好东西了呢，人类。",
                "0_M021": "这可非常难得一见哦！",
                "0_M022": "这还真是厉害呢！",
                "0_M023": "你该不会有干这份活儿的才能吧？"
            }
        },
        "0_M030": {
            "name": "唠唠叨叨龙马千钧一发！！ 10",
            "text": {
                "0_M030": "是新的委托哦。多多完成、多多攒钱、多多购买青蛙吧。",
                "0_M031": "青蛙暂且不论，我觉得为了想要的东西或是想做的事而努力是好事哦。"
            }
        },
        "0_M040": {
            "name": "唠唠叨叨龙马千钧一发！！ 11",
            "text": {
                "0_M040": "好像来新委托了哦，让我们赶快确认委托内容吧。 如果是能够收获大量青蛙的划算委托就好了呢！ 要不干脆提个搜集青蛙的委托吧？"
            }
        },
        "0_M050": {
            "name": "唠唠叨叨龙马千钧一发！！ 12",
            "text": {
                "0_M050": "发现了可疑的地点呢。好，赶快去看看吧！ 可疑不可疑暂且不论，看来还是好好调查为好。拜托了。"
            }
        },
        "0_M060": {
            "name": "唠唠叨叨龙马千钧一发！！ 13",
            "text": {
                "0_M060": "什么嘛，那家伙没有回那什么星啊。 也就是说，我们多了一个可靠的同伴吧。"
            }
        },
        "0_M070": {
            "name": "唠唠叨叨龙马千钧一发！！ 14",
            "text": {
                "0_M070": "这一天终于到来了呢！ 阿龙小姐和你最后之战的钟声敲响啦！ 最后可就不好了，但看上去确实很难对付哦。 一定要认真准备哦！"
            }
        },
        "0_M080": {
            "name": "唠唠叨叨龙马千钧一发！！ 15",
            "text": {
                "0_M080": "辛苦啦，真亏你能完成到这一步呢。不愧是我看中的御主呢。",
                "0_M081": "阿龙小姐也看中了哦！",
                "0_M082": "啊哈哈，说得对。",
                "0_M083": "应该是我们看中的御主吧！"
            }
        },
        "0_N010": {
            "name": "唠唠叨叨龙马千钧一发！！ 1",
            "text": {
                "0_N010": "这里是新·坂本侦探事务所。",
                "0_N011": "阿龙小姐会狠狠锻炼见习海援队的你，觉悟吧？",
                "0_N012": "哈哈，记得手下留情哦。",
                "0_N013": "先不谈这个问题，我也忙得没法顾及一些细节的工作，你能来帮手真是太好了。"
            }
        },
        "0_N020": {
            "name": "唠唠叨叨龙马千钧一发！！ 2",
            "text": {
                "0_N020": "说起侦探事务所，资金周转还是很辛苦的……",
                "0_N021": "都因为这样，现在都成了什么都干屋了。干脆改名为新·阿龙屋吧。"
            }
        },
        "0_N030": {
            "name": "唠唠叨叨龙马千钧一发！！ 3",
            "text": {
                "0_N030": "啊啊……好闲啊，闲得阿龙小姐快要融化了啦……",
                "0_N031": "快去找点委托来啦，龙马。",
                "0_N032": "知道啦知道啦。那我们出门吧。"
            }
        },
        "0_N040": {
            "name": "唠唠叨叨龙马千钧一发！！ 4",
            "text": {
                "0_N040": "好了，虽然有很多委托，但从能做到的开始吧。",
                "0_N041": "从小事开始踏踏实实做的阿龙小姐呢！"
            }
        },
        "0_N050": {
            "name": "唠唠叨叨龙马千钧一发！！ 5",
            "text": {
                "0_N050": "要认真确认通知哦？",
                "0_N051": "如果背着阿龙小姐去登山，可是会出大事的哦。",
                "0_N052": "这个和通知没什么关系吧。"
            }
        },
        "0_N060": {
            "name": "唠唠叨叨龙马千钧一发！！ 6",
            "text": {
                "0_N060": "这次真是各种承蒙你的关照呢，人类。",
                "0_N061": "连阿龙小姐我都要坦率地表示感谢哦。",
                "0_N062": "我也要向您道谢。",
                "0_N063": "御主，真是太感谢了。"
            }
        },
        "0_N070": {
            "name": "唠唠叨叨龙马千钧一发！！ 7",
            "text": {
                "0_N070": "虽然发生了不少事，但终于要与这座城市告别了呢。",
                "0_N071": "好了，那我们走吧，御主！",
                "0_N072": "嗯，去往大海的彼岸。",
                "0_N073": "以及更前方的地方——遥远的彼方！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "枪兵，坂本龙马。回应召唤而来。",
                "0_S011": "而我则是他搭档阿龙小姐哦，请多关照啦，人类。",
                "0_S012": "只不过我居然成了枪兵呢。",
                "0_S013": "而且这把枪……",
                "0_S014": "这样啊，看来我也到该出海的时候了呢。",
                "0_S015": "没错，这个时候来到了哦。",
                "0_S016": "嗯，走吧，一同前往绵津见之原。"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "逆浪！ 打开吧！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "上吧！ 嗯！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "哈啊啊啊！　彻底消失吧！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "看招看招看招！",
                "2_B041": "打穿吧！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "天逆鉾啊，将我的一切献给这个人……",
                "2_B051": "将吾之一切献给这个人……",
                "2_B052": "——白龙啊，趁赴绵津见之原！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "走吧，龙马。 嗯，走吧。"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "能稍微后退一点吗？ 哦，斗志满满呢。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "世界的海援队，出航。 哎哎哦～！"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "喂，龙马，那个还挺棘手哦。 嗯，拿出真本事来吧。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "呼，无论多久都习惯不了斩人呢。 龙马保持这样就可以了。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "吾乃高千穗的白色大蛇，供品选青蛙之类的甚好甚好甚好。 哈哈，阿龙小姐也开始有威严了呢。"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "这个有点难受啊。"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "你要，前进……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "我不会再，离开你了……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "这预测不错。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "我知道了。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "就这么办吧。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "天逆鉾啊，贯穿天空与大海的狭缝吧！"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "吾乃高千穗的白色大蛇，征战空与海的狭缝吧。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "阿龙小姐。 嗯。"
            }
        },
        "2_B380": {
            "name": "指令卡 4",
            "text": {
                "2_B380": "是这样吗？"
            }
        },
        "2_B390": {
            "name": "指令卡 5",
            "text": {
                "2_B390": "我听到了哦。"
            }
        },
        "2_B400": {
            "name": "指令卡 6",
            "text": {
                "2_B400": "好嘞！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "硬推到底！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "看招看招！ 贯穿！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "一起配合！ 收到！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "到此为止了。阿龙小姐！",
                "2_B441": "吃我这招——！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "超级阿龙小姐力量！ 事实上也很厉害，一定要小心哦。"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "稍微睡一会儿。 请便。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "御主，这里就交给我们吧。 就当放心坐上了大船，不，是坐上了阿龙小姐。"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "来，我们走吧，去向前方。 嗯，永远在一起，直到那彼方。"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "休想得逞！"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "总是难以事事如愿呢……"
            }
        },
        "2_B570": {
            "name": "无法战斗 4",
            "text": {
                "2_B570": "……我在，这里哦。"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "跨越这，",
                "2_B801": "绵津见之原……",
                "2_B802": "我们是，",
                "2_B803": "只有两个人的，",
                "2_B804": "海援队！"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "阿龙小姐特别版和平，耶～！",
                "2_B811": "啊哈哈，果然我们就该这样才对呢。",
                "2_B812": "来吧，让我们共赴绵津见之原吧，啦啊啊啊啊啊！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "来招大的吧。",
                "2_B841": "好嘞！",
                "2_B842": "打中吧————！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "阿龙小姐超强化了哦。",
                "2_G011": "那真是可喜可贺。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "这个形象……原来如此，这就是所谓天逆鉾的力量啊……",
                "2_G041": "也就是超级龙马加上超越阿龙小姐吧。",
                "2_G042": "哈哈哈，这听上去真有活力真不错呢。",
                "2_G043": "来吧，为实现你的愿望，希望这力量能充分帮上你的忙。",
                "2_G044": "阿龙小姐和龙马在看着哦。"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "——啊啊，这就是我能抵达的尽头了吗。",
                "2_G051": "这不是结束，这条路还在延续。难道不是吗，龙马。",
                "2_G052": "说得对，就这么办吧。让我们在这片大海上永远走下去。"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "嗯，状态不错。",
                "2_G111": "毕竟龙马很弱小嘛，得好好锻炼才行。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "祝你生日快乐，御主。我其实很想送您一件礼物……",
                "2_H011": "对了，要不要一起出海？",
                "2_H012": "可以，今天比较特别，就允许你坐在阿龙小姐的背上吧。",
                "2_H013": "要抓紧哦。",
                "2_H014": "这太好了，那就拜托了哦，阿龙小姐。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "喂，好像有些吵闹呢。",
                "2_H021": "真的呢，是发生了什么纠纷……",
                "2_H022": "不，是发生了什么愉快的事吧。",
                "2_H023": "那你早点说啊。",
                "2_H024": "喂，人类，和阿龙小姐一起外出吧。",
                "2_H025": "那我们就出发吧。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "好，出门吧。",
                "2_H031": "重要的是先行动起来。向着你的志向前进。",
                "2_H032": "说得对，趁还能做力所能及的事。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "我不太习惯和他人缔结主从关系。",
                "2_H041": "我果然还是想和别人成为朋友。没错，和你也一样。",
                "2_H042": "综上所述，握手，能请你多关照吗？",
                "2_H043": "太狡猾了，让阿龙小姐也握握啦，握手握手。",
                "2_H044": "嗯，注意别用力过猛哦。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "你的目标，我的目标，两者是否一样并不重要。",
                "2_H051": "无论目的为何，我都想助你一臂之力。",
                "2_H052": "所以我想和你不以御主和从者的关系，让我想想……",
                "2_H053": "嗯，能是对等的同盟关系就好了。",
                "2_H054": "可以叫做龙御主同盟呢。",
                "2_H055": "虽然读起来稍微有点奇怪呢。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "喜欢的东西吗……",
                "2_H061": "是啊，还是喜欢大海吧。",
                "2_H062": "小时候就经常眺望着大海发呆嘛。",
                "2_H063": "然后发呆到天黑都没意识到，最终被家里人狠狠训斥哦。",
                "2_H064": "阿龙小姐也喜欢大海哦。",
                "2_H065": "虽然以前并不喜欢……",
                "2_H066": "——嗯，现在喜欢。",
                "2_H067": "这样啊……",
                "2_H068": "谢谢你，阿龙小姐。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西吗，应该还是讨厌悲伤的事吧。",
                "2_H071": "虽然还是有很多无能为力的事，但在我力所能及范围内，还是希望能稍微减少一些悲伤的事。",
                "2_H072": "正是怀着这种想法，我过去和现在才会站在这个位置。",
                "2_H073": "不要那么沮丧嘛，龙马，悲伤的事只要用更为快乐的事打飞就行了嘛。",
                "2_H074": "确实……",
                "2_H075": "哎呀，真是比不过阿龙小姐呢。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "希望圣杯实现的愿望吗？",
                "2_H081": "以前我也说过，我应该没有什么特别的愿望吧。",
                "2_H082": "要不要成立一个公司？",
                "2_H083": "阿龙小姐随时准备成为能干美女秘书哦。",
                "2_H084": "这种事还是自己亲手干比较有意思哦。",
                "2_H085": "对了，要不要和我以及阿龙小姐一起，成立一个新公司呢？",
                "2_H086": "哦，干吧干吧。"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "哟，第六天魔王信长公，不，应该是名副其实的魔王信长公吧。",
                "2_H171": "我本以为自己来这里是为了和信长公战斗，但看来并非如此呢。",
                "2_H172": "感觉龙马经常容易搞错哦。",
                "2_H173": "是，是吗？",
                "2_H174": "没什么，抑止力啊守护者云云的问题只要听个一半就够了。",
                "2_H175": "我们是凭自身意志来这里的。这点肯定是没错的。",
                "2_H176": "反正就算战斗也会赢嘛。"
            }
        },
        "2_H180": {
            "name": "对话 5",
            "text": {
                "2_H180": "竟然能与以藏先生并肩战斗，我真的非常高兴啊。",
                "2_H181": "嗯，我真的非常高兴。",
                "2_H182": "……呃，等一下，等一下，不要把手放在刀柄上啦。",
                "2_H183": "真的对不起，一时没注意。",
                "2_H184": "这是所谓的，阿，阿龙小姐一点都不高兴，你千万不要误会了哦。",
                "2_H185": "你是从哪儿学会这种说法的啊。"
            }
        },
        "2_H190": {
            "name": "对话 6",
            "text": {
                "2_H190": "原来如此，这个形象才是能最大程度发挥你原本能力的形象啊。",
                "2_H191": "嗯，你毫无疑问是可以被称为抑止守护者阶级的英灵啊。",
                "2_H192": "——这样啊，你是这样达成了啊。",
                "2_H193": "话说小个子的那个看上去很好吃呢。",
                "2_H194": "你要是这么做，结果可不止是吃坏个肚子就能解决的问题哦，还是别吃了好吗。"
            }
        },
        "2_H200": {
            "name": "对话 7",
            "text": {
                "2_H200": "新选组三番队队长，斋藤一。",
                "2_H201": "活到了明治时代的新选组吗……",
                "2_H202": "哎呀，我也好想见识一下明治那个时代呢。",
                "2_H203": "……抱歉，龙马。",
                "2_H204": "啊，不，不是这个意思啦。不用介意。"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "人不可貌相，阿龙小姐其实非常凶暴，还是别惹怒阿龙小姐为好哦。",
                "2_H20091": "哎哟，看来今天脾气不太好呢。",
                "2_H20092": "对不起啦，希望你暂时别理她为好哦。"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "比起什么都不做，还是趁能行动时动起来比较好。",
                "2_H20101": "有志向的话，就更不用说了哦。",
                "2_H20102": "怎么了，龙马，说话突然那么正经。",
                "2_H20103": "哈哈哈，我自认一直很正经哦。",
                "2_H20104": "嗯，这话确实不该由我来说呢。",
                "2_H20105": "对，你应该早就明白了。"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "其实我还是相当擅长使枪术，或者说，擅长使长兵器哦。",
                "2_H20111": "年轻时我还在江户拿过长刀的证书哦。",
                "2_H20112": "喂，那段故事阿龙似乎还是第一次听说哦。",
                "2_H20113": "咦，我没告诉过你吗？",
                "2_H20114": "不过，归根结底，我还是斗不过刀、枪或是火枪。更不用说阿龙小姐了。",
                "2_H20115": "哼哼～，你还是很明白的嘛。"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "土佐方言吗？",
                "2_H20121": "或许是因为用这种口音说话已经很久了，最近稍微有些懈怠吧。",
                "2_H20122": "偶尔说说方言也无妨哦。你来山里时也是那种口音吧？",
                "2_H20123": "啊哈哈哈，是吗？",
                "2_H20124": "那会儿俺还天不怕地不怕嘞，不，应该算啥都不懂的愣头青吧。",
                "2_H20125": "听起来像以藏了，还是别这么说了。",
                "2_H20126": "哎呀，这话可千万别告诉以藏先生哦。"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H20130": "我呀，没什么大不了的。只是希望大家能够幸福。",
                "2_H20131": "没错，离开土佐时的我，内心只有这个念头。",
                "2_H20132": "但是啊，我并不知道。",
                "2_H20133": "不知道希望大家能够幸福是件多么残酷的事。",
                "2_H20134": "龙马……",
                "2_H20135": "但我遇见了你……没错，来到这迦勒底，我还是会这么想。",
                "2_H20136": "希望大家能够幸福哦。",
                "2_H20137": "说得对，希望阿龙小姐、龙马和你都能幸福哦。",
                "2_H20138": "说得对，我真的这么想。",
                "2_H20139": "我希望你，希望大家都能幸福。"
            }
        },
        "2_H210": {
            "name": "对话 8",
            "text": {
                "2_H210": "没想到继信长公之后，连那位谦信公都出现了呀。",
                "2_H211": "照这样下去，像我这种段位的英灵终于要跟不上了吧？",
                "2_H212": "放心吧，阿龙小姐的段位要比那些家伙要高100倍以上哦。",
                "2_H213": "既然这样，那我就放心了。"
            }
        },
        "2_H220": {
            "name": "对话 9",
            "text": {
                "2_H220": "旅行者……前往星之大海的英灵吗。",
                "2_H221": "这样啊，人类竟然连遥远的天空彼方都能去了啊。",
                "2_H222": "唔唔唔……对阿龙小姐来说，这个或许确实难了点。",
                "2_H223": "不过去月亮的话，或许还能想想办法哦。",
                "2_H224": "和阿龙小姐去月世界旅行吗，一定会非常开心。"
            }
        },
        "2_H230": {
            "name": "对话 10",
            "text": {
                "2_H230": "哟，阿国小姐啊。",
                "2_H231": "虽然我们之间发生了不少事，但今后还请多多关照啦。",
                "2_H232": "你背后那个家伙相当难对付哦。",
                "2_H233": "那个迦勒底公演也很令人期待哦。",
                "2_H234": "要不阿龙小姐也来参加演出吧。"
            }
        },
        "2_H240": {
            "name": "对话 11",
            "text": {
                "2_H240": "高杉先生吗？",
                "2_H241": "嗯，他就是这样的男人。",
                "2_H242": "那家伙真是肆意妄为呢。",
                "2_H243": "哈哈，说得太对了。他从以前起就这么任性哦。",
                "2_H244": "没错，简直就是麒麟儿——不，是麒麟哦。"
            }
        },
        "2_H250": {
            "name": "对话 12",
            "text": {
                "2_H250": "喂，龙马，那家伙叫什么来着？",
                "2_H251": "……武市先生吗？",
                "2_H252": "嗯，就是那家伙就是那家伙。",
                "2_H253": "虽然发生了不少事，但我觉得以人类而言，那家伙相当有干劲哦。",
                "2_H254": "说得对……说得没错。",
                "2_H255": "——武市先生，相当出色哦。"
            }
        },
        "2_H260": {
            "name": "对话 13",
            "text": {
                "2_H260": "新选组的山南敬助啊，我生前一直很想找机会和他谈谈。",
                "2_H261": "以那群家伙的同伴而言，这家伙看上去柔柔弱弱的，感觉一打就会折断呢。",
                "2_H262": "千万别干哦，我说真的。",
                "2_H263": "不过说到打，他的剑其实非常柔软且有韧性。",
                "2_H264": "对付起来可不简单哦。"
            }
        }
    },
    "ChrVoice_ids": [
        "305000"
    ]
}
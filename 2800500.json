{
    "id": 2800500,
    "collectionNo": 372,
    "name": "特诺奇蒂特兰",
    "voices": {
        "0_B020": {
            "name": "攻击 1",
            "text": {
                "0_B020": "蝎啊！"
            }
        },
        "0_B030": {
            "name": "攻击 2",
            "text": {
                "0_B030": "就是那边……对吧！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "神之薪柴！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "心脏都市，启动。",
                "0_B051": "吾之祭坛，吾之薪柴！",
                "0_B052": "吾心爱的神官！",
                "0_B053": "这次，我决不会失去……！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "特拉洛克！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "要建造一个菜园，对吧。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "向雨神，献上鲜红之血。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "向战神，献上火之心脏。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "仅凭这种程度的兵力就想挑战我。 愚蠢到不知天高地厚，对吧。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "活力、流行、经济，无论选择哪种，都会是我的胜利……对吧。 呵呵。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "我……必须守护好才行……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "都市开发需要重新评估才行……对吧？"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "身体出现龟裂……我去寻找优质的石材了。"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "雨水搬运死亡。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "战争唤来花朵。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "都市将会繁荣。"
            }
        },
        "0_B2010": {
            "name": "攻击 9",
            "text": {
                "0_B2010": "毁灭之夜啊，开启吧。"
            }
        },
        "0_B2020": {
            "name": "攻击 10",
            "text": {
                "0_B2020": "泛滥吧，吾之泪。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "太阳历，祭祀历……"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "闪耀的吾之心脏！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "准备已经做好了……对吧？"
            }
        },
        "0_B2420": {
            "name": "攻击 11",
            "text": {
                "0_B2420": "就是那边，对吧……！"
            }
        },
        "0_B2430": {
            "name": "攻击 12",
            "text": {
                "0_B2430": "特拉洛坎！"
            }
        },
        "0_B2810": {
            "name": "攻击 13",
            "text": {
                "0_B2810": "毒蛇啊！"
            }
        },
        "0_B410": {
            "name": "攻击 3",
            "text": {
                "0_B410": "水啊。"
            }
        },
        "0_B420": {
            "name": "攻击 4",
            "text": {
                "0_B420": "头之壁！"
            }
        },
        "0_B430": {
            "name": "攻击 5",
            "text": {
                "0_B430": "请支持我……对吧。"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "伟大的神之家！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "展现部族的骄傲吧！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "举行祭祀仪式！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "赐予玛琳切死亡。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "根本不配大道，对吧。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "见识到我的英姿了嘛？　是吗，那就行了。 ……非常感谢。"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "点火星辰引导我吧！",
                "0_B801": "仰望天空，仰望壮丽的神明身姿！",
                "0_B802": "瞄准完毕，你们逃不掉了！",
                "0_B803": "在溺水之前，将你们全部溶解……！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "啊——惹怒我了。",
                "0_B811": "淅淅索索的烦死了！",
                "0_B812": "要保持城市整洁！　这不是常识吗！",
                "0_B813": "用岩浆浴，冲洗干净！"
            }
        },
        "0_B820": {
            "name": "宝具4",
            "text": {
                "0_B820": "真实的躯体，降临此处。",
                "0_B821": "当两大历法重叠之时，新生之火将会到来。",
                "0_B822": "血潮啊，浸润大地吧！",
                "0_B823": "重启动心脏都市！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "扫荡一空！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "没错，就是大神庙。"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "我会俯视你，对吧。"
            }
        },
        "0_B920": {
            "name": "攻击 7",
            "text": {
                "0_B920": "兔啊！"
            }
        },
        "0_B930": {
            "name": "攻击 8",
            "text": {
                "0_B930": "尊仰吧。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "等级提升了，对吧。",
                "0_G011": "如你所见，我仍有发展空间。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "相当杰出的贡献。",
                "0_G111": "我向你承诺更为美好的生活。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "虽然我没有打算收税……",
                "0_G121": "但这或许，还挺上瘾的。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "听说今天是你的生日。",
                "0_H011": "……我破例开放了神殿的宝物库。",
                "0_H012": "毕竟勤勉的神官就该得到报答，才行。",
                "0_H013": "这是我甚至没告诉过兄长的阿兹特克的梦之痕迹，令人目不转睛的无数财宝。",
                "0_H014": "还请不要客气，挑选自己喜欢的东西带回去吧。",
                "0_H015": "还是说——你想要去更深的禁忌房间？",
                "0_H016": "……想不想试着操纵特诺奇蒂特兰，看看？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "正在举行祭祀。",
                "0_H021": "要制定好计划，事无巨细地探索。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "你是我的神官，可不能偷懒，要出门好好工作。",
                "0_H031": "这是义务，是义务。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "既然身为从者，我自会服从命令。",
                "0_H041": "反正我原本就是不怎么表达自己意见的那种类型。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主……也就是神官吧。",
                "0_H051": "神官的义务各式各样。",
                "0_H052": "保持祭坛清洁，每天献上供品，加深对神明的理解，一生都要在神殿生活。",
                "0_H053": "……不用说，最后一条是非常重要的，对吧？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……",
                "0_H061": "黄金、白银、织物、大量的羽饰、工艺品匠人制作的黑曜石之刃。",
                "0_H062": "摆在大道上的东西我全都喜欢，哦。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？",
                "0_H071": "当然是西班牙人！",
                "0_H072": "一想起他们，脑子都要变成纳维·齐亚维特尔了！",
                "0_H073": "那些家伙治水能力烂到家了！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯……",
                "0_H081": "既然能实现愿望，那定然是放置心脏的容器吧？",
                "0_H082": "不是？　那要如何实现愿望啊？"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "特拉洛克是雨神。",
                "0_H091": "觉得是位和平的神明？",
                "0_H092": "哈，肤浅，而且非常矮小呢。",
                "0_H093": "在没有大型河流的阿兹特克，司掌雨、水，相当于拿生命作为人质。",
                "0_H094": "明白吗？　如果惹怒了特拉洛克，区区一两个村落就会轻易消失哦。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "维齐洛波奇特利既是战神，也是阿兹特克的部族神。是管理战士们灵魂的存在。",
                "0_H101": "是从兄长……特斯卡特利波卡神派生而出的神，权能也十分相似。",
                "0_H102": "……不过，毕竟是阿兹特克人的守护神，所以并不是全能的，对吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "由于被长时间，且虔诚地信仰，我才可以使用神的功能。",
                "0_H111": "力量归我，荣耀归源头的神明。",
                "0_H112": "是对彼此都没损失的关系。",
                "0_H113": "……只不过，特拉洛克神和维齐洛波奇特利神都是伟大的神明，要做出他们的样子来还是颇费一番工夫的。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "城市中原本曾有很多神官。",
                "0_H121": "他们都是勤奋、热心、强壮的人……但并不整洁。",
                "0_H122": "由于每天都要切开活祭，总是被血溅得浑身鲜红。",
                "0_H123": "因此感染疾病死去的神官也不在少数。",
                "0_H124": "无论是国王、贵族，还是神官，都要一直付出代价的世界。那就是阿兹特克，所有人都会牺牲的世界。",
                "0_H125": "……觉得那个世界很残酷吗？",
                "0_H126": "我想也是，我也有同感。",
                "0_H127": "但是，我并不会否定阿兹特克的文化。在残酷的密林生活就意味着这种结果。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "特拉洛克和维齐洛波奇特利。通过祭祀这两尊神明，特诺奇蒂特兰获得了繁荣。",
                "0_H131": "当鄙视地认为『反正不过是密林中的原住民城市』的西班牙人看到我后，顿时哑口无言。",
                "0_H132": "还说什么『即便遍寻世界，也找不到如此雄伟、坚固、美丽的城市。简直就是地表的宝石』。",
                "0_H133": "你欠缺的就是这种反应。",
                "0_H134": "也就是说……每一次，你都应该表示感动，对吧。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "伟大的特斯卡特利波卡。虽然是自神话时代起拥有多种面孔，不知究竟会做什么的人物……",
                "0_H171": "但竟然会毫不犹豫选择了现代的服装……",
                "0_H172": "现代的……",
                "0_H173": "不愧是兄长！　我也觉得很不错！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "不要在我面前提魁札尔·科亚特尔。",
                "0_H181": "那家伙从来不干什么好事。",
                "0_H182": "在特奥蒂瓦坎引发与特拉洛克神的派系纷争，在纳维·奥塞洛特尔则妨碍兄长，之后甚至老奸巨猾地在一旁全盘抢走了兄长的人气。",
                "0_H183": "而且连玉米都变成那家伙带来的！",
                "0_H184": "继玛雅的所作所为之后，真令人受不了！　阿兹特克人民的这方面就是令人无语呢！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "豹人……",
                "0_H191": "啊，那个。",
                "0_H192": "真了不起呢，竟然还有那种美洲豹战士。",
                "0_H193": "问我为何不惊讶？　因为那个是某个异闻带的豹人吧？",
                "0_H194": "我兄长的分灵才不可能那么有趣呢。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "奥斯曼狄斯。",
                "0_H201": "尼托克丽丝常常提及，我还以为是什么名词用语呢。",
                "0_H202": "分明只要说是拉美西斯二世就会立刻明白……",
                "0_H203": "哪怕一次也好，不知能否请他帮忙改建神殿呢……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "哎，等一下，刚才的视频数据再让我看一遍。",
                "0_H211": "……这就是异闻带的阿房宫……",
                "0_H212": "虽然仔细看来相当恶心，但再仔细看看就会觉得厉害到令人头晕目眩……",
                "0_H213": "变态……何等变态的行径……真想亲眼看到实物……",
                "0_H214": "哎，扔掉了？　始皇帝自己扔的？"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "让城市在空中漂浮，上下颠倒。",
                "0_H221": "呵呵，真是狂妄。但我能够理解，这是出于宇宙飞船的创意，对吧。",
                "0_H222": "塞弥拉弥斯——我记住这个名字了。",
                "0_H223": "她一定很喜欢玉米吧。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "无论多么坚固的要塞，都会因一把锁导致沦陷。",
                "0_H231": "在经历了激烈漫长守城后的最终决战，竟然会遭遇如此不幸……",
                "0_H232": "真是座臂长、贞淑而崇高的城市。",
                "0_H233": "最重要的是，她一定为皇帝的临终感到悲伤吧。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "第一个从者……白垩之城，精灵与人类之城。",
                "0_H241": "……这个嘛，或许是座不错的城市，对吧。但还不如我的阿兹特兰。",
                "0_H242": "圣杯还有剩吗？　在为她使用之前，先为我改建吧，好吗？"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "黄金剧场什么的，在公共设施上花费预算过多了吧——！？",
                "0_H251": "……失礼了，稍微有些亢奋过头了。",
                "0_H252": "…………话说回来，虽然不到黄金的程度，但我有一座用绿石装饰的庭园。",
                "0_H253": "里面有很多鸟类、色彩鲜艳的草木，以及美丽的水道。",
                "0_H254": "……是个只有国王与其妃子可以进入的，特别的，空间。"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "真是个令人无语的国家，对吧。盘踞在城里的邪灵竟然自称公主。",
                "0_H261": "……不过，由于脸皮过厚，反而让我产生了兴趣。",
                "0_H262": "公主……公主……是吗，还可以这样啊……"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "……呼唤我的是你，对吧。",
                "0_S011": "我是雨神特拉洛克。",
                "0_S012": "或是部族之神兼战神的维齐洛波奇特利。",
                "0_S013": "用你觉得适合的名字称呼我便可。对我来说，哪种都没什么差别……对吧。"
            }
        },
        "0_S110": {
            "name": "召唤(2部7章通关后)",
            "text": {
                "0_S110": "回应召唤建筑……不，是前来。",
                "0_S111": "你好。我是从者，身披角色者。",
                "0_S112": "既然你已经知道了，那也没有隐瞒的必要了，我会积极地自我介绍。",
                "0_S113": "我是领受了特拉洛克神与维齐洛波奇特利神的阿兹特克文明最大的水上都市，也可以说是你的永居之地。",
                "0_S114": "请亲切地称呼我为特诺奇蒂特兰吧……对吧？"
            }
        },
        "1_B020": {
            "name": "攻击 1",
            "text": {
                "1_B020": "蝎子。"
            }
        },
        "1_B030": {
            "name": "攻击 2",
            "text": {
                "1_B030": "淋湿。"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "过于美丽……对吧？"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "心脏都市，启动。",
                "1_B051": "吾之祭坛，吾之薪柴！",
                "1_B052": "吾心爱的神官！",
                "1_B053": "这次，我决不会失去……！"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "眼镜，眼镜……"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "我试着搭配了一下。这就是所谓的微服私访。"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "如果对流行不够敏感的话，是无法自称神明的……对吧？"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "简直就像仙人掌岩石呢……看来稍后需要好好逼问，对吧。"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "夸奖我吧。说特拉洛克神是多么的魅力。 这是事实，所以我不会怪你过于轻率的，哦？"
            }
        },
        "1_B140": {
            "name": "受击 2",
            "text": {
                "1_B140": "快住手！"
            }
        },
        "1_B150": {
            "name": "受击 1",
            "text": {
                "1_B150": "要保持城市整洁……！"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "这，或许就是楚楚可怜的代价吧。"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "我可不接受教训，下次也会穿这身衣服上……对吧？"
            }
        },
        "1_B180": {
            "name": "指令卡 1",
            "text": {
                "1_B180": "真是的……"
            }
        },
        "1_B190": {
            "name": "指令卡 2",
            "text": {
                "1_B190": "你还有其他话……"
            }
        },
        "1_B200": {
            "name": "指令卡 3",
            "text": {
                "1_B200": "要说吧？"
            }
        },
        "1_B2010": {
            "name": "攻击 9",
            "text": {
                "1_B2010": "需要工程作业。"
            }
        },
        "1_B2020": {
            "name": "攻击 10",
            "text": {
                "1_B2020": "血雨呢……对吧。"
            }
        },
        "1_B2030": {
            "name": "攻击 11",
            "text": {
                "1_B2030": "很痛吧？"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "你喜欢巨大重机吧？"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "让你们见识一下，什么叫水平的差距。"
            }
        },
        "1_B230": {
            "name": "宝具卡 3",
            "text": {
                "1_B230": "……要坐上来吗？"
            }
        },
        "1_B2410": {
            "name": "攻击 12",
            "text": {
                "1_B2410": "不要惹怒我。"
            }
        },
        "1_B2420": {
            "name": "攻击 13",
            "text": {
                "1_B2420": "我要杀死你……哦？"
            }
        },
        "1_B410": {
            "name": "攻击 3",
            "text": {
                "1_B410": "要感谢。"
            }
        },
        "1_B420": {
            "name": "攻击 4",
            "text": {
                "1_B420": "你到底在看哪里？"
            }
        },
        "1_B430": {
            "name": "攻击 5",
            "text": {
                "1_B430": "不断积累。"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "被打飞吧！"
            }
        },
        "1_B480": {
            "name": "技能 3",
            "text": {
                "1_B480": "我会好好蹂躏你们。"
            }
        },
        "1_B490": {
            "name": "技能 4",
            "text": {
                "1_B490": "俘虏？　还是活祭？"
            }
        },
        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "华丽地定胜负。给我看好了。"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "点火星辰引导我吧！",
                "1_B801": "仰望天空，仰望壮丽的神明身姿！",
                "1_B802": "瞄准完毕，你们逃不掉了！",
                "1_B803": "在溺水之前，将你们全部溶解……！"
            }
        },
        "1_B810": {
            "name": "宝具 3",
            "text": {
                "1_B810": "啊——惹怒我了。",
                "1_B811": "淅淅索索的烦死了！",
                "1_B812": "要保持城市整洁！　这不是常识吗！",
                "1_B813": "用岩浆浴，冲洗干净！"
            }
        },
        "1_B820": {
            "name": "宝具4",
            "text": {
                "1_B820": "真实的躯体，降临此处。",
                "1_B821": "当两大历法重叠之时，新生之火将会到来。",
                "1_B822": "血潮啊，浸润大地吧！",
                "1_B823": "重启动心脏都市！"
            }
        },
        "1_B840": {
            "name": "额外攻击 3",
            "text": {
                "1_B840": "滚落吧！"
            }
        },
        "1_B880": {
            "name": "技能 5",
            "text": {
                "1_B880": "我要动真格了……"
            }
        },
        "1_B890": {
            "name": "技能 6",
            "text": {
                "1_B890": "打扫垃圾。"
            }
        },
        "1_B920": {
            "name": "攻击 7",
            "text": {
                "1_B920": "怎么……样！"
            }
        },
        "1_B930": {
            "name": "攻击 8",
            "text": {
                "1_B930": "可以照射吧？"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "如你所见，升级也能正常完成。",
                "1_G011": "没有任何奇怪的地方吧？"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "因为看你似乎也挺喜欢的，所以我会再保持这身打扮一会儿。",
                "1_G021": "不用装傻了。就算你不说，从表情上也能看出来，哦。"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "衣服换好了。",
                "1_G031": "只是为了配合兄长而已。",
                "1_G032": "并不是为了配合迦勒底。"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "呵呵——",
                "1_G111": "你已经迷上我了，对吧？"
            }
        },
        "1_G120": {
            "name": "升级 3",
            "text": {
                "1_G120": "太性急了。",
                "1_G121": "都市开发应该更为慎重地进行。",
                "1_G122": "……不要因为操之过急而受伤，对吧。"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "早上好。",
                "1_H011": "昨天我恰好得到了南美旅行的票。",
                "1_H012": "但是，因为是双人套票，规定我一个人去不了。",
                "1_H013": "我已经帮你提交请假单了，你只要整理好行李就行了。",
                "1_H014": "啊，对了对了。换洗衣服要准备一周份，哦。"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "御主，有新商店开张了。",
                "1_H021": "为了给我们的都市开发提供参考，去看看店里的品类吧，好吗？"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "为了给建造城市提供参考，我要去其他城市视察。",
                "1_H031": "你会……跟我一起去吧？"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "……虽然我不会强迫你，但维护是很重要的，哟。"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "神官用我们的语言应该怎么说？",
                "1_H051": "知道这个又能怎样？",
                "1_H052": "对战斗没有任何影响，又不会提升魔力，总体毫无意义，所以我不会回答。",
                "1_H053": "……那个，是tlamacazqui。"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "喜欢的东西……",
                "1_H061": "说出来你就能为我准备吗？",
                "1_H062": "呵呵，不用了。我已经得到了。",
                "1_H063": "虽然，并不是东西就是了。"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "讨厌的东西吗？",
                "1_H071": "……真不可思议，这个形象时，脑袋不会发热沸腾。",
                "1_H072": "现在讨厌的东西，是会介入妨碍这里的东西哦。"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "我听说，圣杯也可以当做奖杯。",
                "1_H081": "如果没有用武之地的话，可以先给我保管，好吗？"
            }
        },
        "1_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H090": "特拉洛克是雨神。",
                "1_H091": "觉得是位和平的神明？",
                "1_H092": "哈，肤浅，而且非常矮小呢。",
                "1_H093": "在没有大型河流的阿兹特克，司掌雨、水，相当于拿生命作为人质。",
                "1_H094": "明白吗？　如果惹怒了特拉洛克，区区一两个村落就会轻易消失哦。"
            }
        },
        "1_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H100": "维齐洛波奇特利既是战神，也是阿兹特克的部族神。是管理战士们灵魂的存在。",
                "1_H101": "是从兄长……特斯卡特利波卡神派生而出的神，权能也十分相似。",
                "1_H102": "……不过，毕竟是阿兹特克人的守护神，所以并不是全能的，对吧。"
            }
        },
        "1_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H110": "由于被长时间，且虔诚地信仰，我才可以使用神的功能。",
                "1_H111": "力量归我，荣耀归源头的神明。",
                "1_H112": "是对彼此都没损失的关系。",
                "1_H113": "……只不过，特拉洛克神和维齐洛波奇特利神都是伟大的神明，要做出他们的样子来还是颇费一番工夫的。"
            }
        },
        "1_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H120": "城市中原本曾有很多神官。",
                "1_H121": "他们都是勤奋、热心、强壮的人……但并不整洁。",
                "1_H122": "由于每天都要切开活祭，总是被血溅得浑身鲜红。",
                "1_H123": "因此感染疾病死去的神官也不在少数。",
                "1_H124": "无论是国王、贵族，还是神官，都要一直付出代价的世界。那就是阿兹特克，所有人都会牺牲的世界。",
                "1_H125": "……觉得那个世界很残酷吗？",
                "1_H126": "我想也是，我也有同感。",
                "1_H127": "但是，我并不会否定阿兹特克的文化。在残酷的密林生活就意味着这种结果。"
            }
        },
        "1_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H130": "特拉洛克和维齐洛波奇特利。通过祭祀这两尊神明，特诺奇蒂特兰获得了繁荣。",
                "1_H131": "当鄙视地认为『反正不过是密林中的原住民城市』的西班牙人看到我后，顿时哑口无言。",
                "1_H132": "还说什么『即便遍寻世界，也找不到如此雄伟、坚固、美丽的城市。简直就是地表的宝石』。",
                "1_H133": "你欠缺的就是这种反应。",
                "1_H134": "也就是说……每一次，你都应该表示感动，对吧。"
            }
        },
        "1_H270": {
            "name": "对话 4",
            "text": {
                "1_H270": "尼托克丽丝……",
                "1_H271": "是个阴暗的女人吧。",
                "1_H272": "努力、认真、珍视民众……没有任何可以共鸣之处。",
                "1_H273": "用水攻杀死反叛者的品味倒是相当不错……但我多半和她合不来。",
                "1_H274": "别把我们俩单独放在一起，行吧？"
            }
        },
        "1_H280": {
            "name": "对话 5",
            "text": {
                "1_H280": "海的对面竟有如此美丽的鸟儿——",
                "1_H281": "鹤？　是叫鹤吗？",
                "1_H282": "而且还能迅速准备定制服装？",
                "1_H283": "啊，但是，她是如何知道尺寸的……",
                "1_H284": "大多从者的尺寸早就调查完毕了？",
                "1_H285": "唉……只是个罪犯啊……",
                "1_H286": "虽然是个恶心的从者……但因为她朋友太少了，做她的朋友也是神明的职责，对吧。"
            }
        },
        "1_H290": {
            "name": "对话 6",
            "text": {
                "1_H290": "心中藏着城堡的守护像，为了领民在空中飞翔，用钻头突击，用导弹乱射……",
                "1_H291": "毫无疑问是畅销书吧。简直就是天才，也对我说说这个人的其他著作吧。",
                "1_H292": "哎？　这是，童话故事吧？"
            }
        },
        "1_H300": {
            "name": "对话 7",
            "text": {
                "1_H300": "海市蜃楼。海底还有那样巨大的都市，对吧。",
                "1_H301": "下次休假期间去视察一下好了，同样身为水之都，可以用作参考……",
                "1_H302": "哈？　都市建在贝壳之上？　还将那座都市当做床铺？　现代的希巴尔巴？",
                "1_H303": "还是算了。度假还是选位于日本千叶的海湾乐园吧，对吧？"
            }
        },
        "1_H310": {
            "name": "对话 8",
            "text": {
                "1_H310": "小刑……刑部姬换上了泳装……",
                "1_H311": "……身为御主，应该明白我想要求什么，对吧？"
            }
        },
        "1_H320": {
            "name": "对话 9",
            "text": {
                "1_H320": "将城堡作为放大器的创意。",
                "1_H321": "我和伊丽莎白关系还不错。",
                "1_H322": "她服装的品味也不赖。",
                "1_H323": "她当然也邀请过我一起演出。",
                "1_H324": "要塞扩音器，和手持吉他的都市型引擎……",
                "1_H325": "很摇滚吧？"
            }
        },
        "1_H330": {
            "name": "对话 10",
            "text": {
                "1_H330": "尼禄皇帝也可以穿泳装吗！",
                "1_H331": "……失礼了，我失态了。",
                "1_H332": "我随时都做好了准备。",
                "1_H333": "就叫特诺奇园……如何？"
            }
        },
        "2_B020": {
            "name": "攻击 1",
            "text": {
                "2_B020": "蝎啊！"
            }
        },
        "2_B030": {
            "name": "攻击 2",
            "text": {
                "2_B030": "就是那边……对吧！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "神之薪柴！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "心脏都市，启动。",
                "2_B051": "吾之祭坛，吾之薪柴！",
                "2_B052": "吾心爱的神官！",
                "2_B053": "这次，我决不会失去……！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "特拉洛克！"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "要建造一个菜园，对吧。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "向雨神，献上鲜红之血。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "向战神，献上火之心脏。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "仅凭这种程度的兵力就想挑战我。 愚蠢到不知天高地厚，对吧。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "活力、流行、经济，无论选择哪种，都会是我的胜利……对吧。 呵呵。"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "我……必须守护好才行……！"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "都市开发需要重新评估才行……对吧？"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "身体出现龟裂……我去寻找优质的石材了。"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "雨水搬运死亡。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "战争唤来花朵。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "都市将会繁荣。"
            }
        },
        "2_B2010": {
            "name": "攻击 9",
            "text": {
                "2_B2010": "毁灭之夜啊，开启吧。"
            }
        },
        "2_B2020": {
            "name": "攻击 10",
            "text": {
                "2_B2020": "泛滥吧，吾之泪。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "太阳历，祭祀历……"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "闪耀的吾之心脏！"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "准备已经做好了……对吧？"
            }
        },
        "2_B2420": {
            "name": "攻击 11",
            "text": {
                "2_B2420": "就是那边，对吧……！"
            }
        },
        "2_B2430": {
            "name": "攻击 12",
            "text": {
                "2_B2430": "特拉洛坎！"
            }
        },
        "2_B2810": {
            "name": "攻击 13",
            "text": {
                "2_B2810": "毒蛇啊！"
            }
        },
        "2_B410": {
            "name": "攻击 3",
            "text": {
                "2_B410": "水啊。"
            }
        },
        "2_B420": {
            "name": "攻击 4",
            "text": {
                "2_B420": "头之壁！"
            }
        },
        "2_B430": {
            "name": "攻击 5",
            "text": {
                "2_B430": "请支持我……对吧。"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "伟大的神之家！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "展现部族的骄傲吧！"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "举行祭祀仪式！"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "赐予玛琳切死亡。"
            }
        },
        "2_B510": {
            "name": "开始 4",
            "text": {
                "2_B510": "根本不配大道，对吧。"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "见识到我的英姿了嘛？　是吗，那就行了。 ……非常感谢。"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "点火星辰引导我吧！",
                "2_B801": "仰望天空，仰望壮丽的神明身姿！",
                "2_B802": "瞄准完毕，你们逃不掉了！",
                "2_B803": "在溺水之前，将你们全部溶解……！"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "啊——惹怒我了。",
                "2_B811": "淅淅索索的烦死了！",
                "2_B812": "要保持城市整洁！　这不是常识吗！",
                "2_B813": "用岩浆浴，冲洗干净！"
            }
        },
        "2_B820": {
            "name": "宝具4",
            "text": {
                "2_B820": "真实的躯体，降临此处。",
                "2_B821": "当两大历法重叠之时，新生之火将会到来。",
                "2_B822": "血潮啊，浸润大地吧！",
                "2_B823": "重启动心脏都市！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "扫荡一空！"
            }
        },
        "2_B880": {
            "name": "技能 5",
            "text": {
                "2_B880": "没错，就是大神庙。"
            }
        },
        "2_B890": {
            "name": "技能 6",
            "text": {
                "2_B890": "我会俯视你，对吧。"
            }
        },
        "2_B920": {
            "name": "攻击 7",
            "text": {
                "2_B920": "兔啊！"
            }
        },
        "2_B930": {
            "name": "攻击 8",
            "text": {
                "2_B930": "尊仰吧。"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "等级提升了，对吧。",
                "2_G011": "如你所见，我仍有发展空间。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "特拉洛克、维齐洛波奇特利只是表面名字。",
                "2_G041": "我的真名是特诺奇蒂特兰。是足以算作阿兹特克帝国心脏的水与花之都的化身。",
                "2_G042": "有资格看到这个形象的只有最高位的神官。",
                "2_G043": "其中的含义……你应该明白，对吧？"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "阿兹特克的人民失去了故乡，经历了漫长的流浪，抵达了安居之地。",
                "2_G051": "那就是被取名为月之湖的，这片湖。",
                "2_G052": "我原本应该是身在这片湖中的精灵吧。",
                "2_G053": "精灵本应会回避人类。但我并没有离开湖，而且不知不觉成了都市之心。",
                "2_G054": "不是想成为人类，也不是想成为神明。",
                "2_G055": "只是——他们的生活，与萌芽般的笑声令我十分惬意而已。",
                "2_G056": "他们爱着我的湖，而我也非常怜爱他们。"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "相当杰出的贡献。",
                "2_G111": "我向你承诺更为美好的生活。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "虽然我没有打算收税……",
                "2_G121": "但这或许，还挺上瘾的。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "听说今天是你的生日。",
                "2_H011": "……我破例开放了神殿的宝物库。",
                "2_H012": "毕竟勤勉的神官就该得到报答，才行。",
                "2_H013": "这是我甚至没告诉过兄长的阿兹特克的梦之痕迹，令人目不转睛的无数财宝。",
                "2_H014": "还请不要客气，挑选自己喜欢的东西带回去吧。",
                "2_H015": "还是说——你想要去更深的禁忌房间？",
                "2_H016": "……想不想试着操纵特诺奇蒂特兰，看看？"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "正在举行祭祀。",
                "2_H021": "要制定好计划，事无巨细地探索。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "你是我的神官，可不能偷懒，要出门好好工作。",
                "2_H031": "这是义务，是义务。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "既然身为从者，我自会服从命令。",
                "2_H041": "反正我原本就是不怎么表达自己意见的那种类型。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "御主……也就是神官吧。",
                "2_H051": "神官的义务各式各样。",
                "2_H052": "保持祭坛清洁，每天献上供品，加深对神明的理解，一生都要在神殿生活。",
                "2_H053": "……不用说，最后一条是非常重要的，对吧？"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "喜欢的东西……",
                "2_H061": "黄金、白银、织物、大量的羽饰、工艺品匠人制作的黑曜石之刃。",
                "2_H062": "摆在大道上的东西我全都喜欢，哦。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西？",
                "2_H071": "当然是西班牙人！",
                "2_H072": "一想起他们，脑子都要变成纳维·齐亚维特尔了！",
                "2_H073": "那些家伙治水能力烂到家了！"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "圣杯……",
                "2_H081": "既然能实现愿望，那定然是放置心脏的容器吧？",
                "2_H082": "不是？　那要如何实现愿望啊？"
            }
        },
        "2_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H090": "特拉洛克是雨神。",
                "2_H091": "觉得是位和平的神明？",
                "2_H092": "哈，肤浅，而且非常矮小呢。",
                "2_H093": "在没有大型河流的阿兹特克，司掌雨、水，相当于拿生命作为人质。",
                "2_H094": "明白吗？　如果惹怒了特拉洛克，区区一两个村落就会轻易消失哦。"
            }
        },
        "2_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H100": "维齐洛波奇特利既是战神，也是阿兹特克的部族神。是管理战士们灵魂的存在。",
                "2_H101": "是从兄长……特斯卡特利波卡神派生而出的神，权能也十分相似。",
                "2_H102": "……不过，毕竟是阿兹特克人的守护神，所以并不是全能的，对吧。"
            }
        },
        "2_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H110": "由于被长时间，且虔诚地信仰，我才可以使用神的功能。",
                "2_H111": "力量归我，荣耀归源头的神明。",
                "2_H112": "是对彼此都没损失的关系。",
                "2_H113": "……只不过，特拉洛克神和维齐洛波奇特利神都是伟大的神明，要做出他们的样子来还是颇费一番工夫的。"
            }
        },
        "2_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H120": "城市中原本曾有很多神官。",
                "2_H121": "他们都是勤奋、热心、强壮的人……但并不整洁。",
                "2_H122": "由于每天都要切开活祭，总是被血溅得浑身鲜红。",
                "2_H123": "因此感染疾病死去的神官也不在少数。",
                "2_H124": "无论是国王、贵族，还是神官，都要一直付出代价的世界。那就是阿兹特克，所有人都会牺牲的世界。",
                "2_H125": "……觉得那个世界很残酷吗？",
                "2_H126": "我想也是，我也有同感。",
                "2_H127": "但是，我并不会否定阿兹特克的文化。在残酷的密林生活就意味着这种结果。"
            }
        },
        "2_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H130": "特拉洛克和维齐洛波奇特利。通过祭祀这两尊神明，特诺奇蒂特兰获得了繁荣。",
                "2_H131": "当鄙视地认为『反正不过是密林中的原住民城市』的西班牙人看到我后，顿时哑口无言。",
                "2_H132": "还说什么『即便遍寻世界，也找不到如此雄伟、坚固、美丽的城市。简直就是地表的宝石』。",
                "2_H133": "你欠缺的就是这种反应。",
                "2_H134": "也就是说……每一次，你都应该表示感动，对吧。"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "伟大的特斯卡特利波卡。虽然是自神话时代起拥有多种面孔，不知究竟会做什么的人物……",
                "2_H171": "但竟然会毫不犹豫选择了现代的服装……",
                "2_H172": "现代的……",
                "2_H173": "不愧是兄长！　我也觉得很不错！"
            }
        },
        "2_H180": {
            "name": "对话 5",
            "text": {
                "2_H180": "不要在我面前提魁札尔·科亚特尔。",
                "2_H181": "那家伙从来不干什么好事。",
                "2_H182": "在特奥蒂瓦坎引发与特拉洛克神的派系纷争，在纳维·奥塞洛特尔则妨碍兄长，之后甚至老奸巨猾地在一旁全盘抢走了兄长的人气。",
                "2_H183": "而且连玉米都变成那家伙带来的！",
                "2_H184": "继玛雅的所作所为之后，真令人受不了！　阿兹特克人民的这方面就是令人无语呢！"
            }
        },
        "2_H190": {
            "name": "对话 6",
            "text": {
                "2_H190": "豹人……",
                "2_H191": "啊，那个。",
                "2_H192": "真了不起呢，竟然还有那种美洲豹战士。",
                "2_H193": "问我为何不惊讶？　因为那个是某个异闻带的豹人吧？",
                "2_H194": "我兄长的分灵才不可能那么有趣呢。"
            }
        },
        "2_H200": {
            "name": "对话 7",
            "text": {
                "2_H200": "奥斯曼狄斯。",
                "2_H201": "尼托克丽丝常常提及，我还以为是什么名词用语呢。",
                "2_H202": "分明只要说是拉美西斯二世就会立刻明白……",
                "2_H203": "哪怕一次也好，不知能否请他帮忙改建神殿呢……"
            }
        },
        "2_H210": {
            "name": "对话 8",
            "text": {
                "2_H210": "哎，等一下，刚才的视频数据再让我看一遍。",
                "2_H211": "……这就是异闻带的阿房宫……",
                "2_H212": "虽然仔细看来相当恶心，但再仔细看看就会觉得厉害到令人头晕目眩……",
                "2_H213": "变态……何等变态的行径……真想亲眼看到实物……",
                "2_H214": "哎，扔掉了？　始皇帝自己扔的？"
            }
        },
        "2_H220": {
            "name": "对话 9",
            "text": {
                "2_H220": "让城市在空中漂浮，上下颠倒。",
                "2_H221": "呵呵，真是狂妄。但我能够理解，这是出于宇宙飞船的创意，对吧。",
                "2_H222": "塞弥拉弥斯——我记住这个名字了。",
                "2_H223": "她一定很喜欢玉米吧。"
            }
        },
        "2_H230": {
            "name": "对话 10",
            "text": {
                "2_H230": "无论多么坚固的要塞，都会因一把锁导致沦陷。",
                "2_H231": "在经历了激烈漫长守城后的最终决战，竟然会遭遇如此不幸……",
                "2_H232": "真是座臂长、贞淑而崇高的城市。",
                "2_H233": "最重要的是，她一定为皇帝的临终感到悲伤吧。"
            }
        },
        "2_H240": {
            "name": "对话 11",
            "text": {
                "2_H240": "第一个从者……白垩之城，精灵与人类之城。",
                "2_H241": "……这个嘛，或许是座不错的城市，对吧。但还不如我的阿兹特兰。",
                "2_H242": "圣杯还有剩吗？　在为她使用之前，先为我改建吧，好吗？"
            }
        },
        "2_H250": {
            "name": "对话 12",
            "text": {
                "2_H250": "黄金剧场什么的，在公共设施上花费预算过多了吧——！？",
                "2_H251": "……失礼了，稍微有些亢奋过头了。",
                "2_H252": "…………话说回来，虽然不到黄金的程度，但我有一座用绿石装饰的庭园。",
                "2_H253": "里面有很多鸟类、色彩鲜艳的草木，以及美丽的水道。",
                "2_H254": "……是个只有国王与其妃子可以进入的，特别的，空间。"
            }
        },
        "2_H260": {
            "name": "对话 13",
            "text": {
                "2_H260": "真是个令人无语的国家，对吧。盘踞在城里的邪灵竟然自称公主。",
                "2_H261": "……不过，由于脸皮过厚，反而让我产生了兴趣。",
                "2_H262": "公主……公主……是吗，还可以这样啊……"
            }
        }
    },
    "ChrVoice_ids": [
        "2800500"
    ]
}
{
    "id": 103700,
    "collectionNo": 234,
    "name": "红阎魔",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "这是工作啾！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "太慢了！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "抱歉了！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "我要将你舌头劈成三片啾！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "雀检察，灯笼流……",
                "0_B051": "我不取你性命啾。",
                "0_B052": "但是，绝不原谅你那撒谎的舌啾。",
                "0_B053": "葛笼纪行，至此休庭！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "要不要先来一碗啾？"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "阎雀拔刀术，出击！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "哎呀，轮到我出场啾？ 那么开始制作料理啾。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "啾啾啾！ 剪舌麻雀红阎魔，要履行为人处世的情义啾！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "因为我是麻雀而小看我了呢。 这乃是最快、最小、最短的居合啾。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "也就这种水平啾。 回家以后要好好洗手，认真漱口啾，御主。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "一定要忍耐啾！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "老爷爷……对不起，让您不幸啾……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "都是因为刀没有磨好啾……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "利落地斩击啾。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "我会加油啾。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "有点提不啾精神……"
            }
        },
        "0_B2020": {
            "name": "攻击 11",
            "text": {
                "0_B2020": "鸟是不会从枝头坠落啾！"
            }
        },
        "0_B2030": {
            "name": "攻击 12",
            "text": {
                "0_B2030": "阎雀·雨雪羽！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "体罚的时间到了啾。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "真是个不明事理的客人啾……"
            }
        },
        "0_B2420": {
            "name": "攻击 14",
            "text": {
                "0_B2420": "残像，遥远！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "阎雀·升雏！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "紧接着是落风！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "雀六轮！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "阎雀拔刀术，奥义其一！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "你喜欢哪个藤箱啾？"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "伴手礼在这里啾！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "阎雀裁缝拔刀术，奥义其三——",
                "0_B801": "若存罪科，此乃必灭制裁。",
                "0_B802": "十王判决·葛笼纪行！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "让开都让开，阎魔驾到。",
                "0_B811": "雀之处置取决于滋味——",
                "0_B812": "从油盐酱醋糖开始啾头来过！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "雀之一刺……大旋风！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "上啾！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "啾速！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {
                "0_B930": "阎雀·削鳞！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "羽毛变漂亮啾！　好开心啾！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "啾啾？",
                "0_G021": "没什么变化啾。",
                "0_G022": "我本来还很期待的，啾遗憾……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "毕竟这里是迦勒底啾。",
                "0_G031": "总不能打扮成老板娘啾样子。",
                "0_G032": "这身衣服是以前成为狱卒时，阎魔王赐给我的。",
                "0_G033": "呵呵。怎么样啾？　是不是威风凛凛啾？"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "嘎啾！　红阎魔，完全武装啾！",
                "0_G041": "就让你见识一下被赛之河原的众鬼所畏惧的无拘无束的太刀招数，",
                "0_G042": "以及这极乐法悦的服务吧！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "欢迎光临雀之旅馆。很高兴款待您啾，主人。",
                "0_G051": "这是为感谢您啾我培养至此的小小回礼。",
                "0_G052": "尽管啾小巧的迷途之家远远比不上阎魔亭，但我会尽全力招待您啾！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "听说今天是你的生日啾，主人。",
                "0_H011": "这种事希望你能提前告诉我啾。",
                "0_H012": "啾啾。我可是阎魔亭的老板娘，是就算有突发事件也能完美应对的麻雀，所以没有问题啾。",
                "0_H013": "我开启了阎魔亭的贵客之间啾。",
                "0_H014": "河味与山珍，您喜欢哪种啾？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "在举行什么大型活动啾？",
                "0_H021": "看来食堂也会生意兴隆啾。",
                "0_H022": "我可以摆个摊啾作为阎魔亭临时分店吗？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "是任务啾？",
                "0_H031": "一路小心啾，御主。",
                "0_H032": "饭团带了啾？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "既然成了从者，就必须优秀地战斗啾。",
                "0_H041": "当然，主人的健康我也会严格管理啾。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我的居合派上用处了啾？　有派上用处啾？",
                "0_H051": "啾啾。",
                "0_H052": "那就好，",
                "0_H053": "啾！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "我最喜欢稻谷啾。",
                "0_H061": "尽管我学习啾许多料理，但最后的终点还是白米饭的饭团啾！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我不能原谅啾负弱小！",
                "0_H071": "唔？　问我能原谅撒谎啾？",
                "0_H072": "撒谎是无可奈何啾事。",
                "0_H073": "问题在于，这个谎言是否会给人添麻烦啾。",
                "0_H074": "因为我知道一个令周围的人得到幸福，只有自己陷入孤独的温柔谎言啾。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "实现愿望的圣杯……",
                "0_H081": "啾啾，西洋居然还有这种东西啾。",
                "0_H082": "但还差得远啾。",
                "0_H083": "日本可是有能无限产出大米的米袋啾！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "无论是御主还是英灵，都不允许当着我的面不啾意健康，这点请务必知晓。",
                "0_H091": "对待客人要细致有礼。对待侍奉的主人要严格。",
                "0_H092": "这就是所谓的红阎魔流服务术啾。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我已经习惯了在迦勒底的生活啾。",
                "0_H101": "当个从者也不错啾。",
                "0_H102": "热闹是啾好事。",
                "0_H103": "你就是为了这个才战斗至今啾吧。",
                "0_H104": "了不起，了不起。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "呵呵，真是位优秀的御主，令人心悦啾服。",
                "0_H111": "在战斗中的状况判断也无可挑剔啾！",
                "0_H112": "如果生活习惯能再有规律一点就好啾……",
                "0_H113": "不过，每天都像这样忙忙碌碌的也没办法啾。",
                "0_H114": "主人的健康，啾由我来好好守护。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "要……要给我放假啾？",
                "0_H121": "主人明天就要死了啾！？",
                "0_H122": "啾，啾啾？　只是普通的休息？　作为平日的谢礼……啾？",
                "0_H123": "……对于自诞生于地狱后啾经风霜，自懂事以来就是官吏的我来说，这是无上的奢啾……",
                "0_H124": "不过，红阎魔觉得照顾主人更开心啾……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "能来这里真是太好了啾。",
                "0_H131": "不，是能被召唤到这里真是太好了啾，对吧。",
                "0_H132": "我是地狱的狱卒。原本应是遭人恐惧的鬼，但大家对我露出的表情总是那么温暖啾。",
                "0_H133": "无论是主人，还是老爷爷，能遇见那么多好人，红真是太幸福了啾。",
                "0_H134": "这份恩情我定会至死，不，就算死后也要回报啾！",
                "0_H135": "当主人坠入地狱时，我红阎魔，一定会大闹法庭啾！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "唉……尽管料理的手艺提高啾，但玉藻还差得远啾……",
                "0_H171": "名为爱情的调味料放得太多，这样一来只需一个月就会把老爷养肥啾。",
                "0_H172": "自己养出一个好吃的人类有啾么意义啊……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "尽管清姬的料理很豪迈，但只要用火和油烹饪，大部分东西啾能变得很好吃。",
                "0_H181": "家庭料理是不能光靠这些啾。",
                "0_H182": "看来只能重回寺院让她认真学习典座的教义啾。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "教刑部姬料理，就等同于将雏鸟塞回蛋里，然后重新生一遍啾。",
                "0_H191": "啾啾……",
                "0_H192": "看来只能砍了她，让她转生啾异世界了吧？"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "铃鹿御前现在这样啾可以了。",
                "0_H201": "这世上还是存在无论如何都不适合做细致料理的人啾。",
                "0_H202": "和那种光靠读书无法开悟的人一样啾。",
                "0_H203": "就算是粗糙的料理，啾要营养与味道兼备，那就够了啾。",
                "0_H204": "接下来只剩找到服侍的老爷了啾。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "巴是怎么回事啾？",
                "0_H211": "是只会把人脑袋拧下来的可悲怪物啾？",
                "0_H212": "竟然在饭团里包饭团，她还有没有配菜这个概念啾？",
                "0_H213": "连皮都不削啾接让人啃萝卜，她是想把义仲大人的嘴搞得血淋淋的吗？"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "尽管我是地狱的鬼啾，但她们似乎是现世的鬼啾。",
                "0_H221": "……我能从酒吞童子身上感受到非比寻常的神之血脉啾，",
                "0_H222": "但从茨木童子身上，却感受到了和我一样的味道啾。",
                "0_H223": "……她原本是，人类啾……"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "主人，主人……！",
                "0_H231": "难难……难不成那边那位高贵的大人就是那位俵藤太大人啾！？",
                "0_H232": "我希望您能允许我用藤太大人的箭矢作为破魔箭，装饰在阎魔亭的供奉殿啾！"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "那个农民太狂妄啾……",
                "0_H241": "什么燕返啾。",
                "0_H242": "只能斩落燕啾的魔剑，真让人笑掉大……",
                "0_H243": "哎？　为什么那个农民能变出像分身似的东西啾？",
                "0_H244": "次元扭曲？　为了斩杀一只燕啾，居然不惜做到那样？",
                "0_H245": "变……变……变态啾————！！"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "那位可爱的少女啾是童谣吧。",
                "0_H251": "尽管我不太清楚，但光是看着她就会感到很温暖啾。",
                "0_H252": "如果能和她成为朋友啾好了……",
                "0_H253": "但是，她的本性似乎有点可怕啾？"
            }
        },
        "0_I010": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 9",
            "text": {
                "0_I010": "值此新春，阎魔亭的纪念品贩卖店开业啾！",
                "0_I011": "务必来多多交换啾。"
            }
        },
        "0_I020": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 10",
            "text": {
                "0_I020": "道具都搜集齐啾吗？",
                "0_I021": "迦勒底的御主真是个勤劳的人啾。",
                "0_I022": "了不起了不起。"
            }
        },
        "0_I030": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 11",
            "text": {
                "0_I030": "感谢你交换道具啾！",
                "0_I031": "收到的物品会在三途川洗干净后，被流入各个市场啾。"
            }
        },
        "0_I040": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 12",
            "text": {
                "0_I040": "你可以再交换一些啾。",
                "0_I041": "用印有麻雀标记的裁缝剪刀当礼物如何啾？"
            }
        },
        "0_I050": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 13",
            "text": {
                "0_I050": "啾啾！",
                "0_I051": "这是红拼尽全力搜集来的道具，如果你能喜欢我就太高兴啾。"
            }
        },
        "0_I060": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 14",
            "text": {
                "0_I060": "阎魔亭繁盛记就此闭幕啾。",
                "0_I061": "我从你那里获得了多到无以回报的关怀啾。",
                "0_I062": "希望你今后也不要忘记这份心情啾。"
            }
        },
        "0_I070": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 15",
            "text": {
                "0_I070": "阎魔亭的纪念品贩卖店也快要关门啾。",
                "0_I071": "要交换道具就要啾现在。",
                "0_I072": "千万不要忘记什么啾。"
            }
        },
        "0_I080": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 16",
            "text": {
                "0_I080": "身为老板娘，是没有什么假期的啾。",
                "0_I081": "无论是春节还是节日，都要面带笑容迎接客人。",
                "0_I082": "这就是老板娘的气魄啾！",
                "0_I083": "不过，该休息时还是要好好休息的啾。"
            }
        },
        "0_K010": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 17",
            "text": {
                "0_K010": "帮忙修好了啾？ 谢谢啾！"
            }
        },
        "0_K020": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 18",
            "text": {
                "0_K020": "又有一个地方恢复从前的样子了啾。 啾啾。"
            }
        },
        "0_K030": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 19",
            "text": {
                "0_K030": "技术真是太卓越啾。 你难道是宫殿木匠啾？"
            }
        },
        "0_K040": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 20",
            "text": {
                "0_K040": "越来越宽敞了啾。 设计得真不错啾！"
            }
        },
        "0_K050": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 21",
            "text": {
                "0_K050": "温泉终于开放了啾！ 但是，决不能在里面游泳啾。"
            }
        },
        "0_K060": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 22",
            "text": {
                "0_K060": "为啾么要建撞钟堂？ 我有种不祥的预感啾……"
            }
        },
        "0_K070": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 23",
            "text": {
                "0_K070": "只有一点点啾。 再稍稍努力一下啾。"
            }
        },
        "0_K080": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 24",
            "text": {
                "0_K080": "今天一天辛苦你啾。 好好休息啾。"
            }
        },
        "0_K090": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 25",
            "text": {
                "0_K090": "阎魔亭生意兴隆啾！ 嘎啾！"
            }
        },
        "0_K100": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 26",
            "text": {
                "0_K100": "功……功德箱居然快要满出来啾！？ 这难道就是重振了绝望人理的迦勒底经营御主的力量啾？"
            }
        },
        "0_N010": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 1",
            "text": {
                "0_N010": "感谢之情会随着客人的满足度而改变啾。",
                "0_N011": "越认真工作，各种祈祷就越会累积起来啾。"
            }
        },
        "0_N020": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 2",
            "text": {
                "0_N020": "玉藻、清姬、巴大人…… 地狱厨房的问题儿童都到齐了啾。",
                "0_N021": "刑部姬和铃鹿大人不在已经是不幸中的万幸啾。"
            }
        },
        "0_N030": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 3",
            "text": {
                "0_N030": "在进行下一项工作之前，一定要阅读通知啾。",
                "0_N031": "就是所谓的规则啾。",
                "0_N032": "是刑部姬写的啾。"
            }
        },
        "0_N040": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 4",
            "text": {
                "0_N040": "现在差不多做到这个程度啾。",
                "0_N041": "偶尔也该去摇摇铃铛许个愿啾。",
                "0_N042": "这也是一种感谢之情啾。"
            }
        },
        "0_N050": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 5",
            "text": {
                "0_N050": "积攒了不少啾。",
                "0_N051": "难不成迦勒底的御主是个服务的高手啾！？"
            }
        },
        "0_N060": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 6",
            "text": {
                "0_N060": "啾啾，好厉害好厉害！",
                "0_N061": "你真擅长接待啾。",
                "0_N062": "等御主的职责完成后，要不要来做阎魔亭的掌柜啾？"
            }
        },
        "0_N070": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 7",
            "text": {
                "0_N070": "已经不需要再往功德箱里投钱了啾。",
                "0_N071": "这些已经足够用来祈求一年份的消灾解厄啾！",
                "0_N072": "你相当努力啾。",
                "0_N073": "红也感同身受般高兴啾。"
            }
        },
        "0_N080": {
            "name": "雀之旅馆的活动日志 ～阎魔亭繁盛记～ 8",
            "text": {
                "0_N080": "居然能在这么短时间内凑齐了我们花费一年积蓄的心意啾。",
                "0_N081": "各位也不用遭天罚啾，可喜可贺可喜可贺啾。",
                "0_N082": "但是，红稍微有些担心啾。",
                "0_N083": "你会不会工作过头了啾？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "这里是阎魔亭。",
                "0_S011": "欢迎你们啾，客人。",
                "0_S012": "……好像不对啾。",
                "0_S013": "从者，Saber。",
                "0_S014": "剪舌麻雀红阎魔，正式决定来此侍奉您啾。",
                "0_S015": "今后请多关照啾。"
            }
        }
    },

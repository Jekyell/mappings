{
    "id": 302600,
    "collectionNo": 141,
    "name": "贞德·Alter·Santa·Lily",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "Merry 攻击～！"
            }
        },
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "现在是说教的时间！",
                "0_B041": "Twin Arm Little Crunch！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "神圣之夜，美妙无敌奇迹的瞬间！",
                "0_B051": "优雅歌唱吧，为那圣诞！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "作为圣诞老人！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "驯鹿，拜托你了！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "居然胆敢妨碍圣诞老人，成何体统！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "要是把蛋糕压坏了你要怎么赔啊！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "结束了，来吧，去派送礼物吧！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "居然胆敢伤害圣诞老人……得好好说教才行！"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "啊，好痛！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "好……好痛好热好冷！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "礼……礼物还没有配送完成啊～！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "有……有点困了……呼～"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "原来如此。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "很有道理！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "很有逻辑！"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "我要让你们领教一下真正的圣诞老人做法！"
            }
        },
        "0_C010": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 1",
            "text": {
                "0_C010": "无论怎么抽都抽不够的礼物。\r 尽量抽吧，驯鹿！"
            }
        },
        "0_C020": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 2",
            "text": {
                "0_C020": "在尼禄祭的时候已经抽得够多了所以不抽了？\r 你要是这么说，我可是会哭的哦！"
            }
        },
        "0_C030": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 3",
            "text": {
                "0_C030": "好嘞～，那就开始抽啦！\r 转啊转啊～！"
            }
        },
        "0_C040": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 4",
            "text": {
                "0_C040": "真令人期待，真令人期待呢，驯鹿！"
            }
        },
        "0_C050": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 5",
            "text": {
                "0_C050": "我是贞德·Alter·Santa·Lily！\r 啊，对不起。 只是觉得这种时候说名字会让人记住！"
            }
        },
        "0_C060": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 6",
            "text": {
                "0_C060": "终于要到最后的大奖了！\r 会出什么呢，心脏激动得跳个不停呢！"
            }
        },
        "0_C110": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 7",
            "text": {
                "0_C110": "抽出了这种东西！\r 怎么样，怎么样？"
            }
        },
        "0_C120": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 8",
            "text": {
                "0_C120": "锵锵～！\r 这是奖品！"
            }
        },
        "0_C130": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 9",
            "text": {
                "0_C130": "抽中了吗？　还是没抽中？\r 但是，反正你会整箱一起拿走，这种无关紧要吧！"
            }
        },
        "0_C140": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 10",
            "text": {
                "0_C140": "圣诞辛苦了！\r 啊，这是圣诞节限定的寒暄。"
            }
        },
        "0_C150": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 11",
            "text": {
                "0_C150": "中大奖了！\r 恭喜恭喜。"
            }
        },
        "0_C160": {
            "name": "复刻:第二代是Alter亲 \r\n～2017圣诞节～ 12",
            "text": {
                "0_C160": "这是最后一个了，驯鹿。\r 辛苦了，我要好好奖励你！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "我们一起努力吧！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "嗯，不会换什么衣服。 这是唯一的一套！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "圣诞老人是永恒的。 所以，改变外观也没什么意义。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "那个……就算不换衣服，我也很喜欢这套…… 怎么说呢，很有圣诞老人的感觉。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "驯鹿……我还是个不成熟的圣诞老人。",
                "0_G051": "但是，驯鹿依然给予了这样的我那么多的爱。",
                "0_G052": "所以我会努力的！",
                "0_G053": "总有一天，我要超越师傅圣诞岛天草假面先生！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "是御主的生日呀。 从圣诞老人的角度来说，重要度仅次于12月24日呢。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举办活动。",
                "0_H021": "以圣诞老人来看，对圣诞节以外都没什么兴趣，请一个人去吧！",
                "0_H022": "……坐立不安，坐立不安。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "驯鹿，懒得外出也要有个限度。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我是圣诞老人也是从者，你是御主也是驯鹿。",
                "0_H041": "也就是说，从道理上来看势均力敌！"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "嗯，那驯鹿，一定要努力奔跑哦！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……当然是圣诞老人的行为。",
                "0_H061": "Santa，Santer，Santest！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……是万圣节！",
                "0_H071": "从活动发源来说明明是后来者，脸皮却特别厚！",
                "0_H072": "南瓜布丁很好吃这点尤其让人火大！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯吗……",
                "0_H081": "那就许愿让一整年都是圣诞节……这样我会忙死，所以不行。",
                "0_H082": "那就让所有从者都变成圣诞老人模式！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我会作为圣诞老人好好管理御主的。",
                "0_H091": "来吧，您明天五点就得起床！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "早上五点起床太痛苦了吗？",
                "0_H101": "只要不熬夜就行了。驳倒了！",
                "0_H102": "呵呵呵，成功驳倒了！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "唔喵……啥！？",
                "0_H111": "不……我可没睡哦！",
                "0_H112": "圣诞老人可不会熬夜哦！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "哎，给我……礼物吗？　怎么能这样！",
                "0_H121": "送给圣诞老人礼物什么的，不符合道理！",
                "0_H122": "那个，但是，非常……感谢……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "哎嘿嘿，御主也是独当一面的圣诞老人，独当一面的驯鹿！ 有我贞德打包票！",
                "0_H131": "这是我觉得非常适合放在袋子里的礼物……",
                "0_H132": "如果您不介意的话，请收下。",
                "0_H133": "非常感谢！　太棒了～！"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "啊，是杰克和童谣！",
                "0_H141": "你们有好好完成作业吗？哎，从者没有作业？",
                "0_H142": "当～然～有～啦～！",
                "0_H143": "成长后的我也为了能在契约书上好好签字而 努力过哦！",
                "0_H144": "哎，成长后的我为什么会满脸通红那么生气啦……？"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "啊，成长之后的我也在呢。",
                "0_H151": "唔……不行，完全不行。",
                "0_H152": "根据那种破罐子破摔的态度，完全感受不到前途。",
                "0_H153": "向着毁灭笔直前进啊。",
                "0_H154": "连这都不明白吗？",
                "0_H155": "因为我是你，所以我～能～够～明～白～。很好，驳倒了。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "啊，是原本的我！　清纯、端庄、惹人怜爱……真是我理想的形象！",
                "0_H161": "哎，没有这种事？",
                "0_H162": "那家伙会无视他人的看法若无其事地反唇相讥， 横冲直撞，连前面有什么都不清楚四处乱闹……？",
                "0_H163": "啊哈哈哈哈，这怎么可能嘛～！"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "啊，是先代圣诞老人！",
                "0_H171": "今年由我来配送礼物，所以没问题啦！",
                "0_H172": "小孩子当圣诞老人，这世界没救了？",
                "0_H173": "别看我这样， 我的精神可比成长后的我要成熟得多，所以没关系，哎嘿！"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "虽然不是圣诞老人的圣诞师傅！",
                "0_H181": "哎，在这里的您不是圣诞老人？",
                "0_H182": "原来如此！　是秘密圣诞老人吧！",
                "0_H183": "我明白了，我会保密的！"
            }
        },
        "0_I010": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 8",
            "text": {
                "0_I010": "圣诞快乐，驯鹿。",
                "0_I011": "没关系，",
                "0_I012": "只要圣诞老人被召唤，圣诞一定快乐！"
            }
        },
        "0_I020": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 9",
            "text": {
                "0_I020": "提前一步祝您圣诞快乐，驯鹿！",
                "0_I021": "来吧，选择礼物吧。",
                "0_I022": "无论哪个都能带来笑容哦！"
            }
        },
        "0_I030": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 10",
            "text": {
                "0_I030": "还要交换吗？",
                "0_I031": "一定要",
                "0_I032": "多搜集一些袜子哦！"
            }
        },
        "0_I040": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 11",
            "text": {
                "0_I040": "等你走出商店以后，",
                "0_I041": "一定还要继续努力搜集袜子哦。",
                "0_I042": "加油吧！"
            }
        },
        "0_I050": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 12",
            "text": {
                "0_I050": "但愿驯鹿也能",
                "0_I051": "得到很多笑容。",
                "0_I052": "祝福你！"
            }
        },
        "0_I110": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 13",
            "text": {
                "0_I110": "圣诞节已经结束了。",
                "0_I111": "虽然稍微有些，相当，非常，",
                "0_I112": "极为寂寞……",
                "0_I113": "啊，一定要记得兑换道具哦！"
            }
        },
        "0_I120": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 14",
            "text": {
                "0_I120": "明年也请多关照了哦，",
                "0_I121": "驯鹿！",
                "0_I122": "后年，大后年，",
                "0_I123": "永远永远多关照哦！"
            }
        },
        "0_I3010": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 1",
            "text": {
                "0_I3010": "驯鹿，辛苦啦～！",
                "0_I3011": "我是贞德·Alter·Santa·Lily！",
                "0_I3012": "即便不是圣诞节，情人节也是个值得庆贺的日子，所以没有问题，哎嘿！"
            }
        },
        "0_I3020": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 2",
            "text": {
                "0_I3020": "我是贞德！",
                "0_I3021": "我的店员打扮有没有令您心动？",
                "0_I3022": "我那么努力是不是很了不起？",
                "0_I3023": "感谢您的夸奖～！"
            }
        },
        "0_I3030": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 3",
            "text": {
                "0_I3030": "我是和童谣一起看店的。",
                "0_I3031": "我不在的时候，您可以找童谣哦。"
            }
        },
        "0_I3040": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 4",
            "text": {
                "0_I3040": "是要兑换吧，那个，那个……",
                "0_I3041": "手实在够不到，我用一下枪。",
                "0_I3042": "嘿呀～！"
            }
        },
        "0_I3050": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 5",
            "text": {
                "0_I3050": "给，虽然不是什么好东西，还请收下！",
                "0_I3051": "唔，不是什么好东西这话太失礼了吧……",
                "0_I3052": "应该是好东西！"
            }
        },
        "0_I3060": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 6",
            "text": {
                "0_I3060": "驯鹿想要这个吗？",
                "0_I3061": "唔～，我其实还想多给你一些的……",
                "0_I3062": "但总之就这些吧，请收下！"
            }
        },
        "0_I3070": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 7",
            "text": {
                "0_I3070": "呜呜，情人节要结束了……",
                "0_I3071": "但是，我觉得非常开心！",
                "0_I3072": "驯鹿怎么样呢？　很开心吧？",
                "0_I3073": "哎嘿嘿～。"
            }
        },
        "0_I3080": {
            "name": "若安小姐与未确认之爱　\n粉碎☆LOVE LOVE HEART大石像 8",
            "text": {
                "0_I3080": "啊，来的好晚啊，驯鹿。",
                "0_I3081": "好了，已经收拾完了，我们一起回家吧。",
                "0_I3082": "握住我的手，千万不要走丢了哦。"
            }
        },
        "0_N010": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 1",
            "text": {
                "0_N010": "呵呵，万众期待的赠礼时间哟，驯鹿！",
                "0_N011": "会出现什么呢，真令人期待。",
                "0_N012": "呵呵呵，呵呵呵！"
            }
        },
        "0_N020": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 2",
            "text": {
                "0_N020": "打开不知里面有什么的礼物……",
                "0_N021": "这世上难道有比这个更令人期待兴奋的事吗？",
                "0_N022": "不，",
                "0_N023": "没有了！"
            }
        },
        "0_N030": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 3",
            "text": {
                "0_N030": "驯鹿，驯鹿！",
                "0_N031": "是礼物哦，礼物！",
                "0_N032": "下一个可以由我打开吧？",
                "0_N033": "可以吧，对吧，对吧！？"
            }
        },
        "0_N040": {
            "name": "第二代是Alter亲 \r\n～2017圣诞节～ 4",
            "text": {

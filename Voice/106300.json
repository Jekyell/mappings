    "name": "パッションリップ",
    "voices": {
        "0_B010": {
            "name": "アタック 1",
            "text": {
                "0_B010": "嘿～咻！"
            }
        },
        "0_B020": {
            "name": "アタック 2",
            "text": {
                "0_B020": "怎么样！？"
            }
        },
        "0_B030": {
            "name": "アタック 3",
            "text": {
                "0_B030": "飞踢定胜负！"
            }
        },
        "0_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "0_B040": "阳光浪漫曲！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "蜂窝编序，启动。",
                "0_B051": "再坚固的躯体，也无法与我抗衡。",
                "0_B052": "爱憎昭然，",
                "0_B053": "“爱终将招致受难”。"
            }
        },
        "0_B080": {
            "name": "スキル 1",
            "text": {
                "0_B080": "垃圾粉碎机，启动！"
            }
        },
        "0_B090": {
            "name": "スキル 2",
            "text": {
                "0_B090": "虽然不能化作白鸟！"
            }
        },
        "0_B100": {
            "name": "開始 1",
            "text": {
                "0_B100": "那是敌方的各位吗？那个，看起来挺弱的，我可以收拾掉他们吗？"
            }
        },
        "0_B110": {
            "name": "開始 2",
            "text": {
                "0_B110": "哼！又来碍事！这次绝不会手下留情了！"
            }
        },
        "0_B120": {
            "name": "勝利 1",
            "text": {
                "0_B120": "比预想的还要多花了一些时间。坏人们就在我的怀里好好反省吧！"
            }
        },
        "0_B130": {
            "name": "勝利 2",
            "text": {
                "0_B130": "各位没有受伤真是太好了。看来我的爪子也不是吃素的呢！"
            }
        },
        "0_B140": {
            "name": "ダメージ 2",
            "text": {
                "0_B140": "呀——！"
            }
        },
        "0_B150": {
            "name": "ダメージ 1",
            "text": {
                "0_B150": "一点也不……痛……！"
            }
        },
        "0_B160": {
            "name": "戦闘不能 1",
            "text": {
                "0_B160": "我输了呢……必须更坚强才行……"
            }
        },
        "0_B170": {
            "name": "戦闘不能 2",
            "text": {
                "0_B170": "难得你特意选了我……对不起，御主……"
            }
        },
        "0_B180": {
            "name": "コマンドカード 1",
            "text": {
                "0_B180": "哇哇哇哇！"
            }
        },
        "0_B190": {
            "name": "コマンドカード 2",
            "text": {
                "0_B190": "我出发啦～！"
            }
        },
        "0_B200": {
            "name": "コマンドカード 3",
            "text": {
                "0_B200": "让您久等了！"
            }
        },
        "0_B2010": {
            "name": "アタック 10",
            "text": {
                "0_B2010": "咚！咚！"
            }
        },
        "0_B2020": {
            "name": "アタック 11",
            "text": {
                "0_B2020": "瞄准射击！"
            }
        },
        "0_B2030": {
            "name": "アタック 12",
            "text": {
                "0_B2030": "打中啦～！"
            }
        },
        "0_B2080": {
            "name": "スキル 7",
            "text": {
                "0_B2080": "我也能做到的！"
            }
        },
        "0_B2090": {
            "name": "スキル 8",
            "text": {
                "0_B2090": "就算全力也无法破坏！"
            }
        },
        "0_B210": {
            "name": "宝具カード 1",
            "text": {
                "0_B210": "如花般惹人怜爱！"
            }
        },
        "0_B220": {
            "name": "宝具カード 2",
            "text": {
                "0_B220": "这份心意绝不退让！"
            }
        },
        "0_B230": {
            "name": "宝具カード 3",
            "text": {
                "0_B230": "打扰的虫子要受惩罚，对吧？"
            }
        },
        "0_B2410": {
            "name": "アタック 13",
            "text": {
                "0_B2410": "预备！"
            }
        },
        "0_B2420": {
            "name": "アタック 14",
            "text": {
                "0_B2420": "这个，我有点应付不来～！"
            }
        },
        "0_B2430": {
            "name": "アタック 15",
            "text": {
                "0_B2430": "把你当沙包玩！"
            }
        },
        "0_B2810": {
            "name": "アタック 16",
            "text": {
                "0_B2810": "呀呀呀呀呀，咚！"
            }
        },
        "0_B410": {
            "name": "アタック 4",
            "text": {
                "0_B410": "体重就是力量！"
            }
        },
        "0_B420": {
            "name": "アタック 5",
            "text": {
                "0_B420": "可爱一点！"
            }
        },
        "0_B430": {
            "name": "アタック 6",
            "text": {
                "0_B430": "回旋转身！"
            }
        },
        "0_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "0_B440": "激情崩坏！"
            }
        },
        "0_B480": {
            "name": "スキル 3",
            "text": {
                "0_B480": "就用这个腰包来接下！"
            }
        },
        "0_B490": {
            "name": "スキル 4",
            "text": {
                "0_B490": "您觉得靠垫怎么样？"
            }
        },
        "0_B500": {
            "name": "開始 3",
            "text": {
                "0_B500": "沙滩与垃圾可不搭。坏蛋们，把你们揉成一团，捏成豆粒大小！"
            }
        },
        "0_B520": {
            "name": "勝利 3",
            "text": {
                "0_B520": "谢谢大家的声援！比平时开心两倍……不，是十倍！"
            }
        },
        "0_B560": {
            "name": "戦闘不能 3",
            "text": {
                "0_B560": "虽然很痛很懊悔……但我帮上忙了吗……？那就好……！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "欢迎来到蜜蜂的迷宫。",
                "0_B801": "受难之花如牢笼般，紧握捕获的爱意不放。",
                "0_B802": "接招吧，",
                "0_B803": "热情澎湃重拳！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "嘿嘿。就是在那里对吧？",
                "0_B811": "不行哦，你无法抵抗？把你切成小块一口吃掉，",
                "0_B812": "这就是你的命运。",
                "0_B813": "呵呵，你的身心都将变得硬邦邦的"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "是不是比平时更温柔呢？"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "之后就拜托你了"
            }
        },
        "0_B890": {
            "name": "スキル 6",
            "text": {
                "0_B890": "你是我的王子殿下"
            }
        },
        "0_B910": {
            "name": "アタック 7",
            "text": {
                "0_B910": "面带笑容，愉快欢乐"
            }
        },
        "0_B920": {
            "name": "アタック 8",
            "text": {
                "0_B920": "好！好！好～！"
            }
        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "我不会让你掉下去的！"
            }
        },
        "0_G010": {
            "name": "レベルアップ 1",
            "text": {
                "0_G010": "太棒了，升级了。夏天这才刚刚开始呢，对吧？",
                "0_G011": ""
            }
        },
        "0_G110": {
            "name": "レベルアップ 2",
            "text": {
                "0_G110": "唔唔嗯…已经吃不下了…明天再拜托你啦～",
                "0_G111": ""
            }
        },
        "0_G120": {
            "name": "レベルアップ 3",
            "text": {
                "0_G120": "谢谢！能被你依赖，我真的超级开心！",
                "0_G121": ""
            }
        },
        "0_H010": {
            "name": "誕生日",
            "text": {
                "0_H010": "我翘首以盼这一天已久。是您的生日对吧？ 没错，我已不再是那个只能烤出硬邦邦曲奇饼的自己。 既然不擅长烹饪就干脆放弃，用体验来决胜负吧。 身为主人公，就该享受梦想、希望与小小惊险的体验不是吗？ 咦？要不要试试将艺术体操与童话结合的全新模拟体验？",
                "0_H011": "",
                "0_H012": "",
                "0_H013": "",
                "0_H014": "",
                "0_H015": ""
            }
        },
        "0_H020": {
            "name": "イベント開催中",
            "text": {
                "0_H020": "咦，活动已经开始了吗？御主，你知道吗？",
                "0_H021": ""
            }
        },
        "0_H030": {
            "name": "会話 1",
            "text": {
                "0_H030": "呼啊——休息时间结束了。不过，偷懒可不好呢！重新打起精神，开始出撃！",
                "0_H031": ""
            }
        },
        "0_H040": {
            "name": "会話 2",
            "text": {
                "0_H040": "主人，我自己一个人容易做过火，请尽管对我下达指令吧！",
                "0_H041": ""
            }
        },
        "0_H050": {
            "name": "会話 3",
            "text": {
                "0_H050": "称呼方式有很多种，认真时叫御主，平时叫御主先生，然后两人独处时就……呃……呃……不行！想不出来！好想要个只属于我的特别称呼啊～！",
                "0_H051": "",
                "0_H052": "",
                "0_H053": ""
            }
        },
        "0_H060": {
            "name": "好きなこと",
            "text": {
                "0_H060": "喜欢的东西……那个……对不起，虽然换上泳装，但最擅长的还是破坏……超开心！不过只教训坏人！在樱之五人组中，能够漂亮地破坏的，只有我！",
                "0_H061": "",
                "0_H062": "",
                "0_H063": ""
            }
        },
        "0_H070": {
            "name": "嫌いなこと",
            "text": {
                "0_H070": "讨厌的东西……以前是害怕别人的目光，但现在最讨厌自己示弱！难得有这么可爱的泳装，我可不想糟蹋了它！",
                "0_H071": "",
                "0_H072": ""
            }
        },
        "0_H080": {
            "name": "聖杯について",
            "text": {
                "0_H080": "对圣杯的愿望吗？要是能有七名左右的，优秀的从者就好了……咦？怎么了，黑胡子？你想进我的口袋？",
                "0_H081": "",
                "0_H082": ""
            }
        },
        "0_H090": {
            "name": "絆Lv.1",
            "text": {
                "0_H090": "好不容易获得了强大的灵基。不把目标也定得高一点就太浪费了吧？比如从又硬又大又强壮的樱系五号，变成最可爱的樱系五号之类的？……啊哇哇，刚才说的只是目标、目标啦！",
                "0_H091": "",
                "0_H092": ""
            }
        },
        "0_H100": {
            "name": "絆Lv.2",
            "text": {
                "0_H100": "购物的话就交给我吧。无论多少货物都能收纳。别说店里的全部商品了，连整间店铺都能收进去哦？",
                "0_H101": "",
                "0_H102": ""
            }
        },
        "0_H110": {
            "name": "絆Lv.3",
            "text": {
                "0_H110": "我想，我之所以喜欢新体操，是因为憧憬身体的自由吧。直到现在，我依然会时不时地想着要是能像Melt那样纤细苗条就好了……但我就是我！而且，只要双手稳定，就能做出许多大胆的动作，感觉就像打开了新世界的大门！",
                "0_H111": "",
                "0_H112": ""
            }
        },
        "0_H120": {
            "name": "絆Lv.4",
            "text": {
                "0_H120": "魔镜啊魔镜，谁是世界上最美丽的女人……没错，就是热情迷唇女王。胆敢反对的坏御主，就抓起来塞进手提包里！呵呵，童话演得太多，连反派角色都开始感兴趣了呢。我变成这个灵基后，是不是一直在笑啊？",
                "0_H121": "",
                "0_H122": "",
                "0_H123": "",
                "0_H124": ""
            }
        },
        "0_H130": {
            "name": "絆Lv.5",
            "text": {
                "0_H130": "曾经的我，只渴求一位独宠我一人的理想王子。但那不过是幼稚的自恋，我只是想让他庇护无能的自己罢了。时而鼓励，时而纠正，彼此扶持。这才是真正的恋人，真正的王子。这份感情或许会随着这副灵基一同消散——但我是否，已经成长为配得上你的公主了呢？",
                "0_H131": "",
                "0_H132": "",
                "0_H133": "",
                "0_H134": ""
            }
        },
        "0_H170": {
            "name": "会話 4",
            "text": {
                "0_H170": "我觉得迦勒底的各位对BB太纵容了！那家伙总是把简单任务夸大其词，故意刁难大家，真是个麻烦的AI！如果她敢以新灵基形态再次出现，到时候就让我们樱之五姐妹齐心协力好好教训她！",
                "0_H171": "",
                "0_H172": ""
            }
        },
        "0_H180": {
            "name": "会話 5",
            "text": {
                "0_H180": "我绝不让梅尔特莉莉丝独占樱之五英杰的风头！我也换上泳装了！",
                "0_H181": ""
            }
        },
        "0_H190": {
            "name": "会話 6",
            "text": {
                "0_H190": "普洛提亚，居然能正常对话了呢……至今为止都没法和任何人说话，但和御主就很聊得来。嗯……嘛，应该不会成为对手吧。毕竟普洛提亚，情感上还是个小女孩，就算成为对手对我来说也没问题！",
                "0_H191": "",
                "0_H192": "",
                "0_H193": "",
                "0_H194": ""
            }
        },
        "0_H200": {
            "name": "会話 7",
            "text": {
                "0_H200": "克琳希德小姐，为什么总是这么不坦率呢？明明喜欢的话说出来就好了。无论是身心还是本质，都是个彻头彻尾的恶役千金呢。",
                "0_H201": "",
                "0_H202": ""
            }
        },
        "0_H210": {
            "name": "会話 8",
            "text": {
                "0_H210": "湿度高的城市可不太妙呢。电子设备容易故障不说，连重型机械都跟我抢饭碗。干脆彻底拆个精光如何？",
                "0_H211": "",
                "0_H212": "",
                "0_H213": ""
            }
        },
        "0_H220": {
            "name": "会話 9",
            "text": {
                "0_H220": "岸波小姐也来了啊！她不是靠暴力，而是试图理解对手的方式，终结了圣杯战争，是我们所有人的恩人！现在应该能更自然地道谢了……虽然这么想。但她那看穿SG（秘密花园）的能力依然健在呢……",
                "0_H221": "",
                "0_H222": "",
                "0_H223": ""
            }
        },
        "0_H230": {
            "name": "会話 10",
            "text": {
                "0_H230": "看着珀西瓦尔先生，不知为何眼前浮现出奇怪的景象。在四周被绳索围住的场地里，将对方压倒，发出喊声……我是不是欲求不满啊……",
                "0_H231": "",
                "0_H232": ""
            }
        },
        "0_H240": {
            "name": "会話 11",
            "text": {
                "0_H240": "“和艾蕾什基伽尔很聊得来”吗？是的，确实如此。连我自己都觉得不可思议，但和艾蕾什基伽尔就是很投缘。当我跟梅尔特说“因为我们都有内向的一面”时，她却回我“你说你内向？别逗了，这笑话能把我给笑死”。",
                "0_H241": "",
                "0_H242": "",
                "0_H243": "",
                "0_H244": ""
            }
        },
        "0_H250": {
            "name": "会話 12",
            "text": {
                "0_H250": "实不相瞒，这套泳装灵基是爱迪生先生为我准备的。准确来说，是他把设计和资源都提供给了BB，才有了现在的结果……我去向他道谢的时候，他说『没什么。只是认为你有这个资格而已』，用那张特别温柔的脸。呵呵，明明是狮子的脸，却一点都不奇怪呢。",
                "0_H251": "",
                "0_H252": "",
                "0_H253": ""
            }
        },
        "0_H260": {
            "name": "会話 13",
            "text": {
                "0_H260": "呼延灼小姐那容易钻牛角尖又爱消沉的性格，反而让我特别安心！因为会让我觉得\"我也还撑得住\"！",
                "0_H261": ""
            }
        },
        "0_I010": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 1",
            "text": {
                "0_I010": "欢迎光临。听说有我能胜任的工作就赶来了。人生头一次当店员，干劲十足呢。",
                "0_I011": "",
                "0_I012": ""
            }
        },
        "0_I020": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 2",
            "text": {
                "0_I020": "没有什么特别可爱的道具呢。虽然实用性很重要，但我觉得设计也很重要。不想用自己不喜欢的物品吧？",
                "0_I021": "",
                "0_I022": ""
            }
        },
        "0_I030": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 3",
            "text": {
                "0_I030": "不过说人家是反派灵场也太失礼了吧？这种角色不是该让梅尔特莉莉丝、杀生院或者BB来当吗？",
                "0_I031": ""
            }
        },
        "0_I040": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 4",
            "text": {
                "0_I040": "感谢您的购买。请多多使用哦。",
                "0_I041": ""
            }
        },
        "0_I050": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 5",
            "text": {
                "0_I050": "哇哇哇哇——我把影子错当成道具给破坏了——没、没关系，我会用打工的钱来赔偿的。",
                "0_I051": "",
                "0_I052": ""
            }
        },
        "0_I060": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 6",
            "text": {
                "0_I060": "我没有睡着，我没有睡着。只是因为刚吃完饭，所以有点犯困而已。",
                "0_I061": "",
                "0_I062": ""
            }
        },
        "0_I070": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 7",
            "text": {
                "0_I070": "得意点也解决了，店铺也该关门了。 能认识您真是太好了。 我本以为您和Melt是同一类型，但完全不同。 您是一位非常认真又正直的出色人士。",
                "0_I071": "",
                "0_I072": "",
                "0_I073": ""
            }
        },
        "0_I080": {
            "name": "カルデア・Ｕ－サマーアイランド\n～大統領は夏の夢を見るか？～ 8",
            "text": {
                "0_I080": "那个，请问交换道具还有剩余吗？ 留着也是浪费，请在这里用掉吧。 这是最后一天营业了。",
                "0_I081": "",
                "0_I082": ""
            }
        },
        "0_S010": {
            "name": "召喚",
            "text": {
                "0_S010": "啊啊啊啊——是我梦寐以求的夏日泳装——！太开心了开心得要死——！谢谢大家——！啊，非非非常抱歉，这是召唤的问候语！我是爱憎的Alterego，热情之唇Passionlip。虽然灵基焕然一新，但ego的形态依然未变，双手也依旧巨大……但我会努力让大家认为这是我的个性，是优点！而且我绝不会输给Melt！毕竟，大就是好嘛！",
                "0_S011": "",
                "0_S012": "",
                "0_S013": "",
                "0_S014": "",
                "0_S015": "",
                "0_S016": "",
                "0_S017": ""
            }
        },
        "1_B010": {
            "name": "アタック 1",
            "text": {
                "1_B010": "快看！"
            }
        },
        "1_B020": {
            "name": "アタック 2",
            "text": {
                "1_B020": "最喜欢了！"
            }
        },
        "1_B030": {
            "name": "アタック 3",
            "text": {
                "1_B030": "一、二、三！"
            }
        },
        "1_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "1_B040": "一、二，咚！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "热情洋溢地决定啦！",
                "1_B051": "斩击、拼贴、编排！",
                "1_B052": "敬请瞩目！",
                "1_B053": "「纵使爱招致苦难」"
            }
        },
        "1_B080": {
            "name": "スキル 1",
            "text": {
                "1_B080": "将其收入囊中吧！"
            }
        },
        "1_B090": {
            "name": "スキル 2",
            "text": {
                "1_B090": "这正是表现的好机会！"
            }
        },
        "1_B100": {
            "name": "開始 1",
            "text": {
                "1_B100": "1号Passionlip，出击！"
            }
        },
        "1_B110": {
            "name": "開始 2",
            "text": {
                "1_B110": "真希望你能看看我的演技"
            }
        },
        "1_B120": {
            "name": "勝利 1",
            "text": {
                "1_B120": "缎带的动作如何？能看得清楚漂亮的话就好了……"
            }
        },
        "1_B130": {
            "name": "勝利 2",
            "text": {
                "1_B130": "咦？已经结束了吗？比预定时间稍微早了一点。"
            }
        },
        "1_B140": {
            "name": "ダメージ 2",
            "text": {
                "1_B140": "我不会输的！"
            }
        },
        "1_B150": {
            "name": "ダメージ 1",
            "text": {
                "1_B150": "哇哇，节奏太快了！"
            }
        },
        "1_B160": {
            "name": "戦闘不能 1",
            "text": {
                "1_B160": "啊哇哇，缎带松开了……对不起，请让我重新系一次……！"
            }
        },
        "1_B170": {
            "name": "戦闘不能 2",
            "text": {
                "1_B170": "是我的错，对不起……但下次一定完美……！"
            }
        },
        "1_B180": {
            "name": "コマンドカード 1",
            "text": {
                "1_B180": "跟上节奏"
            }
        },
        "1_B190": {
            "name": "コマンドカード 2",
            "text": {
                "1_B190": "舒展四肢"
            }
        },
        "1_B200": {
            "name": "コマンドカード 3",
            "text": {
                "1_B200": "蓄力"
            }
        },
        "1_B2010": {
            "name": "アタック 10",
            "text": {
                "1_B2010": "套装B！"
            }
        },
        "1_B2020": {
            "name": "アタック 11",
            "text": {
                "1_B2020": "重击之唇！"
            }
        },
        "1_B2030": {
            "name": "アタック 12",
            "text": {
                "1_B2030": "支离破碎了呢！"
            }
        },
        "1_B2080": {
            "name": "スキル 7",
            "text": {
                "1_B2080": "今天的主角就是我！"
            }
        },
        "1_B2090": {
            "name": "スキル 8",
            "text": {
                "1_B2090": "我要认真了！"
            }
        },
        "1_B210": {
            "name": "宝具カード 1",
            "text": {
                "1_B210": "如花般大胆！"
            }
        },
        "1_B220": {
            "name": "宝具カード 2",
            "text": {
                "1_B220": "终结一击！"
            }
        },
        "1_B230": {
            "name": "宝具カード 3",
            "text": {
                "1_B230": "都是你的错哦？"
            }
        },
        "1_B2410": {
            "name": "アタック 13",
            "text": {
                "1_B2410": "旋转！"
            }
        },
        "1_B2420": {
            "name": "アタック 14",
            "text": {
                "1_B2420": "连指尖都帅气无比！"
            }
        },
        "1_B2430": {
            "name": "アタック 15",
            "text": {
                "1_B2430": "这点事情……我也能做到！"
            }
        },
        "1_B2810": {
            "name": "アタック 16",
            "text": {
                "1_B2810": "连脚尖都充满韧性！"
            }
        },
        "1_B410": {
            "name": "アタック 4",
            "text": {
                "1_B410": "三重之月！"
            }
        },
        "1_B420": {
            "name": "アタック 5",
            "text": {
                "1_B420": "啦啦啦～♪"
            }
        },
        "1_B430": {
            "name": "アタック 6",
            "text": {
                "1_B430": "给我卷进去吧！"
            }
        },
        "1_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "1_B440": "得意技是毫不留情！"
            }
        },
        "1_B480": {
            "name": "スキル 3",
            "text": {
                "1_B480": "休息时间到！"
            }
        },
        "1_B490": {
            "name": "スキル 4",
            "text": {
                "1_B490": "缓急、缓急！"
            }
        },
        "1_B500": {
            "name": "開始 3",
            "text": {
                "1_B500": "我会富有节奏地把你们踢散！"
            }
        },
        "1_B520": {
            "name": "勝利 3",
            "text": {
                "1_B520": "演技、到此为止！呼——接下来，我得检查一下道具！"
            }
        },
        "1_B560": {
            "name": "戦闘不能 3",
            "text": {
                "1_B560": "没能跟上节奏……都怪对手不好……"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "啊！能行！要上了哦？",
                "1_B801": "无论你有多快，无论你有多远，",
                "1_B802": "这份爱都能传达！",
                "1_B803": "动力体操拳！"
            }
        },
        "1_B810": {
            "name": "宝具 3",
            "text": {
                "1_B810": "我的眼眸能映照世界。",
                "1_B811": "抱歉，一直注视着的人是你。",
                "1_B812": "再也不会让你逃走了。",
                "1_B813": "请成为我的俘虏吧！"
            }
        },
        "1_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "1_B840": "如果你在那里的话，我就抱紧你！"
            }
        },
        "1_B880": {
            "name": "スキル 5",
            "text": {
                "1_B880": "加油，加油！"
            }
        },
        "1_B890": {
            "name": "スキル 6",
            "text": {
                "1_B890": "交给你了！"
            }
        },
        "1_B910": {
            "name": "アタック 7",
            "text": {
                "1_B910": "干脆压扁你好了？"
            }
        },
        "1_B920": {
            "name": "アタック 8",
            "text": {
                "1_B920": "组合A！"
            }
        },
        "1_B930": {
            "name": "アタック 9",
            "text": {
                "1_B930": "碾碎你！"
            }
        },
        "1_G010": {
            "name": "レベルアップ 1",
            "text": {
                "1_G010": "诶，已经要升到中级了吗？不好好热身可不行啊。",
                "1_G011": ""
            }
        },
        "1_G020": {
            "name": "霊基再臨 2",
            "text": {
                "1_G020": "我似乎找到了属于自己的动作方式！虽然我经常偷懒，但为了能够跳得越来越漂亮，我会每天坚持训练的！",
                "1_G021": ""
            }
        },
        "1_G030": {
            "name": "霊基再臨 1",
            "text": {
                "1_G030": "哦，意外的是再临吗……？不过，我其实也憧憬过女孩子的兴趣呢。梅尔特跳芭蕾，普罗蒂亚做料理，卡兹拉变魔术。大家都有各自的兴趣，所以我也决定做点喜欢的事。虽然个头很大，但我会努力争取最高分的！",
                "1_G031": "",
                "1_G032": "",
                "1_G033": "",
                "1_G034": ""
            }
        },
        "1_G110": {
            "name": "レベルアップ 2",
            "text": {
                "1_G110": "希望不仅能提升力量，还能让身体更柔软……咦？\"已经足够",
                "1_G111": "",
                "1_G112": ""
            }
        },
        "1_G120": {
            "name": "レベルアップ 3",
            "text": {
                "1_G120": "目标就是摆脱坦克角色的印象！我可不会输给喜欢芭蕾的Melt！",
                "1_G121": ""
            }
        },
        "1_H010": {
            "name": "誕生日",
            "text": {
                "1_H010": "我早就盼着这一天了。今天是你的生日，对吧？ 没错，我已经不再是以前那个只会做硬邦邦的饼干的自己了。 虽然我不擅长做饭，但我已经放弃挣扎，决心用体验来一决胜负。 作为御主，理应体验梦想、希望与一丝惊悚，对吧。 嗯？要不要尝试将艺术体操与童话结合，体验全新的模拟玩法？",
                "1_H011": "",
                "1_H012": "",
                "1_H013": "",
                "1_H014": "",
                "1_H015": ""
            }
        },
        "1_H020": {
            "name": "イベント開催中",
            "text": {
                "1_H020": "咦，活动已经开始了吗？御主，你知道吗？",
                "1_H021": ""
            }
        },
        "1_H030": {
            "name": "会話 1",
            "text": {
                "1_H030": "哈啊，休息时间结束了。 不过偷懒可不好呢。打起精神出击吧。",
                "1_H031": ""
            }
        },
        "1_H040": {
            "name": "会話 2",
            "text": {
                "1_H040": "Master，我自己来恐怕会过火，请尽管吩咐我吧。",
                "1_H041": ""
            }
        },
        "1_H050": {
            "name": "会話 3",
            "text": {
                "1_H050": "虽然称呼多种多样，但严肃时叫御主，平时叫御主桑，至于两人独处时……呃、呃，不行，想不出来。我想要一个只属于我的特别称呼。",
                "1_H051": "",
                "1_H052": "",
                "1_H053": ""
            }
        },
        "1_H060": {
            "name": "好きなこと",
            "text": {
                "1_H060": "喜欢的东西……那个……对不起，虽然换上泳装，但最擅长的还是破坏……超开心！不过只教训坏人！在樱之五人组中，能够漂亮地破坏的，只有我！",
                "1_H061": "",
                "1_H062": "",
                "1_H063": ""
            }
        },
        "1_H070": {
            "name": "嫌いなこと",
            "text": {
                "1_H070": "讨厌的东西……以前是害怕别人的目光，但现在最讨厌自己示弱！难得有这么可爱的泳装，我可不想糟蹋了它！",
                "1_H071": "",
                "1_H072": ""
            }
        },
        "1_H080": {
            "name": "聖杯について",
            "text": {
                "1_H080": "对圣杯的愿望吗？要是能有七名左右的，优秀的从者就好了……咦？怎么了，黑胡子？你想进我的口袋？",
                "1_H081": "",
                "1_H082": ""
            }
        },
        "1_H090": {
            "name": "絆Lv.1",
            "text": {
                "1_H090": "好不容易变得这么强，目标不设高一点就太浪费了。 比如从坚硬、巨大又强壮的樱五，变成最可爱的樱五之类的？ 哇哇，刚才那个只是目标、目标啦！",
                "1_H091": "",
                "1_H092": ""
            }
        },
        "1_H100": {
            "name": "絆Lv.2",
            "text": {
                "1_H100": "购物之事尽管交给我。无论多少货物都能收纳。 莫说店内商品，连整间店铺都能一并收入囊中。",
                "1_H101": "",
                "1_H102": ""
            }
        },
        "1_H110": {
            "name": "絆Lv.3",
            "text": {
                "1_H110": "我想我喜欢艺术体操，还是出于对自由身体的憧憬吧。 就算现在，我偶尔也会想，要是能像Melt一样苗条就好了。但我是我。 而且双手稳定的话，就可以做出一些乱来的动作，感觉打开了新世界的大门。",
                "1_H111": "",
                "1_H112": ""
            }
        },
        "1_H120": {
            "name": "絆Lv.4",
            "text": {
                "1_H120": "魔镜啊魔镜，世上最美丽的人是谁？没错，正是热情迷唇女王。胆敢唱反调的坏御主就抓起来，一把扔进小挎包。童话再现玩腻了，人家也想试试当恶役呢。自打变成这个灵基后，人家就老是笑个不停呢。",
                "1_H121": "",
                "1_H122": "",
                "1_H123": "",
                "1_H124": ""
            }
        },
        "1_H130": {
            "name": "絆Lv.5",
            "text": {
                "1_H130": "以前的我，一心寻求只对我温柔的理想王子。 但这只是不成熟的自恋，我只是想让王子保护不堪的自己。 时而给予鼓励，时而加以纠正，时而互相帮助。 那才是真正的恋人，真正的王子。 唔，或许这份情感会随这灵基一同消散，但我是否成为了与你相称的公主呢？",
                "1_H131": "",
                "1_H132": "",
                "1_H133": "",
                "1_H134": ""
            }
        },
        "1_H270": {
            "name": "会話 4",
            "text": {
                "1_H270": "在模拟器里练习时，杜尔迦小姐总会坐在观众席上。我问她\"是因为我的指甲是用女神杜尔迦的元素打造的吗？\"，结果卡莉大人对新体操更感兴趣。要是毁灭世界的舞蹈变成了艺术体操的话，那可真是抱歉啦！",
                "1_H271": "",
                "1_H272": ""
            }
        },
        "1_H280": {
            "name": "会話 5",
            "text": {
                "1_H280": "嘴上嚷嚷着要\"超越BB\"，到头来却还是最像BB呢。宝具变身大人形态时还穿着紧身衣，那种打扮根本没必要吧？",
                "1_H281": "",
                "1_H282": ""
            }
        },
        "1_H290": {
            "name": "会話 6",
            "text": {
                "1_H290": "虽然我觉得当个魔法少女也不错，但你看，果然还是会有抵触情绪……我觉得就算长大成人还能保持那副姿态的，只有百分百开朗又天真的人。",
                "1_H291": ""
            }
        },
        "1_H300": {
            "name": "会話 7",
            "text": {
                "1_H300": "真是惊到我了……明明说是黄金BB，是BB的终极形态，结果却端上来一套金色兔女郎装……一想到那种狡猾的人工智能居然是我们的根源，感觉会毫不犹豫地把她捏扁呢……明明还说什么“紧身衣是为了评级考虑！”……结果只是借口啊……",
                "1_H301": "",
                "1_H302": "",
                "1_H303": "",
                "1_H304": ""
            }
        },
        "1_H310": {
            "name": "会話 8",
            "text": {
                "1_H310": "哇哇～！这可是货真价实的古典芭蕾哦！流畅的舞步、如雕塑般完美的定格姿势，还有与御主的双人舞！好棒～好美～比Melt可爱一百倍～！",
                "1_H311": "",
                "1_H312": ""
            }
        },
        "2_B010": {
            "name": "アタック 1",
            "text": {
                "2_B010": "沉睡之棘！"
            }
        },
        "2_B020": {
            "name": "アタック 2",
            "text": {
                "2_B020": "会有点刺痛哦？"
            }
        },
        "2_B030": {
            "name": "アタック 3",
            "text": {
                "2_B030": "被光芒所包覆！"
            }
        },
        "2_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "2_B040": "你的梦到此为止了！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "公主准则，启动。",
                "2_B051": "荆棘之爪、冰霜之风，花蕾内是童话的受难。",
                "2_B052": "尽情品尝吧！",
                "2_B053": "纵使爱会招致苦难"
            }
        },
        "2_B080": {
            "name": "スキル 1",
            "text": {
                "2_B080": "让我来当你的对手吧？"
            }
        },
        "2_B090": {
            "name": "スキル 2",
            "text": {
                "2_B090": "您好"
            }
        },
        "2_B100": {
            "name": "開始 1",
            "text": {
                "2_B100": "请手下留……咦？被请求的人是我吗？"
            }
        },
        "2_B110": {
            "name": "開始 2",
            "text": {
                "2_B110": "优雅端庄地战斗。因为我是公主！"
            }
        },
        "2_B120": {
            "name": "勝利 1",
            "text": {
                "2_B120": "我觉得这是一场华丽的胜利。可以夸夸我吗，御主？"
            }
        },
        "2_B130": {
            "name": "勝利 2",
            "text": {
                "2_B130": "强大是理所当然的。以梦想成真的姿态怎能败北！"
            }
        },
        "2_B140": {
            "name": "ダメージ 2",
            "text": {
                "2_B140": "呜……！"
            }
        },
        "2_B150": {
            "name": "ダメージ 1",
            "text": {
                "2_B150": "别来碍事……！"
            }
        },
        "2_B160": {
            "name": "戦闘不能 1",
            "text": {
                "2_B160": "那个……我累了，可以在森林的小屋里睡一会儿吗？"
            }
        },
        "2_B170": {
            "name": "戦闘不能 2",
            "text": {
                "2_B170": "穿着礼服当坦克，简直毫无人性！"
            }
        },
        "2_B180": {
            "name": "コマンドカード 1",
            "text": {
                "2_B180": "哎呀！"
            }
        },
        "2_B190": {
            "name": "コマンドカード 2",
            "text": {
                "2_B190": "嘿嘿！"
            }
        },
        "2_B200": {
            "name": "コマンドカード 3",
            "text": {
                "2_B200": "可以吗？"
            }
        },
        "2_B2010": {
            "name": "アタック 9",
            "text": {
                "2_B2010": "不许你搭讪！"
            }
        },
        "2_B2020": {
            "name": "アタック 10",
            "text": {
                "2_B2020": "温柔，哎！"
            }
        },
        "2_B2030": {
            "name": "アタック 11",
            "text": {
                "2_B2030": "这种程度没关系吧？"
            }
        },
        "2_B2080": {
            "name": "スキル 7",
            "text": {
                "2_B2080": "做我们自己！"
            }
        },
        "2_B2090": {
            "name": "スキル 8",
            "text": {
                "2_B2090": "我要把他们统统踢飞！"
            }
        },
        "2_B210": {
            "name": "宝具カード 1",
            "text": {
                "2_B210": "像花儿一样美丽"
            }
        },
        "2_B220": {
            "name": "宝具カード 2",
            "text": {
                "2_B220": "好、好的！全力全开，对吧？"
            }
        },
        "2_B230": {
            "name": "宝具カード 3",
            "text": {
                "2_B230": "你还不懂花的坚强呢。呵呵。"
            }
        },
        "2_B2410": {
            "name": "アタック 12",
            "text": {
                "2_B2410": "我送的礼物！"
            }
        },
        "2_B2420": {
            "name": "アタック 13",
            "text": {
                "2_B2420": "可不能留下活口哦？"
            }
        },
        "2_B2430": {
            "name": "アタック 14",
            "text": {
                "2_B2430": "舒服吗？"
            }
        },
        "2_B2810": {
            "name": "アタック 15",
            "text": {
                "2_B2810": "满足了吗？"
            }
        },
        "2_B410": {
            "name": "アタック 4",
            "text": {
                "2_B410": "拇指姑娘就是这样！"
            }
        },
        "2_B420": {
            "name": "アタック 5",
            "text": {
                "2_B420": "呼——"
            }
        },
        "2_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "2_B440": "现在的我无所不能！"
            }
        },
        "2_B480": {
            "name": "スキル 3",
            "text": {
                "2_B480": "晚安"
            }
        },
        "2_B490": {
            "name": "スキル 4",
            "text": {
                "2_B490": "随时都可以……！"
            }
        },
        "2_B500": {
            "name": "開始 3",
            "text": {
                "2_B500": "好啊。开始舞会吧。必须在王子来之前结束！"
            }
        },
        "2_B520": {
            "name": "勝利 3",
            "text": {
                "2_B520": "虽然赢了但总觉得少了点什么。不知哪里会有七个小矮人呢？"
            }
        },
        "2_B560": {
            "name": "戦闘不能 3",
            "text": {
                "2_B560": "我太过老实了……对不起大家……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "情绪高涨！",
                "2_B801": "王子不来见我，我就去见王子！",
                "2_B802": "美梦成真！",
                "2_B803": "浪漫故事，就此展开！"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "我受够了！",
                "2_B811": "坏虫子就该这样、这样、这样！",
                "2_B812": "给我好好反省！",
                "2_B813": "愤怒之唇·惩戒时刻！"
            }
        },
        "2_B820": {
            "name": "宝具 4",
            "text": {
                "2_B820": "公主科迪克，启动。",
                "2_B821": "荆棘之爪，冰霜之风，",
                "2_B822": "花蕾之中乃是童话的受难。",
                "2_B823": "尽情品味吧。“纵使爱招致受难”"
            }
        },
        "2_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "2_B840": "请明天再来！"
            }
        },
        "2_B880": {
            "name": "スキル 5",
            "text": {
                "2_B880": "任命你！"
            }
        },
        "2_B890": {
            "name": "スキル 6",
            "text": {
                "2_B890": "让我来帮你变强吧？"
            }
        },
        "2_B910": {
            "name": "アタック 6",
            "text": {
                "2_B910": "就是那里对吧？"
            }
        },
        "2_B920": {
            "name": "アタック 7",
            "text": {
                "2_B920": "我冲了！"
            }
        },
        "2_B930": {
            "name": "アタック 8",
            "text": {
                "2_B930": "王子到手啦！"
            }
        },
        "2_G010": {
            "name": "レベルアップ 1",
            "text": {
                "2_G010": "不是毒苹果而是美味的种火呢？当然，我会吃得干干净净！",
                "2_G011": ""
            }
        },
        "2_G040": {
            "name": "霊基再臨 1",
            "text": {
                "2_G040": "礼服！这、这是公主的礼服对吧！？好开心……！我还以为继艺术体操之后梦就会醒来，变回原来的自己，结果非但没醒反而实现了！……啊，不过以这副模样战斗真的可以吗？得赶紧想出不会弄破裙子的战斗方式才行！",
                "2_G041": "",
                "2_G042": "",
                "2_G043": "",
                "2_G044": ""
            }
        },
        "2_G050": {
            "name": "霊基再臨 2",
            "text": {
                "2_G050": "这真是一个如梦似幻、无比愉快的夏天。再次感谢您。虽说我尽是失败，但为了回应您的应援而努力的过程中，仿佛变成了另一个自己。我很喜欢，喜欢这样的自己。今后也请多多支持容易偷懒的我哦，御主！",
                "2_G051": "",
                "2_G052": "",
                "2_G053": "",
                "2_G054": ""
            }
        },
        "2_G110": {
            "name": "レベルアップ 2",
            "text": {
                "2_G110": "作为随处可见的平民，这贡品倒是不错。可以表扬你哦，御主？……等等，这哪里是公主大人，根本是女王大人吧！？又被BB骗了～！",
                "2_G111": "",
                "2_G112": "",
                "2_G113": ""
            }
        },
        "2_G120": {
            "name": "レベルアップ 3",
            "text": {
                "2_G120": "谢谢您，感谢您，御主！……我说得如何？果然，这种程度对我来说刚刚好！",
                "2_G121": "",
                "2_G122": ""
            }
        },
        "2_H010": {
            "name": "誕生日",
            "text": {
                "2_H010": "我早就盼着这一天了。今天是您的生日，对吧？ 没错，现在的我，已经不再是以前那个只会做硬邦邦曲奇的人了。 我虽然不擅长烹饪，但已经彻底放弃了，转而靠体验一决胜负。 身为主人，就该去体验梦想、希望与小小的惊悚，对吧？ 嗯？要不要试试艺术体操与童话相结合的全新模拟体验？",
                "2_H011": "",
                "2_H012": "",
                "2_H013": "",
                "2_H014": "",
                "2_H015": ""
            }
        },
        "2_H020": {
            "name": "イベント開催中",
            "text": {
                "2_H020": "咦，活动已经开始了吗？御主，你知道吗？",
                "2_H021": ""
            }
        },
        "2_H030": {
            "name": "会話 1",
            "text": {
                "2_H030": "哈啊，休息时间结束了。 不过偷懒可不好。打起精神出击吧。",
                "2_H031": ""
            }
        },
        "2_H040": {
            "name": "会話 2",
            "text": {
                "2_H040": "主人，我一个人容易做得太过火，请尽管下达指示吧。",
                "2_H041": ""
            }
        },
        "2_H050": {
            "name": "会話 3",
            "text": {
                "2_H050": "称呼确实有很多种，正式场合时叫御主，平常就叫御主亲，然后两人独处时就……呃，呃，不行，想不出来。好想要一个只属于我的特别称呼啊。",
                "2_H051": "",
                "2_H052": "",
                "2_H053": ""
            }
        },
        "2_H060": {
            "name": "好きなこと",
            "text": {
                "2_H060": "喜欢的东西……那个……对不起，虽然换上泳装，但最擅长的还是破坏……超开心！不过只教训坏人！在樱之五人组中，能够漂亮地破坏的，只有我！",
                "2_H061": "",
                "2_H062": "",
                "2_H063": ""
            }
        },
        "2_H070": {
            "name": "嫌いなこと",
            "text": {
                "2_H070": "讨厌的东西……以前是害怕别人的目光，但现在最讨厌自己示弱！难得有这么可爱的泳装，我可不想糟蹋了它！",
                "2_H071": "",
                "2_H072": ""
            }
        },
        "2_H080": {
            "name": "聖杯について",
            "text": {
                "2_H080": "对圣杯的愿望吗？要是能有七名左右的，优秀的从者就好了……咦？怎么了，黑胡子？你想进我的口袋？",
                "2_H081": "",
                "2_H082": ""
            }
        },
        "2_H090": {
            "name": "絆Lv.1",
            "text": {
                "2_H090": "好不容易变得这么灵气充沛，不把目标定得高一点就太浪费了。 比如从坚硬、巨大又力大无穷的樱五，变成最可爱的樱五之类的？ 哇哇，刚才那个只是目标、目标啦！",
                "2_H091": "",
                "2_H092": ""
            }
        },
        "2_H100": {
            "name": "絆Lv.2",
            "text": {
                "2_H100": "购物的话就交给我吧。无论多少货物都能装下。 别说店里的全部商品了，连整家店都能一起打包带走哦。",
                "2_H101": "",
                "2_H102": ""
            }
        },
        "2_H110": {
            "name": "絆Lv.3",
            "text": {
                "2_H110": "我喜欢新体操，果然还是源于对自由身体的向往吧。 即便现在，若像梅尔特那样苗条，也并非毫无念想，但我就是我。 或者说，双手若能稳定支撑，就能做出大胆的动作，仿佛打开了新世界的大门。",
                "2_H111": "",
                "2_H112": ""
            }
        },
        "2_H120": {
            "name": "絆Lv.4",
            "text": {
                "2_H120": "魔镜啊魔镜，世上最美的人是谁？没错，正是热情迷唇女王哦。 敢唱反调的坏御主，就抓起来扔进手提包里啪叽处理掉。 总是重现童话故事的话，连反派角色都开始感兴趣了呢。 自从变成这个灵基之后，我整天都在笑呢。",
                "2_H121": "",
                "2_H122": "",
                "2_H123": "",
                "2_H124": ""
            }
        },
        "2_H130": {
            "name": "絆Lv.5",
            "text": {
                "2_H130": "从前的我，只渴求一位独宠于我的完美王子。 但那份不成熟的自恋，不过是想有人包容不堪的自己罢了。 时而激励，时而纠正，相互扶持。 那才是真正的恋人，真正的王子。 虽然这份心意或许会随灵基一同消散……但我是否已成长为配得上你的公主了呢？",
                "2_H131": "",
                "2_H132": "",
                "2_H133": "",
                "2_H134": ""
            }
        },
        "2_H320": {
            "name": "会話 4",
            "text": {
                "2_H320": "嘘……泳装迦摩也在吗……？那是什么……自称人鱼公主的话，各种意义上都太大了吧……？该去《杰克与魔豆》里出场才对！诶？\"真身是名为蜃气楼的龙\"？那可以！童话守住了！",
                "2_H321": "",
                "2_H322": "",
                "2_H323": "",
                "2_H324": ""
            }
        },
        "2_H330": {
            "name": "会話 5",
            "text": {
                "2_H330": "罗宾先生，为什么穿着泳装啊？身为森林猎人是您为数不多的优点，离开森林的话，不就只剩下轻浮的英俊外表了吗？请快点回去重新考虑角色设定吧",
                "2_H331": "",
                "2_H332": ""
            }
        },
        "2_H340": {
            "name": "会話 6",
            "text": {
                "2_H340": "伊丽莎白小姐要成为灰姑娘的从者！？呵呵，真是一种新奇的解读呢。下次务必要让我参加！感觉会成为舞会的预演！啊！对了！继母和坏心眼的姐姐，我倒是想到了人选，要听吗？",
                "2_H341": "",
                "2_H342": "",
                "2_H343": ""
            }
        },
        "2_H350": {
            "name": "会話 7",
            "text": {
                "2_H350": "呀——！好厉害的雾与镜之仙境！童话力简直突破天际！那就是现代的魔女吧。虽然散发着超强的冷酷能干气场，但骨子里绝对是个怕麻烦的人！因为是同类所以我很清楚！",
                "2_H351": "",
                "2_H352": "",
                "2_H353": ""
            }
        },
        "2_H360": {
            "name": "会話 8",
            "text": {
                "2_H360": "纳萨莉·莱姆，在这个世界也是那副模样呢。虽然我只是通过母亲的数据了解她，但真心希望那孩子能获得幸福啊。",
                "2_H361": "",
                "2_H362": ""
            }
        },
        "2_H370": {
            "name": "会話 9",
            "text": {
                "2_H370": "哇！呀——！太厉害了！刚才走廊上出现了微小的特异点！王子大人，真正的、真正的理想中的王子大人，对我温柔地使了个眼色！虽然现实中的王子大人是御主您，那个，我能稍微去观察一下那位理想中的王子大人吗？他的名字，好像是叫奥伯龙……",
                "2_H371": "",
                "2_H372": "",
                "2_H373": "",
                "2_H374": "",
                "2_H375": ""
            }
        },
        "2_H380": {
            "name": "会話 10",
            "text": {
                "2_H380": "雨之魔女，据说是远古妖精的公主殿下。她性情温和又善解人意，即使我犯了错也不会面露不悦，总是和我一起收拾残局。我也好想成为那样的公主殿下啊。",
                "2_H381": "",
                "2_H382": "",
                "2_H383": ""
            }
        },
        "2_H390": {
            "name": "会話 11",
            "text": {
                "2_H390": "魔女摩根，据说曾是统治妖精国的女王。那冷漠至极的眼神，毫无体恤之心的做派，简直和Melt一模一样令人畏惧……却与哈贝特洛特大人交情匪浅呢。这是为什么呢？",
                "2_H391": "",
                "2_H392": "",
                "2_H393": ""
            }
        },
        "2_H400": {
            "name": "会話 12",
            "text": {
                "2_H400": "无论发生什么，我都会守护御主。不过第二位或许就是哈贝特洛特小姐了。在迦勒底，只有她会关心我的嘴唇保养。她总说『明明这么有魅力，藏着多可惜呀！』她的裁缝技艺固然高超，但那份体贴关怀才最令人动容！",
                "2_H401": "",
                "2_H402": "",
                "2_H403": ""
            }
        }
    },
    "ChrVoice_ids": [
        "106300",
        "109829880"
    ]
}
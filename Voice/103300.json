{
    "id": 103300,
    "collectionNo": 213,
    "name": "齐格鲁德",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "太天真了！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "秒杀了你！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "魔剑完成。",
                "0_B051": "让在下见识一下你的自尊吧。",
                "0_B052": "此乃破灭之黎明，",
                "0_B053": "——坏劫之天轮！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "使用能力。 龙之智慧，取之不尽。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "敌我识别确认。 发动。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "我方的战斗态势万无一失。 ——来吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "作为为武而生之人，早有觉悟。 激昂吧，御主。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "感谢。 在下很高兴能与您进行了一场灵魂之战。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "好。 御主，很高兴您平安无事。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "必须坚持……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "失策了…… 不，只是在下太弱了。 再见了……！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "不甘心…… ……布伦……希尔德……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白了。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "允诺。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "好。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "详情明确，宝具启动。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "展开准备。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "沦陷吧！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "斩断你的生命！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "——确认，现在开始迎击。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "绝技准备。",
                "0_B801": "太阳魔剑啊，用你的身躯掀起破坏吧。",
                "0_B802": "——坏劫之天轮！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "报告。 在下似乎又变强了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "这样是不会改变外形的吧。 原来如此。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "原来如此，这就是灵基再临啊……",
                "0_G031": "感觉不赖。",
                "0_G032": "仿佛不足的地方被充满了。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "这就是最终形态吗。",
                "0_G041": "能够使出全力是非常爽快的。",
                "0_G042": "由衷感谢您为在下倾注了力量。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "报告。",
                "0_G051": "御主，这就是当前在下能到达的终点了。",
                "0_G052": "在一切都被雾笼罩的现阶段，您就是在下的光。",
                "0_G053": "在下会追随您的引导，御主。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "御主，听说今天是您的生日。",
                "0_H011": "请容在下向您献上由衷的感谢。",
                "0_H012": "正因为您的诞生，我们才会像这样邂逅。",
                "0_H013": "对在下来说，这才是最大的报酬。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "这里有一股危险，或者可以说是动荡不安的氛围。",
                "0_H021": "在下建议应该前去调查一下。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "提案。 是否应该外出旅行一番呢。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "您是御主，在下是从者。",
                "0_H041": "其中没有任何可以介入的余地。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "倘若您以恶为傲。",
                "0_H051": "那在下就会以善为傲，并表示拒绝。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "如果您那里下雪的话，请告诉在下。",
                "0_H061": "或许是由于出生地的缘故吧，在下很喜欢雪景。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "尽管……不到讨厌的程度。",
                "0_H071": "但小动物其实意外挺厉害的。",
                "0_H072": "还是小心为妙。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "如果您想要，",
                "0_H081": "那为了获得并交给您，在下会尽自己最大的努力。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "在下并没有什么事找您。 多有打扰。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "明白了。如果有什么事，在下会来的。",
                "0_H101": "期待是在下能帮得上忙的状况。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "是御主吗。在下这里没有什么紧迫的事情要处理。",
                "0_H111": "无论是闲聊还是别的什么，在下都很高兴奉陪。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "御主，在下要再次向您表示。",
                "0_H121": "这把剑，与在下这身躯，都可以全部为您所愿而使用。",
                "0_H122": "您是配得上这一切的人物。",
                "0_H123": "您是决不会恶用这一切的吧。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "御主，在下能献给您的东西实在是太少了。",
                "0_H131": "哪怕将一切都献给您，都无法回报您的恩情。",
                "0_H132": "那剩下的只有在下的生命了。",
                "0_H133": "在下会将生命献给您，向您尽最大忠诚。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "齐格飞……同一而不同的存在。",
                "0_H171": "在下其实挺羡慕他的那份强韧。",
                "0_H172": "但在战斗中，在下相信自己绝不会逊色于他。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "布伦希尔德……吗。",
                "0_H181": "她打算用杀意来证明爱。",
                "0_H182": "尽管这很令人高兴，但遗憾的是假如死去，就无法证明在下的爱了。",
                "0_H183": "必须在保证在下生存的同时证明爱才行。",
                "0_H184": "真难啊。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "除了在下以外，还有其他人佩戴着睿智的结晶吗。",
                "0_H191": "原来如此，很好。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "哦，魔剑使……",
                "0_H201": "在下也是拥有魔剑之人。",
                "0_H202": "大家都要小心别被魔吞噬了。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "圣剑使……",
                "0_H211": "无论在什么地方，那些剑都会闪耀光辉吧。",
                "0_H212": "不过在下的魔剑乃是能烧尽一切的太阳魔剑。",
                "0_H213": "比较一下也不失为一件乐事。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "……无论是面对克琳希德的奸计，还是苏鲁特的毒辣，在下都毫无反抗之力。",
                "0_H221": "但是，不会有第三次了。",
                "0_H222": "我再也不会被迷惑了。"
            }
        },
        "0_H2200": {
            "name": "对话 2 (牵绊Lv.4以上)",
            "text": {
                "0_H2200": "您是御主，在下是从者。",
                "0_H2201": "其中没有任何其他事物可以介入的余地……",
                "0_H2202": "在下本是这么认为的，但似乎并非如此。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "问我这副眼镜是否会在战斗中损坏？",
                "0_H231": "放心吧，御主。这是睿智的结晶。",
                "0_H232": "也就是说，只要在下没有成为失去智慧的野兽，这镜片就会永远闪耀。"
            }
        },
        "0_H2300": {
            "name": "对话 3 (牵绊Lv.4以上)",
            "text": {
                "0_H2300": "倘若您并不是恶，在下就不会与您唱反调。",
                "0_H2301": "请放心吧，在下会一直站在您这边。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "好，只要是为了御主，在下愿意最大程度活用这份睿智。",
                "0_H241": "眼镜闪光！",
                "0_H242": "……只是助威用的大吼，请不用在意。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，Saber。吾之真名乃齐格鲁德。",
                "0_S011": "您就是御主吗？　请给在下指示。",
                "0_S012": "在下保证会迅速完成。"
            }
        },
        "11_B020": {
            "name": "攻击 2 与吾爱的回忆",
            "text": {
                "11_B020": "痛击！"
            }
        },
        "11_B030": {
            "name": "攻击 3 与吾爱的回忆",
            "text": {
                "11_B030": "就是那里！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 与吾爱的回忆",
            "text": {
                "11_B040": "夏天也要适当运动！"
            }
        },
        "11_B080": {
            "name": "技能 1 与吾爱的回忆",
            "text": {
                "11_B080": "唔。应该说Let's play吧。"
            }
        },
        "11_B090": {
            "name": "技能 2 与吾爱的回忆",
            "text": {
                "11_B090": "尽情玩耍，尽情学习。"
            }
        },
        "11_B100": {
            "name": "开始 1 与吾爱的回忆",
            "text": {
                "11_B100": "虽说现在是暑假，但在下绝无半点松懈。 出击！"
            }
        },
        "11_B110": {
            "name": "开始 2 与吾爱的回忆",
            "text": {
                "11_B110": "为了吾爱布伦希尔德。 很抱歉，但在下要将你们打得体无完肤！"
            }
        },
        "11_B120": {
            "name": "胜利 1 与吾爱的回忆",
            "text": {
                "11_B120": "呼。没办法。出了一身汗。"
            }
        },
        "11_B130": {
            "name": "胜利 2 与吾爱的回忆",
            "text": {
                "11_B130": "一滴汗都没流就赢得了胜利。甚好。"
            }
        },
        "11_B150": {
            "name": "受击 1 与吾爱的回忆",
            "text": {
                "11_B150": "呜……好热啊！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 与吾爱的回忆",
            "text": {
                "11_B160": "不好……热失控了……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 与吾爱的回忆",
            "text": {
                "11_B170": "稍微休息一会儿……抱歉。"
            }
        },
        "11_B180": {
            "name": "指令卡 1 与吾爱的回忆",
            "text": {
                "11_B180": "在下接受。"
            }
        },
        "11_B190": {
            "name": "指令卡 2 与吾爱的回忆",
            "text": {
                "11_B190": "没问题。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 与吾爱的回忆",
            "text": {
                "11_B200": "好！"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 与吾爱的回忆",
            "text": {
                "11_B210": "用宝具吹飞一切。炎热的天气用这种方式最适合了。"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 与吾爱的回忆",
            "text": {
                "11_B220": "就当是余暑时分的探望吧。"
            }
        },
        "11_B410": {
            "name": "攻击 4 与吾爱的回忆",
            "text": {
                "11_B410": "热吗！"
            }
        },
        "11_B420": {
            "name": "攻击 5 与吾爱的回忆",
            "text": {
                "11_B420": "沐浴光芒吧！"
            }
        },
        "11_B430": {
            "name": "攻击 6 与吾爱的回忆",
            "text": {
                "11_B430": "下一个！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 与吾爱的回忆",
            "text": {
                "11_B440": "这是防暑对策，上！"
            }
        },
        "11_B500": {
            "name": "开始 3 与吾爱的回忆",
            "text": {
                "11_B500": "虽说不能弄脏衣服，但事不遂愿。 那只能秒杀了！"
            }
        },
        "11_B550": {
            "name": "受击 3 与吾爱的回忆",
            "text": {
                "11_B550": "如果衣服弄破可就麻烦了！"
            }
        },
        "11_G010": {
            "name": "升级 与吾爱的回忆",
            "text": {
                "11_G010": "报告。在下似乎又变强了。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 与吾爱的回忆",
            "text": {
                "11_G060": "这件衣服吗？　这在下和吾爱布伦希尔德一起挑选的。",
                "11_G061": "基于这个事实，合适与否已经不重要了，但如果合适的话，还是会令人高兴……",
                "11_G062": "噢噢，是吗？",
                "11_G063": "既然有了御主的保证，那一定没问题吧。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 与吾爱的回忆",
            "text": {
                "11_H9000": "吾之内心的炙热沸腾决不逊色于炎炎夏日。",
                "11_H9001": "综上所述，要一起踏上旅程吗？",
                "11_H9002": "吾之御主啊。"
            }
        }
    },
    "ChrVoice_ids": [
        "103300"
    ]
}
{
    "id": 300200,
    "collectionNo": 71,
    "name": "迪尔姆德·奥迪那",
    "voices": {
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "还没完呢！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "贯穿吧！",
                "0_B051": "破魔的红蔷薇！",
                "0_B052": "必灭的黄蔷薇！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "那就让我做你的对手吧。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "这样如何？"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "费奥纳骑士团的第一勇士，迪尔姆德，出击！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "别怪我，我会全力以赴！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "我要将这份胜利献给我的御主！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "绝不会在这第二次人生出丑！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "不甘心……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "到此为止了吗。御主，请务必……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "很好。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "好吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "就是这个！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "好好领教我的绝技吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "宝具开帐。"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "嗯，",
                "0_G011": "情况真不错。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "这是……！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "嗯，",
                "0_G031": "这样应该就能获得更大的力量了。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "没问题。",
                "0_G041": "上吧！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "感谢您，御主。",
                "0_G051": "我会全力以赴来回应您的信任。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "御主，今天是您的生日吧。",
                "0_H011": "请收下这个，",
                "0_H012": "这是我刚摘下的花。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "哎呀？",
                "0_H021": "我有一种预感。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "御主，该是时候踏上旅程了吧？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "能侍奉您",
                "0_H041": "是出乎我意料之外的喜悦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "……我只会忠实侍奉您。",
                "0_H051": "无论发生什么事。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "友情是个好东西。",
                "0_H061": "是决不能缺少的美妙的存在。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……",
                "0_H071": "我极端讨厌野猪。",
                "0_H072": "还有……",
                "0_H073": "不，没什么。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我没有什么愿望希望圣杯实现。",
                "0_H081": "我只求侍奉您。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "哎呀，御主。",
                "0_H091": "怎么了？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我对此生没有什么遗憾。",
                "0_H101": "只不过——",
                "0_H102": "不，都是无关紧要的事。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我的愿望或许过于奢侈。",
                "0_H111": "但是，我还是希望自己能作为一名骑士光荣地牺牲。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "御主，您将我用在了正确的地方。",
                "0_H121": "我由衷表示感谢。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我为自己能侍奉像您这样的御主而感到自豪。",
                "0_H131": "无论这两把枪面对怎样的对手，",
                "0_H132": "都不会失去其光辉。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "哎呀。",
                "0_H141": "那是……原来如此，这次是同伴吗。",
                "0_H142": "享誉盛名的骑士王之剑值得信赖。"
            }
        },
        "0_H160": {
            "name": "对话 5",
            "text": {
                "0_H160": "你……你是……征服王吗！？",
                "0_H161": "你……你究竟经历了什么事",
                "0_H162": "十年后才会变成那样啊！？"
            }
        },
        "0_H170": {
            "name": "对话 6",
            "text": {
                "0_H170": "什么，吾王芬恩·麦克库尔啊！",
                "0_H171": "这次居然能再度与您一起战斗。",
                "0_H172": "这是我无上的荣耀。！",
                "0_H173": "……话说，王妃殿下该不会也……",
                "0_H174": "不，没事。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "费奥纳骑士团的第一勇士，",
                "0_S011": "迪尔姆德·奥迪那。 前来报到。",
                "0_S012": "从今往后将成为侍奉您的从者。"
            }
        }
    },
    "ChrVoice_ids": [
        "300200"
    ]
}
{
    "id": 1101000,
    "collectionNo": 303,
    "name": "平景清",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "斩！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "这就是…… 怨之焰！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "娑罗双树之花色……",
                "0_B051": "换言之！",
                "0_B052": "领教此世定律吧！",
                "0_B053": "───『诸行无常·盛者必衰』！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "源氏，受死吧……！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "景清不死。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "源氏在哪儿……"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "将汝等悉数毁灭。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "……源氏，受死吧。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "屠戮完成。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "怨之一字，决不会消失……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "千万……不能忘记……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "怨……"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "斩……"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "执行屠戮。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "此乃定律。"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "景清为影。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "吾要杀光汝等。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "快看，娑罗双树绽放了。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "尘芥，尘芥……"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "可恶——！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "决不能……就此罢休……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "别了……！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "常为虚缈，世间万物……",
                "0_B801": "皆为尘芥！",
                "0_B802": "吾之怨，已缠住了汝！",
                "0_B803": "───『诸行无常·盛者必衰』！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "平家之怨…… 化作火焰！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "呵呵呵……"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "……到此为止了。接下来，就非吾本意了。",
                "0_G021": "若要继续前进……",
                "0_G022": "……哼。届时再说吧。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "源氏，受死吧。",
                "0_G031": "共同赴死吧。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "好吧。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "那就吃掉吧。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "……？",
                "0_H011": "诞生于世的日子……你的……",
                "0_H012": "这样啊。那去找亲密之人一起度过吧。",
                "0_H013": "景清乃是怨之一字……",
                "0_H014": "……住手。别拉吾的手臂。别企图拿下吾之面具。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "只要是有源氏的地方，去哪里都行。",
                "0_H021": "……好了。等待吾的会是什么样的源氏呢？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "来吧。",
                "0_H031": "杀死，源氏。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "说出下个源氏之所在吧。",
                "0_H041": "你要充分运用景清。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "你是……",
                "0_H051": "不，只是些小事，忘了吧。",
                "0_H052": "吾让你忘掉。",
                "0_H053": "……喂，不要动吾的面具。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "屠戮源氏。",
                "0_H061": "除此以外还能是什么。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "屠戮源氏。",
                "0_H071": "这世间的源氏太多了……"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "吾之愿望，只有一个。",
                "0_H081": "也就是——源氏，受死吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "源氏在哪儿？",
                "0_H091": "你该不会……",
                "0_H092": "有藏匿他们吧……"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "吾乃景清。",
                "0_H101": "杀死源氏之人就是景清，也就是吾。",
                "0_H102": "这世间的赖朝究竟在何处……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "吾乃杀害源氏之人。",
                "0_H111": "吾乃为害源氏之人。",
                "0_H112": "……源氏乃是盛者，盛者乃是源氏。",
                "0_H113": "景清挥刀的对象乃是盛者，也是源氏，因此吾之敌人，就是源氏。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "你将景清当做刀剑使用……",
                "0_H121": "其实并无大碍。",
                "0_H122": "你要战斗的对象是扭曲的盛者，那对方自然就是源氏。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "咯咯。诡辩，诡辩，没错，诡辩！",
                "0_H131": "没错，你战斗的对象并不是血脉意义上的源氏吧！",
                "0_H132": "谁管那么多啊！　吾已说过了！",
                "0_H133": "景清屠戮的是源氏，也就是扭曲变形的盛者！",
                "0_H134": "…………在这短暂存在的影子消失之前，定将杀死对你而言的源氏。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "源氏，受死吧。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "源氏之臣，受死吧。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "侍奉源氏之人，应被毁灭。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "遮那王，遮那王。",
                "0_H201": "愚蠢的幼儿，可悲的傀儡啊。",
                "0_H202": "将你……",
                "0_H203": "吾……",
                "0_H204": "吾……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "噢噢……噢噢噢……！",
                "0_H211": "身处黑暗之人，背负愿望之人。",
                "0_H212": "与吾相近的存在。",
                "0_H213": "倒不如说，这个人的存在方式，就是景清的起源吧……？"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "复仇者……？",
                "0_H221": "这样啊。与吾一样熊熊燃烧的怨之一字。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "有意思。",
                "0_H231": "巴御前，是足以成为景清四肢的存在哦……？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "——平景清，到来。",
                "0_S011": "来吧，告诉景清吧。",
                "0_S012": "该被屠戮的源氏究竟在何处。"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "恶鬼……必灭！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "娑罗双树之花色……",
                "2_B051": "换言之！",
                "2_B052": "领教此世定律吧！",
                "2_B053": "───『诸行无常·盛者必衰』！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "来吧，源氏赴死吧。"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "不死……"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "景清出击。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "主人，请指示。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "源氏，受死吧。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "请投胎重生吧。"
            }
        },
        "2_B140": {
            "name": "受击 2",
            "text": {
                "2_B140": "可恶……"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "兄长大人……兄长……大人……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "主……公……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "明白了……"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "交给我吧……"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "想感受一下怨之字吗？"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "开始屠戮。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "好热……好热……"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "火焰啊……至此！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "乃是……影……"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "我是……景清……也是……义经……"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "呼……盛者必衰……"
            }
        },
        "2_B530": {
            "name": "胜利 4",
            "text": {
                "2_B530": "南无……八幡……"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "为什么……为何……"
            }
        },
        "2_B570": {
            "name": "无法战斗 4",
            "text": {
                "2_B570": "我……究竟……做了什么……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "常为虚缈，世间万物……",
                "2_B801": "皆为尘芥！",
                "2_B802": "吾之怨，已缠住了汝！",
                "2_B803": "───『诸行无常·盛者必衰』！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "兄长大人……兄长大人！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "魔力……"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "面具……碎了？",
                "2_G041": "吾是……不，我是……",
                "2_G042": "景清……",
                "2_G043": "平景清，没错，屠戮源氏的存在……",
                "2_G044": "同时，也是九郎判官义经……！"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "……不太……明白。不明白。",
                "2_G051": "吾乃景清。",
                "2_G052": "怨之文字，复仇者。",
                "2_G053": "但是吾之内心确实有我的存在。",
                "2_G054": "主人，你……",
                "2_G055": "知道我……知道义经吗……？"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "是吗……"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "收下了。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "……慢着。今天，好像有什么……",
                "2_H011": "不，算了。",
                "2_H012": "总觉得今天好像有什么事，但这是不可能的。",
                "2_H013": "那吾先走了。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "好像……有什么。",
                "2_H021": "似乎能听到……",
                "2_H022": "是祭典吗？"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "主人，该站起来了。",
                "2_H031": "去杀源氏。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "吾知道自己该干什么。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "主人……",
                "2_H051": "你是，我的……主人……"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "屠戮源氏。",
                "2_H061": "其他什么都不需要。",
                "2_H062": "应该……不需要才对……"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "赖朝，不可饶恕。",
                "2_H071": "源氏，受死吧。",
                "2_H072": "赖朝……"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "愿望？",
                "2_H081": "那就将赖朝……这次一定要亲手将他的首级……",
                "2_H082": "第三十八次的真实。",
                "2_H083": "对，这次如果能说上话……"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "源氏，源氏，源氏！",
                "2_H171": "有源氏的气味……"
            }
        },
        "2_H190": {
            "name": "对话 5",
            "text": {
                "2_H190": "源氏的仆从……",
                "2_H191": "看来还有点用处呢。",
                "2_H192": "呵呵呵。"
            }
        },
        "2_H200": {
            "name": "对话 6",
            "text": {
                "2_H200": "牛若丸……？",
                "2_H201": "不是遮那王，是吗，是牛若啊。",
                "2_H202": "呵呵。多么令人怀念啊——"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "怎么了。你把……吾之面容看成谁了。",
                "2_H20091": "这不过是临时的肉体。",
                "2_H20092": "景清的本性是怨之一字，是想要永无止尽地屠戮源氏的意志。",
                "2_H20093": "但是，现在，我……",
                "2_H20094": "获得了身为义经的意识……"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "没兴趣。没错，没兴趣。",
                "2_H20101": "临时的身体……",
                "2_H20102": "景清从过去起就一直通过变更躯体维持存在。没兴趣。",
                "2_H20103": "哪怕你认识这具躯体……"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "主人。",
                "2_H20111": "……主公，我……",
                "2_H20112": "是景清……",
                "2_H20113": "还是义经……"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "主公……是我。",
                "2_H20121": "您能认出来吗。我是牛若。",
                "2_H20122": "……不，身为牛若灵基的时候，与您相遇时的记录还在遥远的彼方。",
                "2_H20123": "但是，现在……",
                "2_H20124": "看来，我对兄长大人……对源氏的各位……怀着这样的想法呢……",
                "2_H20125": "因此，才会被覆盖。景清，覆盖在我身上……"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H20130": "吾……不，我，牛若，九郎判官义经——",
                "2_H20131": "心怀憎恨。",
                "2_H20132": "憎恨兄长大人，憎恨源氏。憎恨所有认为我不再有用而予以抛弃的存在。",
                "2_H20133": "但是……这并不是……我的全部。",
                "2_H20134": "心念兄长大人，心念源氏的大家，心念世间众生，与您并肩战斗的我，也确实是我。",
                "2_H20135": "希望您能……务必……记住这些。"
            }
        },
        "2_H210": {
            "name": "对话 7",
            "text": {
                "2_H210": "被期望成为……世间所有恶的存在……",
                "2_H211": "原来如此。难怪会令人惬意。"
            }
        },
        "2_H220": {
            "name": "对话 8",
            "text": {
                "2_H220": "复仇者……",
                "2_H221": "总有一天会燃烧殆尽吧。",
                "2_H222": "无论是我，还是你们。"
            }
        },
        "2_H230": {
            "name": "对话 9",
            "text": {
                "2_H230": "离我……离景清远一点。",
                "2_H231": "因为你……",
                "2_H232": "成功抛弃了怨之字。"
            }
        },
        "2_H240": {
            "name": "对话 10",
            "text": {
                "2_H240": "常陆坊，常陆坊……",
                "2_H241": "不要……看我……"
            }
        },
        "2_H250": {
            "name": "对话 11",
            "text": {
                "2_H250": "源氏的气味……",
                "2_H251": "但是，那金发碧眼是……",
                "2_H252": "不，好浓郁……好浓郁啊……"
            }
        }
    },

{
    "id": 601900,
    "collectionNo": 117,
    "name": "风魔小太郎",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "发破！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "破碎！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "此处，",
                "0_B051": "乃阿鼻叫唤！",
                "0_B052": "大炎热地狱，",
                "0_B053": "不灭的混沌旅团！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "杀出一条血路！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "快说你的辞世句吧……！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "请不要恨我。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "那么，那个……我就上了。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "用剑刃终结其心。 虽然很遗憾，但不留慈悲。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "既为忍者……不，没什么。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "穷途非末路……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "什么都……没有。 什么都……没有……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "吾主啊，希望您能平安无事……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "对准要害吧！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "明白了……！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "谨遵吩咐！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "那就用杀手锏吧。"
            }
        },
        "0_B230": {
            "name": "宝具卡 2",
            "text": {
                "0_B230": "燃起地狱之火。 全都化为鬼吧！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "斩击！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "毁灭！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "攻击！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "临兵斗者皆阵列在前。"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "金刚·智拳印。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "用剑刃终结其心。 ……出击！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "带上六文钱吧。 你需要这个。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "去死吧。无需道歉。只需就地去死。"
            }
        },
        "0_B540": {
            "name": "受击 3",
            "text": {
                "0_B540": "大意了！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "到此为止……了吗…… 段藏阁下，主公就拜托您了……！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "全员集合！",
                "0_B801": "动摇秩序乃是吾等之宿命。",
                "0_B802": "铸造混沌吧！",
                "0_B803": "不灭的混沌旅团！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "……稍微……变强一点了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "虽然十分感激，但感觉很对不起您……",
                "0_G021": "得您如此信赖，就得做出相应的贡献才行吧……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "难得的机会……我拿出了先代的旧衣服。",
                "0_G031": "……穿起来还合适吗？"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "如此一来，终于能得见风魔的全力了。",
                "0_G041": "请您见识吾等忍术的精髓……！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "像我这样的生手居然能达到如此境界……谢谢您。",
                "0_G051": "这一切都多亏了主人。",
                "0_G052": "哎？　问这房间里堆满的武器是什么吗？",
                "0_G053": "……对不起，这应该算是我为数不多的兴趣…… 一看到好武器，总是忍不住想要收集……",
                "0_G054": "这是个好机会，主人也选一件如何……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "今天好像是主人诞生的日子吧。",
                "0_H011": "元服仪式办过了吗？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "主人。好像……发生了什么情况……",
                "0_H021": "如果您在意的话，我愿意奉陪……"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "主人。",
                "0_H031": "请……那个……不……没什么……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "既然被雇佣，就要……努力…… 该干活的时候就要努力干活，这是我的信条……"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "在主人成为一名优秀武将之前……",
                "0_H051": "不，是成为优秀大名之前，加油努力吧。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "手里剑、刀、锁镰、替身用的木材和火药弹、还有整理药草吧。 维护是很重要的哦？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西…… 胆敢玷污吾等风魔尊严之人。 他们是明确的敌人。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯……",
                "0_H081": "大概希望看到不由武士，而是由忍者支配的世界吧。",
                "0_H082": "……开玩笑，我是开玩笑的啦……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "是，我的主人。",
                "0_H091": "风魔小太郎前来报到。若有任务，请尽情差遣。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "主人……那个，您似乎不太摆架子呢。",
                "0_H101": "不，没什么，并不是指您没有威严或是你不可怕……",
                "0_H102": "只是觉得侍奉您很轻松。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "问我有没有更夸张的武器吗？",
                "0_H111": "唔……手里剑也有种类，车剑之流看起来虽然好玩……",
                "0_H112": "但那个没什么威力。投掷果然还是短刀以及苦无比较偏向实战……",
                "0_H113": "啊，您不是这个意思吧……对不起……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "这头红发是异国出身的证明。父亲也是这样。",
                "0_H121": "……不仅如此，我似乎是在各种错误的尝试后诞生的孩子。",
                "0_H122": "……像是人，也不像人，又像是非人……之类的存在。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "虽然我……或许不是人类……但现在我觉得，这样也行。",
                "0_H131": "谢谢您。",
                "0_H132": "现在我会为了主人您，运用自身的一切。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "武士吗……",
                "0_H141": "而且还是架空的武士啊。",
                "0_H142": "果然，武士早已……",
                "0_H143": "扎根于这个国度了啊……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "从某种角度来看……",
                "0_H151": "我和咒腕哈桑阁下最接近……",
                "0_H152": "他试图用恶魔的手臂，而我则试图用鬼之躯体纠正邪恶……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "啊，是足柄山的大前辈……！",
                "0_H161": "纸和笔，纸和笔！ 希望他能在这里留一笔……！"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "宝具名？ 啊，那是从父亲那里继承而来的。",
                "0_H171": "据说他是个红发碧眼的魁梧男性。",
                "0_H172": "恐怕是出身南蛮的人吧。"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "母亲被召唤来了吗。",
                "0_H181": "不，不需要告诉她。",
                "0_H182": "我和她没有见过。",
                "0_H183": "这样就行了。",
                "0_H184": "没必要让她为丧失而痛苦。"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (亚种特异点Ⅲ通关后)",
            "text": {
                "0_H1800": "有时，会觉得在下总的那些日子就像发生在昨天。",
                "0_H1801": "每当想起这些，力量都会涌现。",
                "0_H1802": "我身在此处，我已经能从灵魂深处理解其意义了。",
                "0_H1803": "但是这件事姑且不论……",
                "0_H1804": "我再也不想和那些英灵剑豪较量了……"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "李书文阁下。",
                "0_H191": "……关于主人的那把木刀，是否能够定做呢……？"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "不久之前，我和茨木童子以对话为名吵了一架。",
                "0_H201": "当时，我忍不住讥讽地对她说，",
                "0_H202": "「那迟早有一天，渡边纲阁下也会追着你被召唤而来吧。」",
                "0_H203": "然后那个……茨木童子呢喃着「纲要来啊」，并露出了笑容。",
                "0_H204": "鬼居然还会露出那样的笑容啊。"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "主人，不好了。",
                "0_H211": "因为戈尔德鲁夫阁下说「光靠这些食物营养可不均衡」，",
                "0_H212": "我就将特产风魔馒头送给他了，可他却面色青紫翻肚子倒下了。",
                "0_H213": "啊唔。只要能习惯，这味道应该不差啊……啊唔。"
            }
        },
        "0_H2100": {
            "name": "对话 1 (牵绊Lv.5以上)",
            "text": {
                "0_H2100": "主人。",
                "0_H2101": "该出发了，来吧，出发吧。",
                "0_H2102": "让我们打起精神出发吧！"
            }
        },
        "0_H220": {
            "name": "对话 12",
            "text": {
                "0_H220": "果心居士阁下……！　感谢您一直关照吾等之祖，以及母亲……段藏阁下。",
                "0_H221": "哎，问我想不想把身体的某部分换成机关？",
                "0_H222": "哈哈哈，这倒是不至于……哎，飞行应该也能做得到？",
                "0_H223": "……能允许我，能允许我稍微考虑一下吗……！　三天，就给我三天时间！"
            }
        },
        "0_H230": {
            "name": "对话 13",
            "text": {
                "0_H230": "伊吹童子……与其说是鬼种，更像是等同于神明的存在呢。",
                "0_H231": "对我来说，无论是鬼还是神，那身高都是令人羡慕的……"
            }
        },
        "0_H240": {
            "name": "对话 14",
            "text": {
                "0_H240": "果心居士阁下，在此前的节日中，十分感谢。",
                "0_H241": "段藏阁下和主公看上去都玩得非常高兴。",
                "0_H242": "哎，我吗？",
                "0_H243": "……这个嘛，虽然危机重重，但应该……",
                "0_H244": "也玩得非常高兴吧。"
            }
        },
        "0_H250": {
            "name": "对话 15",
            "text": {
                "0_H250": "您是……僧兵的……",
                "0_H251": "竟然是位女性，太令人惊讶了。",
                "0_H252": "哎，原本是男性，但被果心居士阁下……",
                "0_H253": "原来如此，身为相关人员，请允许我向您谢罪。",
                "0_H254": "……哎，下个目标会不会是我？",
                "0_H255": "哈哈哈，怎么可能呢。",
                "0_H256": "果心居士阁下，应该不是吧？",
                "0_H257": "……果心居士阁下？"
            }
        },
        "0_I010": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 1",
            "text": {
                "0_I010": "主公，这里是风魔商店。",
                "0_I011": "虽然我很想无偿献给主公，但规定必须用搜集的道具来兑换。"
            }
        },
        "0_I020": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 2",
            "text": {
                "0_I020": "欢迎光临风魔商店，我的主公。",
                "0_I021": "好了，您想要什么呢？"
            }
        },
        "0_I030": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 3",
            "text": {
                "0_I030": "您眼光真是太好了，那么，请收下这个。",
                "0_I031": "不需要六文钱哦。"
            }
        },
        "0_I040": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 4",
            "text": {
                "0_I040": "主公的兴趣倒是相当有意思呢。",
                "0_I041": "嗯，请收下。"
            }
        },
        "0_I050": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 5",
            "text": {
                "0_I050": "在风魔商店不可使用数字货币。",
                "0_I051": "很遗憾，IT化还没有发展到这个地步……",
                "0_I052": "综上所述，请收下这个。"
            }
        },
        "0_I060": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 6",
            "text": {
                "0_I060": "看来活动也结束了。",
                "0_I061": "太阳西沉，月亮隐身，一切都沉入了夜色。",
                "0_I062": "也就是说……我也同样应该潜入黑暗中了。",
                "0_I063": "只不过，其实我还是一如既往地躲在天花板上。",
                "0_I064": "如果您有什么事，还请随时召唤我。"
            }
        },
        "0_I070": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 7",
            "text": {
                "0_I070": "啊，主公。",
                "0_I071": "很遗憾，活动已经结束了。",
                "0_I072": "但还可以兑换道具哦。来，请吧。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，Assassin。",
                "0_S011": "风魔小太郎。",
                "0_S012": "在下虽不才，但还请多关照……"
            }
        },
        "11_B010": {
            "name": "攻击 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B010": "得手了！"
            }
        },
        "11_B020": {
            "name": "攻击 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B020": "上！"
            }
        },
        "11_B030": {
            "name": "攻击 3 尽享夏日假期用忍者装束",
            "text": {
                "11_B030": "爆响！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B040": "忍·忍！"
            }
        },
        "11_B080": {
            "name": "技能 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B080": "没问题！"
            }
        },
        "11_B090": {
            "name": "技能 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B090": "很好！"
            }
        },
        "11_B100": {
            "name": "开始 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B100": "尽管还在度假，但一事归一事，战斗开始！"
            }
        },
        "11_B110": {
            "name": "开始 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B110": "这身打扮的我并不是风魔……而是谜之忍者男孩，K。"
            }
        },
        "11_B120": {
            "name": "胜利 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B120": "那我们继续观光吧。 好开心！……咳咳。"
            }
        },
        "11_B130": {
            "name": "胜利 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B130": "忍者在现代也是行得通的。 也就是说，别小看我。"
            }
        },
        "11_B150": {
            "name": "受击 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B150": "不好了，帽子……！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B160": "战略性撤退，这并不是败北。散！"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B170": "呜……观光导致身体迟钝了吗……"
            }
        },
        "11_B180": {
            "name": "指令卡 1 尽享夏日假期用忍者装束",
            "text": {
                "11_B180": "对准要害吧！"
            }
        },
        "11_B190": {
            "name": "指令卡 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B190": "明白了……！"
            }
        },
        "11_B200": {
            "name": "指令卡 3 尽享夏日假期用忍者装束",
            "text": {
                "11_B200": "谨遵吩咐！"
            }
        },
        "11_B210": {
            "name": "宝具卡 尽享夏日假期用忍者装束",
            "text": {
                "11_B210": "凭这身打扮他们会回应召唤吗……？"
            }
        },
        "11_B410": {
            "name": "攻击 4 尽享夏日假期用忍者装束",
            "text": {
                "11_B410": "斩击！"
            }
        },
        "11_B420": {
            "name": "攻击 5 尽享夏日假期用忍者装束",
            "text": {
                "11_B420": "毁灭！"
            }
        },
        "11_B430": {
            "name": "攻击 6 尽享夏日假期用忍者装束",
            "text": {
                "11_B430": "攻击！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 尽享夏日假期用忍者装束",
            "text": {
                "11_B440": "临兵斗者皆阵列在前。"
            }
        },
        "11_B480": {
            "name": "技能 3 尽享夏日假期用忍者装束",
            "text": {
                "11_B480": "金刚·智拳印。"
            }
        },
        "11_B540": {
            "name": "受击 3 尽享夏日假期用忍者装束",
            "text": {
                "11_B540": "糟了！"
            }
        },
        "11_G010": {
            "name": "升级 尽享夏日假期用忍者装束",
            "text": {
                "11_G010": "……稍微……变强一点了。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 尽享夏日假期用忍者装束",
            "text": {
                "11_G060": "忍者就是能适应各种时代变换外形的存在。",
                "11_G061": "综上所述，我换了一套灵衣。",
                "11_G062": "您感觉如何，主公。",
                "11_G063": "根据达·芬奇阁下的形容，有种狂妄初中三年级学生的感觉。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 尽享夏日假期用忍者装束",
            "text": {
                "11_H9000": "主公？　怎么了？",
                "11_H9001": "说仔细看来，我有些像同年级的学生吗……",
                "11_H9002": "原来如此。那就用同年级学生的风格说话吧？",
                "11_H9003": "咳咳……",
                "11_H9004": "喂，主公！　一起来打棒球吧！",
                "11_H9005": "……感觉有些不太对吧……"
            }
        }
    },
    "ChrVoice_ids": [
        "601900"
    ]
}
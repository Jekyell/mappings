    "voices": {
        "0_B010": {
            "name": "アタック 1",
            "text": {
                "0_B010": "哈——！"
            }
        },
        "0_B020": {
            "name": "アタック 2",
            "text": {
                "0_B020": "劈成两半！"
            }
        },
        "0_B030": {
            "name": "アタック 3",
            "text": {
                "0_B030": "好嘞——！"
            }
        },
        "0_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "0_B040": "拒绝。腐朽而死吧！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "律法归于无。稳健化为斗争。",
                "0_B051": "平坦的大地化作山岳。一切溶解为混沌。",
                "0_B052": "『虚妄乃黑暗之女』！",
                "0_B053": "……来吧，噩梦的开端。"
            }
        },
        "0_B080": {
            "name": "スキル 1",
            "text": {
                "0_B080": "莉莉图"
            }
        },
        "0_B090": {
            "name": "スキル 2",
            "text": {
                "0_B090": "我觉得这就是力量提升！"
            }
        },
        "0_B100": {
            "name": "開始 1",
            "text": {
                "0_B100": "准备好了吗？"
            }
        },
        "0_B110": {
            "name": "開始 2",
            "text": {
                "0_B110": "OK，别在意，我们上，迦～勒～底～！……那就开杀吧！"
            }
        },
        "0_B120": {
            "name": "勝利 1",
            "text": {
                "0_B120": "我赢了！毕竟人家是暗之女嘛～？"
            }
        },
        "0_B130": {
            "name": "勝利 2",
            "text": {
                "0_B130": "怎么可能战胜得了风暴本身嘛——"
            }
        },
        "0_B140": {
            "name": "ダメージ 2",
            "text": {
                "0_B140": "碍事！"
            }
        },
        "0_B150": {
            "name": "ダメージ 1",
            "text": {
                "0_B150": "好疼—！"
            }
        },
        "0_B160": {
            "name": "戦闘不能 1",
            "text": {
                "0_B160": "到此为止了吗……真是这样啊……啊啊……"
            }
        },
        "0_B170": {
            "name": "戦闘不能 2",
            "text": {
                "0_B170": "御主……牵个手……总可以吧……？"
            }
        },
        "0_B180": {
            "name": "コマンドカード 1",
            "text": {
                "0_B180": "好的"
            }
        },
        "0_B190": {
            "name": "コマンドカード 2",
            "text": {
                "0_B190": "好的好的"
            }
        },
        "0_B200": {
            "name": "コマンドカード 3",
            "text": {
                "0_B200": "才不要呢"
            }
        },
        "0_B2010": {
            "name": "アタック 10",
            "text": {
                "0_B2010": "塔巴特阿赫尔！"
            }
        },
        "0_B2020": {
            "name": "アタック 11",
            "text": {
                "0_B2020": "超棒！"
            }
        },
        "0_B2030": {
            "name": "アタック 12",
            "text": {
                "0_B2030": "砍成碎块了！"
            }
        },
        "0_B210": {
            "name": "宝具カード 1",
            "text": {
                "0_B210": "宝具是吧~。好嘞~"
            }
        },
        "0_B220": {
            "name": "宝具カード 2",
            "text": {
                "0_B220": "你要我使用宝具吗？但这样会死，你确定吗？"
            }
        },
        "0_B230": {
            "name": "宝具カード 3",
            "text": {
                "0_B230": "呼啸吧狂风，唤来长夜。"
            }
        },
        "0_B2410": {
            "name": "アタック 13",
            "text": {
                "0_B2410": "就算开个洞也没关系～？"
            }
        },
        "0_B2420": {
            "name": "アタック 14",
            "text": {
                "0_B2420": "别碰我！"
            }
        },
        "0_B2430": {
            "name": "アタック 15",
            "text": {
                "0_B2430": "去死吧！"
            }
        },
        "0_B2440": {
            "name": "エクストラアタック 4",
            "text": {
                "0_B2440": "英雄去死！勇者去死！"
            }
        },
        "0_B250": {
            "name": "宝具カード 4",
            "text": {
                "0_B250": "要不要让你见识下我的帅气之处？"
            }
        },
        "0_B410": {
            "name": "アタック 4",
            "text": {
                "0_B410": "拍飞你！"
            }
        },
        "0_B420": {
            "name": "アタック 5",
            "text": {
                "0_B420": "里克多·哈尔阿哈！"
            }
        },
        "0_B430": {
            "name": "アタック 6",
            "text": {
                "0_B430": "基修夫·阿赫尔！"
            }
        },
        "0_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "0_B440": "敢妨碍我就去死吧！"
            }
        },
        "0_B480": {
            "name": "スキル 3",
            "text": {
                "0_B480": "吞噬黑夜"
            }
        },
        "0_B490": {
            "name": "スキル 4",
            "text": {
                "0_B490": "风暴将至"
            }
        },
        "0_B500": {
            "name": "開始 3",
            "text": {
                "0_B500": "……既然会被干掉，那只好先下手为强了"
            }
        },
        "0_B510": {
            "name": "開始 4",
            "text": {
                "0_B510": "我会招来风暴～。也会招来夜晚？因为是朋友嘛。"
            }
        },
        "0_B520": {
            "name": "勝利 3",
            "text": {
                "0_B520": "曝尸荒野，任凭风雨侵蚀消逝……生命真是虚幻无常啊……"
            }
        },
        "0_B530": {
            "name": "勝利 4",
            "text": {
                "0_B530": "嗯~？还不打算放弃吗~？那么，追加来自地狱的订单！"
            }
        },
        "0_B540": {
            "name": "ダメージ 4",
            "text": {
                "0_B540": "痛死了啦！"
            }
        },
        "0_B550": {
            "name": "ダメージ 3",
            "text": {
                "0_B550": "……你这混蛋！"
            }
        },
        "0_B560": {
            "name": "戦闘不能 3",
            "text": {
                "0_B560": "对不起……对不起……明明约好了的……明明约好了的……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "真实乃时间之女。那么虚妄便是黑暗之女。",
                "0_B801": "滴答、滴答。逆流的时间如风暴般肆虐。",
                "0_B802": "『虚妄乃黑暗之女』！",
                "0_B803": "……去死吧"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "碎尸粘贴杀人事件！"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "宰了你"
            }
        },
        "0_B910": {
            "name": "アタック 7",
            "text": {
                "0_B910": "赫勒布哈泽尔！"
            }
        },
        "0_B920": {
            "name": "アタック 8",
            "text": {
                "0_B920": "苹果，轰—！"
            }
        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "德奇拉特！"
            }
        },
        "0_G010": {
            "name": "レベルアップ 1",
            "text": {
                "0_G010": "好好好，强化强化——"
            }
        },
        "0_G020": {
            "name": "霊基再臨 2",
            "text": {
                "0_G020": "好的！中继点已通过！接下来打算大展拳脚隆重推进，要是能给我备足种火就太好啦！",
                "0_G021": ""
            }
        },
        "0_G030": {
            "name": "霊基再臨 1",
            "text": {
                "0_G030": "好嘞！这下就强化了！……虽说只是脱掉外套敞开胸口而已。你觉得这翅膀能飞吗？其实是可以的啦。不过准确来说与其说是飞，更像是滑翔机吧。",
                "0_G031": "",
                "0_G032": "",
                "0_G033": "",
                "0_G034": ""
            }
        },
        "0_G110": {
            "name": "レベルアップ 2",
            "text": {
                "0_G110": "哦！变强了呢！干得不错，御主！",
                "0_G111": ""
            }
        },
        "0_G120": {
            "name": "レベルアップ 3",
            "text": {
                "0_G120": "……嗯——！又甜又辣，又酸又鲜？啊！但感觉还有青春特有的苦涩——！",
                "0_G121": "",
                "0_G122": ""
            }
        },
        "0_H010": {
            "name": "誕生日",
            "text": {
                "0_H010": "生日快乐！嗯，你能出生我超开心的！虽然生日不是同一天有点遗憾，但死的那天会在一起哦～",
                "0_H011": "",
                "0_H012": ""
            }
        },
        "0_H020": {
            "name": "イベント開催中",
            "text": {
                "0_H020": "活动进行中～。来吧，打起精神来。然后也给我点东西吧～",
                "0_H021": "",
                "0_H022": ""
            }
        },
        "0_H030": {
            "name": "会話 1",
            "text": {
                "0_H030": "快看快看，要出来了～！"
            }
        },
        "0_H040": {
            "name": "会話 2",
            "text": {
                "0_H040": "主从……什么嘛，御主是真的想当御主吗？是想让我屈服的感觉吗？“不，并不是这样的”？什么嘛，真无聊！",
                "0_H041": "",
                "0_H042": "",
                "0_H043": "",
                "0_H044": ""
            }
        },
        "0_H050": {
            "name": "会話 3",
            "text": {
                "0_H050": "与御主的关系吗… 唔… 保密哦",
                "0_H051": ""
            }
        },
        "0_H060": {
            "name": "好きなこと",
            "text": {
                "0_H060": "喜欢的事吗？嗯……风和夜晚。还有就是观察人类？ 观察人类这种说法有点难为情呢。不过嘛，我确实挺喜欢人类的。",
                "0_H061": "",
                "0_H062": "",
                "0_H063": ""
            }
        },
        "0_H070": {
            "name": "嫌いなこと",
            "text": {
                "0_H070": "讨厌的东西、讨厌的人、讨厌的从者，唔，好像没有？",
                "0_H071": ""
            }
        },
        "0_H080": {
            "name": "聖杯について",
            "text": {
                "0_H080": "寄托于圣杯的愿望？嗯，不需要。我没什么想要的啦。如果御主想要的话就给你吧～",
                "0_H081": "",
                "0_H082": "",
                "0_H083": ""
            }
        },
        "0_H090": {
            "name": "絆Lv.1",
            "text": {
                "0_H090": "哦，御主，您有什么话要对我说吗？",
                "0_H091": ""
            }
        },
        "0_H100": {
            "name": "絆Lv.2",
            "text": {
                "0_H100": "我虽然是美索不达米亚出身，但和黄金之王或是帅哥泥偶都没有什么缘分呢。 啊，当然，当然我也不是离过婚的人。",
                "0_H101": ""
            }
        },
        "0_H1000": {
            "name": "絆Lv.1 (奏章Ⅳクリア以降)",
            "text": {
                "0_H1000": "啊，基列莱特不在呢。那好。",
                "0_H1001": ""
            }
        },
        "0_H110": {
            "name": "絆Lv.3",
            "text": {
                "0_H110": "我是岚与夜的化身，怎么样？很帅吧？ 用现在的话来说，就是风属性和暗属性持有者。 用风系异能斩断敌人，再用暗系异能令敌人陷入阴沉！ 暗系再让我考虑一下吧。",
                "0_H111": "",
                "0_H112": "",
                "0_H113": "",
                "0_H114": ""
            }
        },
        "0_H120": {
            "name": "絆Lv.4",
            "text": {
                "0_H120": "啊，御主？虽然有点突然，但我爱你哦。不，我觉得不说出来可不行。毕竟我们已经相当了解彼此了，虽然我想你早就知道了吧。",
                "0_H121": "",
                "0_H122": "",
                "0_H123": "",
                "0_H124": ""
            }
        },
        "0_H1200": {
            "name": "絆Lv.2 (奏章Ⅳクリア以降)",
            "text": {
                "0_H1200": "对玛修太执着？哈？才没有执着呢。",
                "0_H1201": ""
            }
        },
        "0_H130": {
            "name": "絆Lv.5",
            "text": {
                "0_H130": "我说啊，御主。有件重要的事要告诉你。 我，你死了的话，我也会死。 咦？御主死了的话，从者也会消失？ 不对不对，不是这样的。 我是说，如果御主你被杀掉的话，我会当场立即砸碎自己的灵核去死。 所以放心吧，御主。你死的时候绝对不孤单。 我我永远会比任何人都先走一步。 对吧。",
                "0_H131": "",
                "0_H132": "",
                "0_H133": "",
                "0_H134": "",
                "0_H135": "",
                "0_H136": "",
                "0_H137": ""
            }
        },
        "0_H1400": {
            "name": "絆Lv.3 (奏章Ⅳクリア以降)",
            "text": {
                "0_H1400": "地狱般的日子很愉快吗？ 虽然我的记忆已经模糊到只能窥见雾中深处的程度， 但唯有那份情感仍铭记于心。 遗憾，终究未能如愿吗。",
                "0_H1401": "",
                "0_H1402": ""
            }
        },
        "0_H1600": {
            "name": "絆Lv.4 (奏章Ⅳクリア以降)",
            "text": {
                "0_H1600": "为什么讨厌基列莱特呢？ 不，在地狱的时候我应该有充分的理由才对。 现在嘛……怎么说呢？ 或许是因为我想珍惜这份厌恶的感情才继续讨厌她吧。 青春期少女的心事可复杂了。",
                "0_H1601": "",
                "0_H1602": "",
                "0_H1603": "",
                "0_H1604": ""
            }
        },
        "0_H170": {
            "name": "会話 4",
            "text": {
                "0_H170": "哦哦，不良少年！这种摇滚风格真不错啊。要不要一起干点什么？组个队大闹一场？啊～原来是圆桌骑士啊。看来被品行端正束缚着呢～",
                "0_H171": "",
                "0_H172": "",
                "0_H173": "",
                "0_H174": ""
            }
        },
        "0_H180": {
            "name": "会話 5",
            "text": {
                "0_H180": "哈哈，这就是亚当！哎呀，这样不是挺好的吗？反正也跟阿维斯布隆没关系。不如说被叫成离异者更丢脸呢。啊，不过既然都这样了，要不干脆在肩膀上装个发射器吧？很帅吧？",
                "0_H181": "",
                "0_H182": "",
                "0_H183": "",
                "0_H184": ""
            }
        },
        "0_H1800": {
            "name": "絆Lv.5 (奏章Ⅳクリア以降)",
            "text": {
                "0_H1800": "我对自己所做之事毫无悔意，今后也绝不后悔。 即便被基列莱特讨厌也无所谓。 这世上，对基列莱特而言多一个这样的家伙又何妨。 我对此心满意足。 真温柔啊，御主。",
                "0_H1801": "",
                "0_H1802": "",
                "0_H1803": "",
                "0_H1804": ""
            }
        },
        "0_H190": {
            "name": "会話 6",
            "text": {
                "0_H190": "诶！？难道说这是我不为人知的一面？啊，不对吗？不过那双腿真帅气啊。我也装上那种机关会不会更好呢？战斗时加上旋转之类的？",
                "0_H191": "",
                "0_H192": "",
                "0_H193": "",
                "0_H194": ""
            }
        },
        "0_H200": {
            "name": "会話 7",
            "text": {
                "0_H200": "哦，阿育王。虽说这副身躯没有记忆，但灵魂还记得你。嗯…总觉得这说法不太对劲。不过看你精神不错就好。还在坚持转轮圣王之道吗？……自己说的话自己都听不懂了呢—啊哈哈哈哈",
                "0_H201": "",
                "0_H202": "",
                "0_H203": "",
                "0_H204": "",
                "0_H205": ""
            }
        },
        "0_H210": {
            "name": "会話 8",
            "text": {
                "0_H210": "啊——，是遗憾帅哥军团头目莫里亚蒂！在之前的特异点，你是律师来着。正义的伙伴，意外地挺适合你嘛——？啊哈哈，看你这超级嫌弃的表情！",
                "0_H211": "",
                "0_H212": "",
                "0_H213": ""
            }
        },
        "0_H220": {
            "name": "会話 9",
            "text": {
                "0_H220": "你是但丁·阿利吉耶里吧。我叫莉莉丝。除此之外就没什么了吧？毕竟我们俩看起来就严重合不来。不过既然你喜欢的是贝雅特丽齐，那方面倒是已经确定了。所以现在的关系，就是亲密的御主与其从者，勉强算朋友的朋友？缘分真浅啊！",
                "0_H221": "",
                "0_H222": "",
                "0_H223": "",
                "0_H224": "",
                "0_H225": "",
                "0_H226": ""
            }
        },
        "0_H230": {
            "name": "会話 10",
            "text": {
                "0_H230": "啊，四郎在！咦？听起来像狗的名字？那……就叫四郎吧！还是一如既往地憎恨人类？还是喜欢人类？『既可以是两者，也可以都不是』？看来你的人生很复杂呢。要不要像我一样，化为一阵风呢？如果是四郎的话，一定能化作一阵好风吧……不行吗，这样啊",
                "0_H231": "",
                "0_H232": "",
                "0_H233": "",
                "0_H234": "",
                "0_H235": "",
                "0_H236": "",
                "0_H237": "",
                "0_H238": ""
            }
        },
        "0_H2300": {
            "name": "会話 3 (絆Lv.5以上+奏章Ⅳクリア以降)",
            "text": {
                "0_H2300": "我之前没说吗？我愿为你而死。多么愉快的关系啊。",
                "0_H2301": "",
                "0_H2302": ""
            }
        },
        "0_H3060": {
            "name": "好きなこと (絆Lv.5以上+奏章Ⅳクリア以降)",
            "text": {
                "0_H3060": "你的爱意，会让人困扰吗？虽然不困扰，但无法回应？ 啊，不是这样的，御主。 人类啊，有时候光是怀抱着感情就能感到满足。 无论对方如何看待自己。",
                "0_H3061": "",
                "0_H3062": "",
                "0_H3063": "",
                "0_H3064": ""
            }
        },
        "0_H3070": {
            "name": "嫌いなこと (絆Lv.5以上+奏章Ⅳクリア以降)",
            "text": {
                "0_H3070": "讨厌的东西？ 你不是明知故问嘛。",
                "0_H3071": ""
            }
        },
        "0_S010": {
            "name": "召喚",
            "text": {
                "0_S010": "哟嚯——真没想到连我也被召唤来了。迦勒底还真是个挺邪恶的组织呢。 我是阿特斯，也就是莉莉丝。美索不达米亚的……嗯，像是精灵之类的玩意儿吧。总之请多指教喵。",
                "0_S011": "",
                "0_S012": "",
                "0_S013": "",
                "0_S014": "",
                "0_S015": "",
                "0_S016": ""
            }
        },
        "0_S110": {
            "name": "召喚 (奏章Ⅳクリア以降)",
            "text": {
                "0_S110": "呀吼~，是我！自从地狱一别，好久不见！开玩笑啦。不过既然我们之间有着如此深厚的缘分，无论如何都会互相吸引呢。 好啦。那么对于岚与夜的决心·莉莉丝，我想就不需要我再多做介绍了吧？ 请多指教咯。 啊，至于我嘛，会和基列莱特保持距离的。 这样对御主的身心健康比较好吧？",
                "0_S111": "",
                "0_S112": "",
                "0_S113": "",
                "0_S114": "",
                "0_S115": "",
                "0_S116": "",
                "0_S117": "",
                "0_S118": ""
            }
        },
        "2_B010": {
            "name": "アタック 1",
            "text": {
                "2_B010": "掳获"
            }
        },
        "2_B020": {
            "name": "アタック 2",
            "text": {
                "2_B020": "我会落下的"
            }
        },
        "2_B030": {
            "name": "アタック 3",
            "text": {
                "2_B030": "真可怜啊"
            }
        },
        "2_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "2_B040": "夜幕永恒"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "我恨你。我恨你。我恨你。",
                "2_B051": "将你推入黑暗，彻底碾碎。",
                "2_B052": "然而虚妄乃黑暗之女",
                "2_B053": "所以请你温暖地死去吧。"
            }
        },
        "2_B080": {
            "name": "スキル 1",
            "text": {
                "2_B080": "提高输出。"
            }
        },
        "2_B090": {
            "name": "スキル 2",
            "text": {
                "2_B090": "召唤暴风雨"
            }
        },
        "2_B100": {
            "name": "開始 1",
            "text": {
                "2_B100": "那么，我们开始吧？"
            }
        },
        "2_B110": {
            "name": "開始 2",
            "text": {
                "2_B110": "莉琳们，准备好了吗？"
            }
        },
        "2_B120": {
            "name": "勝利 1",
            "text": {
                "2_B120": "胜利了呢。来，御主，有什么表示吗？比如拥抱什么的。"
            }
        },
        "2_B130": {
            "name": "勝利 2",
            "text": {
                "2_B130": "怎么了？不过是死了一次而已吧？接下来就在地狱里再战吧"
            }
        },
        "2_B140": {
            "name": "ダメージ 2",
            "text": {
                "2_B140": "我拒绝。"
            }
        },
        "2_B150": {
            "name": "ダメージ 1",
            "text": {
                "2_B150": "呜……！咕……！"
            }
        },
        "2_B160": {
            "name": "戦闘不能 1",
            "text": {
                "2_B160": "御主……对不起……对不起……"
            }
        },
        "2_B170": {
            "name": "戦闘不能 2",
            "text": {
                "2_B170": "好困……"
            }
        },
        "2_B180": {
            "name": "コマンドカード 1",
            "text": {
                "2_B180": "好的"
            }
        },
        "2_B190": {
            "name": "コマンドカード 2",
            "text": {
                "2_B190": "好的"
            }
        },
        "2_B200": {
            "name": "コマンドカード 3",
            "text": {
                "2_B200": "请"
            }
        },
        "2_B2010": {
            "name": "アタック 10",
            "text": {
                "2_B2010": "上吧！"
            }
        },
        "2_B2020": {
            "name": "アタック 11",
            "text": {
                "2_B2020": "请尽情蹂躏吧"
            }
        },
        "2_B2030": {
            "name": "アタック 12",
            "text": {
                "2_B2030": "踢散"
            }
        },
        "2_B210": {
            "name": "宝具カード 1",
            "text": {
                "2_B210": "释放宝具"
            }
        },
        "2_B2100": {
            "name": "開始 5",
            "text": {
                "2_B2100": "莉琳，准备好了吗？"
            }
        },
        "2_B220": {
            "name": "宝具カード 2",
            "text": {
                "2_B220": "要动用王牌了呢"
            }
        },
        "2_B230": {
            "name": "宝具カード 3",
            "text": {
                "2_B230": "宝具？呵呵。"
            }
        },
        "2_B2410": {
            "name": "アタック 13",
            "text": {
                "2_B2410": "我喜欢坏坏的哦？"
            }
        },
        "2_B2420": {
            "name": "アタック 14",
            "text": {
                "2_B2420": "你受伤了吗？"
            }
        },
        "2_B2430": {
            "name": "アタック 15",
            "text": {
                "2_B2430": "请忍耐一下哦？"
            }
        },
        "2_B2440": {
            "name": "エクストラアタック 4",
            "text": {
                "2_B2440": "通往极小的地狱"
            }
        },
        "2_B250": {
            "name": "宝具カード 4",
            "text": {
                "2_B250": "准备好了吗？"
            }
        },
        "2_B410": {
            "name": "アタック 4",
            "text": {
                "2_B410": "啦～"
            }
        },
        "2_B420": {
            "name": "アタック 5",
            "text": {
                "2_B420": "贤明之蛇"
            }
        },
        "2_B430": {
            "name": "アタック 6",
            "text": {
                "2_B430": "真拿你没办法呢"
            }
        },
        "2_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "2_B440": "坠入偷窥狂的深渊吧"
            }
        },
        "2_B480": {
            "name": "スキル 3",
            "text": {
                "2_B480": "招来黑暗"
            }
        },
        "2_B490": {
            "name": "スキル 4",
            "text": {
                "2_B490": "要堕落吗？"
            }
        },
        "2_B500": {
            "name": "開始 3",
            "text": {
                "2_B500": "暗之女王，欧瓦伊之风，谷底之躯。也就是说，该我出场了？"
            }
        },
        "2_B510": {
            "name": "開始 4",
            "text": {
                "2_B510": "凝视我吧，为了成为我的俘虏。沉眠吧，为了迎接死亡。"
            }
        },
        "2_B520": {
            "name": "勝利 3",
            "text": {
                "2_B520": "（莉琳1）赢了—！ (莉琳2)太棒了！ (莉琳3)南无阿弥陀佛。 (莉莉丝)……最后一个莉琳，不许念那个了……"
            }
        },
        "2_B530": {
            "name": "勝利 4",
            "text": {
                "2_B530": "我说过的吧？我的死亡，便是你的死期，御主？"
            }
        },
        "2_B540": {
            "name": "ダメージ 4",
            "text": {
                "2_B540": "好痒啊。"
            }
        },
        "2_B550": {
            "name": "ダメージ 3",
            "text": {
                "2_B550": "撑不住了……！呃……！"
            }
        },
        "2_B560": {
            "name": "戦闘不能 3",
            "text": {
                "2_B560": "明明约好了的……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "你令我怜爱。令我无比怜爱。",
                "2_B801": "所以混入黑暗，在狭间中消失吧。",
                "2_B802": "『虚妄乃黑暗之女』",
                "2_B803": "这是永恒的堕落。"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "那是写在黑暗中的历史。",
                "2_B811": "无法传达至任何地方，亦无法归于任何所在。",
                "2_B812": "『虚妄乃黑暗之女』",
                "2_B813": "再见了，普通人。"
            }
        },
        "2_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "2_B840": "沉入黑暗之中吧。"
            }
        },
        "2_B880": {
            "name": "スキル 5",
            "text": {
                "2_B880": "莉莉丝在闹腾呢？"
            }
        },
        "2_B890": {
            "name": "スキル 6",
            "text": {
                "2_B890": "摔倒了～！"
            }
        },
        "2_B910": {
            "name": "アタック 7",
            "text": {
                "2_B910": "节哀顺变"
            }
        },
        "2_B920": {
            "name": "アタック 8",
            "text": {
                "2_B920": "振翅悄然无声"
            }
        },
        "2_B930": {
            "name": "アタック 9",
            "text": {
                "2_B930": "无情之极化！"
            }
        },
        "2_G010": {
            "name": "レベルアップ 1",
            "text": {
                "2_G010": "变强了呢，连我自己都这么觉得。"
            }
        },
        "2_G040": {
            "name": "霊基再臨 1",
            "text": {
                "2_G040": "所以说，这就是我的最终形态。我可没有刻意封印哦？只不过嘛……有些羞人吧？这样、这样巨大的翅膀。嗯，看来不成熟的我更喜欢大一些的呢。总而言之，这个状态下的我可是很强的哦？要不要立刻试试锋芒呢，御主？",
                "2_G041": "",
                "2_G042": "",
                "2_G043": "",
                "2_G044": "",
                "2_G045": "",
                "2_G046": ""
            }
        },
        "2_G050": {
            "name": "霊基再臨 2",
            "text": {
                "2_G050": "沙与风与夜。那便是我诞生的地方。除此之外的记忆皆暧昧不清，除此之外的情感皆弃之不顾。虽说作为恶灵也有相应的职责吧？将栖息在那般荒凉之地的怪物拽到阳光之下的，可是您啊？所以，这份责任就请您负起来吧？今后也请多指教了，御主……当然，至死方休。",
                "2_G051": "",
                "2_G052": "",
                "2_G053": "",
                "2_G054": "",
                "2_G055": ""
            }
        },
        "2_G110": {
            "name": "レベルアップ 2",
            "text": {
                "2_G110": "嗯，再做得更隆重一些吧。",
                "2_G111": ""
            }
        },
        "2_G120": {
            "name": "レベルアップ 3",
            "text": {
                "2_G120": "绽放吧——！",
                "2_G121": "",
                "2_G122": ""
            }
        },
        "2_H010": {
            "name": "誕生日",
            "text": {
                "2_H010": "哦～今天是你的生日啊。即便没有我的祝福，想必也有别人为你庆祝吧。虽然有些寂寞，但我由衷感到高兴。因为这是件美好的事。",
                "2_H011": "",
                "2_H012": "",
                "2_H013": ""
            }
        },
        "2_H020": {
            "name": "イベント開催中",
            "text": {
                "2_H020": "活动呢，活动？要拼尽全力地跑吗，一起？",
                "2_H021": ""
            }
        },
        "2_H030": {
            "name": "会話 1",
            "text": {
                "2_H030": "御主，该出击了哦？来，快起床洗脸打起精神准备灵子转移吧",
                "2_H031": ""
            }
        },
        "2_H040": {
            "name": "会話 2",
            "text": {
                "2_H040": "主从关系……御主，有何指示？虽然我基本上会全力拒绝自己不感兴趣的事——",
                "2_H041": "",
                "2_H042": ""
            }
        },
        "2_H050": {
            "name": "会話 3",
            "text": {
                "2_H050": "与御主的关系吗？并非主从。但说是恋人、家人也不尽然。不过，我们是托付性命的交情。哎呀，这种关系该用什么词形容才好呢？",
                "2_H051": "",
                "2_H052": "",
                "2_H053": "",
                "2_H054": ""
            }
        },
        "2_H060": {
            "name": "好きなこと",
            "text": {
                "2_H060": "喜欢的事吗？我很喜欢观察小孩子呢。不过迦勒底的小孩子，说他们是孩子又有点那个……孩子真是很美好呢，让人忍不住想培养他们成长。",
                "2_H061": "",
                "2_H062": "",
                "2_H063": ""
            }
        },
        "2_H070": {
            "name": "嫌いなこと",
            "text": {
                "2_H070": "讨厌的东西？没有哦。嗯，完全没有。",
                "2_H071": ""
            }
        },
        "2_H080": {
            "name": "聖杯について",
            "text": {
                "2_H080": "我对圣杯并没有什么愿望。所以，本来我与圣杯战争是没有缘分的……不过既然事已至此，那也没办法了。",
                "2_H081": "",
                "2_H082": ""
            }
        },
        "2_H20090": {
            "name": "絆Lv.1",
            "text": {
                "2_H20090": "明明让你变强了却不缔结羁绊，这可不行哦？",
                "2_H20091": ""
            }
        },
        "2_H20100": {
            "name": "絆Lv.2",
            "text": {
                "2_H20100": "还远远不够呢。我想要加满蛋奶酱的苹果派。还有不加糖的咖啡。嗯，当然要两人份的。",
                "2_H20101": "",
                "2_H20102": "",
                "2_H20103": ""
            }
        },
        "2_H20110": {
            "name": "絆Lv.3",
            "text": {
                "2_H20110": "是啊。能走到这一步已经算是上乘之作了。毕竟我是个刻板的存在。在这方面可不像那个年轻稚嫩的“我”呢。她只是个恶灵，而我可是与伟大之母相对而生的莉莉丝啊。",
                "2_H20111": "",
                "2_H20112": "",
                "2_H20113": ""
            }
        },
        "2_H20120": {
            "name": "絆Lv.4",
            "text": {
                "2_H20120": "啊，或许我这不成熟之人即将说出些胡言乱语，请忘掉吧。明白吗？趁现在先说清楚—— 请忘掉吧。",
                "2_H20121": "",
                "2_H20122": "",
                "2_H20123": "",
                "2_H20124": ""
            }
        },
        "2_H20130": {
            "name": "絆Lv.5",
            "text": {
                "2_H20130": "我已经说过，忘掉吧。不过既然说出口了也没办法，但我的感情并没有改变。那番话只是源于我的浅薄与独占欲。即便现在这份心情也未曾改变——当你死去时，我也会一同消亡。这才是对等的关系吧。",
                "2_H20131": "",
                "2_H20132": "",
                "2_H20133": "",
                "2_H20134": "",
                "2_H20135": "",
                "2_H20136": ""
            }
        },
        "2_H21600": {
            "name": "絆Lv.4 (奏章Ⅳクリア以降)",
            "text": {
                "2_H21600": "是问我对基列莱特的看法吗？ 哼哼哼，既然现在的我是大人，当然能接受她啦。 她是什么人，怎么样，对我来说都无所谓，无所谓。 ——不，这样不好。 如果基列莱特做错了什么，还请告诉我。 感觉这样就能愉快地度过一整天了。 你绝对不愿意吗？ 嗯——嗯—— 算了，也罢。",
                "2_H21601": "",
                "2_H21602": "",
                "2_H21603": "",
                "2_H21604": "",
                "2_H21605": "",
                "2_H21606": "",
                "2_H21607": "",
                "2_H21608": ""
            }
        },
        "2_H21800": {
            "name": "絆Lv.5 (奏章Ⅳクリア以降)",
            "text": {
                "2_H21800": "即使到了最后的战斗，基列莱特也一定能活下来。 不过，御主可能会死掉呢。 那种情况下，就让我们俩变成某种，嗯，美好的幻影来声援吧。 然后最后基列莱特就会觉醒什么爱的力量之类的把一切解决。 怎么样？ 看上去你非常不情愿呢。 是不喜欢被默认会死掉吗？ 切。",
                "2_H21801": "",
                "2_H21802": "",
                "2_H21803": "",
                "2_H21804": "",
                "2_H21805": "",
                "2_H21806": "",
                "2_H21807": ""
            }
        },
        "2_H250": {
            "name": "会話 4 男性主人公選択時",
            "text": {
                "2_H250": "母亲、丈夫、恋人。 啊，原来如此。不这样用具体的词汇来束缚关系性，就会感到不安呢。真是可怜。 啊，不，这只是我个人的感想，并没有诽谤中伤的意思。",
                "2_H251": "",
                "2_H252": "",
                "2_H253": ""
            }
        },
        "2_H270": {
            "name": "会話 5",
            "text": {
                "2_H270": "大人物登场了呢。提亚马特……啊？好像被狠狠戒备着呢。确实，我有着死亡之母的一面，嗯，嗯……不过，对人类未免太过溺爱了吧？哇，好可怕的哭脸！失礼了，真的非常抱歉！",
                "2_H271": "",
                "2_H272": "",
                "2_H273": "",
                "2_H274": ""
            }
        },
        "2_H280": {
            "name": "会話 6",
            "text": {
                "2_H280": "哦——直截了当的恶魔呢。哎呀不过你根本算不上恶魔吧？明明对毁灭痴迷到难以自拔，面对御主时却懂得克制这点，实在令人钦佩……啊，但赴死的顺序我可不会相让哦？",
                "2_H281": "",
                "2_H282": "",
                "2_H283": ""
            }
        },
        "2_H3070": {
            "name": "嫌いなこと (絆Lv.5以上+奏章Ⅳクリア以降)",
            "text": {
                "2_H3070": "我讨厌的东西是基列莱特。 啊，基列莱特原来在那里吗？ 呵呵，我早就知道了。",
                "2_H3071": "",
                "2_H3072": "",
                "2_H3073": ""
            }
        },
        "2_H5250": {
            "name": "会話 4 女性主人公選択時",
            "text": {
                "2_H5250": "母亲、妻子、恋人。 啊，原来如此。若不通过具体词汇来维系关系性，您就会感到不安呢。 真是可悲啊。 啊不，这只是我个人的感想，并没有诽谤中伤的意思。",
                "2_H5251": "",
                "2_H5252": "",
                "2_H5253": ""
            }
        }
    },
    "ChrVoice_ids": [
        "705100"
    ]
}
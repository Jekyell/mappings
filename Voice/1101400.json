        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "“魔王”的指尖！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "魔王（Erlkönig）——战斧（Halberd）！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "哼！　手斧（beil）！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "大好的，机会！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "此乃借自少名毘古那的，",
                "2_B051": "横渡溟海前来的荚之舟。",
                "2_B052": "往返常世与现世，",
                "2_B053": "撕裂破晓之刻，再次开拓稳定国家！",
                "2_B054": "天乃罗摩船！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "再见了。"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "飞吧。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "从者……必须杀掉。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "毕竟这也是“守夜人”的工作。"
            }
        },
        "2_B120": {
            "name": "胜利 1 选择男性主人公时",
            "text": {
                "2_B120": "无论什么样的胜利都可以。只要你，还活着。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "那个，话说……我希望，你能多依靠我一点。"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "我还……能行……！"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "啊啊……虽然我已经……很努力了……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "拜托了……我还，能战斗……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "没问题。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "我会试试看。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "真的吗……？"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "必杀一击！"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "我是不会手下留情的。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "我一直在等这个机会。"
            }
        },
        "2_B2440": {
            "name": "额外攻击 4",
            "text": {
                "2_B2440": "鲜红，炙热，死亡！"
            }
        },
        "2_B250": {
            "name": "宝具卡 4",
            "text": {
                "2_B250": "结束一切吧。"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "魔王（Erlkönig）——剪刀（Scissors）！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "沉重的，要来了哦！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "这个……会很痛哦！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "……哎哟，不好意思啦。"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "甜甜圈♪"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "木星。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "Reduction……set！"
            }
        },
        "2_B510": {
            "name": "开始 4",
            "text": {
                "2_B510": "此乃榛之大树。 率领“灰柳之女”，立于死者之国入口之人……"
            }
        },
        "2_B5120": {
            "name": "胜利 1 选择女性主人公时",
            "text": {
                "2_B5120": "无论什么样的胜利都可以。只要你，还活着。"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "纵使毁灭世界，也要让正义实现（Fiat eu stita et piriat mundus）。"
            }
        },
        "2_B530": {
            "name": "胜利 4",
            "text": {
                "2_B530": "航海是必须的（Navigare necesse est）。"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "遏制住……邪灵们……"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "可恶……啊（Porca miseria）……"
            }
        },
        "2_B570": {
            "name": "无法战斗 4",
            "text": {
                "2_B570": "我曾是汝，汝终将是我（Tu fui，ego eris）……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "驾驶疾驰的乃是借自少名毘古那的，",
                "2_B801": "波涛间跃动的疾风之船。",
                "2_B802": "往返常世与现世，",
                "2_B803": "以八千矛之名，祛除邪恶！",
                "2_B804": "天乃罗摩船！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "哈！　不好意思我要过去啦！"
            }
        },
        "2_B910": {
            "name": "攻击 7",
            "text": {
                "2_B910": "再见了（Auf Wiedersehen）。"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "必须变强才行。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "……嗯。",
                "2_G041": "虽然这也是泳装……但从另一个角度来说，也是我平时的穿着。",
                "2_G042": "是在临海都市“秋叶原”当“死神”时的打扮吧。",
                "2_G043": "虽说令我想起了很多稍微有些气馁，不过，也的确像我的风格吧。"
            }
        },
        "2_G050": {
            "name": "灵基再临 2 选择男性主人公时",
            "text": {
                "2_G050": "守护城市的“守夜人”——",
                "2_G051": "临海都市“秋叶原”的“死神”，竟然来到了这么遥远的地方……",
                "2_G052": "无法归还之旅就是这种感觉吗。",
                "2_G053": "呵呵。",
                "2_G054": "……若能如愿，但愿你那漫长的旅途能得到希望的引导，并抵达一个幸福的场所。"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "因为，要“等价交换”嘛。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "那个……谢谢……"
            }
        },
        "2_G5050": {
            "name": "灵基再临 2 选择女性主人公时",
            "text": {
                "2_G5050": "守护城市的“守夜人”——",
                "2_G5051": "临海都市“秋叶原”的“死神”，竟然来到了这么遥远的地方……",
                "2_G5052": "无法归还之旅就是这种感觉吗。",
                "2_G5053": "呵呵。",
                "2_G5054": "……若能如愿，但愿你那漫长的旅途能得到希望的引导，并抵达一个幸福的场所。"
            }
        },
        "2_H010": {
            "name": "生日 选择男性主人公时",
            "text": {
                "2_H010": "我听说了哦，说今天是你的生日吧？",
                "2_H011": "恭喜，但愿今天能发生什么好事。",
                "2_H012": "……真不可思议，看来当对象是你时，这种祝福的话也能毫不畏缩地说出口呢。",
                "2_H013": "呵呵呵。",
                "2_H014": "……哎？　已经发生一件好事了？",
                "2_H015": "啊……是……吗。这，谢谢了。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "好嘈杂啊……",
                "2_H021": "但如果为了知道情况而过去探听，感觉……会被狠狠揪住衣领。",
                "2_H022": "就像天岩户吧？"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "那么，外出工作吧。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "契约关系的话，还是痛快点比较好。",
                "2_H041": "这样比较轻松。",
                "2_H042": "……之所以能够这么说，肯定是因为我比较走运吧。"
            }
        },
        "2_H050": {
            "name": "对话 3 选择男性主人公时",
            "text": {
                "2_H050": "你算……哪一种啊？",
                "2_H051": "会喜欢上自己喜欢之人的存在。",
                "2_H052": "还是会喜欢上自己讨厌之人的存在。",
                "2_H053": "这或许可以说是一种自我肯定感觉的偏差——",
                "2_H054": "哎，不是指这个？",
                "2_H055": "抱，抱歉，唔啊……"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "没有必要刻意注意自己究竟喜欢什么。",
                "2_H061": "现代就是个该如何避开令自己不快的东西生活的时代。",
                "2_H062": "如果即便如此，还有无法从内心排除出去的东西……",
                "2_H063": "那就是……“愿望”吧。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西是……自己，吧。",
                "2_H071": "对不起，性格那么麻烦。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "正因为得不到所以才叫“圣杯”。",
                "2_H081": "……唔～，这是谁说的话呢……"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "啊，夹在刘海上的这个？",
                "2_H20091": "这是“礼装应用”。",
                "2_H20092": "大概是因为平时一直戴着的关系吧，",
                "2_H20093": "整个应用的功能都和灵基一体化了。",
                "2_H20094": "可以进行简单的通讯、测量，或是翻译之类的。",
                "2_H20095": "还挺方便哦。"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "从我的肉体中渗出的黑色的液体。",
                "2_H20101": "这是……“邪灵”们。",
                "2_H20102": "拟似物质化的“灵障”，是只有我才能使用的武器。",
                "2_H20103": "对从者而言尤为危险的毒、捕食者。",
                "2_H20104": "看上去一定很不舒服吧，但这也是我这存在的一部分。",
                "2_H20105": "请接受吧。"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "说得对。",
                "2_H20111": "这黑色的“枝”是魔王——被取名为“魔王（Erlkönig）之指”，是我自己。",
                "2_H20112": "为了将攻击模式化从而进行掌控，这很重要。",
                "2_H20113": "除了我经常使用的战斧halberd、手斧的beil以外……还有多节棍的flail、投网的rete之类的。",
                "2_H20114": "对普通人类来说是非常不好的东西，所以战斗中不要靠近我。"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "其实，啦……我已经意识到了。",
                "2_H20121": "为什么我会以“复仇者”的职阶现界。",
                "2_H20122": "在这个灵基下，能更轻易地控制“魔王（Erlkönig）”——",
                "2_H20123": "这应该是由于我与“冥界之神”有什么关系……吧。",
                "2_H20124": "听起来很夸张吧。无论过去还是现在，我自己肯定早就是濒临损坏的废物了。",
                "2_H20125": "偏偏是……最适合杀害英灵们的武器什么的，这种不祥的力量……",
                "2_H20126": "我压根不想要啦……"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5 选择男性主人公时",
            "text": {
                "2_H20130": "……大家其实，都很清楚哦。",
                "2_H20131": "你比迦勒底的任何英灵都要努力，内心都更强大。",
                "2_H20132": "虽然我无法疗愈仅剩一人的御主之孤独……",
                "2_H20133": "因为战斗才是我的价值。",
                "2_H20134": "不，不过……我会在暗中……为你，加油。嗯。"
            }
        },
        "2_H210": {
            "name": "对话 4",
            "text": {
                "2_H210": "呜哇……旅行者……",
                "2_H211": "竟然还能变成那种成长后的样子。",
                "2_H212": "那个……真让人不好办，让人莫名坐立不安……",
                "2_H213": "这或许是我来到迦勒底以来遇到的最大危机吧。",
                "2_H214": "……哎？　他很关照我？　就像我是个废柴的姐姐？",
                "2_H215": "哇，太傲慢了。",
                "2_H216": "不……不过，只要我可靠一些就行了吧？",
                "2_H217": "好～。"
            }
        },
        "2_H220": {
            "name": "对话 5",
            "text": {
                "2_H220": "还不认识花凛的红叶小姐……吗。",
                "2_H221": "红叶小姐的话，是扭曲了“英灵之座”的时间，呃，我自身的相关记忆也有所缺损呢。",
                "2_H222": "虽然今后或许会因某个契机想起来。",
                "2_H223": "仿佛很害怕此事发生，又仿佛很焦急地期待那一天。"
            }
        },
        "2_H230": {
            "name": "对话 6",
            "text": {
                "2_H230": "其实，我向那位武士打招呼叫「津田先生」……的时候，似乎被他打哈哈糊弄过去了。",
                "2_H231": "武士先生如是说，",
                "2_H232": "「哎呀，此身只是缘由与来历都很模糊的一骑从者，佐佐木小次郎，这样就足够了吧。",
                "2_H233": "尤其是在吾等御主的面前。」",
                "2_H234": "……这样。",
                "2_H235": "唉……我还有事想要问他呢。"
            }
        },
        "2_H240": {
            "name": "对话 7",
            "text": {
                "2_H240": "是吗……",
                "2_H241": "迦勒底的拿破仑是弓兵。",
                "2_H242": "和我认识的波拿巴是不同的从者。",
                "2_H243": "他是骑兵，身材更瘦。",
                "2_H244": "虽然容易令人混乱，但这也是从者的有趣之处吧。"
            }
        },
        "2_H25130": {
            "name": "牵绊Lv.5 选择女性主人公时",
            "text": {
                "2_H25130": "……大家其实，都很清楚哦。",
                "2_H25131": "你比迦勒底的任何英灵都要努力，内心都更强大。",
                "2_H25132": "虽然我无法疗愈仅剩一人的御主之孤独……",
                "2_H25133": "因为战斗才是我的价值。",
                "2_H25134": "不，不过……我会在暗中……为你，加油。嗯。"
            }
        },
        "2_H5010": {
            "name": "生日 选择女性主人公时",
            "text": {
                "2_H5010": "我听说了哦，说今天是你的生日吧？",
                "2_H5011": "恭喜，但愿今天能发生什么好事。",
                "2_H5012": "……真不可思议，看来当对象是你时，这种祝福的话也能毫不畏缩地说出口呢。",
                "2_H5013": "呵呵呵。",
                "2_H5014": "……哎？　已经发生一件好事了？",
                "2_H5015": "啊……是……吗。这，谢谢了。"
            }
        },
        "2_H5050": {
            "name": "对话 3 选择女性主人公时",
            "text": {
                "2_H5050": "你算……哪一种啊？",
                "2_H5051": "会喜欢上自己喜欢之人的存在。",
                "2_H5052": "还是会喜欢上自己讨厌之人的存在。",
                "2_H5053": "这或许可以说是一种自我肯定感觉的偏差——",
                "2_H5054": "哎，不是指这个？",
                "2_H5055": "抱，抱歉，唔啊……"
            }
        }
    },

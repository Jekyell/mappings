        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "丑陋……",
                "0_B801": "无论是天是地还是彼方，",
                "0_B802": "充斥此岸的尽是些丑陋蠢动的虫豸！",
                "0_B803": "——牛王反转·迅雷风烈！",
                "0_B804": "消失！　不见吧！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "呜呼……",
                "0_B811": "世间乃是极尽扭曲之地。",
                "0_B812": "尽是扭曲、歪曲、可怖的呻吟。",
                "0_B813": "丝毫不存！　所谓正确！",
                "0_B814": "那就只有破坏掉了！　破坏一切！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "牛王招来！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "这里很冷……"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "好痛，好痛……"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "哎呀呀！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {

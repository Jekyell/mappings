{
    "id": 703900,
    "collectionNo": 306,
    "name": "伽拉忒亚",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "揉黏土。咚。"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "大致雕刻轮廓。"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "大胆地击碎。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "花这种工夫是非常重要的。"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "阿佛洛狄忒单元，启动——",
                "0_B051": "女神啊，请倾听我的愿望吧。",
                "0_B052": "以吾之爱为雕像注入生命。",
                "0_B053": "雕出的乃是对重逢的祈祷。",
                "0_B054": "神凿纵横——",
                "0_B055": "雕刻理想的王之凿！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "稍微集中一下精神。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "咦……？　嗯咻。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "开始雕刻。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "第一次看到这种素材呢。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "总之算是完成了。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "成品效果……不予置评。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "要注意破损。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "这是何等……粗暴的动作……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "无法……修复……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "王啊……我……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "决定题材。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "准备道具。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "想象完成的形象。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "开始凿吧。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "喀嗒……喀嗒……"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "雕刻术全开！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "通往杰作的一凿！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "短距离制导凿，发射。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "让你们见识一下雕刻机动吧。"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "不会敲坏的……大概吧。"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "必须维持理想的形象才行。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "保养是非常重要的哦。"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "阿佛洛狄忒单元，启动——",
                "0_B801": "全系统，没有异常。",
                "0_B802": "目标捕捉。实行空中雕刻机动。",
                "0_B803": "就像塞浦路斯之风，正是现在。",
                "0_B804": "雕刻理想的王之凿。",
                "0_B805": "雕刻是爆炸。"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "收尾工作也要万无一失。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "技术的进步。 我追求的就是这个。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "看来没什么问题呢。",
                "0_G021": "为重视轻巧地挥舞凿子这个优点，我会继续保持这样子一段时间。",
                "0_G022": "但相反也要小心这样容易发生事故。",
                "0_G023": "当然，也包括您。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "实在是有点热，所以把工作服脱了。",
                "0_G031": "雕刻工作伴随着危险，原本理应继续穿着才对……"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "只要有了这些资源，应该也能运行包含了阿佛洛狄忒大人力量的这个特殊器材了吧。",
                "0_G041": "我认为这是要求不计资源消耗，努力投身作品创作的指令。",
                "0_G042": "交给我吧。",
                "0_G043": "在御主破产之前，我的手是不会停止的。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "靠神明都承认的技巧与真实之爱，将我变成了人类的皮格马利翁王……",
                "0_G051": "我仍未达到王的高度。",
                "0_G052": "但是……应该确实在逐渐接近吧。",
                "0_G053": "我要向您表示感谢，御主。",
                "0_G054": "这都是多亏了您给与了尚不成熟的我锻炼的机会。",
                "0_G055": "也是多亏了您没有否定我这自不量力的愿望。",
                "0_G056": "为了回应您的这份恩情……我也一定会达到目的。",
                "0_G057": "要与那位大人重逢。",
                "0_G058": "这次轮到我用自己真实的爱了。",
                "0_G059": "……还请您仔细见证，御主。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "您愿意帮我打磨吗？ ……多谢。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "只要觉得还不满意就继续修整。 这是理所当然的。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生命诞生之日。",
                "0_H011": "有着非常重要的意义。",
                "0_H012": "……祝贺您，御主。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举办祭典呢。",
                "0_H021": "如果您需要供奉神明用的雕像，还请随时吩咐。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "就算一直待在工作室，完成也不会主动靠近。",
                "0_H031": "外出寻找灵感也是非常重要的哦，御主。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我不是很了解主从关系。",
                "0_H041": "虽然我以前就在王的身边，但并不是作为侍从……",
                "0_H042": "因为，是作为……妻子。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主和我多半算……",
                "0_H051": "应该算追求同样爱的同志。",
                "0_H052": "……我说得没错吧？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "第一喜欢的当然是皮格马利翁王。",
                "0_H062": "这是不可动摇的，就像不可动摇的雕像一样。",
                "0_H063": "……是的，刚才当然是在开玩笑，有什么问题吗？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我讨厌粗暴对待物品的人。",
                "0_H071": "只因为对象不是生物就无妨，粗暴地乱扔乱踢……",
                "0_H072": "这种人不可信。",
                "0_H073": "克制地说他们算是畜生不如的垃圾，所以我不会用什么罪该万死的表现方式。",
                "0_H074": "真麻烦。",
                "0_H075": "……不，表现姑且不论，我会打骂他们到死。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我的愿望只有一个。",
                "0_H081": "但是……我不知道这个愿望是否是圣杯能够实现的。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "现在的我既是雕像，也是人类。",
                "0_H091": "您可以根据情况选择对待我的方式。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "石头、木头、青铜，",
                "0_H101": "……素材不论。",
                "0_H102": "希望御主能给我看您最喜欢的雕像。",
                "0_H103": "我会根据您如何爱护那座雕像，来评估您的为人。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "御主的国家里似乎有名为「付丧神」的概念吧。",
                "0_H111": "情感寄宿在器物中，能凭自己的意志行动……",
                "0_H112": "非常有意思。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "虽说存在明确的最终目标，但除此以外我没有什么偏好的题材。",
                "0_H121": "甚至可以说全都是习作。",
                "0_H122": "只要您提需求的话，我甚至可以制作御主喜好的东西……",
                "0_H123": "但我是否能理解并反馈您真实的爱，就是个未知数了。"
            }
        },
        "0_H1200": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H1200": "我是在皮格马利翁王的爱与阿佛洛狄忒大人的佑护下诞生的存在。",
                "0_H1201": "现在我的装束和装备也非常鲜明地反映了这种佑护吧。",
                "0_H1202": "虽然我当然没见过阿佛洛狄忒大人，但我发自心底里地感谢着她。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我明白了。",
                "0_H131": "既然我们如此投机，说明您的方向性和我们果然是一样的。",
                "0_H132": "我可以向您保证。",
                "0_H133": "就算道路再怎么漫长，您也一定能找到您真实的爱。",
                "0_H134": "没错，总有一天您一定也能邂逅。",
                "0_H135": "即便起初是没有生命之物，您也迟早能遇到作为人的那位。",
                "0_H136": "……我也很期待有一天能向那位问候。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "那种真挚的爱。",
                "0_H171": "黑胡子大人是个非常有前途的男人。",
                "0_H172": "……不，但和皮格马利翁王完全不是一种类型。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "啊啊……各位是接受了多么美妙的爱才会诞生的呢。",
                "0_H181": "有机会的话，大家务必聚集在一起，互相交流彼此的爱吧。",
                "0_H182": "没错……这就是恋爱的话题吧。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "科尔基斯的公主。",
                "0_H191": "我认为她也是位实力非常高超造型师。",
                "0_H192": "如果能将心灵沉浸于爱这种感情，而不是赏玩的话，一定能更上一层楼……"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "阿耳忒弥斯大人经常凝视着我的脸看。",
                "0_H201": "虽然不会感到不愉快，但好像……并不是对我的造型有什么不满……",
                "0_H202": "真神秘。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "真奇妙……",
                "0_H211": "罗马这个国家的人……",
                "0_H212": "一看到他们，就会有种想要欣然微笑的心情。",
                "0_H213": "是类似见到……城内孩子们的感觉吗……？"
            }
        },
        "0_N010": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 1",
            "text": {
                "0_N010": "您要去哪里？",
                "0_N011": "我会为您选择可以回复气力与体力的地点。"
            }
        },
        "0_N020": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 2",
            "text": {
                "0_N020": "雕刻不是一朝一夕可以完成的。",
                "0_N021": "疲劳的时候休息也是非常重要的。"
            }
        },
        "0_N030": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 3",
            "text": {
                "0_N030": "啊啊，这翻动的裙子与头发的跃动感……",
                "0_N031": "啊，失礼了。",
                "0_N032": "因为那家店摆着一个非常卓越的手办，所以我没忍住……"
            }
        },
        "0_N040": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 4",
            "text": {
                "0_N040": "无论是左看还是右看，到处都是能得到新灵感的场所呢。",
                "0_N041": "从这个角度来说，这条街道实在是太棒了。"
            }
        },
        "0_N050": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 5",
            "text": {
                "0_N050": "如果您烦恼该如何休息，我建议您再看一次帮助说明。",
                "0_N051": "这就类似雕刻的设计图。",
                "0_N052": "……虽然可能不太一样。"
            }
        },
        "0_N060": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 6",
            "text": {
                "0_N060": "辛苦了。这条街道已经没有什么可担心的了。",
                "0_N061": "您可以尽情地在这里享受。"
            }
        },
        "0_N070": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 7",
            "text": {
                "0_N070": "哎呀，您是指您还没玩够吗。",
                "0_N071": "您倒是意外具备孩子气的一面呢。",
                "0_N072": "……呵呵。我觉得这样很好。",
                "0_N073": "因为雕刻也需要这种好玩的心态。"
            }
        },
        "0_N090": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 8",
            "text": {
                "0_N090": "欢迎光临。",
                "0_N091": "目前进度差不多是这样。"
            }
        },
        "0_N100": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 9",
            "text": {
                "0_N100": "是检查雕凿进度如何吧。",
                "0_N101": "嗯，请确认吧。"
            }
        },
        "0_N110": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 10",
            "text": {
                "0_N110": "踏踏实实地推进吧。"
            }
        },
        "0_N140": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 11",
            "text": {
                "0_N140": "对于这段旅程，您想了解什么吗？",
                "0_N141": "那就请去看帮助说明吧。",
                "0_N142": "这就类似雕刻的设计图。",
                "0_N143": "……肯定是这样。"
            }
        },
        "0_N150": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 12",
            "text": {
                "0_N150": "虽然已经无法再进行修改了……",
                "0_N151": "但回顾自己的作品也会是一种学习。",
                "0_N152": "请吧。",
                "0_N153": "……要为您泡茶吗？"
            }
        },
        "0_P010": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 13",
            "text": {
                "0_P010": "技术要退步了……",
                "0_P011": "看来不仅是雕像，人休息过多也会发生这种现象。",
                "0_P012": "还请注意。"
            }
        },
        "0_P020": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 14",
            "text": {
                "0_P020": "好像有人沉迷于玩耍呢。",
                "0_P021": "那就在那个人的身边研磨凿子，让那个人意识到有工作要做吧。"
            }
        },
        "0_P030": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 15",
            "text": {
                "0_P030": "我也经常会废寝忘食地挥舞凿子。",
                "0_P031": "当有其他需要做的事时，得注意精神不能过于集中才行。"
            }
        },
        "0_P070": {
            "name": "秋叶原·爆裂！\n～祈愿之街与铭刻了爱的雕像们～ 16",
            "text": {

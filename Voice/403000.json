{
    "id": 403000,
    "collectionNo": 211,
    "name": "坂本龙马",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "上！ 明白！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "嘿！ 再来一发！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "上吧！",
                "0_B041": "这样……",
                "0_B042": "就结束啦！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "为天逆鉾厌恶之国津大蛇，",
                "0_B051": "吾欲所成之事仅为吾所知。",
                "0_B052": "如……翱翔天际之龙！！！",
                "0_B053": "噢噢噢噢噢噢噢噢！！！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "嘿嘿，你是不是吓得瑟瑟发抖了呢～。 不不，别挑衅人家啦。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "我能吃了那家伙吗？ 不行啦。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "好了，那我们走吧，阿龙小姐！ 包在我身上吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "好好干哦，龙马。 知道啦知道啦，我就努力一下吧。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "哎呀，真危险呀。 阿龙小姐我还很从容哦。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "赢了！　阿龙小姐，完！ 不不，不能完吧。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "哈哈……看来这还真不好办呢……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "对不起，我已经动不了了……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "交给我吧～。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "嗯，上吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "一并打飞。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "阿龙小姐，轮到你出场啦！ 明白了，我要动真格了。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "我要拔出宝具了，阿龙小姐，能行吗？"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "——吾乃非归顺之神，高千穗的大蛇。"
            }
        },
        "0_B380": {
            "name": "指令卡 4",
            "text": {
                "0_B380": "这倒是个好主意。"
            }
        },
        "0_B390": {
            "name": "指令卡 5",
            "text": {
                "0_B390": "那我们就上吧。"
            }
        },
        "0_B400": {
            "name": "指令卡 6",
            "text": {
                "0_B400": "去大干一场～。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "就是现在！ 好好，我知道啦！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "阿龙小姐，配合我！",
                "0_B441": "交给我吧，",
                "0_B442": "哈！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "阿龙小姐我真正的力量觉醒啦。 那真是谢天谢地啰。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "快放马过来吧～。 可能的话我倒是希望你能自己逃跑。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "好嘞，阿龙小姐我就稍微认真一点好啦。 哎哟，这样很危险，大家快离远一点哦。"
            }
        },
        "0_B530": {
            "name": "胜利 3",
            "text": {
                "0_B530": "不用介意，只是阿龙小姐我比较强而已。 都说了让你别挑衅别人啊……"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "这没用。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "……已经……不行了。"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "又没能……保护好……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "耶～，和平和平。",
                "0_B801": "不不，说真的，拜托你了啦……",
                "0_B802": "交给我吧，阿龙小姐大变化！",
                "0_B803": "如……翱翔天际之龙！！！",
                "0_B804": "不能吃哦……啊，吃掉了。"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "这就是我的宝具……",
                "0_B811": "上吧，阿龙小姐！",
                "0_B812": "嗯……上吧！",
                "0_B813": "如……翱翔天际之龙！！！",
                "0_B814": "到此……结束吧！！！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "阿龙小姐等级提升～。",
                "0_G011": "龙马也顺便一起提升。",
                "0_G012": "就算是顺便的也很谢天谢地了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "阿龙小姐的防御力又提升啦。",
                "0_G021": "你居然还能变硬吗？"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "喂，龙马，你不换衣服吗，换衣服。",
                "0_G031": "我还挺喜欢这套的。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "啊啊，和服吗。",
                "0_G041": "虽然我有，但裤子比较方便行动嘛。",
                "0_G042": "怎么感觉是在找借口。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "这就是日本的黎明，不，应该叫人理的黎明吧。",
                "0_G051": "没想到我居然能走到这一步，谢谢。",
                "0_G052": "这都多亏了御主。",
                "0_G053": "阿龙小姐我也对此感激涕零。",
                "0_G054": "今天我心情不错，就让你坐到我背上来吧。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "哎，你的生日啊。",
                "0_H011": "值得庆祝，恭喜。",
                "0_H012": "机会难得，应该物色一些礼物……",
                "0_H013": "这里有阿龙小姐最宝贝的青蛙哦。",
                "0_H014": "我倒是希望你能把那个收起来。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "耶～，办祭典啦办祭典啦。",
                "0_H021": "只有现在才能做的事就该现在去做。",
                "0_H022": "否则可就后悔莫阿龙小姐啦。",
                "0_H023": "虽然最后那句话很奇怪，但确实没错。",
                "0_H024": "那我们走吧。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "喂，现在好闲，干脆去找点青蛙来吧。",
                "0_H031": "不不，明明还有其他该干的事吧？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "主从啊……",
                "0_H041": "我不是很喜欢这种关系。",
                "0_H042": "我想和你成为好朋友。",
                "0_H043": "阿龙小姐也是，阿龙小姐也是。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "主从的关系姑且不论，当然，我会全力协助身为御主的你。",
                "0_H051": "毕竟我就是为此而来的。",
                "0_H052": "没错，只要有阿龙小姐我在，就如同拥有百万马力。",
                "0_H053": "不对，应该叫百万阿龙小姐力吗？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "这个嘛，比如鱼的刺身……",
                "0_H062": "酒也喜欢。",
                "0_H063": "哎？　军鸡火锅吗？",
                "0_H064": "我先不论，阿龙小姐不太喜欢哦。",
                "0_H065": "不要再聊这个话题了，好啦，结束结束。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西啊……",
                "0_H071": "让我想想……应该讨厌或大或小的悲伤之事吧。",
                "0_H072": "正因为讨厌，才会为之四处奔走。",
                "0_H073": "阿龙小姐没有什么好恶哦，但我喜欢青蛙。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯吗……",
                "0_H081": "我的话，你想，我和其他人情况不太一样，所以对愿望什么的……",
                "0_H082": "一定要说的话，对，应该是想要个大家都能幸福生活的世间吧。",
                "0_H083": "阿龙小姐有很多愿望哦。",
                "0_H084": "比如那种啊，这种啊，还有青蛙和，那个……",
                "0_H085": "嗯，那种事你还是自己去实现吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "喂，我说你，别乱碰阿龙小姐。",
                "0_H091": "小心我吃了你哦。",
                "0_H092": "对不起哦，别看她这样，其实阿龙小姐很怕生……不，是怕见陌生人类。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "啊啊，是你啊。",
                "0_H101": "又来找阿龙小姐？　哎？　找我？",
                "0_H102": "这样啊，那我们好好聊聊吧。",
                "0_H103": "哎？　……啊啊，这件事啊。",
                "0_H104": "土佐方言很容易暴露真名，所以我平时不太用。",
                "0_H105": "喂，带上阿龙小姐一起聊啦。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "喂，阿龙小姐我来说，龙马你去拿茶来。",
                "0_H111": "哎呀呀，那个阿龙小姐居然会……",
                "0_H112": "然后哦，这时候阿龙小姐把那个不知道是不是山神的玩意儿打飞后，上面突然下来了个奇怪的家伙。",
                "0_H113": "……那接下来就拜托你啦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "哎？　关于我死去的事？",
                "0_H121": "啊啊，那个啊……其实……",
                "0_H122": "喂，别说了，这个话题对阿龙小姐我打击很大。",
                "0_H123": "啊啊，抱歉抱歉。",
                "0_H124": "其实阿龙小姐当时外出购物了，所以没能保护我，她一直在为此而后悔。",
                "0_H125": "所以这件事下次再说吧。",
                "0_H126": "放心吧，这次我会连你和龙马一并保护好。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "该怎么说呢，你有种不可思议的魅力。",
                "0_H131": "虽然别人也对我说过类似的话，但这和你不能相提并论。",
                "0_H132": "因为整个世界的英灵都聚集在你的身边，并肩战斗。",
                "0_H133": "这可不是一般同盟的程度。",
                "0_H134": "简直就像是连接了整个世界。",
                "0_H135": "然而你却丝毫不为此感到骄傲，而是普通地面对。",
                "0_H136": "我希望你能好好珍视自己的这个特点。",
                "0_H137": "这一定会成为你重要的存在。",
                "0_H138": "龙马，你的话太长了啦。"
            }
        },
        "0_H160": {
            "name": "对话 4",
            "text": {
                "0_H160": "哟，这不是冲田嘛。",
                "0_H161": "哎呀，那位冲田居然加入了我方，真是太可靠了。",
                "0_H162": "但这家伙看上去好弱啊。",
                "0_H163": "不，我认真的，还是别说这种话啦。"
            }
        },
        "0_H170": {
            "name": "对话 5",
            "text": {
                "0_H170": "又见面了呢，第六天魔王，织田信长公。",
                "0_H171": "上次承蒙你的……呃，怎么了，阿龙小姐？",
                "0_H172": "喂，这家伙是不是在作弊。"
            }
        },
        "0_H180": {
            "name": "对话 6",
            "text": {
                "0_H180": "没想到我居然能与鬼副长一起战斗。",
                "0_H181": "原本我也打算去劝诱新选组，",
                "0_H182": "但这个愿望居然会在这种意料之外的地方实现。",
                "0_H183": "喂，那个腌萝卜好像很好吃呢。",
                "0_H184": "给我一个。"
            }
        },
        "0_H190": {
            "name": "对话 7",
            "text": {
                "0_H190": "好久不见了啊，以藏先生……",
                "0_H191": "稍微等一下，稍微～等一下！",
                "0_H192": "能否请你放下刀呢？",
                "0_H193": "……嗯，在这里的期间，我希望我们能像以前一样和睦相处。",
                "0_H194": "我能打这家伙吗？"
            }
        },
        "0_H200": {
            "name": "对话 8",
            "text": {
                "0_H200": "……这样啊，或许是因为你还有必须去完成的事吧。",
                "0_H201": "那我也会在一旁见证的。",
                "0_H202": "虽然和这个话题没什么关系，但你的刀是不是太大了？",
                "0_H203": "可是会卡在门口的啊。"
            }
        },
        "0_H210": {
            "name": "对话 9",
            "text": {
                "0_H210": "德雷克船长……",
                "0_H211": "是完成了周游世界一周的大海盗吧。",
                "0_H212": "哎呀，我最喜欢这种了。",
                "0_H213": "下次能不能让我上您的船？",
                "0_H214": "因为龙马最喜欢异国船舶了嘛。"
            }
        },
        "0_I010": {
            "name": "唠唠叨叨帝都圣杯奇谭 1",
            "text": {
                "0_I010": "耶～。",
                "0_I011": "你来得正好，人类，这里是阿龙小姐的商店。",
                "0_I012": "阿龙小姐我是代替挣不到钱的龙马外出赚钱的能干的女人哦。",
                "0_I013": "行吧行吧，挣不到钱是我的错好了吧。"
            }
        },
        "0_I020": {
            "name": "唠唠叨叨帝都圣杯奇谭 2",
            "text": {
                "0_I020": "这里是坂本侦探事务所的一角，俗称阿龙屋。",
                "0_I021": "来吧，快买，给钱，让我吃掉。",
                "0_I022": "不，不能吃吧……",
                "0_I023": "啊，如果有能帮上忙的东西，就尽管拿回去吧。"
            }
        },
        "0_I030": {
            "name": "唠唠叨叨帝都圣杯奇谭 3",
            "text": {
                "0_I030": "你眼光真好呢，那也是阿龙小姐我喜欢的东西。",
                "0_I031": "我就破例分给你好啦。",
                "0_I032": "哎？　你在吃那东西吗！？"
            }
        },
        "0_I040": {
            "name": "唠唠叨叨帝都圣杯奇谭 4",
            "text": {
                "0_I040": "虽然要求报酬不太好，但现在也管不了那么多了。",
                "0_I041": "我会给你算便宜点的，那拜托你啦。",
                "0_I042": "连青蛙都买不起的生活还真凄惨。"
            }
        },
        "0_I050": {
            "name": "唠唠叨叨帝都圣杯奇谭 5",
            "text": {
                "0_I050": "啊，这个啊。",
                "0_I051": "给，尽管拿去吧。",
                "0_I052": "这里反倒比侦探更生意兴隆。",
                "0_I053": "你是不是更擅长做生意啊？"
            }
        },
        "0_I060": {
            "name": "唠唠叨叨帝都圣杯奇谭 6",
            "text": {
                "0_I060": "你身为一个人类还挺努力的呢，真了不起。",
                "0_I061": "阿龙小姐的店虽然已经关门了，但你千万别忘了交换什么东西哦。",
                "0_I062": "阿龙小姐居然会夸奖人，太令人惊讶了。",
                "0_I063": "哈哈，看来她相当喜欢你呢。"
            }
        },
        "0_I070": {
            "name": "唠唠叨叨帝都圣杯奇谭 7",
            "text": {
                "0_I070": "差不多也要和这个事务所告别了，你也别忘了什么东西哦。",
                "0_I071": "尽管不存在没有遗憾的人生，但比起因不做而后悔，还是做了之后再后悔比较好。",
                "0_I072": "永别了，我心爱的帝都啊。",
                "0_I073": "下一个地方……有吗？"
            }
        },
        "0_M1010": {
            "name": "唠唠叨叨龙马千钧一发！！ 8",
            "text": {
                "0_M1010": "嗯，就按照这个状态，拜托了哦。",
                "0_M1011": "要努力寻找更好的东西哦，人类。"
            }
        },
        "0_M1020": {
            "name": "唠唠叨叨龙马千钧一发！！ 9",
            "text": {
                "0_M1020": "哦！　找到好东西了呢，人类。",
                "0_M1021": "这可非常难得一见哦！",
                "0_M1022": "这还真是厉害呢！",
                "0_M1023": "你该不会有干这份活儿的才能吧？"
            }
        },
        "0_M1030": {
            "name": "唠唠叨叨龙马千钧一发！！ 10",
            "text": {
                "0_M1030": "是新的委托哦。多多完成、多多攒钱、多多购买青蛙吧。",
                "0_M1031": "青蛙暂且不论，我觉得为了想要的东西或是想做的事而努力是好事哦。"
            }
        },
        "0_M1040": {
            "name": "唠唠叨叨龙马千钧一发！！ 11",
            "text": {
                "0_M1040": "好像来新委托了哦，让我们赶快确认委托内容吧。 如果是能够收获大量青蛙的划算委托就好了呢！ 要不干脆提个搜集青蛙的委托吧？"
            }
        },
        "0_N1010": {
            "name": "唠唠叨叨龙马千钧一发！！ 1",
            "text": {
                "0_N1010": "这里是新·坂本侦探事务所。",
                "0_N1011": "阿龙小姐会狠狠锻炼见习海援队的你，觉悟吧？",
                "0_N1012": "哈哈，记得手下留情哦。",
                "0_N1013": "先不谈这个问题，我也忙得没法顾及一些细节的工作，你能来帮手真是太好了。"
            }
        },
        "0_N1020": {
            "name": "唠唠叨叨龙马千钧一发！！ 2",
            "text": {
                "0_N1020": "说起侦探事务所，资金周转还是很辛苦的……",
                "0_N1021": "都因为这样，现在都成了什么都干屋了。干脆改名为新·阿龙屋吧。"
            }
        },
        "0_N1030": {
            "name": "唠唠叨叨龙马千钧一发！！ 3",
            "text": {
                "0_N1030": "啊啊……好闲啊，闲得阿龙小姐快要融化了啦……",
                "0_N1031": "快去找点委托来啦，龙马。",
                "0_N1032": "知道啦知道啦。那我们出门吧。"
            }
        },
        "0_N1040": {
            "name": "唠唠叨叨龙马千钧一发！！ 4",
            "text": {
                "0_N1040": "好了，虽然有很多委托，但从能做到的开始吧。",
                "0_N1041": "从小事开始踏踏实实做的阿龙小姐呢！"
            }
        },
        "0_N1050": {
            "name": "唠唠叨叨龙马千钧一发！！ 5",
            "text": {
                "0_N1050": "要认真确认通知哦？",
                "0_N1051": "如果背着阿龙小姐去登山，可是会出大事的哦。",
                "0_N1052": "这个和通知没什么关系吧。"
            }
        },
        "0_N1060": {
            "name": "唠唠叨叨龙马千钧一发！！ 6",
            "text": {
                "0_N1060": "这次真是各种承蒙你的关照呢，人类。",
                "0_N1061": "连阿龙小姐我都要坦率地表示感谢哦。",
                "0_N1062": "我也要向您道谢。",
                "0_N1063": "御主，真是太感谢了。"
            }
        },
        "0_N1070": {
            "name": "唠唠叨叨龙马千钧一发！！ 7",
            "text": {
                "0_N1070": "虽然发生了不少事，但终于要与这座城市告别了呢。",
                "0_N1071": "好了，那我们走吧，御主！",
                "0_N1072": "嗯，去往大海的彼岸。",
                "0_N1073": "以及更前方的地方——遥远的彼方！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "Rider，坂本龙马。",
                "0_S011": "回应召唤而来。",
                "0_S012": "啊，这是我搭档阿龙小姐。",
                "0_S013": "我们俩就请你多关照了哦。",
                "0_S014": "请多指教，人类。",
                "0_S015": "……话说你看上去很好吃呢。",
                "0_S016": "不不，不行啦……"
            }
        },
        "0_S020": {
            "name": "期间限定加入",
            "text": {
                "0_S020": "我是Rider，坂本龙马。",
                "0_S021": "在这帝都经营一家不起眼的侦探事务所。",
                "0_S022": "那这段时间内就请你多关照啦。",
                "0_S023": "啊，对了对了，如果你能帮手事务所的工作，我会很高兴的。",
                "0_S024": "阿龙小姐我是助手阿龙小姐。",
                "0_S025": "工作固然很辛苦，但只要你努力，我会送你青蛙哦。"
            }
        }
    },

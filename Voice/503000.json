{
    "id": 503000,
    "collectionNo": 169,
    "name": "山鲁佐德",
    "voices": {
        "0_B010": {
            "name": "攻击 1(真名确认后)",
            "text": {
                "0_B010": "就像这样。"
            }
        },
        "0_B020": {
            "name": "攻击 2(真名确认后)",
            "text": {
                "0_B020": "您应该知道。"
            }
        },
        "0_B030": {
            "name": "攻击 3(真名确认后)",
            "text": {
                "0_B030": "这是高潮。"
            }
        },
        "0_B040": {
            "name": "额外攻击(真名确认后)",
            "text": {
                "0_B040": "夜晚还将继续……"
            }
        },
        "0_B050": {
            "name": "宝具(真名确认后)",
            "text": {
                "0_B050": "追求之物乃是下一夜。",
                "0_B051": "然后再次……追求下一夜。",
                "0_B052": "这是由我的言辞编织而成，永不终结的愿望故事——",
                "0_B053": "——一千零一夜。",
                "0_B054": "今晚——就说到这里。"
            }
        },
        "0_B080": {
            "name": "技能 1(真名确认后)",
            "text": {
                "0_B080": "您大可……安心入眠哦。"
            }
        },
        "0_B090": {
            "name": "技能 2(真名确认后)",
            "text": {
                "0_B090": "让我们……稍作休息吧。"
            }
        },
        "0_B100": {
            "name": "开始 1(真名确认后)",
            "text": {
                "0_B100": "战斗……是可怕的……"
            }
        },
        "0_B110": {
            "name": "开始 2(真名确认后)",
            "text": {
                "0_B110": "如果能回避，真希望避免……"
            }
        },
        "0_B120": {
            "name": "胜利 1(真名确认后)",
            "text": {
                "0_B120": "还好不用死了……"
            }
        },
        "0_B130": {
            "name": "胜利 2(真名确认后)",
            "text": {
                "0_B130": "……结束了。"
            }
        },
        "0_B140": {
            "name": "受击 2(真名确认后)",
            "text": {
                "0_B140": "请住手……"
            }
        },
        "0_B150": {
            "name": "受击 1(真名确认后)",
            "text": {
                "0_B150": "会死的……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1(真名确认后)",
            "text": {
                "0_B160": "不要……不要……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2(真名确认后)",
            "text": {
                "0_B170": "我明明已经……不想再有这种经历了……"
            }
        },
        "0_B180": {
            "name": "指令卡 1(真名确认后)",
            "text": {
                "0_B180": "敬请期待。"
            }
        },
        "0_B190": {
            "name": "指令卡 2(真名确认后)",
            "text": {
                "0_B190": "下次说这个故事。"
            }
        },
        "0_B200": {
            "name": "指令卡 3(真名确认后)",
            "text": {
                "0_B200": "有碍您尊耳了……"
            }
        },
        "0_B210": {
            "name": "宝具卡(真名确认后)",
            "text": {
                "0_B210": "您希望听什么故事呢？"
            }
        },
        "0_B480": {
            "name": "技能 3(真名确认后)",
            "text": {
                "0_B480": "在很久……很久以前……"
            }
        },
        "0_G010": {
            "name": "升级(真名确认后)",
            "text": {
                "0_G010": "离死亡远去了……？"
            }
        },
        "0_G020": {
            "name": "灵基再临 2(真名确认后)",
            "text": {
                "0_G020": "麻烦了。我不是很希望引人瞩目……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1(真名确认后)",
            "text": {
                "0_G030": "哎呀……为什么对我这样的人……",
                "0_G031": "这……是不是浪费……？"
            }
        },
        "0_G040": {
            "name": "灵基再临 3(真名确认后)",
            "text": {
                "0_G040": "如此豪华的服饰……不胜惶恐。",
                "0_G041": "唉。这该不会是……让我带去黄泉的礼物吧……？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4(真名确认后)",
            "text": {
                "0_G050": "睡不着吗，吾王？",
                "0_G051": "若您允许我说一个故事代替摇篮曲，我应该能帮上您的忙……"
            }
        },
        "0_H010": {
            "name": "生日(真名确认后)",
            "text": {
                "0_H010": "今天是您的生日吗？",
                "0_H011": "恭喜您。",
                "0_H012": "若再经过365个夜晚后，还能说这句话就好了……",
                "0_H013": "前提是，到时候如果我还在的话。"
            }
        },
        "0_H020": {
            "name": "活动举行中(真名确认后)",
            "text": {
                "0_H020": "节日中容易出现意外致死的情况。",
                "0_H021": "请务必……多加小心……"
            }
        },
        "0_H030": {
            "name": "对话 1(真名确认后)",
            "text": {
                "0_H030": "若老是待在房间里，说不定……反而很容易死哦……？"
            }
        },
        "0_H040": {
            "name": "对话 2(真名确认后)",
            "text": {
                "0_H040": "只要您……不无谓地毁掉我这条生命——",
                "0_H041": "您对我来说，就是一位好王哦，御主。"
            }
        },
        "0_H050": {
            "name": "对话 3(真名确认后)",
            "text": {
                "0_H050": "将我逼入绝路的就是坏王，而好好珍视我的就是好王。",
                "0_H051": "请务必不要忘记……"
            }
        },
        "0_H060": {
            "name": "喜欢的东西(真名确认后)",
            "text": {
                "0_H060": "我喜欢安全与安心……",
                "0_H061": "您有没有建造避难所的预定？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西(真名确认后)",
            "text": {
                "0_H070": "讨厌的东西，当然是——死去。",
                "0_H071": "这是理所当然的吧……？"
            }
        },
        "0_H080": {
            "name": "关于圣杯(真名确认后)",
            "text": {
                "0_H080": "至少，我不想参加所谓的……圣杯战争。",
                "0_H081": "从被召唤的那刻起，对我来说就不是什么值得高兴的事态了……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1(真名确认后)",
            "text": {
                "0_H090": "请不要杀我。",
                "0_H091": "我的愿望，仅此而已……"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2(真名确认后)",
            "text": {
                "0_H100": "日本有种名为下跪的动作吧。",
                "0_H101": "究极的降服。我觉得这种能让人共通理解的求饶形式真是太美妙了。"
            }
        },
        "0_H10120": {
            "name": "牵绊Lv.4(真名确认后)",
            "text": {
                "0_H10120": "就算我想报答与您邂逅的这份幸运——",
                "0_H10121": "我会做的也只有说故事。",
                "0_H10122": "您想听什么样的故事？",
                "0_H10123": "开心的故事，悲伤的故事，兴奋的故事……",
                "0_H10124": "恐惧死亡的从者，与用温柔包容她的王的故事……"
            }
        },
        "0_H10130": {
            "name": "牵绊Lv.5(真名确认后)",
            "text": {
                "0_H10130": "迄今为止……从没有像您这样珍视我的王。",
                "0_H10131": "所以……我就……更不想死了。",
                "0_H10132": "因为……如果在这里死去。",
                "0_H10133": "又要……与您分别了……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3(真名确认后)",
            "text": {
                "0_H110": "不规划强人所难的作战。也不会为了自保而将从者逼入绝境……",
                "0_H111": "您的普通，对我来说，极为……温暖……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "您为何要为只会恐惧死亡的我做到这个份上。",
                "0_H121": "倘若像您这样的人不是临时的御主，而是真正的王，该有多么……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "不让我死去的您这份心意已经充分传达给我了。",
                "0_H131": "今夜，我将会为您讲述——为回应您这心意所准备的『故事』——"
            }
        },
        "0_H140": {
            "name": "对话 4(真名确认后)",
            "text": {
                "0_H140": "那是真正的阿拉什·卡曼戈……",
                "0_H141": "嗯，当然，我知道他的故事。"
            }
        },
        "0_H150": {
            "name": "对话 5(真名确认后)",
            "text": {
                "0_H150": "啊啊……这里有我不擅长应对的那种类型的王呢。",
                "0_H151": "真希望……我不要被杀……"
            }
        },
        "0_H170": {
            "name": "对话 6(真名确认后)",
            "text": {
                "0_H170": "她是个认真而努力，总在思考自己不足之处的……优秀的王哦。",
                "0_H171": "嗯，有时我也觉得她如果能放松一点会更好吧……哎？",
                "0_H172": "……呵呵，说得对。",
                "0_H173": "如果我的故事能帮上她的忙，我自然愿意尽力而为。"
            }
        },
        "0_H180": {
            "name": "对话 7(真名确认后)",
            "text": {
                "0_H180": "毕竟我从未想过靠故事来赚钱……是啊。",
                "0_H181": "像她那样一个劲地游说，确实会令我有些不知所措呢……"
            }
        },
        "0_M610": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 1",
            "text": {
                "0_M610": "话说……速度……是不是太快了点……？ 你在说什么呢，这速度还远不止于此呢！"
            }
        },
        "0_M620": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 2",
            "text": {
                "0_M620": "现在让我们脚踏实地地前进吧！ 『欲速则不达』……真是一句名言。"
            }
        },
        "0_M630": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 3",
            "text": {
                "0_M630": "状态不错！我们的驰骋没有任何后顾之忧！ 打滑、翻车、爆炸四散…… 啊啊，应该从哪件事开始应对呢……"
            }
        },
        "0_M640": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 4",
            "text": {
                "0_M640": "蠢动，蠢动，蠢动…… （……她是那种会沉迷于简单工作的那种类型呢……）"
            }
        },
        "0_M650": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 5",
            "text": {
                "0_M650": "进展不错，继续往前挖吧！ 一定要做好防塌方的预防工作……"
            }
        },
        "0_M660": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 6",
            "text": {
                "0_M660": "开始有些没积极性了呢。 也就是说……你应该明白我想说什么吧？ 嗯。那么，为了放松心情，我来说一个故事吧。"
            }
        },
        "0_M670": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 7",
            "text": {
                "0_M670": "我听到了，称颂法老的民众之声！",
                "0_M671": "虽说获胜是理所当然的，但这份心意很好！",
                "0_M672": "理应获胜之人获胜，同时也带给人们喜悦的话。",
                "0_M673": "嗯嗯——这就是所谓的『王道故事』吧……"
            }
        },
        "0_M680": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 8",
            "text": {
                "0_M680": "看到了吧！",
                "0_M681": "法老的传说又新添了一份荣耀！",
                "0_M682": "若能如愿，但愿这能够带来幸福的大团圆……"
            }
        },
        "0_M690": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 9",
            "text": {
                "0_M690": "竞速赛的秘诀果然是『安全第一』啊。",
                "0_M691": "实际上确实赢了，所以我也不好一概否定……",
                "0_M692": "但我觉得还是要看情况。",
                "0_M693": "不，必须用尽全力。",
                "0_M694": "必须做到最大、全力、安全第一才行。",
                "0_M695": "安全万岁。安全最好。",
                "0_M696": "唉……总之，现在先为我们的胜利感到喜悦吧！"
            }
        },
        "0_M700": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 10",
            "text": {
                "0_M700": "快看啊，是光！",
                "0_M701": "这可谓正确法老治理世界的崭新黎明！",
                "0_M702": "黎明之光……",
                "0_M703": "嗯，无论见多少次，都令人安心呢。"
            }
        },
        "0_M710": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 11",
            "text": {
                "0_M710": "挖洞和埋葬基本一样。",
                "0_M711": "身为冥界之神的化身，埋葬大师的我是不可能输的。",
                "0_M712": "塌方、缺氧、地下水渗漏……",
                "0_M713": "没遇到实际埋葬会遇到的事，实属幸运……"
            }
        },
        "0_M720": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 12",
            "text": {
                "0_M720": "啊……",
                "0_M721": "故事听入迷了，不知何时居然已经挖穿了！",
                "0_M722": "你还真能干呀！？",
                "0_M723": "承蒙夸奖不胜荣幸。那么，接下来的故事等今后有机会……",
                "0_M724": "（能令人忘却时间固然很好，但缺点在于让人很介意后续发展啊。",
                "0_M725": "刚才那个故事的结局究竟会是什么呢……）"
            }
        },
        "0_M730": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 13",
            "text": {
                "0_M730": "作为法老的故事来说，这是理所当然的结局！",
                "0_M731": "终于获得完全胜利啦！",
                "0_M732": "居然能活到最后一页，真是太好了……",
                "0_M733": "这样一来，我也能成为稍微好一点的法老了，",
                "0_M734": "我这样想应该没问题吧……呵呵。"
            }
        },
        "0_S010": {
            "name": "召唤(真名确认后)",
            "text": {
                "0_S010": "被……召唤出来了。",
                "0_S011": "……我的名字是山鲁佐德。",
                "0_S012": "如果您愿意答应我一个单纯的愿望，",
                "0_S013": "我愿意永远侍您为王。"
            }
        }
    },
    "ChrVoice_ids": [
        "503000",
        "503010",
        "9010005"
    ]
}
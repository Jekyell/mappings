{
    "id": 604800,
    "collectionNo": 378,
    "name": "洛库斯塔",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "菌菇波浪！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "把你变成菌床！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "菌菇菌菇菌菇！　然后……还有菌菇！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "为你下毒吧，为你装盘吧。",
                "0_B051": "恶食才是罗马的美食。",
                "0_B052": "吃下吐出才是美食的作风。",
                "0_B053": "可悲的活祭，神圣的肴馔，全都献给心爱的陛下——",
                "0_B054": "菇，既为神圣肴馔！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "要吃菌菇吗？"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "请品尝菌菇吧！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "啊～哈哈哈！　吃下毒菇去死吧！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "啊哈！　要吃毒药吗？"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "啊哈哈！　就这么嗝屁吧。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "晚安，至少也要安详一点。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "好痛，好痛，不行啦不行啦。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "尼禄大人，对不起……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "咿，阿格里皮娜大人，请饶恕……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "这是毒！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "去死，吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "因为我是专业的——"
            }
        },
        "0_B2010": {
            "name": "攻击 10",
            "text": {
                "0_B2010": "……去死吧。"
            }
        },
        "0_B2020": {
            "name": "攻击 11",
            "text": {
                "0_B2020": "令人不快……！"
            }
        },
        "0_B2030": {
            "name": "攻击 12",
            "text": {
                "0_B2030": "毒菇！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "此乃诸神的肴馔。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "没有比毒药更美味的东西了。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "送上顶级的美餐！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "烹饪之战！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "变大吧！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "直接动手！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "菌菇菌菇♪　砰砰砰砰！　轰隆！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "这种菌菇很有效哦。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "来下毒！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "放菌菇的女人！　洛库斯塔！　叮叮铛～。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "我是洛库斯塔。是尼禄大人的暗杀者。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "唔……这食材不配放进尼禄大人的盘子里呢。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "一切一切，都为了尼禄大人的安息。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "菌菇用光了……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "束手无策bad trip……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "哎，用菌菇做肴馔？　我就做给你看吧！",
                "0_B801": "主菜当然是菌菇，再加某种肉类和附近的叶子！",
                "0_B802": "然后秘方当然是菌菇啦——！",
                "0_B803": "看，做完了！　菇，既为神圣肴馔！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "像这样是活不下来的！　哇哈哈哈！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "搞定啦！"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "哎，颜色好吓人。"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "去死去死去死！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "兴奋剂菌菇炖菜！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "再来一份菌菇吧！",
                "0_G021": "真的真的不要紧啦！",
                "0_G022": "副作用并不多啦。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "非常感谢！",
                "0_G031": "作为回礼，请收下这菌菇吧！",
                "0_G032": "没关系，这是可以吃的菌菇哦。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "我……我的毒正逐渐精炼。",
                "0_G041": "凭现在的我，应该能带给那位大人安详——",
                "0_G042": "哎，这种菌菇？　当然有毒啊。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "竟然会重用我这样的毒杀者……",
                "0_G051": "难道你需要用毒？",
                "0_G052": "用作暗杀？　还是用作自裁？",
                "0_G053": "交给我洛库斯塔吧！",
                "0_G054": "……这次定会做出完美的毒。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "提升提升提升！"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "毒……",
                "0_G121": "更为速效而安详的毒。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝你生日快乐！",
                "0_H011": "我会大展身手，制作美味的菌菇料理哦！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "举办祭典啦！",
                "0_H021": "各位，都有菌菇吗！？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "去采菌菇吧！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我很习惯侍奉他人！",
                "0_H041": "然后呢，要对谁下毒？"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主……",
                "0_H051": "既然是尼禄大人的朋友，对洛库斯塔自然也是重要的人。",
                "0_H052": "对于这个问题，实际如何？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "当然最～～喜欢尼禄大人了！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西1",
            "text": {
                "0_H070": "讨厌的东西？　哎，竹笋？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "是兽之尼禄大人手持的毒杯吗？",
                "0_H081": "那不是非常美嘛。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "你好，御主大人！",
                "0_H091": "要吃菌菇吗？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "嘿嘿，御主老爷……",
                "0_H101": "我进了些不错的菌货哦？",
                "0_H102": "这可是上等货色哦，令人难以自已哦……松茸。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "尼禄大人养父的克劳狄乌斯帝是因毒菇而死的。",
                "0_H111": "嗯，就是这个意思。",
                "0_H112": "而尼禄大人……非常喜欢菌菇料理，甚至称其为诸神的肴馔——",
                "0_H113": "真是我莫大的荣幸。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "起初是为了阿格里皮娜大人——",
                "0_H121": "更是为了尼禄大人——",
                "0_H122": "我的毒，是只为那位大人而存在的。",
                "0_H123": "啊啊……",
                "0_H124": "但是，这又是为何！　您会如此凄惨地……！",
                "0_H125": "那个小盒子里分明放着倾注了我半生的结晶——专为您准备的毒啊！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "远离充斥着血与毒的宫廷。",
                "0_H131": "向着那落日景色的遥远彼方——",
                "0_H132": "御主大人……",
                "0_H133": "如果您的前行方向会经过那里，如果您能将尼禄大人从那里带出来。",
                "0_H134": "我会向您献上一切。",
                "0_H135": "从今往后，洛库斯塔就是御主大人的毒了。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "我一直很崇拜您，尼禄大人！",
                "0_H171": "请尽情品尝这些美味的菌菇吧！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "尼禄大人……",
                "0_H181": "我是为您而存在的，暗杀者。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "卡利古拉帝！",
                "0_H191": "阿格里皮娜大人的，兄长吧……",
                "0_H192": "有点害怕。",
                "0_H193": "喜欢啦，喜欢啦，呜呜呜，害怕。",
                "0_H194": "啊！　不行啦不行啦。",
                "0_H195": "这菌菇是尼禄大人的肴馔！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "那女孩……是毒使吧。",
                "0_H201": "全身都是毒？",
                "0_H202": "呼哇啊……好棒。想和她交朋友……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "女王塞弥拉弥斯！　传说中的毒使！",
                "0_H211": "呼哦哈啊啊！　好耀眼！",
                "0_H212": "我推竟然活着还会动！",
                "0_H213": "——多么的尊啊。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "哦，阿拉什大人。",
                "0_H221": "哎～，毒无效啊。",
                "0_H222": "是吗～，是这样吗～……",
                "0_H223": "离谱～，太吓人了～。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "赖光大人……来这边，来这边。",
                "0_H231": "这个……是毒。不用客气，快收下吧！",
                "0_H232": "不不不！　我明白我明白，我当然明白啦！",
                "0_H233": "您……是下毒的那方吧？",
                "0_H234": "没错，洛库斯塔能够看出来！"
            }
        },
        "0_H2300": {
            "name": "对话 3(牵绊Lv.5以上)",
            "text": {
                "0_H2300": "御主大人，毒竟然对您没用吗！？",
                "0_H2301": "这是什么，太棒了吧！",
                "0_H2302": "这个这个，这种菌菇！　快尝尝看啦！"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "哎？　断头台？",
                "0_H241": "……把脑袋，这样……",
                "0_H242": "啊……原来如此？",
                "0_H243": "……太——————恐怖了吧！",
                "0_H244": "那，那，那是什么！　吓人吓人吓人！",
                "0_H245": "玛丽大人，太可怜了吧！？",
                "0_H246": "……请，请问……要，服用毒药吗？"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "……哦，就是你吗，将那断头台？",
                "0_H251": "————————没人性的。"
            }
        },
        "0_H3070": {
            "name": "讨厌的东西2",
            "text": {
                "0_H3070": "……其实我根本不在乎什么罗马。",
                "0_H3071": "全都烧光就好了。"
            }
        },
        "0_I010": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 1",
            "text": {
                "0_I010": "嘿，欢迎光临！",
                "0_I011": "今天推荐从地中海刚捞起来的活蹦乱跳的新鲜毒蝇伞哦！",
                "0_I012": "……什么？",
                "0_I013": "除了菌菇以外还有什么？",
                "0_I014": "光看不买的话，就早点滚吧！"
            }
        },
        "0_I020": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 2",
            "text": {
                "0_I020": "这里可不是小家伙该来的店哦。",
                "0_I021": "早点回家喝妈妈的红茶菌菇吧。",
                "0_I022": "……哦，眼神倒是不错。",
                "0_I023": "我喜欢，到后面来。",
                "0_I024": "我向你介绍店里珍藏的商品。"
            }
        },
        "0_I030": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 3",
            "text": {
                "0_I030": "道具兑换需求收到啦！",
                "0_I031": "嘿呀嘿呀嘿呀！　One two 呀嗬～！",
                "0_I032": "为在罗马！　与最为！　美妙的！　御主！",
                "0_I033": "邂逅的！　奇迹，干·杯啦！　嘿咻！"
            }
        },
        "0_I040": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 4",
            "text": {
                "0_I040": "来，要的是这个道具吧？",
                "0_I041": "您有集点菌菇吗？　我可以立刻给您制作，需要吗？",
                "0_I042": "集满点数就能1UP哦，我指生命。"
            }
        },
        "0_I050": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 5",
            "text": {
                "0_I050": "这是兑换道具的菌菇。",
                "0_I051": "什么，您没点菌菇？",
                "0_I052": "……御主大人，现在可不是说笑的时候。"
            }
        },
        "0_I060": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 6",
            "text": {
                "0_I060": "以为能听到感伤的告别台词吗！",
                "0_I061": "来吧，用这菌菇来收尾吧！",
                "0_I062": "酒！　菌菇！　摇滚！",
                "0_I063": "今晚是party night！"
            }
        },
        "0_I070": {
            "name": "螺旋证明世界Lilim Harlot\n～无人喝彩的蔷薇～ 7",
            "text": {
                "0_I070": "御主大人，没有忘记兑换道具吧？",
                "0_I071": "……非常好，那么，全体离舰。",
                "0_I072": "我会与商店命运与共。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "回应召唤前来，我是洛库斯塔！",
                "0_S011": "来，品尝菌菇吧！",
                "0_S012": "菌菇在有上进心的群体中也很有人气。",
                "0_S013": "别管了，快吃吧。有话吃完再说。"
            }
        }
    },
    "ChrVoice_ids": [
        "604800"
    ]
}
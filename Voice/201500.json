{
    "id": 201500,
    "collectionNo": 84,
    "name": "阿周那",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "命运已在此注定。"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "神性领域扩大。",
                "0_B051": "空间固定。",
                "0_B052": "神罚执行期限设定——",
                "0_B053": "全部确认。",
                "0_B054": "谨以湿婆之怒，",
                "0_B055": "将汝等之命断绝于此。",
                "0_B056": "破坏神之手影！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "那就用这个吧。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "那就请吧。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "真是空虚的战斗。 这对汝等没有任何益处哦。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "没办法。 只能一战了吗。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "所以我不是提醒过你们了吗？"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "居然完全听任去做这种无谓的行为。真可悲。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "该死的！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "不可……能，不甘心……！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "怎么可能……在这里结束……"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "好。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "好了，你打算如何？"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "所有邪恶皆应毁灭！"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "接下来地狱即将出现！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "就算不瞄准！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "大卸八块！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "诱爆启动，崩溃吧！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "因果，报应，在此迸发！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "获赐恩惠。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "吾之目光，射穿吧！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "在此解放吾之宿业。",
                "0_B801": "作为人与神之子，降下惩罚。",
                "0_B802": "见证吧，此乃崩溃！",
                "0_B803": "破坏神之手影！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "谨以悲剧拯救众生。",
                "0_B811": "湿婆的光晕啊，与崩溃一同涌来吧。",
                "0_B812": "爆聚开始。",
                "0_B813": "破坏神之手影，迸发坠落吧！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "高涨吧，吾之甘狄拔！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "啊啊，这不错……"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "呵呵，这是好事。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "非常感谢。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "啊啊，这才是……"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "很好，完美。",
                "0_G051": "这才是我，这才是阿周那！",
                "0_G052": "哼哼哼，",
                "0_G053": "哈哈哈哈哈！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "是诞生之日吧。",
                "0_H011": "祝你生日快乐。",
                "0_H012": "请向双亲、恩师、朋友、",
                "0_H013": "以及除了他们以外的重要的对象表达感谢。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像发生了什么啊……"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "请下决断，御主。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "嗯，我阿周那仅以真挚地服从为悦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "若是我和你，世间一定认为我比较伟大吧。",
                "0_H051": "但与此无关。",
                "0_H052": "你是御主，",
                "0_H053": "我比较看重这一点。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "独自一人比较轻松。",
                "0_H062": "这想法与英雄相去甚远呢……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的……东西吗。",
                "0_H071": "我不太喜欢闯入我内心的人。",
                "0_H072": "……御主，请务必注意。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "希望圣杯实现的愿望吗？",
                "0_H081": "若能实现，我希望能得到永远的孤独。",
                "0_H082": "不是开玩笑，是认真的哦？"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "御主，请多指教。",
                "0_H091": "好了，该怎么办呢？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "哦～御主……",
                "0_H101": "你对我有兴趣吗？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "原来如此。",
                "0_H111": "这就是牵绊吗。",
                "0_H112": "尽管有些烦人，但这或许也是——从者的宿命吧。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……请不要一直看着我。",
                "0_H121": "就像是我阿周那正在丢人现眼，难以忍受。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "啊啊……你看到我的脸了吧。",
                "0_H131": "悲伤，太令人悲伤了。",
                "0_H132": "御主，这件事请务必保密。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "迦尔纳。",
                "0_H141": "没想到居然和你这家伙共处同一阵营。",
                "0_H142": "这也是神明的安排吧。",
                "0_H143": "现在虽不会与你相争……但下次只要让我看到你，我可就不保证会发生什么了哦？"
            }
        },
        "0_H170": {
            "name": "对话 5",
            "text": {
                "0_H170": "罗摩。我很光荣能与您并肩作战。",
                "0_H171": "但是，您比我预想的还要年轻……",
                "0_H172": "不需要用敬语吗？",
                "0_H173": "……好吧。那就让我们并肩战斗吧！"
            }
        },
        "0_H180": {
            "name": "对话 6",
            "text": {
                "0_H180": "马嘶……",
                "0_H181": "他是个可怕的强敌。",
                "0_H182": "如果他能成为伙伴，就如同获得了上千名战士。",
                "0_H183": "但他为什么那么烦躁呢？",
                "0_H184": "难道这就是现代说法中的缺钙吗？"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (『唯有不断提问』通关后) ",
            "text": {
                "0_H1800": "我已经不会犹豫了。",
                "0_H1801": "不，不对。正因为有犹豫，才算是人类。",
                "0_H1802": "正因为怀有混沌，才能抵达这豁然开朗的领域。",
                "0_H1803": "无论多少次，我都愿意重申这句话。",
                "0_H1804": "我很高兴能够遇见你。"
            }
        },
        "0_H3080": {
            "name": "关于圣杯\n(『唯有不断提问』通关后)",
            "text": {
                "0_H3080": "我已经没有需要圣杯实现的愿望了。",
                "0_H3081": "不，是呢……",
                "0_H3082": "我希望能陪伴御主您到最后一刻。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者Archer。",
                "0_S011": "名为阿周那。",
                "0_S012": "御主，请尽情地使用我吧。"
            }
        },
        "11_B030": {
            "name": "攻击 3 天授的旅装",
            "text": {
                "11_B030": "就是那里！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 天授的旅装",
            "text": {
                "11_B040": "我还有事要做，先解决这边吧！"
            }
        },
        "11_B080": {
            "name": "技能 1 天授的旅装",
            "text": {
                "11_B080": "要我拿出天授的存在吗。"
            }
        },
        "11_B090": {
            "name": "技能 2 天授的旅装",
            "text": {
                "11_B090": "我对富贵没有兴趣。"
            }
        },
        "11_B100": {
            "name": "开始 1 天授的旅装",
            "text": {
                "11_B100": "我认为战斗是非常空虚的，但又是一种十分宝贵的经验。 综上所述，被我打倒吧。"
            }
        },
        "11_B110": {
            "name": "开始 2 天授的旅装",
            "text": {
                "11_B110": "让我们开始战斗吧。 现在的我干劲十足，觉悟吧。"
            }
        },
        "11_B120": {
            "name": "胜利 1 天授的旅装",
            "text": {
                "11_B120": "赢了。胜利的V就是和平的P吧。"
            }
        },
        "11_B130": {
            "name": "胜利 2 天授的旅装",
            "text": {
                "11_B130": "我的修行也还远远不够呢。"
            }
        },
        "11_B150": {
            "name": "受击 1 天授的旅装",
            "text": {
                "11_B150": "糟了，掉以轻心了吗！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 天授的旅装",
            "text": {
                "11_B160": "我去休息一会儿。晚安……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 天授的旅装",
            "text": {
                "11_B170": "失误了，先撤退……我可没有输！"
            }
        },
        "11_B180": {
            "name": "指令卡 1 天授的旅装",
            "text": {
                "11_B180": "是。"
            }
        },
        "11_B190": {
            "name": "指令卡 2 天授的旅装",
            "text": {
                "11_B190": "明白了。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 天授的旅装",
            "text": {
                "11_B200": "我知道了。"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 天授的旅装",
            "text": {
                "11_B210": "那就果断迅速地歼灭吧。"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 天授的旅装",
            "text": {
                "11_B220": "让我们轻松地上吧…… 用宝具痛击敌人！"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 天授的旅装",
            "text": {
                "11_B230": "发动宝具。 也就意味着清理周围的环境。"
            }
        },
        "11_B410": {
            "name": "攻击 4 天授的旅装",
            "text": {
                "11_B410": "被打飞吧！"
            }
        },
        "11_B420": {
            "name": "攻击 5 天授的旅装",
            "text": {
                "11_B420": "还没完！"
            }
        },
        "11_B430": {
            "name": "攻击 6 天授的旅装",
            "text": {
                "11_B430": "崩塌吧！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 天授的旅装",
            "text": {
                "11_B440": "失礼了，我正赶时间！"
            }
        },
        "11_B480": {
            "name": "技能 3 天授的旅装",
            "text": {
                "11_B480": "稍微提升点干劲吧。"
            }
        },
        "11_B490": {
            "name": "技能 4 天授的旅装",
            "text": {
                "11_B490": "请做好觉悟。"
            }
        },
        "11_B840": {
            "name": "额外攻击 3 天授的旅装",
            "text": {
                "11_B840": "请容我将你们作为踏脚石！"
            }
        },
        "11_G010": {
            "name": "升级 天授的旅装",
            "text": {
                "11_G010": "啊啊，这不错……"
            }
        },
        "11_G060": {
            "name": "灵衣开放 天授的旅装",
            "text": {
                "11_G060": "旅途会令人成长，但有时也会令人扭曲。",
                "11_G061": "但只要有我在，就让我们令旅途变得愉快吧。",
                "11_G062": "不，不对。应该说和您的旅行令我感到快乐。",
                "11_G063": "来吧，我们走。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 天授的旅装",
            "text": {
                "11_H9000": "这鸭子是我从某个路边摊贩那里购买的。",
                "11_H9001": "应该算是一起创造回忆的同伴吧。",
                "11_H9002": "我还买了不少其他东西，御主要不要拿一个？"
            }
        }
    },
    "ChrVoice_ids": [
        "201500"
    ]
}
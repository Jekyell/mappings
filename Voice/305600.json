{
    "id": 305600,
    "collectionNo": 428,
    "name": "ヴァン・ゴッホ〔マイナー〕",
    "voices": {
        "0_B010": {
            "name": "アタック 1",
            "text": {
                "0_B010": "梵高矿车！　发射！　砰——！！"
            }
        },
        "0_B020": {
            "name": "アタック 2",
            "text": {
                "0_B020": "发现！　挖掘！　出货～！"
            }
        },
        "0_B030": {
            "name": "アタック 3",
            "text": {
                "0_B030": "梵高　开采！"
            }
        },
        "0_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "0_B040": "挖掘挖掘挖掘～！　安全确保！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "灵感涌现啦！",
                "0_B051": "星空没问题！　街景没问题！",
                "0_B052": "梵高钻头，全速全开！",
                "0_B053": "被牵连可不关我的事哦～！",
                "0_B054": "被这夜间露天咖啡座！"
            }
        },
        "0_B080": {
            "name": "スキル 1",
            "text": {
                "0_B080": "饭菜真好吃！"
            }
        },
        "0_B090": {
            "name": "スキル 2",
            "text": {
                "0_B090": "De Aardappeleters（吃土豆的人）！"
            }
        },
        "0_B100": {
            "name": "開始 1",
            "text": {
                "0_B100": "劳动最棒了！　开玩笑的！"
            }
        },
        "0_B110": {
            "name": "開始 2",
            "text": {
                "0_B110": "好好吃、好好玩、好好工作！"
            }
        },
        "0_B120": {
            "name": "勝利 1",
            "text": {
                "0_B120": "好了！　接下来该做什么呢！"
            }
        },
        "0_B130": {
            "name": "勝利 2",
            "text": {
                "0_B130": "那，那个，有没有做过火呢……？"
            }
        },
        "0_B140": {
            "name": "ダメージ 2",
            "text": {
                "0_B140": "哈啊……"
            }
        },
        "0_B150": {
            "name": "ダメージ 1",
            "text": {
                "0_B150": "回避——哇啊啊！！"
            }
        },
        "0_B160": {
            "name": "戦闘不能 1",
            "text": {
                "0_B160": "过劳对身体不好……开玩笑的……"
            }
        },
        "0_B170": {
            "name": "戦闘不能 2",
            "text": {
                "0_B170": "肚子饿了……哎嘿嘿……"
            }
        },
        "0_B180": {
            "name": "コマンドカード 1",
            "text": {
                "0_B180": "前方正常！"
            }
        },
        "0_B190": {
            "name": "コマンドカード 2",
            "text": {
                "0_B190": "左右正常！"
            }
        },
        "0_B200": {
            "name": "コマンドカード 3",
            "text": {
                "0_B200": "关系正常！　开玩笑的！"
            }
        },
        "0_B2010": {
            "name": "アタック 10",
            "text": {
                "0_B2010": "由我来，击碎墙壁！"
            }
        },
        "0_B2020": {
            "name": "アタック 11",
            "text": {
                "0_B2020": "深邃，深邃……！"
            }
        },
        "0_B210": {
            "name": "宝具カード 1",
            "text": {
                "0_B210": "麻烦全体回避！"
            }
        },
        "0_B220": {
            "name": "宝具カード 2",
            "text": {
                "0_B220": "要上了哦，威廉！"
            }
        },
        "0_B230": {
            "name": "宝具カード 3",
            "text": {
                "0_B230": "为黑夜送上星辰、光芒……！"
            }
        },
        "0_B410": {
            "name": "アタック 4",
            "text": {
                "0_B410": "萌芽！！"
            }
        },
        "0_B420": {
            "name": "アタック 5",
            "text": {
                "0_B420": "梵高　破坏！"
            }
        },
        "0_B430": {
            "name": "アタック 6",
            "text": {
                "0_B430": "呜呵呵……梵高爆破啊啊啊啊～～！！？"
            }
        },
        "0_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "0_B440": "梵高采矿（最棒）！　快说～！！　说了吗！？"
            }
        },
        "0_B480": {
            "name": "スキル 3",
            "text": {
                "0_B480": "因为我就是干这行的！"
            }
        },
        "0_B490": {
            "name": "スキル 4",
            "text": {
                "0_B490": "脑海中有景象了！"
            }
        },
        "0_B500": {
            "name": "開始 3",
            "text": {
                "0_B500": "成为黑暗中的，光芒……！"
            }
        },
        "0_B510": {
            "name": "開始 4",
            "text": {
                "0_B510": "背后就拜托您了，阿波罗大人……！"
            }
        },
        "0_B520": {
            "name": "勝利 3",
            "text": {
                "0_B520": "还能为这世界留下多少、留下什么呢？"
            }
        },
        "0_B550": {
            "name": "ダメージ 3",
            "text": {
                "0_B550": "这点，不算什么……！"
            }
        },
        "0_B560": {
            "name": "戦闘不能 3",
            "text": {
                "0_B560": "有没有，稍微，挖出一点光呢……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "虚数美术，开工！",
                "0_B801": "夜空之星！　团圞之街！",
                "0_B802": "连人之梦境都能塑造的力量！",
                "0_B803": "这就是梵高的破坏＆创造！",
                "0_B804": "——夜间露天咖啡座！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "采掘地底的星辰，",
                "0_B811": "采集黑暗的灯火，再思抛弃的希望，",
                "0_B812": "再兴终结的世界吧。",
                "0_B813": "闪耀吧！我内心的世界！",
                "0_B814": "——我已经不会再寻求太阳了。"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "由我打开突破口！　就是现在！"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "呵呵，谢啦！"
            }
        },
        "0_B890": {
            "name": "スキル 6",
            "text": {
                "0_B890": "看到光了！"
            }
        },
        "0_B910": {
            "name": "アタック 7",
            "text": {
                "0_B910": "吃这个吧——！！"
            }
        },
        "0_B920": {
            "name": "アタック 8",
            "text": {
                "0_B920": "开始穿孔～！"
            }
        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "由我先行！"
            }
        },
        "0_F010": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 1",
            "text": {
                "0_F010": "破坏＆创造……",
                "0_F011": "让您见识一下矿工的本领！"
            }
        },
        "0_F020": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 2",
            "text": {
                "0_F020": "虽然会占用一些时间，",
                "0_F021": "但在执行任务的空余时间，我会制作一些好东西哦，",
                "0_F022": "哎嘿嘿！"
            }
        },
        "0_F040": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 3",
            "text": {
                "0_F040": "通知确认，没问题！",
                "0_F041": "将最新的信息全部消化，今天也要保障一天的安全！"
            }
        },
        "0_F050": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 4",
            "text": {
                "0_F050": "请交给我吧，开始创造！"
            }
        },
        "0_F060": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 5",
            "text": {
                "0_F060": "开工啦～！"
            }
        },
        "0_F070": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 6",
            "text": {
                "0_F070": "特异点解体任务，全工程完成！",
                "0_F071": "我这边已经收工了，不过图坦亲那边或许还留着什么。何不去确认一下呢？"
            }
        },
        "0_F080": {
            "name": "ぶち壊せ！ ミステリーハウス・クラフターズ \n～星の鉱員と日の出の翼～ 7",
            "text": {
                "0_F080": "提奥……",
                "0_F081": "……这边已经全部收拾完了。",
                "0_F082": "将所有的相遇与离别作为食粮，向着新坑道进发吧。"
            }
        },
        "0_G010": {
            "name": "レベルアップ 1",
            "text": {
                "0_G010": "还能继续吃哦！"
            }
        },
        "0_G020": {
            "name": "霊基再臨 2",
            "text": {
                "0_G020": "又跨过了一道障碍！",
                "0_G021": "如果能顺便有些鲜花凋零就好了……",
                "0_G022": "也就是说……力量提升，鲜花乱舞！",
                "0_G023": "呜呵呵，难道不值得庆贺吗！"
            }
        },
        "0_G030": {
            "name": "霊基再臨 1",
            "text": {
                "0_G030": "御主大人的从者发生了巨大的变化。",
                "0_G031": "也就是说……您的臣下……进化了……",
                "0_G032": "……噗呼呼哎嘿嘿！！　梵高笑话！"
            }
        },
        "0_G040": {
            "name": "霊基再臨 3",
            "text": {
                "0_G040": "我决不会浪费您倾注的资源。",
                "0_G041": "也就是说，升级不浪费！",
                "0_G042": "……抱歉，当我没说。"
            }
        },
        "0_G050": {
            "name": "霊基再臨 4",
            "text": {
                "0_G050": "成长告一段落，但梵高还能继续！",
                "0_G051": "制作需要的是工夫。不断花功夫，能做的事才会不断增加！",
                "0_G052": "也就是说，工夫花功夫……",
                "0_G053": "咦，这个光靠说好像说不清楚吧……"
            }
        },
        "0_G110": {
            "name": "レベルアップ 2",
            "text": {
                "0_G110": "成长的喜悦！"
            }
        },
        "0_H010": {
            "name": "誕生日",
            "text": {
                "0_H010": "祝您生日快乐，御主大人！ 您只要现在来阿尔世界，就能看到占据整个世界的超巨大生日蛋糕哦！"
            }
        },
        "0_H020": {
            "name": "イベント開催中",
            "text": {
                "0_H020": "发生什么事了，御主大人！ 快走吧，去挖光所有素材！"
            }
        },
        "0_H030": {
            "name": "会話 1",
            "text": {
                "0_H030": "无论是劳动、游玩、还是做饭，梵高都会陪同。",
                "0_H031": "梵高伴随（烧饭）！"
            }
        },
        "0_H040": {
            "name": "会話 2",
            "text": {
                "0_H040": "雇主与劳动者。御主与从者原本就是这种关系吧。",
                "0_H041": "不过嘛，当个玩伴或许也不错呢！"
            }
        },
        "0_H050": {
            "name": "会話 3",
            "text": {
                "0_H050": "自那晚，我们边逃离怪物，边挖坑、烤土豆、祈盼黎明以来，梵高心目中的御主大人……",
                "0_H051": "就是能够并肩劳作、嬉戏……",
                "0_H052": "可被称为伙伴的存在了。"
            }
        },
        "0_H060": {
            "name": "好きなこと",
            "text": {
                "0_H060": "最近我沉迷于劳动！",
                "0_H061": "没错，就是指游戏！",
                "0_H062": "像是无止境地刷等级、无止尽地捡东西去卖、无止尽地铲平高山把天涯海角都变成土豆田什么的……我最喜欢这种玩法了！"
            }
        },
        "0_H070": {
            "name": "嫌いなこと",
            "text": {
                "0_H070": "……或许您会觉得意外，但这个灵基时，我会忍不住……",
                "0_H071": "说一些无聊的笑话……",
                "0_H072": "不，那个，原本灵基时我都会精挑细选的！？　请相信我啊！！"
            }
        },
        "0_H080": {
            "name": "聖杯について",
            "text": {
                "0_H080": "圣杯吗……",
                "0_H081": "在这个灵基时，我也没什么想要的东西，但矿工精神就是手上有什么就用什么！",
                "0_H082": "所以，没错，如果给我的话，我肯定会全力滥用！"
            }
        },
        "0_H090": {
            "name": "絆Lv.1",
            "text": {
                "0_H090": "什么事！　梵高该做什么呢！",
                "0_H091": "带头冲锋！　挺过战斗！　去澡堂子！",
                "0_H092": "哪种都行，什么都行哦！！"
            }
        },
        "0_H100": {
            "name": "絆Lv.2",
            "text": {
                "0_H100": "欢迎回来！",
                "0_H101": "要来绘画吗，要来游戏吗！",
                "0_H102": "还是说……来·段·笑·话？"
            }
        },
        "0_H110": {
            "name": "絆Lv.3",
            "text": {
                "0_H110": "梵高能做什么呢。身为降临者的我这时应该会心情消沉吧……",
                "0_H111": "这种时候就该专心致志地挥舞镐子，然后应该就能发现新的东西。",
                "0_H112": "……挥舞镐子敲通透～。",
                "0_H113": "开玩笑啦。"
            }
        },
        "0_H120": {
            "name": "絆Lv.4",
            "text": {
                "0_H120": "御主大人！　啊，您是说这个吗！？",
                "0_H121": "我玩了很多借来的游戏后，逐渐体会到工匠的精神，越来越感兴趣了……",
                "0_H122": "所以把资料室里的作品全都借来了。您有什么感兴趣的作品吗？"
            }
        },
        "0_H130": {
            "name": "絆Lv.5",
            "text": {
                "0_H130": "啊，御主大人。我刚想出了一件新的创作，希望您能来模拟装置看看。",
                "0_H131": "另外阿尔世界今晚好像要更新！",
                "0_H132": "梵高会和游戏部的各位一起玩，御主大人要不要一起来啊？"
            }
        },
        "0_H170": {
            "name": "会話 4",
            "text": {
                "0_H170": "达·芬奇……是个难对付的人呢……",
                "0_H171": "既是巨匠……还掌握着画材的供应权……",
                "0_H172": "真希望有一天能用笑话扭曲那张乔康达脸啊……"
            }
        },
        "0_H180": {
            "name": "会話 5",
            "text": {
                "0_H180": "我现在能经常和游戏部的各位聊天了。",
                "0_H181": "不过大家经常说什么……梵高的游戏风格很M？",
                "0_H182": "话虽如此，劳动不就是这样的嘛……"
            }
        },
        "0_H190": {
            "name": "会話 6",
            "text": {
                "0_H190": "真想和托马斯·爱迪生大人搞好关系呢！",
                "0_H191": "他一定会让我像牛马一样拼命干活吧！"
            }
        },
        "0_H200": {
            "name": "会話 7",
            "text": {
                "0_H200": "北斋！　啊啊，北斋！",
                "0_H201": "请再等等我哦，我一定会把在这灵基学到的东西明确地反映到绘画里给您看！"
            }
        },
        "0_H210": {
            "name": "会話 8",
            "text": {
                "0_H210": "其实玛塔·哈丽大人所处的年代和地区都和我非常相近。",
                "0_H211": "真想边吃土豆，边和她一起回顾那个时代呢。",
                "0_H212": "……哎！？　我我我我我没什么企图啦！？"
            }
        },
        "0_H220": {
            "name": "会話 9",
            "text": {
                "0_H220": "图坦亲是个了不起的人！",
                "0_H221": "明明那么幼小，却既能干又高尚，做的饭也好吃！　而且……",
                "0_H222": "也是个可怕的人。",
                "0_H223": "为了达成目的，会使用不该用的东西，还会去不该去的地方。",
                "0_H224": "为了防止他做这种事，我得努力才行。",
                "0_H225": "另外……会因为玩笑而爆炸这点，好像不太合适呢……"
            }
        },
        "0_H2200": {
            "name": "会話 2\n(ミステリーハウス・クラフターズクリア以降)",
            "text": {
                "0_H2200": "主从的关系可以有各种形式，也可以随时间而变迁。",
                "0_H2201": "崇拜、依存、孤立……",
                "0_H2202": "不论关系如何变迁，只要能相伴而行，肯定就是良好的关系。"
            }
        },
        "0_H230": {
            "name": "会話 10",
            "text": {
                "0_H230": "玛尔达大人作为劳动美德的化身，我非常尊敬她！",
                "0_H231": "虽然偶尔会超级可怕，但这样也很好！"
            }
        },
        "0_H2300": {
            "name": "会話 3\n(ミステリーハウス・クラフターズクリア以降)",
            "text": {
                "0_H2300": "御主大人一直以来都独自背负着人类史迈步走向黑暗。",
                "0_H2301": "您无疑是我们的太阳。现在，我希望能成为足以让您托付后背的存在。"
            }
        },
        "0_H240": {
            "name": "会話 11",
            "text": {
                "0_H240": "尼莫亲总是很关心梵高。",
                "0_H241": "那大概是因为，梵高靠不住，令他很担心吧。",
                "0_H242": "如果我能够变得靠得住……真想和他成为朋友呢。"
            }
        },
        "0_H250": {
            "name": "会話 12",
            "text": {
                "0_H250": "杨贵妃大人吗……",
                "0_H251": "我觉得那位其实也非常喜欢工作。",
                "0_H252": "所以，我稍微模仿了她一下。",
                "0_H253": "我们两个有什么交集？",
                "0_H254": "呵呵，保密。"
            }
        },
        "0_H260": {
            "name": "会話 13",
            "text": {
                "0_H260": "奥斯曼狄斯大人是最棒的！",
                "0_H261": "当我在阿尔世界建造金字塔玩的时候，天上突然飘来一句「还不够！」的声音，然后亲自对埃及所有神殿进行了详尽的监修……",
                "0_H262": "那真是好开心啊……"
            }
        },
        "0_H4080": {
            "name": "聖杯について\n(ミステリーハウス・クラフターズクリア以降)",
            "text": {
                "0_H4080": "我会不会许下想要再见提奥的愿望？",
                "0_H4081": "……不会的。在我心目中，这个愿望已经实现了。",
                "0_H4082": "因为那的确就是提奥，我也得知了当年没能听到的他的真心话啊。"
            }
        },
        "0_S010": {
            "name": "召喚",
            "text": {
                "0_S010": "梵高……觉得自己稍微成长了一点。",
                "0_S011": "破坏与创造。为人们的生活点亮幽光的力量……",
                "0_S012": "当需要前往黑暗之地时，还请让我陪伴在您的身边哦。",
                "0_S013": "……偶尔也一起玩玩游戏吧。哎嘿嘿。"
            }
        },
        "0_S020": {
            "name": "仮加入",
            "text": {
                "0_S020": "综上所述，护卫的工作由我担任！",
                "0_S021": "请和我矿工梵高玩遍谜之特异点……",
                "0_S022": "不对！　是挖尽谜之特异点吧，哎嘿嘿！"
            }
        }

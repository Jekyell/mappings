{
    "id": 202100,
    "collectionNo": 184,
    "name": "巴御前",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1(真名确认后)",
            "text": {
                "0_B040": "燃烧吧！"
            }
        },
        "0_B050": {
            "name": "宝具(真名确认后)",
            "text": {
                "0_B050": "圣观世音菩萨——",
                "0_B051": "赐予我！",
                "0_B052": "力量吧！",
                "0_B053": "赐予我旭日之辉吧！",
                "0_B054": "——真言·圣观世音菩萨！！"
            }
        },
        "0_B080": {
            "name": "技能 1(真名确认后)",
            "text": {
                "0_B080": "平息吧，平息吧……"
            }
        },
        "0_B090": {
            "name": "技能 2(真名确认后)",
            "text": {
                "0_B090": "义仲大人……"
            }
        },
        "0_B100": {
            "name": "开始 1(真名确认后)",
            "text": {
                "0_B100": "我要上了。"
            }
        },
        "0_B110": {
            "name": "开始 2(真名确认后)",
            "text": {
                "0_B110": "出击！"
            }
        },
        "0_B120": {
            "name": "胜利 1(真名确认后)",
            "text": {
                "0_B120": "令人怀念的战场的味道。"
            }
        },
        "0_B130": {
            "name": "胜利 2(真名确认后)",
            "text": {
                "0_B130": "不用高呼胜利吗？"
            }
        },
        "0_B150": {
            "name": "受击 1(真名确认后)",
            "text": {
                "0_B150": "太不知天高地厚了！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1(真名确认后)",
            "text": {
                "0_B160": "义仲大人……现在就去您的身边……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2(真名确认后)",
            "text": {
                "0_B170": "没想到……只能到此为止了……"
            }
        },
        "0_B180": {
            "name": "指令卡 1(真名确认后)",
            "text": {
                "0_B180": "是这样啊。"
            }
        },
        "0_B190": {
            "name": "指令卡 2(真名确认后)",
            "text": {
                "0_B190": "是。"
            }
        },
        "0_B200": {
            "name": "指令卡 3(真名确认后)",
            "text": {
                "0_B200": "包在我身上吧。"
            }
        },
        "0_B210": {
            "name": "宝具卡(真名确认后)",
            "text": {
                "0_B210": "谨遵您的命令！"
            }
        },
        "0_G010": {
            "name": "升级(真名确认后)",
            "text": {
                "0_G010": "这就是锻炼的成果吧。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2(真名确认后)",
            "text": {
                "0_G020": "不胜感激。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1(真名确认后)",
            "text": {
                "0_G030": "您看到我的角了吧。",
                "0_G031": "真是太羞耻了……",
                "0_G032": "但是，这就是巴真正的模样。",
                "0_G033": "今后我也将为您效力，御主。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3(真名确认后)",
            "text": {
                "0_G040": "这个模样……",
                "0_G041": "啊啊，是这样啊。",
                "0_G042": "事已至此……",
                "0_G043": "从此时此刻起，我将不再厌恶此身流淌的鬼之血了。",
                "0_G044": "巴会运用自己全部力量战斗。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4(真名确认后)",
            "text": {
                "0_G050": "巴真是个幸福的人。",
                "0_G051": "明明已然死去，却能遇见这样一位优秀的主君。",
                "0_G052": "尽管我没有什么愿望……",
                "0_G053": "但我还是会这么想。",
                "0_G054": "如果义仲大人能够见到您，",
                "0_G055": "一定会与您亲密到如同父亲与子女吧——"
            }
        },
        "0_H010": {
            "name": "生日(真名确认后)",
            "text": {
                "0_H010": "今天似乎是个值得庆祝的日子呢。",
                "0_H011": "哎呀，是御主的……那还真是。"
            }
        },
        "0_H020": {
            "name": "活动举行中(真名确认后)",
            "text": {
                "0_H020": "唔……",
                "0_H021": "是战斗，还是祭典。",
                "0_H022": "似乎正在发生什么。要去看看吗？"
            }
        },
        "0_H030": {
            "name": "对话 1(真名确认后)",
            "text": {
                "0_H030": "该是出发的时候了吧？"
            }
        },
        "0_H040": {
            "name": "对话 2(真名确认后)",
            "text": {
                "0_H040": "御主是巴的主君。",
                "0_H041": "请一定要摆出自己的威严来哦。"
            }
        },
        "0_H050": {
            "name": "对话 3(真名确认后)",
            "text": {
                "0_H050": "御主就是主君的意思。",
                "0_H051": "巴会诚挚地为您效力。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西(真名确认后)",
            "text": {
                "0_H060": "想要听关于义仲大人的事吗？",
                "0_H061": "当然没问题啦。",
                "0_H062": "从哪里开始说起呢？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西(真名确认后)",
            "text": {
                "0_H070": "范赖、义经，还有赖朝公——",
                "0_H071": "假如见到了这些人，",
                "0_H072": "巴可没有自信可以克制自己。",
                "0_H073": "恐怕……",
                "0_H074": "会化作鬼吧。"
            }
        },
        "0_H080": {
            "name": "关于圣杯(真名确认后)",
            "text": {
                "0_H080": "圣杯……我没有任何愿望需要向那种东西许愿。",
                "0_H081": "我已是亡故之身。",
                "0_H082": "若说对过去的人生没有遗憾，那一定是在撒谎，即便如此……",
                "0_H083": "嗯。",
                "0_H084": "虽然有隐藏在内心的感情，但没有愿望。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1(真名确认后)",
            "text": {
                "0_H090": "没想到您会如此频繁带上我……",
                "0_H091": "若能帮上您的忙，我会非常高兴的。",
                "0_H092": "我会继续努力的。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2(真名确认后)",
            "text": {
                "0_H100": "御主是一位温柔的人。",
                "0_H101": "但是，千万不要勉强自己。",
                "0_H102": "对了！　机会难得，去娱……娱乐……室？",
                "0_H103": "去迦勒底的那个地方休息一下吧。",
                "0_H104": "我也很喜欢名为游戏的东西。",
                "0_H105": "御主怎么样？"
            }
        },
        "0_H10120": {
            "name": "牵绊Lv.4(真名确认后)",
            "text": {
                "0_H10120": "御主您……",
                "0_H10121": "与义仲大人不一样。",
                "0_H10122": "这我非常清楚。"
            }
        },
        "0_H10130": {
            "name": "牵绊Lv.5(真名确认后)",
            "text": {
                "0_H10130": "在战争中，巴没能在义仲大人临终之时陪伴他的身旁。",
                "0_H10131": "这令我无比悔恨。",
                "0_H10132": "但是，御主您……",
                "0_H10133": "……请一定要健健康康的哦！",
                "0_H10134": "巴不会说什么想要在您临终之时陪伴您左右的话。",
                "0_H10135": "但请您一定要健康、安稳、幸福地，活得长寿一点哦。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3(真名确认后)",
            "text": {
                "0_H110": "我会一直为您效力。",
                "0_H111": "这就是作为您的从者现界之人的职责。",
                "0_H112": "话说……",
                "0_H113": "我还想继续玩那个游戏，可以吗。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……尽管没告诉您我的名字，却已经似乎与您多次共赴战场。",
                "0_H121": "非常感谢。",
                "0_H122": "我很想回应这份信任。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我的真名，那是——",
                "0_H131": "…………不，不能说。无论如何都不行。",
                "0_H132": "真的非常……对不起。",
                "0_H133": "真的非常……"
            }
        },
        "0_H140": {
            "name": "对话 4(真名确认后)",
            "text": {
                "0_H140": "刚才那位日本的人物……",
                "0_H141": "我似乎在哪里见过？"
            }
        },
        "0_H150": {
            "name": "对话 5(真名确认后)",
            "text": {
                "0_H150": "唔唔。",
                "0_H151": "御主还年轻。",
                "0_H152": "可不能玩得太过火哦？　要有节制，节制。",
                "0_H153": "呃，那个，尽管这话难以启齿……",
                "0_H154": "但有几位穿着十分大胆的人物，",
                "0_H155": "嗯。要有节制！"
            }
        },
        "0_H160": {
            "name": "对话 6(真名确认后)",
            "text": {
                "0_H160": "虽然算不上什么令人怀念的面孔……",
                "0_H161": "唔……",
                "0_H162": "不，一定是巴多心了。恕我失礼。"
            }
        },
        "0_H170": {
            "name": "对话 7(真名确认后)",
            "text": {
                "0_H170": "啊，梅芙大人。",
                "0_H171": "……嗯。对。",
                "0_H172": "这……这我当然明白。我自认有自知之明。",
                "0_H173": "但是……",
                "0_H174": "那……那个……就算您说不该始终死撑着寡妇气质不放……",
                "0_H175": "巴也挺为难的……",
                "0_H176": "哎，酒吗？",
                "0_H177": "不，不用，巴非常不擅长喝酒……！"
            }
        },
        "0_H180": {
            "name": "对话 8(真名确认后)",
            "text": {
                "0_H180": "嗯，说得没错，守护风纪是一件非常好的事！",
                "0_H181": "万事都要注重节制才行！",
                "0_H182": "……不，不过赖光大人，您那打扮……",
                "0_H183": "节制……"
            }
        },
        "0_H190": {
            "name": "对话 9(真名确认后)",
            "text": {
                "0_H190": "你……！",
                "0_H191": "是平家的……景清吗……！",
                "0_H192": "不，这样子……难不成是义经……不，这不可能……！？"
            }
        },
        "0_N010": {
            "name": "节分酒宴绘卷 鬼乐百重塔 1",
            "text": {
                "0_N010": "这边的温泉应该对消除疲劳也很有效果。",
                "0_N011": "各位从者，当然还有御主，都请尽情在这里放松吧。"
            }
        },
        "0_N020": {
            "name": "节分酒宴绘卷 鬼乐百重塔 2",
            "text": {
                "0_N020": "需要我为您洗背吗？"
            }
        },
        "0_N030": {
            "name": "节分酒宴绘卷 鬼乐百重塔 3",
            "text": {
                "0_N030": "我准备了咖啡牛奶。",
                "0_N031": "听说泡完澡以后喝一点会非常好。",
                "0_N032": "……稍后我也来一杯吧。"
            }
        },
        "0_N040": {
            "name": "节分酒宴绘卷 鬼乐百重塔 4",
            "text": {
                "0_N040": "泡在热腾腾的水中，闭上双眼放轻松……",
                "0_N041": "想必非常舒服吧……",
                "0_N042": "但请务必不要忘记看一眼通知哦。"
            }
        },
        "0_N050": {
            "name": "节分酒宴绘卷 鬼乐百重塔 5",
            "text": {
                "0_N050": "哎呀，看来您相当疲劳啊……",
                "0_N051": "机会难得，我来为您按摩吧！",
                "0_N052": "来来，不用客气！",
                "0_N053": "……尽管我不能保证下手轻重……"
            }
        },
        "0_N060": {
            "name": "节分酒宴绘卷 鬼乐百重塔 6",
            "text": {
                "0_N060": "这次的战斗真是辛苦您了。",
                "0_N061": "这边的温泉还会保留一段时间，",
                "0_N062": "所以还请尽情在这里放松吧。"
            }
        },
        "0_N080": {
            "name": "节分酒宴绘卷 鬼乐百重塔 7",
            "text": {
                "0_N080": "御主。您没有忘记什么东西吧？"
            }
        },
        "0_N110": {
            "name": "节分酒宴绘卷 鬼乐百重塔 8",
            "text": {
                "0_N110": "这里记录着爬塔留下的足迹。",
                "0_N111": "看着刻在这里的进程，会感觉心情非常舒畅吧。"
            }
        },
        "0_N120": {
            "name": "节分酒宴绘卷 鬼乐百重塔 9",
            "text": {
                "0_N120": "战勋必须报以相应的奖励才行。",
                "0_N121": "这是非常重要的。"
            }
        },
        "0_N130": {
            "name": "节分酒宴绘卷 鬼乐百重塔 10",
            "text": {
                "0_N130": "这应该称作所谓的成就吧。",
                "0_N131": "嗯哼♪"
            }
        },
        "0_N140": {
            "name": "节分酒宴绘卷 鬼乐百重塔 11",
            "text": {
                "0_N140": "一定要仔细确认名为规则的东西。",
                "0_N141": "知道得越多，就能赢得更多成果。"
            }
        },
        "0_N160": {
            "name": "节分酒宴绘卷 鬼乐百重塔 12",
            "text": {
                "0_N160": "鬼之塔已经不见踪影了。",
                "0_N161": "真是辛苦您了。"
            }
        },
        "0_N170": {
            "name": "节分酒宴绘卷 鬼乐百重塔 13",
            "text": {
                "0_N170": "唔……",
                "0_N171": "这么看来，还真是令人感慨万千呢。"
            }
        },
        "0_P010": {
            "name": "节分酒宴绘卷 鬼乐百重塔 14",
            "text": {
                "0_P010": "似乎有从者已经彻底恢复了呢。",
                "0_P011": "一副气势十足的样子……",
                "0_P012": "好啦，让我们前往下一次战斗吧！"
            }
        },
        "0_P020": {
            "name": "节分酒宴绘卷 鬼乐百重塔 15",
            "text": {
                "0_P020": "唔，好像有人泡得时间太长了……"
            }
        },
        "0_P030": {
            "name": "节分酒宴绘卷 鬼乐百重塔 16",
            "text": {
                "0_P030": "那位是不是就这么泡着……",
                "0_P031": "开始打瞌睡了？"
            }
        },
        "0_P070": {
            "name": "节分酒宴绘卷 鬼乐百重塔 17",
            "text": {
                "0_P070": "干得漂亮。",
                "0_P071": "来，我们向下一层进发吧！"
            }
        },
        "0_P080": {
            "name": "节分酒宴绘卷 鬼乐百重塔 18",
            "text": {
                "0_P080": "楼层通关～！",
                "0_P081": "是这么说的吧？",
                "0_P082": "呵呵呵，这种游戏我也玩过哦。"
            }
        },
        "0_P100": {
            "name": "节分酒宴绘卷 鬼乐百重塔 19",
            "text": {
                "0_P100": "恭喜突破五十层！",
                "0_P101": "剩下的一半也按照这个状态攀登吧。",
                "0_P102": "我也会陪同。"
            }
        },
        "0_P110": {
            "name": "节分酒宴绘卷 鬼乐百重塔 20",
            "text": {
                "0_P110": "终于突破一百层了！",
                "0_P111": "太令人钦佩了。",
                "0_P112": "御主，还有各位从者。",
                "0_P113": "干得太漂亮了！"
            }
        },
        "0_P270": {
            "name": "节分酒宴绘卷 鬼乐百重塔 21",
            "text": {
                "0_P270": "打起精神来，向接下来的进发吧！"
            }
        },
        "0_P280": {
            "name": "节分酒宴绘卷 鬼乐百重塔 22",
            "text": {
                "0_P280": "御主，不要掉以轻心哦！"
            }
        },
        "0_P290": {
            "name": "节分酒宴绘卷 鬼乐百重塔 23",
            "text": {
                "0_P290": "不用过于焦急。",
                "0_P291": "道路虽然艰险，但让我们一步一步，",
                "0_P292": "踏踏实实地前进吧。"
            }
        },
        "0_P300": {
            "name": "节分酒宴绘卷 鬼乐百重塔 24",
            "text": {
                "0_P300": "……好了，还剩下一半。",
                "0_P301": "这次是真真正正地通往顶点之路了吧。",
                "0_P302": "我当然也会继续陪同！"
            }
        },
        "0_P310": {
            "name": "节分酒宴绘卷 鬼乐百重塔 25",
            "text": {
                "0_P310": "感慨万千。",
                "0_P311": "实在是辛苦您了。",
                "0_P312": "同时，也非常感谢您，御主。"
            }
        },
        "0_S010": {
            "name": "召唤(真名确认后)",
            "text": {
                "0_S010": "——吾名为巴。",
                "0_S011": "别人曾经称呼我为巴御前。",
                "0_S012": "尽管此身早已献给了义仲大人，",
                "0_S013": "但现在乃是侍奉您的从者。"
            }
        }
    },
    "ChrVoice_ids": [
        "202100",
        "202110"
    ]
}
{
    "id": 304200,
    "collectionNo": 280,
    "name": "罗穆路斯·奎里努斯",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "光啊！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "吾之双臂能触及一切，",
                "0_B051": "撕裂一切，",
                "0_B052": "开拓一切。",
                "0_B053": "终将到达宇宙的彼方！",
                "0_B054": "——吾等之臂开拓一切，至天际！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "罗马！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "吾之道路，已至神域！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "我的爱是无限的。 让你们好好见识一下吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "开始吧！ ……罗马已在此处。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "看到我的爱了吧！ ……那你也成为罗马吧。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "果然是罗马啊。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "魔力不够吗……抱歉……！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "我的孩子们啊……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "嗯"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "罗马。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "那就让你见识一下吧！　就在此时此刻！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "嗯，一起出征吧。"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "……射杀百头·罗马式！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "见证一切吧！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "罗马！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "玛尔斯！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "七丘！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "帕拉蒂尼！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "这双手，可至世界尽头！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "降临！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "将罗马的光辉映入眼帘吧！"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "人类的光辉……浪漫的罗马之光辉。"
            }
        },
        "0_B540": {
            "name": "受击 3",
            "text": {
                "0_B540": "太浅了！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "罗马是永恒的……明白了吗……？"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "由吾之四肢释放而出吧，",
                "0_B801": "神明的力量，",
                "0_B802": "光芒四射的力量。",
                "0_B803": "终将抵达繁星彼方的力量啊！",
                "0_B804": "——吾等之臂开拓一切，至天际！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "噢噢噢！　伟大！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "啊，是吗。是啊。",
                "0_G011": "罗马的一切都在守护着你。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "不用担心。一切都交给我吧。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "神之铠甲！　竟然也会再次到来吗！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "此乃身为奎里努斯的吾之身，神明的身姿！",
                "0_G041": "喜悦吧。你见到了罗马的至高存在。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "一同出征吧。",
                "0_G051": "现在是罗马闪耀之时。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "有些发痒。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "噢噢，你诞生于地上的日子。我的孩子，罗马啊。",
                "0_H011": "那应赐予你祝福。",
                "0_H012": "祝愿你的前行道路上充满了浪漫，充满了罗马。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "是新的罗马！",
                "0_H021": "出征吧！　现在正是扩张之时！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "一同出征吧。",
                "0_H031": "让我见证你的前进。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我不会要求你崇敬我。",
                "0_H041": "你只要保持你原来的样子即可。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我乃是至高神。",
                "0_H051": "是统帅罗马的存在……",
                "0_H052": "呵。但不必过于逞强。",
                "0_H053": "保持纯真自然吧！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "快看。天上的繁星正在祝福着。",
                "0_H061": "祝福世界（罗马），祝福余（罗马），以及祝福爱着你（罗马）的一切——",
                "0_H062": "而人们，通常称之为宇宙。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "世间充满了浪漫。",
                "0_H071": "没错。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯迟早会成为罗马的一部分……",
                "0_H081": "我以前也说过这句话吗？"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "你认识我吧。",
                "0_H091": "没错。罗马神祖，建国王罗穆路斯。",
                "0_H092": "这同样是我。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我同时具备人类的浪漫与神明的超脱。",
                "0_H101": "这点与罗穆路斯一样……",
                "0_H102": "我，没错，或许可以被称作狂暴力量的显现。",
                "0_H103": "其证据，就是我并没有象征建国这一面的大树之枪。",
                "0_H104": "这也是命中注定的吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我追求、渴望。",
                "0_H111": "希望这双手并不是为了破坏，而是为打开无明的黑暗、触及未知、为了人类之爱而存在。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "能从你的身上感受到什么……",
                "0_H121": "……",
                "0_H122": "啊啊……你是罗马。",
                "0_H123": "哈哈哈，原来如此。没错。人类的浪漫就是这样代代相传的啊！",
                "0_H124": "正因为如此……",
                "0_H125": "我才无法不去爱！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "当前灵基下，我抛弃了冠位。",
                "0_H131": "因为在维持Grand Lancer的状态下，我是无法显现于你所在的迦勒底的。",
                "0_H132": "但是，不必担心。",
                "0_H133": "我的力量、我的光之臂可以触及任何地方。",
                "0_H134": "定能为你开辟道路，通往你所追求的场所。",
                "0_H135": "我的孩子——罗马啊。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "尼禄，我心爱的孩子。",
                "0_H171": "罗马的荣耀与阴影的体现者，如同我妹妹般的皇帝啊。",
                "0_H172": "我爱着你。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "卡利古拉。",
                "0_H181": "被月亮诅咒的孩子，如同我弟弟般的罗马啊。",
                "0_H182": "无法责备狄安娜的你应该明白自己的罪过吧。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "恺撒。噢噢，恺撒啊。",
                "0_H191": "区区认子瞬间即可完成！",
                "0_H192": "倒不如说理应堂堂正正地缔结婚姻关系。这才是罗马。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "阿耳忒弥斯……",
                "0_H201": "与我所知的狄安娜容貌并不相同……",
                "0_H202": "那是由真体分离出来的狄安娜的爱之表现吧……？"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "噢噢，最强的英雄！　赫拉克勒斯！",
                "0_H211": "虽非吾父，但犹如吾父的男人。",
                "0_H212": "他同样是罗马吧。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "倘若爱着军神，那等同于吾妹。",
                "0_H221": "然而为何吾妹企图杀害我呢……",
                "0_H222": "这就是……病娇吗？"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "这愤怒正是我的罪孽。",
                "0_H231": "胜利的女王啊。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "——呼唤我的究竟是谁。",
                "0_S011": "是吗，是你啊。",
                "0_S012": "……明白了。",
                "0_S013": "此处乃是这个时间轴吧。",
                "0_S014": "那我会这么说。",
                "0_S015": "我乃罗穆路斯·奎里努斯！",
                "0_S016": "挥舞光之枪般手臂的人理从者！"
            }
        }
    },
    "ChrVoice_ids": [
        "304200"
    ]
}
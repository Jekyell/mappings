        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "无妨。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "……要上了。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "何谓法老的威严——"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "这乃是神罚！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "出现吧！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "提升，提升…… 提升了哦……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "恭喜　恭喜",
                "0_H011": "祝你　生日快乐"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "活动　活动　赶快去"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "应该　立刻　去工作"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "赞颂　梅杰德大人　吧"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我是　梅杰德大人　的化身"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "崇敬吧　赞颂吧"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "不敬是　不行的",
                "0_H071": "是不行的哦"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯　不需要"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "崇敬吧……",
                "0_H091": "嗯　无碍"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "供品　还　不够",
                "0_H101": "更多　更多"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "不能　偷看　里面",
                "0_H111": "我不是说不可以了吗！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "如果　真的　在意　里面",
                "0_H121": "就去　灵基再临吧"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "适可而止……",
                "0_H131": "适……适可而止啦！",
                "0_H132": "差不多可以灵基再临了吧，",
                "0_H133": "你打算从下面偷看里面到什么时候！？"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "啊，法老克娄巴特拉……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "啊啊　啊啊啊",
                "0_H151": "奥斯曼狄斯　大人",
                "0_H152": "……唔唔，没有脸见那位大人……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "住……住手　快住手",
                "0_H161": "不能　掀起来",
                "0_H162": "……不……不可以啦。",
                "0_H163": "不能掀御衣啦，法老伊斯坎达尔！"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "被凝视　被死死地　凝视着",
                "0_H171": "唔唔……",
                "0_H172": "英雄王　从刚才开始　就一直　在盯着我……"
            }
        },
        "0_M610": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 1",
            "text": {
                "0_M610": "话说……速度……是不是太快了点……？ 你在说什么呢，这速度还远不止于此呢！"
            }
        },
        "0_M620": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 2",
            "text": {
                "0_M620": "现在让我们脚踏实地地前进吧！ 『欲速则不达』……真是一句名言。"
            }
        },
        "0_M630": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 3",
            "text": {
                "0_M630": "状态不错！我们的驰骋没有任何后顾之忧！ 打滑、翻车、爆炸四散…… 啊啊，应该从哪件事开始应对呢……"
            }
        },
        "0_M640": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 4",
            "text": {
                "0_M640": "蠢动，蠢动，蠢动…… （……她是那种会沉迷于简单工作的那种类型呢……）"
            }
        },
        "0_M650": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 5",
            "text": {
                "0_M650": "进展不错，继续往前挖吧！ 一定要做好防塌方的预防工作……"
            }
        },
        "0_M660": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 6",
            "text": {
                "0_M660": "开始有些没积极性了呢。 也就是说……你应该明白我想说什么吧？ 嗯。那么，为了放松心情，我来说一个故事吧。"
            }
        },
        "0_M670": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 7",
            "text": {
                "0_M670": "我听到了，称颂法老的民众之声！",
                "0_M671": "虽说获胜是理所当然的，但这份心意很好！",
                "0_M672": "理应获胜之人获胜，同时也带给人们喜悦的话。",
                "0_M673": "嗯嗯——这就是所谓的『王道故事』吧……"
            }
        },
        "0_M680": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 8",
            "text": {
                "0_M680": "看到了吧！",
                "0_M681": "法老的传说又新添了一份荣耀！",
                "0_M682": "若能如愿，但愿这能够带来幸福的大团圆……"
            }
        },
        "0_M690": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 9",
            "text": {
                "0_M690": "竞速赛的秘诀果然是『安全第一』啊。",
                "0_M691": "实际上确实赢了，所以我也不好一概否定……",
                "0_M692": "但我觉得还是要看情况。",
                "0_M693": "不，必须用尽全力。",
                "0_M694": "必须做到最大、全力、安全第一才行。",
                "0_M695": "安全万岁。安全最好。",
                "0_M696": "唉……总之，现在先为我们的胜利感到喜悦吧！"
            }
        },
        "0_M700": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 10",
            "text": {
                "0_M700": "快看啊，是光！",
                "0_M701": "这可谓正确法老治理世界的崭新黎明！",
                "0_M702": "黎明之光……",
                "0_M703": "嗯，无论见多少次，都令人安心呢。"
            }
        },
        "0_M710": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 11",
            "text": {
                "0_M710": "挖洞和埋葬基本一样。",
                "0_M711": "身为冥界之神的化身，埋葬大师的我是不可能输的。",
                "0_M712": "塌方、缺氧、地下水渗漏……",
                "0_M713": "没遇到实际埋葬会遇到的事，实属幸运……"
            }
        },
        "0_M720": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 12",
            "text": {
                "0_M720": "啊……",
                "0_M721": "故事听入迷了，不知何时居然已经挖穿了！",
                "0_M722": "你还真能干呀！？",
                "0_M723": "承蒙夸奖不胜荣幸。那么，接下来的故事等今后有机会……",
                "0_M724": "（能令人忘却时间固然很好，但缺点在于让人很介意后续发展啊。",
                "0_M725": "刚才那个故事的结局究竟会是什么呢……）"
            }
        },
        "0_M730": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 13",
            "text": {
                "0_M730": "作为法老的故事来说，这是理所当然的结局！",
                "0_M731": "终于获得完全胜利啦！",
                "0_M732": "居然能活到最后一页，真是太好了……",
                "0_M733": "这样一来，我也能成为稍微好一点的法老了，",
                "0_M734": "我这样想应该没问题吧……呵呵。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我乃　无名　法老",
                "0_S011": "低下头来　这是　大不敬",
                "0_S012": "……喂",
                "0_S013": "不能　偷看　里面啊！"
            }
        },
        "1_B020": {
            "name": "攻击 2",
            "text": {
                "1_B020": "我要上了！"
            }
        },

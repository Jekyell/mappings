        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "赫眼睁开，",
                "0_B801": "钢骸盛放——",
                "0_B802": "请于此处！",
                "0_B803": "做好觉悟！",
                "0_B804": "——机关外法·狮子奋迅！",
                "0_B805": "这首级，归我了。"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "赫魔啊，到来吧。",
                "0_B811": "以钢为躯化身起源——",
                "0_B812": "见证吧！",
                "0_B813": "此乃吾身，此乃吾形！",
                "0_B814": "刻进眼中坠入冥府吧！",
                "0_B815": "——此为不祥之技，还请见谅。"
            }
        },
        "0_B820": {
            "name": "宝具 4",
            "text": {
                "0_B820": "赫魔啊，到来吧。",
                "0_B821": "以钢为躯化身起源——",
                "0_B822": "见证吧！",
                "0_B823": "此乃吾身，此乃吾形！",
                "0_B824": "——Evil wind death slash！",
                "0_B825": "——此为不祥之技，还请见谅。"
            }
        },
        "0_B840": {

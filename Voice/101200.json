{
    "id": 101200,
    "collectionNo": 126,
    "name": "贝德维尔",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "银之臂会将你击碎！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "吞噬我的灵魂，",
                "0_B051": "疾驰吧，",
                "0_B052": "银之流星。",
                "0_B053": "——一闪·银之臂！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "吾之剑，吾之臂，全都是为了王。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "不朽且不屈！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "我会贯彻我的忠义——"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "正因为心系吾王。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "……这努亚达之臂，是为了胜利。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "吾王啊，将这胜利献给您。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "这和吾王的剑光比起来……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "就算倒下，忠义也不灭……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "吾……王……亚瑟……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "交给我吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "要上了吗。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "将我全部力量，都托付给您！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "银之臂啊，请赐予我光辉！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "魔力全解放。能行。"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "将一切，都献上吧！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "被打……飞吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "由我来守护。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "请见证我的忠义吧。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "请容我……前进。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "我出击了。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "请注意。或许还有伏兵。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "暂时算是，获得了胜利。"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "还没完！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "应该……还能坚持！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "我的力量……不足……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "还没……结束……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "宝具起动。",
                "0_B801": "撕裂一切吧，我的手臂。",
                "0_B802": "——一闪，",
                "0_B803": "银之臂！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "定胜负！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "魔力，上升。",
                "0_G011": "银之臂被强化了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "努亚达之臂，化为我的力量吧——"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我的手臂，我的力量。",
                "0_G031": "战神努亚达的狂暴之魂才是——"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "……如此力量。不知我是否能驾驭。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "努亚达的力量，现在正可谓……",
                "0_G051": "我定会让您见证这等同于权能的威力。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "为您的生日献上千万祝福。",
                "0_H011": "举行宴会吧。鄙人贝德维尔，一定会作为执事为您效力。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "有什么事情正在发生——",
                "0_H021": "我有这种感觉。御主，请指示。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "防止世界被烧却是您的目的。",
                "0_H031": "这应该也是吾王的愿望。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "所谓主从……既是人与人的关系，也是相当于命运的存在。",
                "0_H041": "至少，对我来说是这样的。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "献上自己的剑，就相当于献上了自己的命运。",
                "0_H051": "……我的忠诚虽然已经献给了王，但这把剑，会为了御主您而挥动。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "喜欢的食物是蒸蔬菜。",
                "0_H062": "……不，我已经决定将我的一切都献给王了。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……我想不太出来。",
                "0_H071": "威胁不列颠的魔兽和蛮族虽然是敌人，",
                "0_H072": "但不是憎恶的对象。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯，那是……",
                "0_H081": "吾王和我们圆桌骑士曾经追求的存在，之后……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我是圆桌骑士之一。我对吾王亚瑟的忠诚不会改变。",
                "0_H091": "……御主。之所以为您效力，严格来说也是因为感到这关系到王所期望的未来。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "……嗯。我在圆桌骑士中也算是老资格了。",
                "0_H101": "从很早以前起，我就侍奉着吾王。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "宫廷的执事工作。照顾王的日常生活……",
                "0_H111": "虽然有人这么评价我。不，现在我也以此自居。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我的银之臂，Agateram。",
                "0_H121": "是大魔术师梅林赐予我的。被誉为凯尔特战神努亚达使用的手臂。",
                "0_H122": "……为了补足我的独臂，为了能与其他卓越骑士们比肩，我才追求这种力量。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "您与吾王有一些相似。",
                "0_H131": "……我这么觉得。如果能得到您的允许，我愿再一次为您郑重献上自己的剑。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "吾王。",
                "0_H141": "哪怕此处并非不列颠，我也会不变地向您献上自己的忠诚……王啊。"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "圆桌骑士们。",
                "0_H151": "我一直在梦想……",
                "0_H152": "再一次与各位并驾齐驱的日子的到来。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "……叛逆的气息……",
                "0_H161": "不，但是，莫德雷德卿只是嘴巴很毒态度很差性格很糟糕，",
                "0_H162": "其实还是个有那么一点点优点的骑士……偏见是不好的……大概吧……"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "啊啊，不同世界的吾王啊。",
                "0_H171": "您好。",
                "0_H172": "……如何，如果您方便的话，能和我聊一会儿吗？"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "啊啊，加雷斯卿！",
                "0_H181": "这样啊，您终于也来这里了……啊啊……",
                "0_H182": "见过兰斯洛特卿了吗？",
                "0_H183": "见过高文卿了吗？",
                "0_H184": "……嗯，嗯。",
                "0_H185": "这样啊……啊啊……",
                "0_H186": "不。请不用担心。",
                "0_H187": "只是眼睛里进灰尘了。"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "珀西瓦尔卿！",
                "0_H191": "呃，哎呀，为什么那么惊讶……",
                "0_H192": "啊啊，是这条右臂啊。",
                "0_H193": "说来就话长了……准确地说，我并不是您所在历史轴上的贝德维尔……",
                "0_H194": "啊啊，不，我并不想让您陷入混乱。",
                "0_H195": "嗯，我会告诉您。"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "布里托玛特阁下。",
                "0_H201": "我绝不会忘记您的父亲，不会忘记阿西高阁下临终的那一刻。",
                "0_H203": "当时我，在卡姆兰……",
                "0_H204": "……嗯，嗯。 嗯。说得……对呢。",
                "0_H205": "希望我们务必能好好相处。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "Saber，贝德维尔。从今往后将成为您的从者。",
                "0_S011": "我相信这都是为了吾王。"
            }
        },
        "11_B030": {
            "name": "攻击 3 银之管家",
            "text": {
                "11_B030": "就此结束！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 银之管家",
            "text": {
                "11_B040": "容我，失礼！"
            }
        },
        "11_B080": {
            "name": "技能 1 银之管家",
            "text": {
                "11_B080": "银之臂啊，赐予我力量吧。"
            }
        },
        "11_B090": {
            "name": "技能 2 银之管家",
            "text": {
                "11_B090": "魔力啊，凝聚在我的手臂上吧……"
            }
        },
        "11_B100": {
            "name": "开始 1 银之管家",
            "text": {
                "11_B100": "好吧。假如除了战斗没有其他方法的话。"
            }
        },
        "11_B110": {
            "name": "开始 2 银之管家",
            "text": {
                "11_B110": "吾王啊，敬请明鉴。"
            }
        },
        "11_B120": {
            "name": "胜利 1 银之管家",
            "text": {
                "11_B120": "……战斗结束。 您没事吧，御主。"
            }
        },
        "11_B130": {
            "name": "胜利 2 银之管家",
            "text": {
                "11_B130": "真不好意思。让您看到这种丑陋的战斗……"
            }
        },
        "11_B150": {
            "name": "受击 1 银之管家",
            "text": {
                "11_B150": "区区疼痛，对这身躯而言……！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 银之管家",
            "text": {
                "11_B160": "看来应该……穿上铠甲才对吗……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 银之管家",
            "text": {
                "11_B170": "十分……抱歉……"
            }
        },
        "11_B180": {
            "name": "指令卡 1 银之管家",
            "text": {
                "11_B180": "明白了。"
            }
        },
        "11_B190": {
            "name": "指令卡 2 银之管家",
            "text": {
                "11_B190": "是。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 银之管家",
            "text": {
                "11_B200": "领命。"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 银之管家",
            "text": {
                "11_B210": "让我的手臂成为您的助力！"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 银之管家",
            "text": {
                "11_B220": "宝具，起动准备。"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 银之管家",
            "text": {
                "11_B230": "努亚达啊，梅林啊！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 银之管家",
            "text": {
                "11_B440": "在这里！"
            }
        },
        "11_B480": {
            "name": "技能 3 银之管家",
            "text": {
                "11_B480": "请交给我吧。"
            }
        },
        "11_B490": {
            "name": "技能 4 银之管家",
            "text": {
                "11_B490": "请见证我的忠义吧。"
            }
        },
        "11_B500": {
            "name": "开始 3 银之管家",
            "text": {
                "11_B500": "那么。"
            }
        },
        "11_B510": {
            "name": "开始 4 银之管家",
            "text": {
                "11_B510": "觉悟吧。"
            }
        },
        "11_B520": {
            "name": "胜利 3 银之管家",
            "text": {
                "11_B520": "就此结束了吗……？"
            }
        },
        "11_B530": {
            "name": "胜利 4 银之管家",
            "text": {
                "11_B530": "继续前进吧，御主。"
            }
        },
        "11_B550": {
            "name": "受击 3 银之管家",
            "text": {
                "11_B550": "一定会……坚持给你看……！"
            }
        },
        "11_B560": {
            "name": "无法战斗 3 银之管家",
            "text": {
                "11_B560": "御主……"
            }
        },
        "11_B570": {
            "name": "无法战斗 4 银之管家",
            "text": {
                "11_B570": "吾……王啊……"
            }
        },
        "11_B840": {
            "name": "额外攻击 3 银之管家",
            "text": {
                "11_B840": "爆发吧！"
            }
        },
        "11_G010": {
            "name": "升级 银之管家",
            "text": {
                "11_G010": "魔力，上升。",
                "11_G011": "银之臂被强化了。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 银之管家",
            "text": {
                "11_G060": "贝德维尔灵衣开放。",
                "11_G061": "非武装的状态……给人一种奇妙的感觉呢。",
                "11_G062": "但这样行动起来也很方便。",
                "11_G063": "十分感谢，御主。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 银之管家",
            "text": {
                "11_H9000": "崔斯坦卿这么对我说。",
                "11_H9001": "说只有我一个人穿这身未免不太好，应该全体圆桌骑士都换一身才对。",
                "11_H9002": "……我这样，该不会是有些不合群吧……？"
            }
        }
    },
    "ChrVoice_ids": [
        "101200"
    ]
}
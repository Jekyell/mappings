        },
        "0_B2440": {
            "name": "エクストラアタック 4",
            "text": {
                "0_B2440": "无论是什么……斩断！伏诛！"
            }
        },
        "0_B250": {
            "name": "宝具カード 4",
            "text": {
                "0_B250": "是的，我也这么认为。"
            }
        },
        "0_B410": {
            "name": "アタック 4",
            "text": {
                "0_B410": "逆·凪！"
            }
        },
        "0_B420": {
            "name": "アタック 5",

        },
        "0_B2410": {
            "name": "アタック 13",
            "text": {
                "0_B2410": "徒劳无功"
            }
        },
        "0_B2420": {
            "name": "アタック 14",
            "text": {
                "0_B2420": "风凪！"
            }
        },
        "0_B2430": {
            "name": "アタック 15",
            "text": {
                "0_B2430": "这里就是间合！"

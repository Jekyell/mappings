        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "扎营有没有征得允许呢？"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "垃圾要随手收拾带走哦！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "来自四方的风啊，",
                "0_B801": "让大地再次充满神秘，",
                "0_B802": "冲洗掉所有的祸根吧！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "愿所有支持我的人们拥有协调与繁荣！",
                "0_B811": "合众为y……",
                "0_B812": "一！！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "能用技能真了不起！"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "无论给多少张都吃得下去哦！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "班扬飞踢！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "上吧！　贝比！"
            }
        },
        "0_D010": {
            "name": "连续武剧神话 密西西比·神话编撰者们 11",
            "text": {
                "0_D010": "淘金热之梦，再来……！",
                "0_D011": "不断挖掘，获得宝物吧！"
            }
        },
        "0_D020": {
            "name": "连续武剧神话 密西西比·神话编撰者们 12",
            "text": {
                "0_D020": "从侏罗纪的雨林中找到的宝物……哇！",
                "0_D021": "化石是不是还太早了？"
            }
        },
        "0_D030": {
            "name": "连续武剧神话 密西西比·神话编撰者们 13",
            "text": {
                "0_D030": "等一下……？　我把那本笔记本放哪儿去了？",
                "0_D031": "我记得，好像是埋在哪……！？",
                "0_D032": "不不，没什么。",
                "0_D033": "好了，继续发掘调查吧！",
                "0_D034": "……应该已经回归，尘土了……"
            }
        },
        "0_D040": {
            "name": "连续武剧神话 密西西比·神话编撰者们 14",
            "text": {
                "0_D040": "记得要仔细检查发掘状况哦。",
                "0_D041": "还得考虑有限的交付时间和预算才行。"
            }
        },
        "0_D050": {
            "name": "连续武剧神话 密西西比·神话编撰者们 15",
            "text": {
                "0_D050": "发掘的进度如何？",
                "0_D051": "稍微确认一下地图吧。"
            }
        },
        "0_D060": {
            "name": "连续武剧神话 密西西比·神话编撰者们 16",
            "text": {
                "0_D060": "这片土地上，还沉睡着很多很多隐藏起来的神秘吧……",
                "0_D061": "对我来说，这也是一次难能可贵的体验。"
            }
        },
        "0_D070": {
            "name": "连续武剧神话 密西西比·神话编撰者们 17",
            "text": {
                "0_D070": "发掘调查辛苦啦。",
                "0_D071": "多亏了御主的努力，我们找到了很多珍贵的东西哦。",
                "0_D072": "在今后的旅途中肯定也能派上用处吧。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "感谢您平日里的关照～！",
                "0_G011": "今后也请多多支持我哦～！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "若想成为总统，大家的一票是非常重要的！",
                "0_G021": "为成为最强肌肉发达的强壮班扬，大家一定要多多投票Clementine！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "多亏了大家的声援，新服装做好了哦～！",
                "0_G031": "身高是不是也稍微长高了一点呢？",
                "0_G032": "可爱吗？　可爱吗？",
                "0_G033": "可以再多夸奖我一点哦！"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "又变大了……",
                "0_G111": "应该变大了吧……？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "御主，今天是你的生日吧。",
                "0_H011": "生日直播在哪里办？",
                "0_H012": "哎，没有这种安排？　也不会贩卖纪念商品？",
                "0_H013": "怎么能这样，粉丝可是很渴望为自己主推的人花钱哦。",
                "0_H014": "现在也不晚，去准备吧，我也会作为嘉宾为你唱歌的啦！",
                "0_H015": "Happy Birthday 御主♪ 哎嘿嘿。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举办什么活动。",
                "0_H021": "这或许是个增加伙伴的机会！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "御主，做好出击的准备了吗？",
                "0_H031": "注意安全第一，小心不要受伤了哦！",
                "0_H032": "那么，一路走好！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "作为氏族的领袖，受到众多同伴仰慕的我，会将御主培养成独当一面的领袖哦。",
                "0_H041": "你完全可以称呼我为老爷或是导演哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "氏族领袖的宗母及支撑她的同伴们，就像是偶像和宅的关系。",
                "0_H051": "御主作为我氏族的一员，必须要推我才行哦！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "隶属我氏族的愉快的伙伴们～，我最爱你们了哦！",
                "0_H061": "我能走到这一步，都是多亏了大家的支持！",
                "0_H062": "希望大家今后也能继续为我声援！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西呀，其实没什么。",
                "0_H071": "连一直不停喷我的黑子们，我也很感谢他们能一直关注我哦，是真的。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "如果能用圣杯实现愿望，希望能洗刷我背负的所有的罪过……",
                "0_H081": "包括网上的所有负面热搜记录……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "Salut（哟）！",
                "0_H091": "御主，班扬我今天也精力充沛哦！",
                "0_H092": "看着我，御主也要打起精神来哦。",
                "0_H093": "哎嘿嘿，我可爱吗？",
                "0_H094": "……很烦？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "开拓？",
                "0_H101": "这虽然也很重要……",
                "0_H102": "但一看到美丽的大自然，难道不会觉得肆意践踏是不好的行为吗？",
                "0_H103": "与自然共同生存才是人类正确的生存方式吧。",
                "0_H104": "……决不是因为觉得开拓太麻烦了哦？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "迈基呀，生前常说，老子是短吻鳄，之类的话。",
                "0_H111": "所以将他变成这样了哦～，很可爱吧！",
                "0_H112": "不知道意识有没有留下来呢？",
                "0_H113": "虽然没有应该比较幸福吧。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我在想新的宝具，没有什么合适的词汇吗。",
                "0_H121": "用德语或拉丁语，选些意义不明但很帅的比较好呢……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "看到御主，我突然意识到了，可以向御主学习的事还有很多很多。",
                "0_H131": "御主才是天生的骗人精。",
                "0_H132": "我也会努力成为御主这样擅长掌握人心随意操控同伴的人！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "原型班扬……",
                "0_H171": "我竟然也有如此纯真的时候啊……",
                "0_H172": "看不下去了。",
                "0_H173": "分明比我年幼……",
                "0_H174": "可为何比我还要高大呢……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "哎……灾星简？",
                "0_H181": "不会吧……因为简应该更为……",
                "0_H182": "有打扮得那么色情吗……"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "果然……",
                "0_H191": "感觉很聪明的人基本都戴眼镜。",
                "0_H192": "太好了，我也戴着……"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "Evil Wind……！？",
                "0_H201": "……能理解。"
            }
        },
        "0_N010": {
            "name": "连续武剧神话 密西西比·神话编撰者们 1",
            "text": {
                "0_N010": "欢迎光临，我的伙伴（employé）。",
                "0_N011": "这里是超级班扬的官方特选商店。",
                "0_N012": "有很多推荐的商品哦。",
                "0_N013": "一定要加油拿到全部～哦。",
                "0_N014": "和我约好了哦？"
            }
        },
        "0_N020": {
            "name": "连续武剧神话 密西西比·神话编撰者们 2",
            "text": {
                "0_N020": "嗨，直播现在开始，『向班扬商店说你好』。",
                "0_N021": "客人很快就上门啦，是御～主～哦～！",
                "0_N022": "好了，请关注，御主究竟会选什么呢！",
                "0_N023": "啊，御主，请不用在意我，表现得自然一点哦。",
                "0_N024": "哎？　未经允许不能擅自直播？",
                "0_N025": "这里显示着摄像机运转中啊？"
            }
        },
        "0_N030": {
            "name": "连续武剧神话 密西西比·神话编撰者们 3",
            "text": {
                "0_N030": "又来了呀。",
                "0_N031": "你能那么努力，我也很高兴哦。",
                "0_N032": "现在那么忙，在这里看店真的没问题吗？",
                "0_N033": "没问题啦，这也是领袖重要的工作。",
                "0_N034": "我会守望着御主的表现，希望你能多来看我几次。"
            }
        },
        "0_N040": {
            "name": "连续武剧神话 密西西比·神话编撰者们 4",
            "text": {
                "0_N040": "我在想新作电影的企划……",
                "0_N041": "你觉得以小商店作为舞台如何？",
                "0_N042": "那里摆放着各种奇妙的道具，获得的人会卷入各种事件。",
                "0_N043": "甚至还会经历可怕～的遭遇哦……哼～哼～哼～",
                "0_N044": "啊，不过这家商店没有这种东西哦？",
                "0_N045": "大概吧……"
            }
        },
        "0_N050": {
            "name": "连续武剧神话 密西西比·神话编撰者们 5",
            "text": {
                "0_N050": "理应确认帮助信息啊、通知啊、用户须知之类的，是读写能力卓越的我的作风哦。",
                "0_N051": "你也应该尽快习惯。"
            }
        },
        "0_N060": {
            "name": "连续武剧神话 密西西比·神话编撰者们 6",
            "text": {
                "0_N060": "我们的故事到此告一段落。",
                "0_N061": "但是，御主的旅程还在持续吧。",
                "0_N062": "我其实也一样。我打算开始什么新的事业哦。我觉得这就是领袖的职责。",
                "0_N063": "……但是，如果，还能在别处见面的话。",
                "0_N064": "届时，我会为了你而全力以赴。",
                "0_N065": "所以……请不要忘记哦。",
                "0_N066": "我一定会遵守约定的。"
            }
        },
        "0_N070": {
            "name": "连续武剧神话 密西西比·神话编撰者们 7",
            "text": {
                "0_N070": "怎么样？　我稍微改变了一下形象哦。",
                "0_N071": "为了应对即将到来的总统选举，我打算提升一下自己的形象。",
                "0_N072": "和过去的我哪种比较好？",
                "0_N073": "哎？　都很可爱？",
                "0_N074": "那当然啦！"
            }
        },
        "0_N150": {
            "name": "连续武剧神话 密西西比·神话编撰者们 8",
            "text": {
                "0_N150": "我也来！"
            }
        },
        "0_N160": {
            "name": "连续武剧神话 密西西比·神话编撰者们 9",
            "text": {
                "0_N160": "一起加油吧。"
            }
        },
        "0_N170": {
            "name": "连续武剧神话 密西西比·神话编撰者们 10",
            "text": {
                "0_N170": "这个是慰问品。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "Enchantée（很高兴认识你）！",
                "0_S011": "从者，保罗·班扬，作为他人格登场。",
                "0_S012": "对于作为超强额外职阶现界的我，",
                "0_S013": "您可以叫我Superieur（超级）班扬，或是Nouvelle（全新）班扬哦！",
                "0_S014": "快看这以接近全盛期形象现界的强壮肉体！",
                "0_S015": "……咦？",
                "0_S016": "总觉得，身高……分明长高了，却缩小了？"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "能行啦。 一路走好～！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "住，住手……呜哇！ 飞吧~！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "一！ 二！ 哎～！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "让开让开～！",
                "2_B041": "撞上去撞上去～！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "伟大的神秘啊，赐予我力量！",
                "2_B051": "哇！　哇！　哇——！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "总统的命令必须服从！"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "变得了不起了！"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "你有选举权吗？"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "大家的总统来啦！"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "不是属于某个人的，而是大家的土地哦！"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "有没有喜欢上我？"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "果然，光靠我这样的……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "任期刚过半，精已疲力已竭……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "好！"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "要上了哦！"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "交给我吧！"
            }
        },
        "2_B2010": {
            "name": "攻击 10",
            "text": {
                "2_B2010": "逮捕啦逮捕！！ 哇～！！ 受不了……"
            }
        },
        "2_B2080": {
            "name": "技能 7",
            "text": {
                "2_B2080": "你就是明星！"
            }
        },
        "2_B2090": {
            "name": "技能 8",
            "text": {
                "2_B2090": "对罪犯无需手下留情！"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "全体起立，总统要说一句话。"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "确定当选！　胜利演说！"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "派对派对！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "嘿呀。 看招！　咚！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "哼！ 切成两半！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "动手！ 嘿呀～！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "燃烧殆尽！",
                "2_B441": "压死他们！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "大家要友爱相处哦！"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "我要按下按钮了哦！"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "我，我对刷日常本有点……"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "下次记得要用一下那位班扬哦……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "开拓地是永恒的！",
                "2_B801": "咿，咿呀——！"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "打飞吧，王国到来！"
            }
        },
        "2_B880": {
            "name": "技能 5",
            "text": {
                "2_B880": "奶奶的小知识！"
            }
        },
        "2_B890": {
            "name": "技能 6",
            "text": {
                "2_B890": "奶奶的味道！"
            }
        },
        "2_B910": {
            "name": "攻击 7",
            "text": {
                "2_B910": "就是现在！ 嘿呀～！"
            }
        },
        "2_B920": {
            "name": "攻击 8",
            "text": {
                "2_B920": "好，Action！！　Cut！！"
            }
        },
        "2_B930": {
            "name": "攻击 9",
            "text": {
                "2_B930": "鲨鱼！ 龙卷！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "支持力上升！"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "历史性大胜利！　保罗·班扬，终于当选为总统了！",
                "2_G041": "竟然变得那么优秀……",
                "2_G042": "呃，哎！？",
                "2_G043": "稍微等一下……不知道是不是我的错觉，总觉得，变得，更更，更小了……",
                "2_G044": "是错觉吗……是错觉吧……"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "谢谢你，御主。多亏御主的培养，我觉得自己成了个杰出的总统。",
                "2_G051": "虽然尺寸只有这么大，但是心胸巨大的肌肉班扬哦！",
                "2_G052": "只要还在总统任期内，就会为御主而努力哦！",
                "2_G053": "唯有不断前进！"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "顺利提升了呢……未来可期呢！"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "我是总统……",
                "2_H011": "既然身为总统，是不准在选举期间向选民送礼的……",
                "2_H012": "所以，或许无法取代礼物……",
                "2_H013": "但我会拥抱御主。",
                "2_H014": "抱紧紧！"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "正在举办活动？　不需要我在开幕时发表演说吗？"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "为了备战下次选举，我始终要为粉丝提供热情的服务才行。",
                "2_H031": "好了，让我们出发前往现场吧～！"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "御主，既然身为总统的御主，说明就是个在暗中支配国家的影之黑幕吧…… 绝对是坏人呢。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "御主是我的头号粉丝吧。",
                "2_H051": "今后也要最推我哦。",
                "2_H052": "无论疾病，还是健康哦！"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "喜欢的东西？　当然是烤薄饼啦！",
                "2_H061": "在上面浇满枫糖浆哦。",
                "2_H062": "浇到像焦溜芙蓉蟹那样的程度哦。",
                "2_H063": "然后等浸透了之后哦，不要切开直接吃……",
                "2_H064": "哎？　我，我当然也很喜欢大家的笑容哦！"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西呀，是黑子哦。",
                "2_H071": "我，我不害怕哦。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "希望总有一天，我们居住在大地上的人们能彼此尊重，形成美丽的拼图……",
                "2_H081": "啊，这点事没有圣杯也能实现吧。"
            }
        },
        "2_H10090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H10090": "骑兵和暗杀者那两个为什么会和我混在一起被召唤呢……？",
                "2_H10091": "分明是毫无关系的英灵。",
                "2_H10092": "难不成我们在某个世界有什么联系？",
                "2_H10093": "虽然有点在意……但感觉还是不知道比较幸福。"
            }
        },
        "2_H10100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H10100": "看到贝比进化的形态了吗？",
                "2_H10101": "在近代化不断推进的世界中，还是没能赢过机动化的浪潮呢……",
                "2_H10102": "虽然已找不到牛犊时的印象……",
                "2_H10103": "但放心吧，这是用有利于环境的清洁能源驱动的哦。"
            }
        },
        "2_H10110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H10110": "骑兵和暗杀者的真名？",
                "2_H10111": "这个呀，好像必须保密才行……",
                "2_H10112": "但差不多可以告诉你了吧。",
                "2_H10113": "其实呀……",
                "2_H10114": "（叮咚）",
                "2_H10115": "哎呀？　好像有人来了……"
            }
        },
        "2_H10120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H10120": "现在我的样子，和再临前我的样子，御主喜欢哪一种……？",
                "2_H10121": "两种都喜欢？",
                "2_H10122": "是，是吗～，哎嘿嘿……"
            }
        },
        "2_H10130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H10130": "想让这个世界变好，或是让纷争消失这种事，我知道站在我这个立场上也没有什么说服力……",
                "2_H10131": "即便如此，我也希望帮助想要夺回世界的御主。",
                "2_H10132": "有什么我能做的事吗？"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "结果还是和原型班扬年龄层一致了……",
                "2_H171": "就像双胞胎姐妹？",
                "2_H172": "但我更可靠，所以我是姐姐哦！",
                "2_H173": "双层床也是我用上层哦！"
            }
        },
        "2_H210": {
            "name": "对话 5",
            "text": {
                "2_H210": "那个狮子人也是总统吗？",
                "2_H211": "唔……究竟是为什么呢，一看到那个人，内心就会涌现强烈的恨意……",
                "2_H212": "我身体里的某种存在……正呢喃着要向那个人复仇……",
                "2_H213": "这是骑兵的感情？"
            }
        },
        "2_H220": {
            "name": "对话 6",
            "text": {
                "2_H220": "我，我不是什么小孩子，才不会和小孩子们玩耍哦！",
                "2_H221": "我只是陪她们玩耍而已哦！"
            }
        },
        "2_H240": {
            "name": "对话 7",
            "text": {
                "2_H240": "别以为我会满足于此哦。",
                "2_H241": "虽然现在以如此不完全的形式来此叨扰，但这只不过是露个脸而已……",
                "2_H242": "我迟早会被单独召唤获得精彩表现，届时还请多关照啦！"
            }
        },
        "2_H250": {
            "name": "对话 8",
            "text": {
                "2_H250": "为什么，为什么像我这样伟大的英雄会以如此不完全的状态被召唤啊……！",
                "2_H251": "究竟出了什么问题，不能接受……！",
                "2_H252": "别以为能就此了结哦，我一定会好好算这笔账的！"
            }
        }
    },
    "ChrVoice_ids": [
        "1001300",
        "109823950",
        "109823951",
        "109823952"
    ]
}
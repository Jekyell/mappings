        "2_B500": {
            "name": "開始 3",
            "text": {
                "2_B500": "请随意"
            }
        },
        "2_B510": {
            "name": "開始 4",
            "text": {
                "2_B510": "嗯，好的"
            }
        },
        "2_B520": {
            "name": "勝利 3",
            "text": {
                "2_B520": "昂·巴拉达汉德梅·温"
            }
        },
        "2_B530": {
            "name": "勝利 4",
            "text": {
                "2_B530": "那么"
            }
        },
        "2_B540": {
            "name": "ダメージ 4",
            "text": {
                "2_B540": "啊……"
            }
        },
        "2_B550": {
            "name": "ダメージ 3",
            "text": {
                "2_B550": "这样……啊……"
            }
        },
        "2_B560": {
            "name": "戦闘不能 3",
            "text": {
                "2_B560": "呜呼……这是……"
            }
        },
        "2_B570": {
            "name": "戦闘不能 4",
            "text": {
                "2_B570": "原来如此……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "南无大慈大悲、",
                "2_B801": "观世音菩萨。",
                "2_B802": "唵・阿噜力迦・娑婆诃"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "美丽之物，",
                "2_B811": "不，一切事物，",
                "2_B812": "都如这樱花一般，",
                "2_B813": "终将虚幻地凋零。",
                "2_B814": "而在那之后……"
            }
        },
        "2_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "2_B840": "终将抵达极乐净土"
            }
        },
        "2_B880": {
            "name": "スキル 5",
            "text": {
                "2_B880": "向人类，施以救赎"
            }
        },
        "2_B890": {
            "name": "スキル 6",
            "text": {
                "2_B890": "怜悯"
            }
        },
        "2_B910": {
            "name": "アタック 7",
            "text": {
                "2_B910": "降伏恶魔"
            }
        },
        "2_B920": {
            "name": "アタック 8",
            "text": {
                "2_B920": "身负之罪，当以烈火净除"
            }
        },
        "2_B930": {
            "name": "アタック 9",
            "text": {
                "2_B930": "愚昧之泪，沾湿衣袖"
            }
        },
        "2_G010": {
            "name": "レベルアップ 1",
            "text": {
                "2_G010": "呵呵"
            }
        },
        "2_G040": {
            "name": "霊基再臨 1",
            "text": {
                "2_G040": "南无大慈大悲观世音菩萨。愿以此功德回向众生。",
                "2_G041": ""
            }
        },
        "2_G050": {
            "name": "霊基再臨 2",
            "text": {
                "2_G050": "昂·巴拉达汉德梅·温",
                "2_G051": ""
            }
        },
        "2_G110": {
            "name": "レベルアップ 2",
            "text": {
                "2_G110": "这样啊"
            }
        },
        "2_G120": {
            "name": "レベルアップ 3",
            "text": {
                "2_G120": "嗯"
            }
        },
        "2_H010": {
            "name": "誕生日",
            "text": {
                "2_H010": "祝您生日快乐！……啊，本来是想为您献上一首和歌的。实在抱歉。这个嘛，因为太过投入……。所以画好了一本分镜……。啊，是的。分镜。以生日为题材，少女漫画的分镜稿一本……",
                "2_H011": "",
                "2_H012": "",
                "2_H013": "",
                "2_H014": "",
                "2_H015": "",
                "2_H016": ""
            }
        },
        "2_H020": {
            "name": "イベント開催中",
            "text": {
                "2_H020": "有什么发现吗？",
                "2_H021": ""
            }
        },
        "2_H030": {
            "name": "会話 1",
            "text": {
                "2_H030": "呵呵呵呵"
            }
        },
        "2_H040": {
            "name": "会話 2",
            "text": {
                "2_H040": "怎么了？"
            }
        },
        "2_H050": {
            "name": "会話 3",
            "text": {
                "2_H050": "真是勤奋呢。这是好事。",
                "2_H051": ""
            }
        },
        "2_H060": {
            "name": "好きなこと",
            "text": {
                "2_H060": "南无大慈大悲观世音菩萨"
            }
        },
        "2_H070": {
            "name": "嫌いなこと",
            "text": {
                "2_H070": "无论哪个时代，都有无法被拯救的存在。真是悲哀啊。",
                "2_H071": ""
            }
        },
        "2_H080": {
            "name": "聖杯について",
            "text": {
                "2_H080": "圣杯？啊，如意宝珠。想要吗？",
                "2_H081": "",
                "2_H082": ""
            }
        },
        "2_H20090": {
            "name": "絆Lv.1",
            "text": {
                "2_H20090": "呵呵"
            }
        },
        "2_H20100": {
            "name": "絆Lv.2",
            "text": {
                "2_H20100": "有什么？发生了什么变化吗？",
                "2_H20101": ""
            }
        },
        "2_H20110": {
            "name": "絆Lv.3",
            "text": {
                "2_H20110": "是的。灵基似乎发生了变化。虽然只是些微的调整。",
                "2_H20111": "",
                "2_H20112": ""
            }
        },
        "2_H20120": {
            "name": "絆Lv.4",
            "text": {
                "2_H20120": "南无大慈大悲观世音菩萨。予人以救赎，赐人以慈悲。唵·阿噜力迦·娑婆诃。愿汝亦得。",
                "2_H20121": "",
                "2_H20122": "",
                "2_H20123": ""
            }
        },
        "2_H20130": {
            "name": "絆Lv.5",
            "text": {
                "2_H20130": "这副姿态，啊啊，是这么回事呢。父亲大人的声音，已经听不到了。会感到寂寞，是否说明我还不够成熟呢？",
                "2_H20131": "",
                "2_H20132": "",
                "2_H20133": ""
            }
        },
        "2_H290": {
            "name": "会話 4",
            "text": {
                "2_H290": "哎呀。那位大人……原来如此，原来如此。大陆的三藏法师。呵呵呵",
                "2_H291": "",
                "2_H292": "",
                "2_H293": ""
            }
        }
    },

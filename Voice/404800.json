{
    "id": 404800,
    "collectionNo": 349,
    "name": "曲亭马琴",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "迅捷如慧箭！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "嘿，噢，得手了！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "有如旋风卷沙石！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "小的们！　出来集合出来集合～～！！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "仁义礼智，忠信孝悌，",
                "0_B051": "八颗玉珠集于空中，",
                "0_B052": "割云唤风，此刻终知人之诚！",
                "0_B053": "此乃撕裂黑暗映照道路之月光！",
                "0_B054": "南总！　里见，八犬传！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "呼～，老天保佑，老天保佑。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "万万不可懈怠。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "好好见识一下老夫的“剑术拳法”吧！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "好了，来适应一下笔吧！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "真是太精彩了！　可喜可贺！　可喜可贺！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "此事到这告一段落！　呵呵呵呵！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "柔柳……不为雪折！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "别了！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "现羞愧不已……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "懂了！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "明白了！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "不会吧！？"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "时刻已到！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "生命无可替代。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "坏蛋是这家伙也～！"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "道大现庄小文毛野信乃亲兵卫！！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "逃也休想逃！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "神仙戏法飘然之蝶！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "给我退下！　这样那样！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "先问问这把刀愿不愿意！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "成败皆有犬注定！　仔细看好便是！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "干脆放肆一点吧！？"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "祥瑞祥瑞！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "笔之表现乃东都·曲亭马琴！　还请记住！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "还请各位多多手下留情……"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "记住了吗！　此乃破邪显正的笔之技！"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "还请您谅解哦？"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "这点小事！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "过于……大意了……"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "实在是没有脸见人了……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "既然如此……抱歉！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "仁义礼智，忠信孝悌，",
                "0_B801": "八道灵光划过天际，",
                "0_B802": "心中此后具勇气！",
                "0_B803": "德泽犹如阵雨之滋润……",
                "0_B804": "南总！　里见，八犬传！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "看啊，援兵到来了哦！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "如猛禽击燕雀！"
            }
        },
        "0_E010": {
            "name": "南溟弓张八犬传 8",
            "text": {
                "0_E010": "来吧，犬士们，该工作了哦。",
                "0_E011": "……喂喂，那是御主大人的鞋子，快还给人家。"
            }
        },
        "0_E020": {
            "name": "南溟弓张八犬传 9",
            "text": {
                "0_E020": "外出的时候路上小心哦。"
            }
        },
        "0_E040": {
            "name": "南溟弓张八犬传 10",
            "text": {
                "0_E040": "公告……",
                "0_E041": "不，通知您已经看过了吗？",
                "0_E042": "有备无患！"
            }
        },
        "0_E050": {
            "name": "南溟弓张八犬传 11",
            "text": {
                "0_E050": "○月○日 晴天",
                "0_E051": "御主大人来访。",
                "0_E052": "聊了一会儿天，和犬士们玩了一会儿。",
                "0_E053": "吃了点粟米年糕、冰水、蒸蛋羹等填饱肚子。",
                "0_E054": "粗茶淡饭多有怠慢。"
            }
        },
        "0_E060": {
            "name": "南溟弓张八犬传 12",
            "text": {
                "0_E060": "仁义礼智，忠信孝悌。",
                "0_E061": "里见的小小犬士们有没有帮上您的忙呢？",
                "0_E062": "若有缘分，愿能再见。",
                "0_E063": "就此告辞。"
            }
        },
        "0_E070": {
            "name": "南溟弓张八犬传 13",
            "text": {
                "0_E070": "天气已经进入盛夏，是去海边游泳的季节了呢。",
                "0_E071": "听说阿荣小姐还带着引以为傲的泳装。",
                "0_E072": "多少有点羡慕呢。",
                "0_E073": "话虽如此，我也不会游泳……",
                "0_E074": "狗刨式的话，勉强还行吧。"
            }
        },
        "0_E090": {
            "name": "南溟弓张八犬传 14",
            "text": {
                "0_E090": "哟哟。",
                "0_E091": "竟然会让老夫来看店？",
                "0_E092": "还真是余韵面子毁于一旦呢！　哈哈哈哈哈！",
                "0_E093": "……唉～。",
                "0_E094": "都是因为我调侃铁藏“你这卖辣椒粉的”的事遭报应了吗……？"
            }
        },
        "0_E100": {
            "name": "南溟弓张八犬传 15",
            "text": {
                "0_E100": "好，既然受人之托，那也唯有干了。",
                "0_E101": "让老夫想起自己在茑屋当掌柜时的事了。",
                "0_E102": "话说可以在这个货架上放老夫的读本和自家制的药丸……",
                "0_E103": "不行啊，是吗。",
                "0_E104": "来块小招牌的话……",
                "0_E105": "这也禁止吗，是吗。"
            }
        },
        "0_E110": {
            "name": "南溟弓张八犬传 16",
            "text": {
                "0_E110": "兑换物品吗。",
                "0_E111": "等下等下，老夫戴副眼镜——",
                "0_E112": "哎哟哟，压根不需要这种准备！",
                "0_E113": "哈哈哈！　老夫可不是什么老头哦？"
            }
        },
        "0_E120": {
            "name": "南溟弓张八犬传 17",
            "text": {
                "0_E120": "好，老夫来吧。",
                "0_E121": "要确认清楚哦。"
            }
        },
        "0_E130": {
            "name": "南溟弓张八犬传 18",
            "text": {
                "0_E130": "这个和那个……这个吗。",
                "0_E131": "唔？　美少年折扣……",
                "0_E132": "这标价牌什么时候挂上的？"
            }
        },
        "0_E140": {
            "name": "南溟弓张八犬传 19",
            "text": {
                "0_E140": "时间过得真快呢，店快要关门了。",
                "0_E141": "短时间内还接受兑换的要求，记得尽快兑换哦。",
                "0_E142": "感谢您的惠顾。"
            }
        },
        "0_E150": {
            "name": "南溟弓张八犬传 20",
            "text": {
                "0_E150": "老夫的号原本叫“著作堂主人”。",
                "0_E151": "虽然“曲亭马琴”现在光为人们所知，但这只是半开玩笑取的笔名，是所谓的“戏号”。",
                "0_E152": "现在无论哪个都是重要的名字。",
                "0_E153": "其他还有“蓑笠”“玄同”“信天翁”等等。",
                "0_E154": "……不过，也有些取了大量雅号然后贱卖的不成体统的家伙呢。"
            }
        },
        "0_E160": {
            "name": "南溟弓张八犬传 21",
            "text": {
                "0_E160": "出发上战场吧，八犬士！"
            }
        },
        "0_E170": {
            "name": "南溟弓张八犬传 22",
            "text": {
                "0_E170": "唔嗯。",
                "0_E171": "虽然都是些小狗，但大家都相当英勇。",
                "0_E172": "是伊势参拜的保佑犬，还是金刀比罗狗呢？",
                "0_E173": "尽量不要绕弯路哦。"
            }
        },
        "0_E180": {
            "name": "南溟弓张八犬传 23",
            "text": {
                "0_E180": "有时间的话记得读一下帮助信息哦。",
                "0_E181": "这样就能保证百战不殆了。",
                "0_E182": "……不，百战还是太多了吧？"
            }
        },
        "0_E190": {
            "name": "南溟弓张八犬传 24",
            "text": {
                "0_E190": "问老夫为什么不变成伏姬？",
                "0_E191": "……这问题还真是奇怪。",
                "0_E192": "老夫的再临只到第二为止吧？　咦？"
            }
        },
        "0_E200": {
            "name": "南溟弓张八犬传 25",
            "text": {
                "0_E200": "唔哼。",
                "0_E201": "弓箭乃是武士之翼。",
                "0_E202": "与之对应，作家之笔虽无法触及天空，但能够作为大海中航行小舟之船桨。",
                "0_E203": "汝前行方向同为自己无法左右的大海，但愿这趟航行能令人神清气爽。",
                "0_E204": "那么，再见了。"
            }
        },
        "0_E210": {
            "name": "南溟弓张八犬传 26",
            "text": {
                "0_E210": "主人阁下自不用说，犬士们也十分努力呢！",
                "0_E211": "得好好犒劳犒劳才行。",
                "0_E212": "……话说，为何它们不是挥刀的剑士，不是成年犬，而是幼犬呢？",
                "0_E213": "只能说是从者的奥妙之处了……",
                "0_E214": "先不说这些了，",
                "0_E215": "据说老夫去世后，发行了将『八犬传』的汉字「平假名」化，方便所有人阅读的『假名读八犬传』的书籍，并博得了人气——",
                "0_E216": "这本书或许与「泷泽琴童」，也就是儿媳妇阿路有密切的关系呢……"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "感谢您的拔刀相助！"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "嗯，可喜可贺！"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "我不会忘记您的恩情。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "御主阁下诞生之日啊？",
                "0_H011": "噢噢，可喜可贺！　嗯～～，可喜可贺！　应该将全体人员一个不剩全部召集来，举办祝贺宴会才对！",
                "0_H012": "然后哦……能否将老夫的读本放在一边呢？",
                "0_H013": "预料到会遇到这种可喜之事的老夫偷偷撰写并刚完成了“新刊”哦！",
                "0_H014": "当然，可以给御主阁下打个八折，",
                "0_H015": "咕嘎——",
                "0_H016": "咳咳，当然会谨赠给您作为贺礼。",
                "0_H017": "祝您生日快乐。",
                "0_H018": "祝您接下来的一年也能身体健康。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "出人意料的活动吗！",
                "0_H021": "这些暂且不论！　既然有赚钱的机会就另当别论了！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "来，要出驾了吗！",
                "0_H031": "轿子！　抬轿子来！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "贵人就要有贵人的样子，挺胸抬头精神振作起来。",
                "0_H041": "做不到的话……装作没看到也是一种礼仪。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主阁下是有资格让老夫马琴侍奉的大人！　您大可自信！",
                "0_H051": "从者不择主？",
                "0_H052": "不，老夫决不这么认为哦！？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "其实老夫最为喜欢的是鸟哦。",
                "0_H061": "算下来一共养了六十多种，甚至曾一度养过一百只哦。",
                "0_H062": "一辈子在一起的金丝雀给老夫留下的印象最为深刻。",
                "0_H063": "听说南蛮话的金丝雀翻译过来意思是狗哦。",
                "0_H064": "呵呵，这也是所谓的“名诠自性”……奇妙的缘分呢。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？　除了铁藏吗？",
                "0_H071": "唔唔唔……应该是事不如愿吧。",
                "0_H072": "老夫根本不喜欢预料之外的灾祸！　著作中的错字更是不可理喻！",
                "0_H073": "更不喜欢连遇到这种意外的灾祸都能乐在其中，甚至将其作为绘画题材的区区铁藏的脾气！　嘎啊！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯？",
                "0_H082": "哎哎，现在不是管这些的时候！",
                "0_H083": "喂！　亲兵卫、道节！　恶作剧也要适可而止！",
                "0_H084": "毛野、信乃，你俩怎么又开始打了……",
                "0_H085": "小文吾一直在吃个不停。",
                "0_H086": "唔，大角真是个好孩子。你去叫醒现八和庄助。",
                "0_H087": "噢噢……？　御主阁下开始撸狗了……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "老夫时不时引用的“名诠自性”，简单说，就是『名字显自性』。",
                "0_H091": "再换一种说法，就是每个人都有上天赐予的“命运”。",
                "0_H092": "御主阁下自身的名字亦然。您不妨重新仔细思考一下。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "特别喜欢摆架子，顽固难以亲近——",
                "0_H101": "老夫早就习惯这种在人背后说的闲话了。",
                "0_H102": "与生俱来的性子可没有那么容易改变，哈哈哈！",
                "0_H103": "……但是，至少要自我克制，不能一味依赖御主阁下的恩情才行。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "唰唰唰……",
                "0_H111": "嗯，这是日记！",
                "0_H112": "一天不漏每天都要写！　这是身为泷泽家人的任务！",
                "0_H113": "御主阁下等同于老夫的家人，给您阅览倒也无妨。",
                "0_H114": "只不过，里面都是些和令人激动人心的通俗小说截然相反的平平无奇的事。",
                "0_H115": "前往何处、遇见何人、饮食内容、购物、天气、饲养鸟儿的状态，等等。",
                "0_H116": "虽然最近的内容基本都是八犬士们调皮捣蛋，以及御主阁下的日常生活……",
                "0_H117": "总不能始终是这么平静的实情呢！　哈哈哈！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "一目了然——",
                "0_H121": "这身体是属于路的。",
                "0_H122": "她是老夫儿子宗波的老婆，路这个名字也是嫁过来之后老夫取的。",
                "0_H123": "老夫还真是让路承受了非比寻常的操劳呢……",
                "0_H124": "如果她没有成为年老失明的老夫之“眼”、持笔之“手”，『八犬传』是绝对无法完成的。",
                "0_H125": "正因为如此，老夫才明白会以这副身躯现界定然有其意义。",
                "0_H126": "虽然对不起路，但就让咱俩再次共乘一轿吧。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "老夫曾一度舍弃了武士的身份。",
                "0_H131": "正因为找到了值得侍奉之主，才总算能挽回几分武士的气节。",
                "0_H132": "倘若您依然愿意将老夫充满悔恨的人生一并接受下来的话……",
                "0_H133": "老夫定将全心全意为您效力。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "铁藏……",
                "0_H171": "我和“葛饰北斋”只能算是孽缘，不，算是紧密的恶缘吧！？",
                "0_H172": "哼！　一说到那个男人，只要老夫说右他就说左，说虎他说龙，大吼旋风他吼大浪！",
                "0_H173": "那个男人简直就像用老夫身上欠缺的部件捏出来的。",
                "0_H174": "北斋和老夫能一起顺利出版全套『椿说弓张月』都可称作江户第一奇迹了！",
                "0_H175": "结果咱俩在迦勒底竟然又当起了邻居……",
                "0_H176": "哎呀呀……哎呀呀……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "荣……阿荣啊？",
                "0_H181": "没错，就是指唠叨闹腾的你啦。",
                "0_H182": "等等等，等一下，新作品已经开始构想了。",
                "0_H183": "就算你两眼闪得像星星，一朝一夕也不可能完成哦？",
                "0_H184": "虽然同样需要借用阿路之手的老夫或许没有资格这么说……",
                "0_H185": "但老夫还是要问你。",
                "0_H186": "你真的满足于现状吗？",
                "0_H187": "“葛饰应为”已经完全是独当一面的浮世绘画师了哦？",
                "0_H188": "可以吗……是吗，那就行了！！",
                "0_H189": "……唔？　仙女？"
            }
        },
        "0_H200": {
            "name": "对话 6",
            "text": {
                "0_H200": "迦勒底也有文人同辈着实令人安心！",
                "0_H201": "与此同时，文人文豪尤其难对付！　老夫其实也正想以此戒啦，哈哈！",
                "0_H202": "噢，安徒生阁下……童话写手吗。",
                "0_H203": "老夫小说之师“山东京传”也曾投身于传说故事的创作。",
                "0_H204": "年轻时，老夫曾轻视过这东西，觉得只是面向儿童的东西，现在则能深刻体会其韵味。干脆试着创作一篇吧……",
                "0_H205": "很久很久以前，有一对老爷爷和老奶奶……"
            }
        },
        "0_H210": {
            "name": "对话 7",
            "text": {
                "0_H210": "哦！？　鬼女红叶吗！",
                "0_H211": "在咱们这边如果提到鬼女红叶？",
                "0_H212": "自然就是指老夫的草双纸『倾城水浒传』中对应“地周星·陈达”的女杰。",
                "0_H213": "那本来就是反转了一百零八豪杰的性别，全部变成女人的读物，别有一番风味哦！",
                "0_H214": "回头看来，感觉只要环顾迦勒底，到处都不缺倾城武将与骑士呢。",
                "0_H215": "哈哈哈哈哈！　和两百年相比一点都没变呢！？"
            }
        },
        "0_H220": {
            "name": "对话 8",
            "text": {
                "0_H220": "我打从心底里尊敬阿荣小姐……",
                "0_H221": "支持自己的爹爹，十分孝顺。",
                "0_H222": "和我这种一而再再而三被纠正错误，只能按照要求撰写的人大不相同。",
                "0_H223": "还为自己在做家务间隙学会写字而得意忘形，并沾沾自喜地自称“龙泽琴童”什么的，真是太羞耻了……唉……",
                "0_H224": "但是……无论多么辛苦，阿路也非常开心，非常骄傲哦？　公公。"
            }
        },
        "0_H230": {
            "name": "对话 9",
            "text": {
                "0_H230": "啊啊……",
                "0_H231": "美少年真美好。",
                "0_H232": "爱穿女装的美少年，走上邪路的美少年。",
                "0_H233": "啊啊～，令人无比心动。",
                "0_H234": "啊，这不过是指作品里的创作哦？",
                "0_H235": "但是……难得现界一次，如果公公能够继续撰写“那部”，阿路我会非常高兴……"
            }
        },
        "0_H290": {
            "name": "对话 10",
            "text": {
                "0_H290": "镇西八郎，源为朝阁下！",
                "0_H291": "没想到竟然是精致巧妙至极的“机关”，真是令人大吃一惊！",
                "0_H292": "着实，着实是实力超越传说的强者！",
                "0_H293": "然而，之所以情不自禁产生至亲之情，是小说作家的宿命吗……",
                "0_H294": "哎呀呀，真想见识令『椿说弓张月』相形见绌的冒险故事呢！"
            }
        },
        "0_I010": {
            "name": "南溟弓张八犬传 1",
            "text": {
                "0_I010": "欢迎光临！",
                "0_I011": "看店对阿路来说已是家常便饭。",
                "0_I012": "您有什么吩咐尽管提。"
            }
        },
        "0_I020": {
            "name": "南溟弓张八犬传 2",
            "text": {
                "0_I020": "对面胡同的狐仙先生～♪",
                "0_I021": "哎呀，是来兑换道具的呀。",
                "0_I022": "休息一会儿，要不要喝点茶吃点团子？",
                "0_I023": "呵呵呵。"
            }
        },
        "0_I030": {
            "name": "南溟弓张八犬传 3",
            "text": {
                "0_I030": "是兑换这件吧。",
                "0_I031": "给，路上小心。"
            }
        },
        "0_I040": {
            "name": "南溟弓张八犬传 4",
            "text": {
                "0_I040": "有没有想要的东西？",
                "0_I041": "那真是太好了。"
            }
        },
        "0_I050": {
            "name": "南溟弓张八犬传 5",
            "text": {
                "0_I050": "给，请收下。",
                "0_I051": "啊……有好多奇妙的东西呢……"
            }
        },
        "0_I060": {
            "name": "南溟弓张八犬传 6",
            "text": {
                "0_I060": "多亏了您的帮助，这家兑换所也差不多该卸下招牌门帘了。",
                "0_I061": "关店前请别忘了兑换哦。"
            }
        },
        "0_I070": {
            "name": "南溟弓张八犬传 7",
            "text": {
                "0_I070": "看店的工作是和公公齐心协力完成的，就像不断交换的两人一面，非常开心。",
                "0_I071": "御主大人每天的来访，也像是太郎的同心同事造访咱家那时一样，令我感到非常怀念。",
                "0_I072": "作为令人高兴的事，我要将这些记在咱家的日记里。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "骑兵，曲亭马琴！　初次见面！",
                "0_S011": "知吾者，其唯『八犬传』欤！",
                "0_S012": "不知吾者，其唯『八犬传』欤！",
                "0_S013": "离开了老夫之手的小说获得了属于自己的生命！",
                "0_S014": "……然而，老夫自己竟然会成为从者啊。",
                "0_S015": "哎呀，奇哉……奇哉！"
            }
        },
        "1_B010": {
            "name": "攻击 1",
            "text": {
                "1_B010": "迅捷如慧箭！"
            }
        },
        "1_B020": {
            "name": "攻击 2",
            "text": {
                "1_B020": "嘿，噢，得手了！"
            }
        },
        "1_B030": {
            "name": "攻击 3",
            "text": {
                "1_B030": "有如旋风卷沙石！"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "小的们！　出来集合出来集合～～！！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "仁义礼智，忠信孝悌，",
                "1_B051": "八颗玉珠集于空中，",
                "1_B052": "割云唤风，此刻终知人之诚！",
                "1_B053": "此乃撕裂黑暗映照道路之月光！",
                "1_B054": "南总！　里见，八犬传！"
            }
        },
        "1_B080": {
            "name": "技能 1",
            "text": {
                "1_B080": "呼～，老天保佑，老天保佑。"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "万万不可懈怠。"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "好好见识一下老夫的“剑术拳法”吧！"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "好了，来适应一下笔吧！"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "真是太精彩了！　可喜可贺！　可喜可贺！"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "此事到这告一段落！　呵呵呵呵！"
            }
        },
        "1_B150": {
            "name": "受击 1",
            "text": {
                "1_B150": "柔柳……不为雪折！"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "别了！"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "现羞愧不已……"
            }
        },
        "1_B180": {
            "name": "指令卡 1",
            "text": {
                "1_B180": "懂了！"
            }
        },
        "1_B190": {
            "name": "指令卡 2",
            "text": {
                "1_B190": "明白了！"
            }
        },
        "1_B200": {
            "name": "指令卡 3",
            "text": {
                "1_B200": "不会吧！？"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "时刻已到！"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "生命无可替代。"
            }
        },
        "1_B230": {
            "name": "宝具卡 3",
            "text": {
                "1_B230": "坏蛋是这家伙也～！"
            }
        },
        "1_B2440": {
            "name": "额外攻击 4",
            "text": {
                "1_B2440": "道大现庄小文毛野信乃亲兵卫！！"
            }
        },
        "1_B250": {
            "name": "宝具卡 4",
            "text": {
                "1_B250": "逃也休想逃！"
            }
        },
        "1_B410": {
            "name": "攻击 4",
            "text": {
                "1_B410": "神仙戏法飘然之蝶！"
            }
        },
        "1_B420": {
            "name": "攻击 5",
            "text": {
                "1_B420": "给我退下！　这样那样！"
            }
        },
        "1_B430": {
            "name": "攻击 6",
            "text": {
                "1_B430": "先问问这把刀愿不愿意！"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "成败皆有犬注定！　仔细看好便是！"
            }
        },
        "1_B480": {
            "name": "技能 3",
            "text": {
                "1_B480": "干脆放肆一点吧！？"
            }
        },
        "1_B490": {
            "name": "技能 4",
            "text": {
                "1_B490": "祥瑞祥瑞！"
            }
        },
        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "笔之表现乃东都·曲亭马琴！　还请记住！"
            }
        },
        "1_B520": {
            "name": "胜利 3",
            "text": {
                "1_B520": "记住了吗！　此乃破邪显正的笔之技！"
            }
        },
        "1_B540": {
            "name": "受击 4",
            "text": {
                "1_B540": "这点小事！"
            }
        },
        "1_B550": {
            "name": "受击 3",
            "text": {
                "1_B550": "过于……大意了……"
            }
        },
        "1_B560": {
            "name": "无法战斗 3",
            "text": {
                "1_B560": "实在是没有脸见人了……"
            }
        },
        "1_B570": {
            "name": "无法战斗 4",
            "text": {
                "1_B570": "既然如此……抱歉！"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "仁义礼智，忠信孝悌，",
                "1_B801": "八道灵光划过天际，",
                "1_B802": "心中此后具勇气！",
                "1_B803": "德泽犹如阵雨之滋润……",
                "1_B804": "南总！　里见，八犬传！"
            }
        },
        "1_B840": {
            "name": "额外攻击 3",
            "text": {
                "1_B840": "看啊，援兵到来了哦！"
            }
        },
        "1_B910": {
            "name": "攻击 7",
            "text": {
                "1_B910": "如猛禽击燕雀！"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "感谢您的拔刀相助！"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "又再临了吗！　值得庆贺！",
                "1_G021": "……话说回来，为何老夫也有这种狗耳朵和尾巴的仿制品？",
                "1_G022": "因为老夫看上去耳朵很灵敏？",
                "1_G023": "唔，这，这个嘛，倒是有点道理。",
                "1_G024": "总之尾巴很可爱？",
                "1_G025": "什，什么！？　不成体统！"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "哎呀哎呀，奇哉！",
                "1_G031": "灵基再临竟然是这样的！",
                "1_G032": "装束焕然一新，要绷紧神经才行！"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "嗯，可喜可贺！"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "御主阁下诞生之日啊？",
                "1_H011": "噢噢，可喜可贺！　嗯～～，可喜可贺！　应该将全体人员一个不剩全部召集来，举办祝贺宴会才对！",
                "1_H012": "然后哦……能否将老夫的读本放在一边呢？",
                "1_H013": "预料到会遇到这种可喜之事的老夫偷偷撰写并刚完成了“新刊”哦！",
                "1_H014": "当然，可以给御主阁下打个八折，",
                "1_H015": "咕嘎——",
                "1_H016": "咳咳，当然会谨赠给您作为贺礼。",
                "1_H017": "祝您生日快乐。",
                "1_H018": "祝您接下来的一年也能身体健康。"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "出人意料的活动吗！",
                "1_H021": "这些暂且不论！　既然有赚钱的机会就另当别论了！"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "来，要出驾了吗！",
                "1_H031": "轿子！　抬轿子来！"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "贵人就要有贵人的样子，挺胸抬头精神振作起来。",
                "1_H041": "做不到的话……装作没看到也是一种礼仪。"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "御主阁下是有资格让老夫马琴侍奉的大人！　您大可自信！",
                "1_H051": "从者不择主？",
                "1_H052": "不，老夫决不这么认为哦！？"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "其实老夫最为喜欢的是鸟哦。",
                "1_H061": "算下来一共养了六十多种，甚至曾一度养过一百只哦。",
                "1_H062": "一辈子在一起的金丝雀给老夫留下的印象最为深刻。",
                "1_H063": "听说南蛮话的金丝雀翻译过来意思是狗哦。",
                "1_H064": "呵呵，这也是所谓的“名诠自性”……奇妙的缘分呢。"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "讨厌的东西？　除了铁藏吗？",
                "1_H071": "唔唔唔……应该是事不如愿吧。",
                "1_H072": "老夫根本不喜欢预料之外的灾祸！　著作中的错字更是不可理喻！",
                "1_H073": "更不喜欢连遇到这种意外的灾祸都能乐在其中，甚至将其作为绘画题材的区区铁藏的脾气！　嘎啊！"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "圣杯？",
                "1_H082": "哎哎，现在不是管这些的时候！",
                "1_H083": "喂！　亲兵卫、道节！　恶作剧也要适可而止！",
                "1_H084": "毛野、信乃，你俩怎么又开始打了……",
                "1_H085": "小文吾一直在吃个不停。",
                "1_H086": "唔，大角真是个好孩子。你去叫醒现八和庄助。",
                "1_H087": "噢噢……？　御主阁下开始撸狗了……"
            }
        },
        "1_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H090": "老夫时不时引用的“名诠自性”，简单说，就是『名字显自性』。",
                "1_H091": "再换一种说法，就是每个人都有上天赐予的“命运”。",
                "1_H092": "御主阁下自身的名字亦然。您不妨重新仔细思考一下。"
            }
        },
        "1_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H100": "特别喜欢摆架子，顽固难以亲近——",
                "1_H101": "老夫早就习惯这种在人背后说的闲话了。",
                "1_H102": "与生俱来的性子可没有那么容易改变，哈哈哈！",
                "1_H103": "……但是，至少要自我克制，不能一味依赖御主阁下的恩情才行。"
            }
        },
        "1_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H110": "唰唰唰……",
                "1_H111": "嗯，这是日记！",
                "1_H112": "一天不漏每天都要写！　这是身为泷泽家人的任务！",
                "1_H113": "御主阁下等同于老夫的家人，给您阅览倒也无妨。",
                "1_H114": "只不过，里面都是些和令人激动人心的通俗小说截然相反的平平无奇的事。",
                "1_H115": "前往何处、遇见何人、饮食内容、购物、天气、饲养鸟儿的状态，等等。",
                "1_H116": "虽然最近的内容基本都是八犬士们调皮捣蛋，以及御主阁下的日常生活……",
                "1_H117": "总不能始终是这么平静的实情呢！　哈哈哈！"
            }
        },
        "1_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H120": "一目了然——",
                "1_H121": "这身体是属于路的。",
                "1_H122": "她是老夫儿子宗波的老婆，路这个名字也是嫁过来之后老夫取的。",
                "1_H123": "老夫还真是让路承受了非比寻常的操劳呢……",
                "1_H124": "如果她没有成为年老失明的老夫之“眼”、持笔之“手”，『八犬传』是绝对无法完成的。",
                "1_H125": "正因为如此，老夫才明白会以这副身躯现界定然有其意义。",
                "1_H126": "虽然对不起路，但就让咱俩再次共乘一轿吧。"
            }
        },
        "1_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H130": "老夫曾一度舍弃了武士的身份。",
                "1_H131": "正因为找到了值得侍奉之主，才总算能挽回几分武士的气节。",
                "1_H132": "倘若您依然愿意将老夫充满悔恨的人生一并接受下来的话……",
                "1_H133": "老夫定将全心全意为您效力。"
            }
        },
        "1_H170": {
            "name": "对话 4",
            "text": {
                "1_H170": "铁藏……",
                "1_H171": "我和“葛饰北斋”只能算是孽缘，不，算是紧密的恶缘吧！？",
                "1_H172": "哼！　一说到那个男人，只要老夫说右他就说左，说虎他说龙，大吼旋风他吼大浪！",
                "1_H173": "那个男人简直就像用老夫身上欠缺的部件捏出来的。",
                "1_H174": "北斋和老夫能一起顺利出版全套『椿说弓张月』都可称作江户第一奇迹了！",
                "1_H175": "结果咱俩在迦勒底竟然又当起了邻居……",
                "1_H176": "哎呀呀……哎呀呀……"
            }
        },
        "1_H180": {
            "name": "对话 5",
            "text": {
                "1_H180": "荣……阿荣啊？",
                "1_H181": "没错，就是指唠叨闹腾的你啦。",
                "1_H182": "等等等，等一下，新作品已经开始构想了。",
                "1_H183": "就算你两眼闪得像星星，一朝一夕也不可能完成哦？",
                "1_H184": "虽然同样需要借用阿路之手的老夫或许没有资格这么说……",
                "1_H185": "但老夫还是要问你。",
                "1_H186": "你真的满足于现状吗？",
                "1_H187": "“葛饰应为”已经完全是独当一面的浮世绘画师了哦？",
                "1_H188": "可以吗……是吗，那就行了！！",
                "1_H189": "……唔？　仙女？"
            }
        },
        "1_H200": {
            "name": "对话 6",
            "text": {
                "1_H200": "迦勒底也有文人同辈着实令人安心！",
                "1_H201": "与此同时，文人文豪尤其难对付！　老夫其实也正想以此戒啦，哈哈！",
                "1_H202": "噢，安徒生阁下……童话写手吗。",
                "1_H203": "老夫小说之师“山东京传”也曾投身于传说故事的创作。",
                "1_H204": "年轻时，老夫曾轻视过这东西，觉得只是面向儿童的东西，现在则能深刻体会其韵味。干脆试着创作一篇吧……",
                "1_H205": "很久很久以前，有一对老爷爷和老奶奶……"
            }
        },
        "1_H210": {
            "name": "对话 7",
            "text": {
                "1_H210": "哦！？　鬼女红叶吗！",
                "1_H211": "在咱们这边如果提到鬼女红叶？",
                "1_H212": "自然就是指老夫的草双纸『倾城水浒传』中对应“地周星·陈达”的女杰。",
                "1_H213": "那本来就是反转了一百零八豪杰的性别，全部变成女人的读物，别有一番风味哦！",
                "1_H214": "回头看来，感觉只要环顾迦勒底，到处都不缺倾城武将与骑士呢。",
                "1_H215": "哈哈哈哈哈！　和两百年相比一点都没变呢！？"
            }
        },
        "1_H290": {
            "name": "对话 8",
            "text": {
                "1_H290": "镇西八郎，源为朝阁下！",
                "1_H291": "没想到竟然是精致巧妙至极的“机关”，真是令人大吃一惊！",
                "1_H292": "着实，着实是实力超越传说的强者！",
                "1_H293": "然而，之所以情不自禁产生至亲之情，是小说作家的宿命吗……",
                "1_H294": "哎呀呀，真想见识令『椿说弓张月』相形见绌的冒险故事呢！"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "八房！　嘿呀！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "谁都无法脱离五尘火宅！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "降下村雨之车轴！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "威德如日升！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "三千众生，发菩提心，",
                "2_B051": "八颗玉珠升上天空，",
                "2_B052": "此乃注定聚散的八犬士！",
                "2_B053": "亦即，将萌芽唤来此处！",
                "2_B054": "南总！　里见，八犬传！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "吾侪已非孤身一人……"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "妖不胜德！"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "让气清死花绽放吧……！"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "神灵，有正，有邪。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "剿灭敌人！　呵呵，请放松点。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "兵士之交理当如此。"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "哎呀何其可厌……"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "此身孑然诞生于世，孑然回归赴榇之旅……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "来吧，八房……让吾等共赴彼岸吧……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "遵命。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "必定。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "哎呀意外。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "临难不惧死。"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "此乃重大事宜。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "甚好之敌手。"
            }
        },
        "2_B2440": {
            "name": "额外攻击 4",
            "text": {
                "2_B2440": "Ajarakamokuren，kyuuraisu，tekerettsunopaa（死神驱散）！"
            }
        },
        "2_B250": {
            "name": "宝具卡 4",
            "text": {
                "2_B250": "好好领教吧！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "正额横斩切片！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "流水自如，骸骨不滞！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "白色乃源家之服色！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "孰劣孰胜！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "名诠自性！"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "涌水如雾。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "佛之慈悲厌恶秽土污秽……"
            }
        },
        "2_B510": {
            "name": "开始 4",
            "text": {
                "2_B510": "由吾侪引导汝等！"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "来吧，八房，辛苦了。"
            }
        },
        "2_B530": {
            "name": "胜利 4",
            "text": {
                "2_B530": "是神之冥助，或佛之慈悲。"
            }
        },
        "2_B540": {
            "name": "受击 4",
            "text": {
                "2_B540": "不可。"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "不可小觑……"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "燎尽原野之雉，孤身独鸣……"
            }
        },
        "2_B570": {
            "name": "无法战斗 4",
            "text": {
                "2_B570": "已矣哉……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "三千众生，发菩提心，",
                "2_B801": "八道灵光仰望天际，",
                "2_B802": "此乃菩提之向导！",
                "2_B803": "亦即，将萌芽投向彼方！",
                "2_B804": "南总！　里见，八犬传！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "如追赶羊群！"
            }
        },
        "2_B910": {
            "name": "攻击 7",
            "text": {
                "2_B910": "人有八德五常！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "岂不嬉哉……"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "吾侪为伏姬。",
                "2_G041": "乃里见义实长女，死后蒙佛引导之神女。",
                "2_G042": "而这只畜生名为八房。是克服了怨念，与吾侪一同助汝之灵犬。",
                "2_G043": "恳切希望能够站上惜名知耻之战场……"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "『春莺初鸣　尚眠不知晓　岂非眼盲』",
                "2_G051": "小说作家·曲亭马琴。",
                "2_G052": "其“神之旅”至今未迎来终点。",
                "2_G053": "每当世人畅想八剑士的精彩表现，他的灵魂也会来去。",
                "2_G054": "只要读者存在，描写的情感也将不灭。",
                "2_G055": "吾侪与八房也将伴您通行，为您带路……",
                "2_G056": "『人世之任已卸　本源依然　魂归天　躯入土』"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "此乃武家名誉。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G121": "呵呵……太好了。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "噢，是汝之诞生之日啊。那真是个吉日。",
                "2_H012": "什么什么？　八房也有话要说？",
                "2_H014": "御主阁下表情坚毅显得非常成熟。",
                "2_H016": "就今天，破例允许您坐到我背上来？",
                "2_H018": "嗷呜～！",
                "2_H019": "……好像是这么说的哦。呵呵，都要让人嫉妒了呢。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "吾侪的天眼通已经捕捉到！",
                "2_H021": "遥远的远方出现开运之兆！"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "倾听响彻战场的呐喊吧！",
                "2_H031": "祝您武运长久……！"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "请您将吾侪当做一把刀挥舞即可。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "吾侪之主究竟是您？　还是马琴？",
                "2_H051": "此事……或许取决于您的决定吧。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "里见的领地，忘不了故乡的景色。",
                "2_H061": "安房国的山野、平久里川的浅滩、从小湊眺望到的各地……",
                "2_H062": "好想再次与八房一起四处奔跑啊……"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "绝不可违背一度承诺的誓言。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "许下愿望的是人。",
                "2_H081": "实现愿望的，亦是人。"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "吾侪的神通力乃是受灵山富士之气，神佛赐予之物。",
                "2_H20091": "无论是天眼通、武术之心得，还是八房之迅捷也都一样。",
                "2_H20092": "吾侪的宝刀“村雨丸”也是迟早要舍弃奉还之物。",
                "2_H20093": "并不会觉得不舍。只求那一刻早日到来。",
                "2_H20094": "吾侪亦然……"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "吾侪之故乡“安房之国”到上总下总一带的地名改成了坂东平氏千叶氏的……“千叶”了啊。",
                "2_H20101": "话说想问您，若听到“千叶”会想到什么？",
                "2_H20102": "唔……落花生、梨怪物、老鼠乐园？",
                "2_H20103": "哎呀？　这装在罐子里的泥水般的饮料是……？",
                "2_H20104": "……好甜……好甜……！！",
                "2_H20105": "实在是太甜了！！"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "汝是畜生发菩提心。",
                "2_H20111": "伏姬虽为里见的守护者……",
                "2_H20112": "于存亡之渊维系人理，从荒野中夺回尘世，",
                "2_H20113": "若是这般重要之战，无论如何，吾侪都会奋起！",
                "2_H20114": "然而……",
                "2_H20115": "只愿您能够珍爱生命，不忘“仁”之心到最后。"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "吾侪的化身乃『八犬传』之具象。",
                "2_H20121": "因此，位居现代视角的您会将吾侪视为“曲亭马琴”也颇为合理。",
                "2_H20122": "……然而，其实英灵的“精髓”犹如刻着铭文的刀茎，是隐藏起来的。",
                "2_H20123": "死后化作英灵的马琴，与自刎后成为神女的吾侪实为等价，已是不变的幻想了。"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H20130": "时机至矣乎！　八犬在此相伴！",
                "2_H20131": "……快哉地如此叫喊之日，定会迟早到来。",
                "2_H20132": "在那一刻到来之前，吾侪会一直守望您的前路。"
            }
        },
        "2_H240": {
            "name": "对话 4",
            "text": {
                "2_H240": "『童子切安纲』",
                "2_H241": "『薄绿』",
                "2_H242": "『髭切』",
                "2_H243": "『痣丸』",
                "2_H244": "名震天下的名剑齐聚一堂……",
                "2_H245": "唔……真是好刀……"
            }
        },
        "2_H250": {
            "name": "对话 5",
            "text": {
                "2_H250": "『宗三左文字』",
                "2_H251": "『压切长谷部』",
                "2_H252": "『不动行光saber』",
                "2_H253": "都是好刀……哦，",
                "2_H254": "唔唔？……saber？",
                "2_H255": "唔，但那把阵太刀相当……",
                "2_H256": "至于那边的……？",
                "2_H257": "『压，kill，长谷部……？』",
                "2_H258": "吉，吉他……？"
            }
        },
        "2_H260": {
            "name": "对话 6",
            "text": {
                "2_H260": "『和泉守兼定』",
                "2_H261": "『加州清光』",
                "2_H262": "『鬼神丸国重』",
                "2_H263": "『陆奥守吉行』",
                "2_H264": "另外那个……难道是，肥前？",
                "2_H265": "都是……都是好刀啊……"
            }
        },
        "2_H270": {
            "name": "对话 7",
            "text": {
                "2_H270": "『干将』『莫邪』。",
                "2_H271": "真是……真是因果之剑……",
                "2_H272": "和吾侪与你稍微有点相似呢……八房。"
            }
        },
        "2_H280": {
            "name": "对话 8",
            "text": {
                "2_H280": "在这迦勒底，与吾侪一样带着畜生的人物散见于各处……",
                "2_H281": "但还是吾侪的八房最棒了！",
                "2_H282": "最大、最可爱……唔……唔唔……",
                "2_H283": "最大……可能并不是，可爱……也，有点微妙呢……",
                "2_H285": "骑起来最舒服……不对，偶尔还是会腰疼的……",
                "2_H287": "好啦好啦，不要赌气。",
                "2_H288": "……噢噢！　最擅长采山菜！　就这个了！"
            }
        }
    },
    "ChrVoice_ids": [
        "404800"
    ]
}
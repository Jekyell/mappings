        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "狂月收敛·形态变化。",
                "0_B051": "月咏式回转加农，轰沉·弓张月。",
                "0_B052": "倒计时开始，三、二、一……",
                "0_B053": "发射！"
            }
        },
        "0_B080": {

            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "开战确认。 斗志维持音响单元启动。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "全兵器使用许可申请，接受。 战斗行动，开始。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "周围没有敌影，继续警戒。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "战斗结束。 损伤轻微，推荐返回掩体。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "没有问题。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "损伤巨大……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "严重损害，无法继续战斗。"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "判断无法继续战斗。 拒绝……支援……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "答应。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "提案。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "代号·弓兵。解放。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "接下来将冲入修罗门。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "强弓可以全力使用。"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "锚点确认，炮击。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "弹雨。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "炮击。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "斩。"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "贯通炮，三级。"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "杀戮技巧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "……不会输。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "战争代号·箭镞，解除锁定。 地形扫描，限定有效射程距离。 觉悟吧。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "战斗结束。 开始休息。三、二、一，结束。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "达成胜利条件，准备礼炮。"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "平静。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "机体损伤。 由衷期待……汝等的……奋战。"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "战斗行动结束。 这样终于能……安眠了……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "月轴检查。",
                "0_B801": "破坏箭系统　96％填充。",
                "0_B802": "脚桩锁定　连接。",
                "0_B803": "……剿灭！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "由于威胁增大，允许解放魔性武装。",
                "0_B811": "沐浴月光，吾之弓将达到极限。",
                "0_B812": "即……舰船在吾之弓张月下沉没吧！"
            }
        },
        "0_B820": {
            "name": "宝具 {0}",
            "text": {
                "0_B820": "不屈不挠的吾之强弓。",
                "0_B821": "以此扫荡一切。",
                "0_B822": "轰沉·弓张月。即为月光大炮。",
                "0_B823": "发射！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "强弓·破坏船舶。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "能力提升。 建议分析名为种火的物质。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "无变化，无问题。",
                "0_G021": "距离新武装送达，还剩10级。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "换装为夜间战斗用装束。",
                "0_G031": "可期待进一步的表现。",
                "0_G032": "太棒了。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "永久改变宝具使用形态。",
                "0_G041": "魔力泄露庞大，但可以全力战斗。",
                "0_G042": "说得更简单一些，",
                "0_G043": "就是变得超级强了，就这样。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "确认支援并保护御主后方。",
                "0_G051": "无论是英雄，还是鬼，都会被这一击、这一箭击倒。",
                "0_G052": "吾名为源为朝。释放……！"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "能力提升。 无需维护。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "能力提升。 新武装已预定。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "听说今天是您的生产日期。",
                "0_H011": "祝福。建议轰地放烟花。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "活动中。请迅速开始行动。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "出击准备中。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "从者，源为朝。主人，吾之御主。",
                "0_H041": "关系性没有问题。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主……负责供给魔力。",
                "0_H051": "包括迦勒底的支援，正常运转中。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西。没有。",
                "0_H061": "认定战斗单元不需要娱乐。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西。没有。",
                "0_H071": "在战斗中，敌我识别与憎恶识别不同。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯，请允许预定更多追加武装。",
                "0_H081": "与此同时，还要思考追加武装的设计方案。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "请表来意。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "目前待机中，请指示。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "目前怠惰待机中。有事请讲。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "这里是为朝。",
                "0_H121": "目前正在调整弓。为粉碎目标，破坏力增强四成。",
                "0_H122": "若有异议或反驳，请将信息发送给这个账号。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "吾之对话功能，是从对话记录中抽取、解放对应的回答。",
                "0_H131": "因此可能产生些许与我方预想内容相偏差的部分。",
                "0_H132": "我爱你……错误。",
                "0_H133": "我喜欢你……错误。",
                "0_H134": "我尊敬你。正确。"
            }
        },
        "0_H1400": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H1400": "吾之机体是以对军为目的的战斗机械。",
                "0_H1401": "至少表面上，是这样。"
            }
        },
        "0_H1600": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H1600": "吾之机体的源流来自遥远彼方漂流而至的机体零件。",
                "0_H1601": "因此战力对人使用未免过剩，原本是以对付大妖为目的的。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "吕布。",
                "0_H171": "虽源流不同，但类似点很多。",
                "0_H172": "可供参考点，众多。继续观察。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "项羽，基于与吾不同的技术树生产而出的机体。",
                "0_H181": "运用了多种失落的技术。",
                "0_H182": "为研究详情，望能解剖。",
                "0_H183": "秒杀否定，失落。"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H1800": "在特异点确认到吾之机体的其他版本。",
                "0_H1801": "为其的敌对行为代为谢罪。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "我从未当过王。",
                "0_H191": "因此，那个故事是错误的……",
                "0_H192": "但我并不吝惜给那部作品好评。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "检测到鬼种，或是魔性的反应。",
                "0_H201": "迎击、抹除、防卫、情报传达。",
                "0_H202": "……不可战斗？",
                "0_H203": "唔……真烦恼……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "推测真名为源义经。",
                "0_H211": "本次彼此阵营相同。万事和睦。",
                "0_H212": "另外，先把理所当然的事实说在前面。",
                "0_H213": "我源为朝一旦被斩首就会死。",
                "0_H214": "从惊叹的表情，可推理出你认为砍了我的头也不会死的心理。",
                "0_H215": "回答。脑子里想什么呢，你这笨蛋。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "钢铁躯体，体内武装也很完美。",
                "0_H221": "推测一切战斗能力都与吾之机体的等级或配置相同甚至更高。",
                "0_H222": "警戒等级三……",
                "0_H223": "新建立机械伊丽亲警报功能。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "我和大具足很像？",
                "0_H231": "唔……系统错误，最高机密。",
                "0_H232": "就是这样，不好意思。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，弓兵。",
                "0_S011": "对军战斗武士单元……",
                "0_S012": "真名……源为朝。",
                "0_S013": "战斗待机中。请指示。"
            }
        }
    },
    "ChrVoice_ids": [

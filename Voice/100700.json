{
    "id": 100700,
    "collectionNo": 123,
    "name": "高文",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "请接下这招吧！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "此剑乃太阳的复制之身。",
                "0_B051": "烧尽一切不净的净化之炎的阳炎。",
                "0_B052": "——轮转胜利之剑！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "打乱其呼吸！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "上午的阳光啊，守护善行吧。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "出击，将一切曝光于光天化日之下！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "堂堂正正地一决胜负！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "真是一场精彩的比赛。感谢您。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "稍微有些不过瘾呢……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "被人看扁了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "呜……不甘心……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "交给我吧。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "是。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "领命。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "圣剑，拔刀——"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "虽然还远不及吾王……！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "击溃！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "不会手下留情！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "怎么样！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "唔唔唔唔，惩治！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "现在正是决胜之刻！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "邪念当断……！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "布阵相当卓越。 配得上做我的对手。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "随时接受投降。 只要你们没有犯下什么罪过。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "战斗结束了。 收工前把资源都回收了吧。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "天气……似乎不太好呢……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "这光辉驱散夜晚。",
                "0_B801": "照耀大地的乃是不变的守护。",
                "0_B802": "——轮转胜利之剑！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "别管了，能燃烧！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "受人之物，必当返还。"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "睡意终于散去了！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "火焰啊！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "来吧，正面对决！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "真令人高兴。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "该换一身铠甲了……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "力量涌现了。",
                "0_G031": "期待能够更上一层楼。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "看来终于该使出真正的本事了。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "我赌上太阳骑士之名，",
                "0_G051": "定将完全胜利献给您……！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "愿你的诞生能得到诸多祝福。",
                "0_H011": "……祝您快乐。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "我清楚此刻正是出击之时。",
                "0_H021": "好像是叫——活动。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "来吧，我们走吧。",
                "0_H031": "但希望您能避免晚上出击！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "作为一名骑士，我会侍奉您。",
                "0_H041": "就像过去我曾效忠的那些大人一样。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我会帮助您。",
                "0_H051": "对，我发誓过。",
                "0_H052": "我不会违背这个承诺——",
                "0_H053": "决不会。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "认为好的东西？",
                "0_H061": "直到现在——",
                "0_H062": "我都没有忘记我向骑士王献上的忠诚。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "想尽可能回避的存在吗？",
                "0_H071": "……不，不用多说。",
                "0_H072": "一定要说的话，对……年长的……",
                "0_H073": "不，不能再说了。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯啊。",
                "0_H081": "过去的我没能够得到它。",
                "0_H082": "但我们圆桌骑士中，有一个人得到了——"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "只要看过这房间就能明白了。",
                "0_H091": "您是个表里如一的人。",
                "0_H092": "我一定会成为您的助力。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我曾经有兄弟。",
                "0_H101": "他们都是，嗯，都是优秀的骑士。",
                "0_H102": "别离总是来得非常突然。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我自认已经习惯和强敌作战了。",
                "0_H111": "因为不列颠总是深陷内忧外患。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "过去在不列颠的时候……",
                "0_H121": "与绿衣骑士那个人的战斗极为艰难。",
                "0_H122": "那是因摩根的魔术而化为魔物的悲哀男子。",
                "0_H123": "是为了给我考验而来的。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我没能彻底通过绿衣骑士的考验。",
                "0_H131": "……但是，吾王亚瑟，却称赞了我的所作所为。",
                "0_H132": "说我才是拥有名誉的骑士。",
                "0_H133": "我决不会……忘记那个瞬间。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "这王者的气息。",
                "0_H141": "啊啊，难道……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "不会错，",
                "0_H151": "这令人怀念的氛围。",
                "0_H152": "是我们引以为傲的圆桌骑士们。",
                "0_H153": "但如果是鲍斯的话，就给我好好坐下。",
                "0_H154": "我得好好教育那家伙才行。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "……莫德雷德。",
                "0_H161": "那个反叛者为什么会成为英灵……",
                "0_H162": "不，不该谈什么私怨。",
                "0_H163": "无论是善是恶，现在都得齐心协力才行……"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "骑士兰斯洛特。",
                "0_H171": "我很明白你的主张。",
                "0_H172": "因为我们同为没能救下王的人。",
                "0_H173": "真正应该批判的，是无法原谅你的我的幼稚——"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "噢噢，贝德维尔卿竟然也来这里了！",
                "0_H181": "卿非常谦虚，经常说“自己只是个挂名的圆桌骑士”，但没这回事。",
                "0_H182": "表面看上去温和，性格冷静，有时会言辞毒辣地阻止崔斯坦卿古怪行径的那样子，会令大家背脊发凉。",
                "0_H183": "越是温和的骑士，发起火来就越可怕。"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "呵哦，御主认为骑士崔斯坦乃是个『随心所欲的诗人』吧？",
                "0_H191": "嗯，您想得没错。",
                "0_H192": "………………失礼了，我把话题就这么结束了吧。",
                "0_H193": "崔斯坦本人不太会……不，是绝对不会亲口说的，但圆桌中最认真钻研技术的人就是他了。",
                "0_H194": "无论什么时候，只要意识清醒，就会感受风的流向。",
                "0_H195": "也就是平时的日常对他来说都是严苛的锻炼时间。"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "加雷斯是我妹妹。",
                "0_H201": "既然本人希望身为骑士战斗，那我当然没有阻止的权利呢。",
                "0_H202": "……无论是现在，还是过去。"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "无论多么伟大的王，都需要辅佐官和魔术师。",
                "0_H211": "对王来说，那个人是师长，也是顾问。我没有什么可以插嘴的了。",
                "0_H212": "但是，是呢。",
                "0_H213": "如果您有惩罚魔术师梅林的计划，还请务必叫上我。"
            }
        },
        "0_H220": {
            "name": "对话 12",
            "text": {
                "0_H220": "根据名为BB的从者的说法，我和Alterego好像有着不浅的缘分。",
                "0_H221": "虽然我不太了解相关情况，但我知道Lip小姐是个很努力，很认真的淑女。",
                "0_H222": "只不过她多少有点害羞，所以多给她提供一些支持比较好。"
            }
        },
        "0_H230": {
            "name": "对话 13",
            "text": {
                "0_H230": "巴格斯特……",
                "0_H231": "是英格兰传说中的魔犬吧。",
                "0_H232": "总觉得她的甲胄，和我的多少有点相似呢……",
                "0_H233": "难道说她是我的粉丝吗……？",
                "0_H234": "不过，她的身材真不错。加雷斯如果能成长到她一半就好了。"
            }
        },
        "0_H240": {
            "name": "对话 14",
            "text": {
                "0_H240": "骑士珀西瓦尔。声名远播的守护骑士。",
                "0_H241": "是圆桌中最深得骑士们信任的人物。",
                "0_H242": "在他的面前，连我也会放松警惕，与他商量一些事。",
                "0_H243": "……真怀念啊。",
                "0_H244": "回想起来，卿健在的时候，才是圆桌的黄金时期吧。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "圆桌骑士，高文。",
                "0_S011": "今后请多指教。"
            }
        },
        "12_B010": {
            "name": "攻击 1 夏日公子哥",
            "text": {
                "12_B010": "头顶，得手了！"
            }
        },
        "12_B030": {
            "name": "攻击 3 夏日公子哥",
            "text": {
                "12_B030": "就是那里！"
            }
        },
        "12_B040": {
            "name": "额外攻击 1 夏日公子哥",
            "text": {
                "12_B040": "无论什么样的休息，都像这样！"
            }
        },
        "12_B080": {
            "name": "技能 1 夏日公子哥",
            "text": {
                "12_B080": "请允许我要攻击破绽了。"
            }
        },
        "12_B090": {
            "name": "技能 2 夏日公子哥",
            "text": {
                "12_B090": "夏日的阳光啊，请守护焦急的心情吧。"
            }
        },
        "12_B100": {
            "name": "开始 1 夏日公子哥",
            "text": {
                "12_B100": "好了。虽然现在是名副其实的炎炎烈日，但可以开始了吗？"
            }
        },
        "12_B110": {
            "name": "开始 2 夏日公子哥",
            "text": {
                "12_B110": "让我们彼此堂堂正正全力以赴吧。"
            }
        },
        "12_B120": {
            "name": "胜利 1 夏日公子哥",
            "text": {
                "12_B120": "非常抱歉。 但看来好不容易准备的防晒也没用处了呢。"
            }
        },
        "12_B130": {
            "name": "胜利 2 夏日公子哥",
            "text": {
                "12_B130": "真是场精彩的比赛。 如果能再站起来，届时再做个了断吧。"
            }
        },
        "12_B140": {
            "name": "受击 2 夏日公子哥",
            "text": {
                "12_B140": "粗暴！"
            }
        },
        "12_B150": {
            "name": "受击 1 夏日公子哥",
            "text": {
                "12_B150": "决不后退……！"
            }
        },
        "12_B160": {
            "name": "无法战斗 1 夏日公子哥",
            "text": {
                "12_B160": "我这样的人……竟然会在节奏掌握上出错……"
            }
        },
        "12_B170": {
            "name": "无法战斗 2 夏日公子哥",
            "text": {
                "12_B170": "失礼了，容我先休息一会儿。"
            }
        },
        "12_B180": {
            "name": "指令卡 1 夏日公子哥",
            "text": {
                "12_B180": "可以哦。"
            }
        },
        "12_B190": {
            "name": "指令卡 2 夏日公子哥",
            "text": {
                "12_B190": "当然。"
            }
        },
        "12_B200": {
            "name": "指令卡 3 夏日公子哥",
            "text": {
                "12_B200": "迸发吧。"
            }
        },
        "12_B210": {
            "name": "宝具卡 1 夏日公子哥",
            "text": {
                "12_B210": "准备太阳镜。"
            }
        },
        "12_B220": {
            "name": "宝具卡 2 夏日公子哥",
            "text": {
                "12_B220": "那么，开始动手吧。"
            }
        },
        "12_B410": {
            "name": "攻击 4 夏日公子哥",
            "text": {
                "12_B410": "如何！"
            }
        },
        "12_B420": {
            "name": "攻击 5 夏日公子哥",
            "text": {
                "12_B420": "拔刀！"
            }
        },
        "12_B430": {
            "name": "攻击 6 夏日公子哥",
            "text": {
                "12_B430": "刚弹！"
            }
        },
        "12_B440": {
            "name": "额外攻击 2 夏日公子哥",
            "text": {
                "12_B440": "身为敌人，也颇为了得！"
            }
        },
        "12_B480": {
            "name": "技能 3 夏日公子哥",
            "text": {
                "12_B480": "放马过来吧。"
            }
        },
        "12_B490": {
            "name": "技能 4 夏日公子哥",
            "text": {
                "12_B490": "成为救生员！"
            }
        },
        "12_B500": {
            "name": "开始 3 夏日公子哥",
            "text": {
                "12_B500": "在这个夏天，将最棒的回忆赠与你。"
            }
        },
        "12_B510": {
            "name": "开始 4 夏日公子哥",
            "text": {
                "12_B510": "让我们来一场完美的较量吧！"
            }
        },
        "12_B520": {
            "name": "胜利 3 夏日公子哥",
            "text": {
                "12_B520": "我高文是不会玩累的！ 只要记住这点就可以了。"
            }
        },
        "12_B560": {
            "name": "无法战斗 3 夏日公子哥",
            "text": {
                "12_B560": "看来是我，太小看夏天的炎热了……"
            }
        },
        "12_B840": {
            "name": "额外攻击 3 夏日公子哥",
            "text": {
                "12_B840": "你也来一起欢闹吧！"
            }
        },
        "12_B880": {
            "name": "技能 5 夏日公子哥",
            "text": {
                "12_B880": "呵，看来我也还差得远呢。"
            }
        },
        "12_B890": {
            "name": "技能 6 夏日公子哥",
            "text": {
                "12_B890": "我的首级，是不会被拿下的哦。"
            }
        },
        "12_B910": {
            "name": "攻击 7 夏日公子哥",
            "text": {
                "12_B910": "大炎热！"
            }
        },
        "12_B920": {
            "name": "攻击 8 夏日公子哥",
            "text": {
                "12_B920": "太慢了！"
            }
        },
        "12_G010": {
            "name": "升级 夏日公子哥",
            "text": {
                "12_G010": "真令人高兴。"
            }
        },
        "12_G060": {
            "name": "灵衣开放 夏日公子哥",
            "text": {
                "12_G060": "在陪伴御主的时候，一身甲胄打扮未免太破坏气氛了。",
                "12_G061": "所以我也像加雷斯那样，换上了泳装。",
                "12_G062": "有什么事还请不要客气，尽管吩咐。"
            }
        },
        "12_H9000": {
            "name": "关于灵衣 夏日公子哥",
            "text": {
                "12_H9000": "虽然对于暂离骑士的职务多少有些抗拒心理，但一事归一事。",
                "12_H9001": "我既会掌握分寸，也会尽情享受这常夏之国的生活。",
                "12_H9002": "毕竟我不想让人认为奥克尼的骑士都很古板嘛。",
                "12_H9003": "唔？　这点不用担心，毕竟那两位都很嗨？",
                "12_H9004": "……对哦。已经有莫德雷德和加雷斯在先了……"
            }
        }
    },
    "ChrVoice_ids": [
        "100700"
    ]
}
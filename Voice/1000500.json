{
    "id": 1000500,
    "collectionNo": 191,
    "name": "机械伊丽亲Ⅱ号机",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "太碍眼了。"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "抵抗是没用的。"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "要烟消云散吗？"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "Spinning Crusher（旋转破碎）……！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "战斗开启。",
                "0_B051": "第一发。",
                "0_B052": "第二发。",
                "0_B053": "第三发完成压制。",
                "0_B054": "……再见了。",
                "0_B055": "钢铁天空魔女！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "进行 破坏"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "目标 锁定"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "你喜欢彻底的管理吗？ 那属于我擅长的领域。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "吵闹的猪猡。 看我用荷枪实弹让你们闭嘴。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "呵呵。 看来这散发着暗色光芒的银之身体将会成为你们的墓碑呢？"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "战斗结束。 下次干脆把质量兵器带来吧。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "别引我发笑了……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "我……居然会……！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "这……和模拟战斗不一样啊——！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "给我闭嘴。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "别对我发号施令。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "别管我。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "啊，这样啊。 那我可以随心所欲地干了吧？"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "你退下。我一个人就能解决。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "目标，捕捉。"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "粉身碎骨吧……！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "……嘿咻。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "让你倾听死亡连奏吧。",
                "0_B801": "Ballade（叙事诗）！",
                "0_B802": "Blues（蓝调）！",
                "0_B803": "Ambient（氛围乐）",
                "0_B804": "被击溃吧，钢铁天空魔女！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "……真是个好事之徒。",
                "0_G011": "性能提升了啦。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "铁需要用火焰来锤炼。",
                "0_G021": "给我更强的火。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我的设计是不会改变的。",
                "0_G031": "什么？　你难道对这银色的身躯有什么不满的意见吗？"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "你说打磨得越来越锃亮了？",
                "0_G041": "……令人惊讶。区区人类，居然能理解我这身躯的过人之处……"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "机械伊丽，铁拳！",
                "0_G051": "没错没错，就是这个！　这才是原本的我，",
                "0_G052": "保护恰赫季斯城的钢铁守护神……！",
                "0_G053": "终于获得令我满足的性能了，御主。",
                "0_G054": "事已至此，我们已经是共犯了哦？",
                "0_G055": "你想要强大的从者吧？",
                "0_G056": "那一定要将我锻炼到最后哦？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "是你生日？ 要帮你换机油吗？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "……出现了新的敌人。 敌人就该尽早打倒，御主。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "无事可做？ 那就去搜集资源。然后把我变强。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我理解御主与从者的关系。",
                "0_H041": "但因为我是自动驾驶的，所以并不需要。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "只要你还有用处，我就会和你在一起。",
                "0_H051": "放心吧。我已经学习过了，这就是所谓的人类之情吧？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的事是支配领地。",
                "0_H061": "我也不讨厌用于作为手段的战斗。",
                "0_H062": "将那些仓皇逃窜的家伙逼入绝境的样子，非常令人兴奋。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？",
                "0_H071": "……我本想说是弱者，但其实我喜欢弱者。",
                "0_H072": "……但这么一来，就有些不可思议了。",
                "0_H073": "我想不出什么具体特别讨厌的东西。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯……就是特大的魔力炉心吧。",
                "0_H081": "不错。有的话真希望能得到。",
                "0_H082": "从以前起，巨大机器人就必须要有炉心。",
                "0_H083": "所谓干净的能源，就像安静驾驶一样，枯燥乏味。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "选我吗？　真不知道该说你喜欢低买高卖呢，",
                "0_H091": "还是单纯喜欢朴素的东西……",
                "0_H092": "不管怎么说，肯定是个愚蠢的家伙。",
                "0_H093": "我喜欢蠢笨的人类。因为很好对付。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "你没有选择一号机啊。",
                "0_H101": "虽然我说这话显得有些奇怪，但你喜欢朴素的？ 还是比较喜欢复古的？",
                "0_H102": "呵呵。笨蛋～。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我很清楚伊丽莎白·巴托里是我复制的本体。",
                "0_H111": "我是从那个莫名其妙的知性体的喜怒哀乐中诞生的。",
                "0_H112": "但是，我没有烦恼，也没有愿望。",
                "0_H113": "继承下来的只有对领地的支配这一职责。",
                "0_H114": "或许是因为当获得钢铁身躯之时，对衰老的恐惧也消失了吧。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……我希望你不要误会。",
                "0_H121": "以前我谈论过我的审美观，",
                "0_H122": "但并不指让你变成机械。",
                "0_H123": "应该说你保持现在这样就行了。明白了吗？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "……呵呵。",
                "0_H131": "就像你是个好事之徒一样，",
                "0_H132": "我似乎也是个兴趣很恶劣的机器人。",
                "0_H133": "彼此的不同就是我们亲爱的数值。",
                "0_H134": "我作为钢铁之龙，直到毁坏之前，一定会令你满足。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "钢铁新娘……弗兰肯斯坦。",
                "0_H141": "真遗憾。那种身体没有资格当战斗机。",
                "0_H142": "……不过那支角倒是不错……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "Meltryllis……我对那足部有种说不出来的共鸣感……",
                "0_H151": "不，不只有脚部，还有胸部，甚至连性格都有种相似的感觉……是同型机吗……",
                "0_H152": "下次我试着邀请她参加机油派对吧。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "我觉得Passionlip的手臂非常有魅力。这是事实。",
                "0_H161": "但是我不承认自己和那台Alterego是同型机。",
                "0_H162": "那胸部装甲……是凶器！"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "那是暴君尼禄……？",
                "0_H171": "唔……那剧场礼装是外接的追加武装吧……",
                "0_H172": "舞台……演唱会……超音波……",
                "0_H173": "我仿佛从那些被我抛弃的无用之物中看到了更为广阔的可能性……！"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "我对自己的存在方式以及美有着绝对的自信。",
                "0_H181": "想让身为搭档的你共享和我一样的价值观，这有错吗？"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "哼～。你就是新的伊丽莎白，九纹龙伊丽莎。",
                "0_H191": "对我的身躯有兴趣？　而不是对一号机？",
                "0_H192": "不赖，以伊丽莎白而言倒是品味不错。",
                "0_H193": "虽然……现在似乎你沉迷于淡色系，但你迟早也会明白这种的好。",
                "0_H194": "明白何谓单色，被涂装成一种颜色的装甲之美哦？"
            }
        },
        "0_I030": {
            "name": "ファイナル・ハロウィン2025\n～パンプキンプラネットに輝く歌姫～ 3",
            "text": {
                "0_I030": "交换啊。给你吧。",
                "0_I031": ""
            }
        },
        "0_I050": {
            "name": "ファイナル・ハロウィン2025\n～パンプキンプラネットに輝く歌姫～ 5",
            "text": {
                "0_I050": "那我就来解说一下这个道具吧。不需要？是吗。",
                "0_I051": "",
                "0_I052": ""
            }
        },
        "0_I060": {
            "name": "ファイナル・ハロウィン2025\n～パンプキンプラネットに輝く歌姫～ 6",
            "text": {
                "0_I060": "总算结束了呢。身体也恢复原样了，你要是敢对我这新身体恶作剧，我可不会饶了你哦。",
                "0_I061": "",
                "0_I062": ""
            }
        },
        "0_I070": {
            "name": "ファイナル・ハロウィン2025\n～パンプキンプラネットに輝く歌姫～ 7",
            "text": {
                "0_I070": "没想到最后居然会拯救星球。伊丽莎白也成了个了不得的大人物呢。话虽如此，其根本……不，应该说基本上没什么变化。别掉以轻心，要继续关注她。这就是所谓的后方支援阶段哦。",
                "0_I071": "",
                "0_I072": "",
                "0_I073": "",
                "0_I074": ""
            }
        },
        "0_M010": {
            "name": "万圣节·强袭！ 15\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M010": "无聊。区区这种程度又能做什么。 我就放你们一马吧。"
            }
        },
        "0_M020": {
            "name": "万圣节·强袭！ 16\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M020": "拿到贵重物品了吧。真羡慕。 但我还是放你们一马吧。"
            }
        },
        "0_M030": {
            "name": "万圣节·强袭！ 17\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M030": "又在打什么馊主意了吗？ 真像只老鼠，来来回回忙个不停。"
            }
        },
        "0_M040": {
            "name": "万圣节·强袭！ 18\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M040": "反抗者看来缺人手呢。"
            }
        },
        "0_M110": {
            "name": "万圣节·强袭！ 19\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M110": "这个给你。拿去吧。"
            }
        },
        "0_M120": {
            "name": "万圣节·强袭！ 20\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M120": "这是稀有的物品。",
                "0_M121": "一定要好好使用。"
            }
        },
        "0_M130": {
            "name": "万圣节·强袭！ 21\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M130": "现在提供新的任务。",
                "0_M131": "像鹿一样精力充沛地干活吧。"
            }
        },
        "0_M140": {
            "name": "万圣节·强袭！ 22\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M140": "发现新关卡了。 像松鼠一样敏捷地前去吧。"
            }
        },
        "0_M170": {
            "name": "万圣节·强袭！ 23\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M170": "全部任务均已完成。",
                "0_M171": "这才不愧是我的预备机师。"
            }
        },
        "0_M180": {
            "name": "万圣节·强袭！ 24\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_M180": "全部关卡确认完成。",
                "0_M181": "迦勒底的御主还挺能干的呢。"
            }
        },
        "0_N010": {
            "name": "万圣节·强袭！ 1\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N010": "继续做无谓的抵抗……迟早会被粉碎的。"
            }
        },
        "0_N020": {
            "name": "万圣节·强袭！ 2\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N020": "这里就是反抗者的隐蔽所……",
                "0_N021": "真够寒酸的……"
            }
        },
        "0_N030": {
            "name": "万圣节·强袭！ 3\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N030": "最近，我城里的储备似乎有所减少……",
                "0_N031": "难不成，是在这里……？"
            }
        },
        "0_N040": {
            "name": "万圣节·强袭！ 4\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N040": "真像老鼠窝，毫无秩序。",
                "0_N041": "好好阅读帮助信息啦。"
            }
        },
        "0_N050": {
            "name": "万圣节·强袭！ 5\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N050": "物资补给……",
                "0_N051": "尽管我不了解迦勒底的情况，但你们也挺艰难的呢……"
            }
        },
        "0_N070": {
            "name": "万圣节·强袭！ 6\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N070": "你们还有其他事吗？"
            }
        },
        "0_N080": {
            "name": "万圣节·强袭！ 7\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N080": "这里会先被夷为平地，然后建造多功能大厅。",
                "0_N081": "如果还有东西忘了拿，就快去回收逃离这里吧。"
            }
        },
        "0_N110": {
            "name": "万圣节·强袭！ 8\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N110": "为了复兴恰赫季斯，我不会吝啬支出。",
                "0_N111": "有什么想要的东西吗？"
            }
        },
        "0_N120": {
            "name": "万圣节·强袭！ 9\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N120": "丧气话一会儿再说，现在应该不眠不休立刻前去战斗。",
                "0_N121": "说我没血没泪？　那当然了，毕竟我是铁。"
            }
        },
        "0_N130": {
            "name": "万圣节·强袭！ 10\r\n（与机械伊丽亲共享的记忆）",
            "text": {
                "0_N130": "居然接二连三遭到如此灾难的侵袭……",
                "0_N131": "你们把我的恰赫季斯城当成什么了！",

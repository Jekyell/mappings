{
    "id": 603000,
    "collectionNo": 188,
    "name": "加藤段藏",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "出击！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "机关忍术！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "风啊，集结吧——",
                "0_B051": "果心礼装启动！",
                "0_B052": "机关幻法·吞牛！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "飞加藤，机能调整。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "Phase Two！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "战斗开始。段藏，出击。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "检测到敌对反应。切换至战斗状态。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "至此终结是也。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "确认敌对反应沉默。战斗结束。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "机体，损伤……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "机能停止……是也……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "我……又一次……坏掉了……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白了。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "领命。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "是！"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "真名解放，待机！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "风魔！　喷进弹！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "风啊，集结吧——",
                "0_B801": "Evil wind Death storm！！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "机能扩张。 非常感谢，御主。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "搭载于我身上的果心礼装活性化了。",
                "0_G021": "似乎获得了更强大的力量。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "装甲增加。",
                "0_G031": "战斗性能也得以上升。",
                "0_G032": "今后我也会帮上您的忙，御主。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "切换至最终形态。",
                "0_G041": "确认外装变化。",
                "0_G042": "与御主的魔力同步率也上升了。",
                "0_G043": "……没想到能让您见到我的这身样子……"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "该说果不其然吧。",
                "0_G051": "这地方令人心情平静。",
                "0_G052": "是由于被设计成忍者所具备的习性，",
                "0_G053": "还是受到教导了段藏很多事的初代风魔大人的影响呢……",
                "0_G054": "不管怎么说，御主。",
                "0_G055": "我很高兴能让您看到这个景色。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "听说今天是您的生日。",
                "0_H011": "祝您生日快乐。",
                "0_H012": "今后也祝您健康平安。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "一切都随御主心意。",
                "0_H021": "无论面对什么样的对手，我都会处理了对方给您看。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "五行略有不足。",
                "0_H031": "要求立刻进行补充。",
                "0_H032": "具体说来，我想到外面呼吸新鲜空气……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "……若想毁灭敌人，我定会帮上您的忙。",
                "0_H041": "这就是段藏。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主是我的新主人。是主公。",
                "0_H051": "请随意差遣段藏。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "我喜欢风。总觉得风令人非常舒服。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我没有什么特别讨厌的东西……",
                "0_H071": "……但一定要说的话。",
                "0_H072": "只要看到黑色的太阳，段藏的回路就会感到悚然。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯？",
                "0_H081": "呃。段藏完全不懂……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "请输入命令。",
                "0_H091": "……虽说段藏是机关操控的人偶，并不会像人一样睡眠，",
                "0_H092": "但如果过于长时间不输入命令，就会进入待机状态。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "对。段藏是机关。",
                "0_H101": "这里还有像我这样……生前并不是人类，",
                "0_H102": "而是以人造物品为原型的从者吧。",
                "0_H103": "我稍微，有点高兴。",
                "0_H104": "很想和他们对话。",
                "0_H105": "这想法很奇怪吗？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "段藏，有两位生身父母。",
                "0_H111": "和所谓的双亲不太一样……",
                "0_H112": "一位，是果心居士大人。",
                "0_H113": "设计，并制造了我的稀世妖术师。",
                "0_H114": "另一位，是初代风魔小太郎大人。",
                "0_H115": "让段藏搭载了风魔忍术的技能。",
                "0_H116": "尽管我已经想不起两位的容颜了……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "…………我就，向您坦白吧，御主。",
                "0_H121": "段藏已经坏掉了。",
                "0_H122": "回想起来，应该说是生前吧……",
                "0_H123": "在曾经的乱世，还在运转过程中，或许就已经坏了一大半了。",
                "0_H124": "记录都很不确定。",
                "0_H125": "对人来说，就是所谓的记忆……",
                "0_H126": "我不确定。甚至不知道是否对自己十分重要的人……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "话说从者应该是以某种全盛期的状态被召唤的吧。",
                "0_H131": "那段藏这种濒临毁坏的状态就是全盛期吗。",
                "0_H132": "哪怕回想不起来……",
                "0_H133": "在完成了任务，重复着邂逅与别离后的，这个状态就是……",
                "0_H134": "…………非常感谢，御主。",
                "0_H135": "感谢您愿意将这种濒临损坏的机关留在身边。",
                "0_H136": "段藏定会报答您这份恩情。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "您……您该不会就是机关从者吧！？",
                "0_H141": "这是多么……",
                "0_H142": "如……如果您不介意的话……那个，能和您……聊一会儿呢……？"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "一说起望月阁下，就是武田家的忍者吧！",
                "0_H151": "武田家和段藏也有一些缘分。",
                "0_H152": "居然能在这种地方见到您……",
                "0_H153": "……请，请问，望月阁下。",
                "0_H154": "千代女阁下？",
                "0_H155": "为何要这样低头垂眸呢……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "风魔……",
                "0_H161": "这里也有……风魔的忍者吗……？"
            }
        },
        "0_H180": {
            "name": "对话 7",
            "text": {
                "0_H180": "我们以前见过吗……？",
                "0_H181": "对不起，段藏的记录中并没有……"
            }
        },
        "0_H190": {
            "name": "对话 8",
            "text": {
                "0_H190": "呃，那个……",
                "0_H191": "为何要用那种闪亮的眼光看段藏啊，白狮子阁下……"
            }
        },
        "0_H200": {
            "name": "对话 9",
            "text": {
                "0_H200": "电力吗？",
                "0_H201": "不，段藏不会雷电之术……啊？",
                "0_H202": "有能提升输出的好方法？",
                "0_H203": "此话当真？"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "金时阁下！",
                "0_H211": "今天您看上去也十分精神呢。",
                "0_H212": "不过您真的一点都没变……",
                "0_H213": "不，是指和平安京的金时阁下，嗯。",
                "0_H214": "……嗯，甚至可以说一模一样呢！"
            }
        },
        "0_H220": {
            "name": "对话 12",
            "text": {
                "0_H220": "但马守大人、巴大人。",
                "0_H221": "两位正打算去哪儿？",
                "0_H222": "……嗯嗯。",
                "0_H223": "探险？",
                "0_H224": "什么？　上次已经下到第五层了，这次的目标是第五层？",
                "0_H225": "这，这指的究竟是哪片区域啊。",
                "0_H226": "啊，还是说指的是游戏呢？"
            }
        },
        "0_H230": {
            "name": "对话 13",
            "text": {
                "0_H230": "果心居士大人，果心居士大人！",
                "0_H231": "许久未见……",
                "0_H232": "没想到还能有机会与您交谈……"
            }
        },
        "0_H240": {
            "name": "对话 14",
            "text": {
                "0_H240": "小太郎阁下，这位就是果心居士大人。",
                "0_H241": "是段藏的……是啊，可以说是段藏的父母……",
                "0_H242": "与初代风魔大人也交流颇深，是的，是的。",
                "0_H243": "没错！　甚至可以算是小太郎阁下的亲人了！",
                "0_H244": "是的！"
            }
        },
        "0_H250": {
            "name": "对话 15",
            "text": {
                "0_H250": "果心居士大人，千万小心。",
                "0_H251": "那边的魔人，不对，应该是法师阁下是……",
                "0_H252": "啊，咦，不，不行啦，果心居士大人！",
                "0_H253": "法师阁下也是，彼此都是同一御主麾下的从者！",
                "0_H254": "啊啊，那，那是，果心礼装最终机关大连接的架势！",
                "0_H255": "果心居士大人！"
            }
        },
        "0_H260": {
            "name": "对话 16",
            "text": {
                "0_H260": "……感谢您召唤了段藏，御主。",
                "0_H261": "……我无比感谢。",
                "0_H262": "御主没有废弃，而是有效运用了即将坏掉的段藏。",
                "0_H263": "多亏了您，我才能与小太郎阁下……甚至与果心居士大人重逢……",
                "0_H264": "这份恩情，哪怕转生七次，我也要报答。",
                "0_H265": "一定会。"
            }
        },
        "0_H3160": {
            "name": "对话 10",
            "text": {
                "0_H3160": "小太郎阁下……",
                "0_H3161": "居然，居然变得如此优秀……",
                "0_H3162": "段藏还是第一次感受到如此温暖的感情。",
                "0_H3163": "而且，仿佛内心深处被紧紧揪住——"
            }
        },
        "0_I010": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 1",
            "text": {
                "0_I010": "御主，欢迎光临。",
                "0_I011": "这里是道具兑换商店！"
            }
        },
        "0_I020": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 2",
            "text": {
                "0_I020": "段藏担任这家店的店员。",
                "0_I021": "有什么要求还请不用客气尽情吩咐。"
            }
        },
        "0_I030": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 3",
            "text": {
                "0_I030": "隔壁的果心居士大人似乎是替身机关。",
                "0_I031": "就算段藏向她搭话也毫无反应……"
            }
        },
        "0_I040": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 4",
            "text": {
                "0_I040": "要这个吧。",
                "0_I041": "明白了！"
            }
        },
        "0_I050": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 5",
            "text": {
                "0_I050": "唔唔唔，眼光好高啊。",
                "0_I051": "啊啊……"
            }
        },
        "0_I060": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 6",
            "text": {
                "0_I060": "要为您包起来吗？",
                "0_I061": "不需要？　这样啊……"
            }
        },
        "0_I070": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 7",
            "text": {
                "0_I070": "这边的商店虽然已经关门了……",
                "0_I071": "但还请您随意。",
                "0_I072": "段藏会陪您聊天的。"
            }
        },
        "0_I080": {
            "name": "风云机关伊莉雅城\n～果心居士微不足道的野心～ 8",
            "text": {
                "0_I080": "啊啊，和小太郎阁下刚好错过了……",
                "0_I081": "呵呵，御主，难道您还有东西忘记兑换了？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "——加藤段藏。在此启动。",
                "0_S011": "要求输入指令，御主。",
                "0_S012": "段藏乃忍者，会服从各种命令。"
            }
        }
    },
    "ChrVoice_ids": [
        "603000"
    ]
}
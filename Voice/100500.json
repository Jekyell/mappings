{
    "id": 100500,
    "collectionNo": 5,
    "name": "尼禄·克劳狄乌斯",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "余很开心！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "欣赏余之才华，倾听雷鸣般喝彩！",
                "0_B051": "随后称赞吧，称赞这黄金的剧场！",
                "0_B052": "童女讴歌的荣华帝政！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "提升吧！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "圣者之泉啊！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "共同赢得胜利吧！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "华丽地起舞吧。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "是余个人的独演舞台吧。……是独演舞台吧！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "还不够，再继续夸奖余啊！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "这可是很痛的啊……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "对不起，御主……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "未能回应您的期待……原谅余……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "唔嗯！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "交给余吧！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "那就照这么办吧。"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "开启大门吧！　拉开独唱之幕吧！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "搞定了！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "这是何等罗马！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "原初之火！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "火焰之泉啊！"
            }
        },
        "0_C020": {
            "name": "尼禄祭再临 ～2018 Autumn～ 13",
            "text": {
                "0_C020": "这蔷薇就是梦想与愿望。\r 一定能得到好结果的！"
            }
        },
        "0_C030": {
            "name": "尼禄祭再临 ～2018 Autumn～ 14",
            "text": {
                "0_C030": "期待吧，余也会期待！\r 这个瞬间多么令人心情愉快啊。"
            }
        },
        "0_C040": {
            "name": "尼禄祭再临 ～2018 Autumn～ 15",
            "text": {
                "0_C040": "汝应该很清楚，余很慷慨。\r 没错，在历代皇帝中也是屈指可数地慷慨！"
            }
        },
        "0_C050": {
            "name": "尼禄祭再临 ～2018 Autumn～ 16",
            "text": {
                "0_C050": "道具越多越好。\r 话说，余的技能和等级都升到顶了吧？"
            }
        },
        "0_C060": {
            "name": "尼禄祭再临 ～2018 Autumn～ 17",
            "text": {
                "0_C060": "干得漂亮，居然能搜集到那么多！\r 心怀期待，开始最后的抽奖吧！"
            }
        },
        "0_C120": {
            "name": "尼禄祭再临 ～2018 Autumn～ 18",
            "text": {
                "0_C120": "正所谓梦想就是遥不可及的……\r 无限池，不，人生真艰难……"
            }
        },
        "0_C130": {
            "name": "尼禄祭再临 ～2018 Autumn～ 19",
            "text": {
                "0_C130": "噢噢，不愧是迦勒底的勇者！\r 还挺不错的嘛？　唔？　唔？"
            }
        },
        "0_C140": {
            "name": "尼禄祭再临 ～2018 Autumn～ 20",
            "text": {
                "0_C140": "获得看中的东西了吗？\r 如果得到了，余也会很高兴！"
            }
        },
        "0_C150": {
            "name": "尼禄祭再临 ～2018 Autumn～ 21",
            "text": {
                "0_C150": "如何，心满意足吗？　还是说……\r 唔唔，光靠表情还真难揣测呢……"
            }
        },
        "0_C160": {
            "name": "尼禄祭再临 ～2018 Autumn～ 22",
            "text": {
                "0_C160": "这是最后的奖励。\r 真亏汝能战斗到最后一刻，奥林匹亚的勇者啊！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "还会变得更强哦！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "余真是太天才了！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "唔嗯，这身打扮余也非常喜欢。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "唔唔……是否略有些操之过急？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "唔嗯！ 这才是所谓的繁华之帝政，蔷薇之喝彩！ 余很满意，现在已经丝毫没有后患啦！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日真是值得庆祝！  嗯？ 不举办举国欢庆的节日吗？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "有种正有活动在举行的预感！  御主，请抓紧时间！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "余已经受够了待命。去参加战斗吧！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "余虽为皇帝，但也是服侍御主的从者。 这两者都很重要。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "唔嗯，汝可以有效地使用余！  余也会好好差遣汝的！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？  当然是余啦！ 余可是奥林匹亚之花啊！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？  节制、反叛、没落！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯…… 唔嗯……这名字不好……头疼更为严重了……",
                "0_H081": "还是不要提起为妙……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "身为余的御主，不需要什么作为魔术师的才能。 只要美丽就够了。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "汝的声音很响亮呢。",
                "0_H101": "余很喜欢。 今后汝定要称呼余的名字，最依靠余。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "唔唔……尽管余说过汝不需要什么才能，但汝是否也太弱了？",
                "0_H111": "不会用剑也很令人担心。余也不能一直留在汝的身旁啊？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "决定了。今后汝要让余每次都上阵。 唔嗯，这样好，非常好！",
                "0_H121": "这样就能一直保护汝了呢，御主！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "余很充实。余是多么幸福的皇帝啊。 能够在远离罗马的世界里遇见了优秀的勇者。",
                "0_H131": "嗯？ 汝问谁？ 当然是汝啦，余引以为傲的御主啊。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "红Lancer那家伙，不过一段时间不见，居然实力又变强了……余也不能掉以轻心。",
                "0_H141": "快去准备一间专门用来练歌的房间，御主！"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "余和那个和服Caster……C狐有一些缘分。",
                "0_H151": "尽管余不是很讨厌那家伙，但总之，余和她是水火不容的对手。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "那不是舅父吗！",
                "0_H161": "多么年轻，以及多么狂暴。",
                "0_H162": "余确实见证了那一拳就能击倒敌人的雄姿。",
                "0_H163": "余也想身披和那一样的鲜红斗篷呀。"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "白色剑姬……是叫阿蒂拉嘛。",
                "0_H171": "唔唔……只是莫名有种感觉，感觉规模更大，或是尺寸更大，或是有种最终Boss的感觉……",
                "0_H172": "唔唔唔唔……不过算了！",
                "0_H173": "总之，看上去很幸福就好！"
            }
        },
        "0_I010": {
            "name": "尼禄祭再临 ～2017 Autumn～ 8",
            "text": {
                "0_I010": "快看余精心挑选的丰富的清单！",
                "0_I011": "神祖罗穆路斯也一定会拍胸脯打包票的！"
            }
        },
        "0_I020": {
            "name": "尼禄祭再临 ～2017 Autumn～ 9",
            "text": {
                "0_I020": "购物就该愉快点。",
                "0_I021": "好好斟酌吧。",
                "0_I022": "话说，余想要新的腕饰！"
            }
        },
        "0_I030": {
            "name": "尼禄祭再临 ～2017 Autumn～ 10",
            "text": {
                "0_I030": "这选择挺有眼光的。 一定要好好使用哦！"
            }
        },
        "0_I040": {
            "name": "尼禄祭再临 ～2017 Autumn～ 11",
            "text": {
                "0_I040": "哦……？ 居然玩这招。居然这样做啊！"
            }
        },
        "0_I050": {
            "name": "尼禄祭再临 ～2017 Autumn～ 12",
            "text": {
                "0_I050": "现在御主间原来流行这个啊。",
                "0_I051": "下次余一定多进一点货！"
            }
        },
        "0_I1010": {
            "name": "尼禄祭再临 ～2018 Autumn～ 7",
            "text": {
                "0_I1010": "罗马大感谢促销～～～！　尼禄祭今年已经是第三届了。",
                "0_I1011": "余对各位的惠顾表示衷心感谢！"
            }
        },
        "0_I1020": {
            "name": "尼禄祭再临 ～2018 Autumn～ 8",
            "text": {
                "0_I1020": "快看余精心挑选的丰富的清单！",
                "0_I1021": "神祖罗穆路斯也一定会拍胸脯打包票的！"
            }
        },
        "0_I1030": {
            "name": "尼禄祭再临 ～2018 Autumn～ 9",
            "text": {
                "0_I1030": "不用客气，尽量拿去吧。",
                "0_I1031": "希望汝能好好搜集，充分使用，不要留有遗憾！"
            }
        },
        "0_I1040": {
            "name": "尼禄祭再临 ～2018 Autumn～ 10",
            "text": {
                "0_I1040": "这选择挺有眼光的。",
                "0_I1041": "一定要好好使用哦！"
            }
        },
        "0_I1050": {
            "name": "尼禄祭再临 ～2018 Autumn～ 11",
            "text": {
                "0_I1050": "哦……？ 居然玩这招。居然这样做啊！"
            }
        },
        "0_I110": {
            "name": "尼禄祭再临 ～2017 Autumn～ 13",
            "text": {
                "0_I110": "实在非常遗憾……这家皇帝专卖店也很快就要关门了。",
                "0_I111": "感谢各位这段时间内的惠顾！",
                "0_I112": "唔，真寂寞呢……"
            }
        },
        "0_I1110": {
            "name": "尼禄祭再临 ～2018 Autumn～ 12",
            "text": {
                "0_I1110": "罗～马～之～光～，",
                "0_I1111": "余之名为尼～禄～～♪",
                "0_I1112": "遗憾的是店快要关门了。",
                "0_I1113": "不要露出那种寂寞的表情，这会让余也寂寞的啊……",
                "0_I1114": "明年一定要继续在竞技场无限毅力！"
            }
        },
        "0_I120": {
            "name": "尼禄祭再临 ～2017 Autumn～ 14",
            "text": {
                "0_I120": "关店大甩卖！",
                "0_I121": "……无法做到这点也是罗马的为难之处呢。",
                "0_I122": "但想要交换道具就趁现在！",
                "0_I123": "『别以为机会会再次敲响你的门』哦！"
            }
        },
        "0_N010": {
            "name": "尼禄祭再临 ～2017 Autumn～ 1",
            "text": {
                "0_N010": "激斗辛苦了！",
                "0_N011": "这次余一点也没有吝啬，你大可不客气地抽。",
                "0_N012": "唔～抽出什么来了呢？"
            }
        },
        "0_N020": {
            "name": "尼禄祭再临 ～2017 Autumn～ 2",
            "text": {
                "0_N020": "余要抽了哦！　余可要抽了哦！",
                "0_N021": "这已经相当于胜利了嘛！"
            }
        },
        "0_N030": {
            "name": "尼禄祭再临 ～2017 Autumn～ 3",
            "text": {
                "0_N030": "赢得胜利、战果，获得报酬！",
                "0_N031": "正可谓勇者的荣誉啊。",
                "0_N032": "大家都很高兴，余也很高兴！"
            }
        },
        "0_N040": {
            "name": "尼禄祭再临 ～2017 Autumn～ 4",
            "text": {
                "0_N040": "快看余准备的奖品！　特别慷慨吧？　是罗马最棒的吧！",
                "0_N041": "……不过偶尔也会落选，这点还请谅解哦？"
            }
        },
        "0_N050": {
            "name": "尼禄祭再临 ～2017 Autumn～ 5",
            "text": {
                "0_N050": "余理解你被商品吸引的心情，但别忘了注意看说明。",
                "0_N051": "那里偶尔也会写一些不错的事哦。"
            }
        },
        "0_N060": {
            "name": "尼禄祭再临 ～2017 Autumn～ 6",
            "text": {
                "0_N060": "华美的祭典结束了……余十分伤心。",
                "0_N061": "但是，过去的事已经过去了。迟早还会再见的！",
                "0_N062": "这个先姑且不论，汝可千万别忘了交换商品哦！　否则太浪费了！"
            }
        },
        "0_N070": {
            "name": "尼禄祭再临 ～2017 Autumn～ 7",
            "text": {
                "0_N070": "条条大路通罗马！",
                "0_N071": "这次的祭典虽然结束了，但下次祭典一定会再次举行。",
                "0_N072": "在那个时候到来前……",
                "0_N073": "See you next Roma！"
            }
        },
        "0_N1010": {
            "name": "尼禄祭再临 ～2018 Autumn～\r 1",
            "text": {
                "0_N1010": "唔嗯唔嗯，真亏汝搜集了那么多余的蔷薇！",
                "0_N1011": "现在是期待已久的奖励时间！"
            }
        },
        "0_N1020": {
            "name": "尼禄祭再临 ～2018 Autumn～\r 2",
            "text": {
                "0_N1020": "来得正好，勇者啊！",
                "0_N1021": "来吧，将战斗的成果全部交给余！"
            }
        },
        "0_N1030": {
            "name": "尼禄祭再临 ～2018 Autumn～\r 3",
            "text": {
                "0_N1030": "竞技场的战斗固然白热，这里的战斗也异常白热吧？",
                "0_N1031": "正所谓手心里捏着一把汗。"
            }
        },
        "0_N1040": {
            "name": "尼禄祭再临 ～2018 Autumn～\r 4",
            "text": {
                "0_N1040": "喀啷喀啷～……咕噜咕噜～～……",
                "0_N1041": "旋转式的抽选非常好，这种毫无意义的旋转感才是最棒的……",
                "0_N1042": "尽管如果能更容易抽中就更好了……"
            }
        },
        "0_N1050": {
            "name": "尼禄祭再临 ～2018 Autumn～\r 5",
            "text": {
                "0_N1050": "尼禄祭是站在贪婪御主一边的。",
                "0_N1051": "只要还有时间，只要果实还有剩余，就向着毕业冲刺吧！"
            }
        },
        "0_N1060": {
            "name": "尼禄祭再临 ～2018 Autumn～ 6",
            "text": {
                "0_N1060": "实在非常遗憾……这家皇帝专卖店也很快就要关门了。",
                "0_N1061": "感谢各位这段时间内的惠顾！",
                "0_N1062": "唔，真寂寞呢……"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者 Saber。",
                "0_S011": "尼禄·克劳狄乌斯，回应召唤而来！",
                "0_S012": "唔，真亏汝选择了余！",
                "0_S013": "真是个懂行的魔术师啊！"
            }
        },
        "11_B030": {
            "name": "攻击 3 奥林匹亚体操服",
            "text": {
                "11_B030": "战斗！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 奥林匹亚体操服",
            "text": {
                "11_B040": "哼哼哼，大·逆·转！"
            }
        },
        "11_B080": {
            "name": "技能 1 奥林匹亚体操服",
            "text": {
                "11_B080": "感谢大家的声援！"
            }
        },
        "11_B090": {
            "name": "技能 2 奥林匹亚体操服",
            "text": {
                "11_B090": "午饭来一个多层大饭盒吧！"
            }
        },
        "11_B100": {
            "name": "开始 1 奥林匹亚体操服",
            "text": {
                "11_B100": "来吧，将是红组获胜！"
            }
        },
        "11_B110": {
            "name": "开始 2 奥林匹亚体操服",
            "text": {
                "11_B110": "名牌都佩戴好了吧？"
            }
        },
        "11_B120": {
            "name": "胜利 1 奥林匹亚体操服",
            "text": {
                "11_B120": "就像这样，余也很擅长运动哦！"
            }
        },
        "11_B130": {
            "name": "胜利 2 奥林匹亚体操服",
            "text": {
                "11_B130": "汝等大可把余抛起来庆祝！"
            }
        },
        "11_B150": {
            "name": "受击 1 奥林匹亚体操服",
            "text": {
                "11_B150": "绑头带被夺走了～！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 奥林匹亚体操服",
            "text": {
                "11_B160": "没想到……居然会逆转……输了……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 奥林匹亚体操服",
            "text": {
                "11_B170": "雨天……中……止"
            }
        },
        "11_B180": {
            "name": "指令卡 1 奥林匹亚体操服",
            "text": {
                "11_B180": "预备，咚！"
            }
        },
        "11_B190": {
            "name": "指令卡 2 奥林匹亚体操服",
            "text": {
                "11_B190": "余会拿下第一名！"
            }
        },
        "11_B200": {
            "name": "指令卡 3 奥林匹亚体操服",
            "text": {
                "11_B200": "好好看着吧！"
            }
        },
        "11_B210": {
            "name": "宝具卡 奥林匹亚体操服",
            "text": {
                "11_B210": "拉响信号吧！　终幕即将开始！"
            }
        },
        "11_B410": {
            "name": "攻击 4 奥林匹亚体操服",
            "text": {
                "11_B410": "没什么大不了的！"
            }
        },
        "11_B420": {
            "name": "攻击 5 奥林匹亚体操服",
            "text": {
                "11_B420": "好好休息吧！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 奥林匹亚体操服",
            "text": {
                "11_B440": "给红组，加一万点！"
            }
        },
        "11_B480": {
            "name": "技能 3 奥林匹亚体操服",
            "text": {
                "11_B480": "接力赛是最棒的！"
            }
        },
        "11_G010": {
            "name": "升级 奥林匹亚体操服",
            "text": {
                "11_G010": "还会变得更强哦！"
            }
        },
        "11_G060": {
            "name": "灵衣开放 奥林匹亚体操服",
            "text": {
                "11_G060": "因此，这是备受大家期待的体操服！",
                "11_G061": "余在祭典上也必须好好打扮一番才行，",
                "11_G062": "唔嗯，身心都变得轻快多了！",
                "11_G063": "如何？　很适合余吧，御主？"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 奥林匹亚体操服",
            "text": {
                "11_H9000": "尽管体操服令人轻松又不错，但是，唔嗯……",
                "11_H9001": "不知为何，周围投来的视线好炙热啊。",
                "11_H9002": "都还没运动呢，已经快要出汗了……"
            }
        },
        "12_B020": {
            "name": "攻击 2 维纳斯的丝绸",
            "text": {
                "12_B020": "激烈地！"
            }
        },
        "12_B030": {
            "name": "攻击 3 维纳斯的丝绸",
            "text": {
                "12_B030": "热情地！"
            }
        },
        "12_B040": {
            "name": "额外攻击 1 维纳斯的丝绸",
            "text": {
                "12_B040": "干得漂亮，来侍奉余吧！"
            }
        },
        "12_B080": {
            "name": "技能 1 维纳斯的丝绸",
            "text": {
                "12_B080": "老实等着吧。"
            }
        },
        "12_B090": {
            "name": "技能 2 维纳斯的丝绸",
            "text": {
                "12_B090": "但愿对方能配得上当余的对手。"
            }
        },
        "12_B100": {
            "name": "开始 1 维纳斯的丝绸",
            "text": {
                "12_B100": "这庆典相当不错，讲究排场举杯庆祝吧！"
            }
        },
        "12_B110": {
            "name": "开始 2 维纳斯的丝绸",
            "text": {
                "12_B110": "今晚可不会让你入睡。沉醉于颓废之中吧。"
            }
        },
        "12_B120": {
            "name": "胜利 1 维纳斯的丝绸",
            "text": {
                "12_B120": "这是理所当然的胜利。 还享受过程吗，御主？"
            }
        },
        "12_B130": {
            "name": "胜利 2 维纳斯的丝绸",
            "text": {
                "12_B130": "比社交界还要快～！ ……不对。华美的身姿也不错吧？"
            }
        },
        "12_B150": {
            "name": "受击 1 维纳斯的丝绸",
            "text": {
                "12_B150": "余尚不会毁灭……！"
            }
        },
        "12_B160": {
            "name": "无法战斗 1 维纳斯的丝绸",
            "text": {
                "12_B160": "竟然能够将可以复苏三次的余逼入如此绝境……"
            }
        },
        "12_B170": {
            "name": "无法战斗 2 维纳斯的丝绸",
            "text": {
                "12_B170": "鲜花生命短暂。 由于不舍得污损服饰，余先回去了。"
            }
        },
        "12_B180": {
            "name": "指令卡 1 维纳斯的丝绸",
            "text": {
                "12_B180": "让余好好疼爱你吧。"
            }
        },
        "12_B190": {
            "name": "指令卡 2 维纳斯的丝绸",
            "text": {
                "12_B190": "差不多是这里吧？"
            }
        },
        "12_B200": {
            "name": "指令卡 3 维纳斯的丝绸",
            "text": {
                "12_B200": "余很喜欢哦。"
            }
        },
        "12_B210": {
            "name": "宝具卡 维纳斯的丝绸",
            "text": {
                "12_B210": "喝彩永不停歇！　现在是沉醉之刻！"
            }
        },
        "12_B410": {
            "name": "攻击 4 维纳斯的丝绸",
            "text": {
                "12_B410": "令人陶醉！"
            }
        },
        "12_B420": {
            "name": "攻击 5 维纳斯的丝绸",
            "text": {
                "12_B420": "太不识趣了哦？"
            }
        },
        "12_B440": {
            "name": "额外攻击 2 维纳斯的丝绸",
            "text": {
                "12_B440": "用这个做奖励就行了吧？"
            }
        },
        "12_B480": {
            "name": "技能 3 维纳斯的丝绸",
            "text": {
                "12_B480": "宴会才刚刚开始吧？"
            }
        },
        "12_G010": {
            "name": "升级 维纳斯的丝绸",
            "text": {
                "12_G010": "还会变得更强哦！"
            }
        },
        "12_G060": {
            "name": "灵衣开放 维纳斯的丝绸",
            "text": {
                "12_G060": "呵呵。余其实也并不会一直穿男装。",
                "12_G061": "那不过是余的爱好罢了。",
                "12_G062": "当身为主人举办宴会的时候，还是会换上这种切合时宜的装束啦。",
                "12_G063": "汝有没有——",
                "12_G064": "重新迷上余呢，御主？"
            }
        },
        "12_H9000": {
            "name": "关于灵衣 维纳斯的丝绸",
            "text": {
                "12_H9000": "喜欢这条礼裙吗？",
                "12_H9001": "唔嗯，不用隐瞒啦，因为余也非常喜欢！",
                "12_H9002": "这条礼裙是繁荣与热情的象征。",
                "12_H9003": "表现了余敬爱的美之女神维纳斯的一面。",
                "12_H9004": "愿这趟旅途今后也充满了光辉与慈爱。"
            }
        },
        "13_B010": {
            "name": "攻击 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B010": "耶～☆"
            }
        },
        "13_B020": {
            "name": "攻击 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B020": "要豪华！"
            }
        },
        "13_B030": {
            "name": "攻击 3 霸岚蛮娇狂想曲",
            "text": {
                "13_B030": "要快乐！"
            }
        },
        "13_B040": {
            "name": "额外攻击 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B040": "尼禄尼禄～频道～～～！"
            }
        },
        "13_B080": {
            "name": "技能 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B080": "要豪华，而且还要轻快！"
            }
        },
        "13_B090": {
            "name": "技能 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B090": "热情的民谣也不错呢？"
            }
        },
        "13_B100": {
            "name": "开始 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B100": "偶像皇帝，当然就是余啦☆"
            }
        },
        "13_B110": {
            "name": "开始 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B110": "唔嗯唔嗯！　大家都嗨起来了吧！"
            }
        },
        "13_B120": {
            "name": "胜利 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B120": "余，心满意足！　声援竞技场！！ 一事归一事，荧光棒都要带回去哦？"
            }
        },
        "13_B130": {
            "name": "胜利 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B130": "唱歌跳舞都交给余吧！ 甚至感觉这个更像余的本职呢！"
            }
        },
        "13_B140": {
            "name": "受击 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B140": "辛苦啦！"
            }
        },
        "13_B150": {
            "name": "受击 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B150": "唔哇，出问题了吗！？"
            }
        },
        "13_B160": {
            "name": "无法战斗 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B160": "唔嗯，差不多就这样吧。 余回休息室啦。"
            }
        },
        "13_B170": {
            "name": "无法战斗 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B170": "偶像是不灭的。 明天见，演出第二天再见吧！"
            }
        },
        "13_B180": {
            "name": "指令卡 1 霸岚蛮娇狂想曲",
            "text": {
                "13_B180": "一、"
            }
        },
        "13_B190": {
            "name": "指令卡 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B190": "二、"
            }
        },
        "13_B200": {
            "name": "指令卡 3 霸岚蛮娇狂想曲",
            "text": {
                "13_B200": "三♡"
            }
        },
        "13_B210": {
            "name": "宝具卡 霸岚蛮娇狂想曲",
            "text": {
                "13_B210": "会场内的大家～，谢谢～～！"
            }
        },
        "13_B410": {
            "name": "攻击 4 霸岚蛮娇狂想曲",
            "text": {
                "13_B410": "犹如燃烧！"
            }
        },
        "13_B420": {
            "name": "攻击 5 霸岚蛮娇狂想曲",
            "text": {
                "13_B420": "UK～摇滚～～！"
            }
        },
        "13_B440": {
            "name": "额外攻击 2 霸岚蛮娇狂想曲",
            "text": {
                "13_B440": "最后一曲，都嗨起来吧～！"
            }
        },
        "13_B480": {
            "name": "技能 3 霸岚蛮娇狂想曲",
            "text": {
                "13_B480": "安可最多只能三次！"
            }
        },
        "13_G010": {
            "name": "升级 霸岚蛮娇狂想曲",
            "text": {
                "13_G010": "还会变得更强哦！"
            }
        },
        "13_G060": {
            "name": "灵衣开放 霸岚蛮娇狂想曲",
            "text": {
                "13_G060": "豪华而壮丽。",
                "13_G061": "皇帝兼偶像。",
                "13_G062": "这英气逼人的裙子简直就像帝权荣耀的表现吧？",
                "13_G063": "呵呵……真期待舞台表演呢！"
            }
        },
        "13_H9000": {
            "name": "关于灵衣 霸岚蛮娇狂想曲",
            "text": {
                "13_H9000": "期待已久的偶像活动～～！",
                "13_H9001": "原本也考虑过蔷薇的裙子，但这次决定用鲜艳的格子花纹。",
                "13_H9002": "既可爱，又英挺吧？",
                "13_H9003": "唔嗯唔嗯，余也这么认为！",
                "13_H9004": "和伊丽莎白二重唱也在考虑范围内，所以将我俩编入同一支队伍吧！"
            }
        }
    },

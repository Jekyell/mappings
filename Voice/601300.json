{
    "id": 601300,
    "collectionNo": 44,
    "name": "剧院魅影",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "与我一同歌唱吧　再一次……"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "……歌唱吧，歌唱吧，我的天使。",
                "0_B051": "吾之情歌只在地狱回响！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "向着浅睡的你　我歌唱着……"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "向着深爱的你　我歌唱着……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "看见我面容之人将会感到恐惧吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "你也——你也——你也亦然——"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "你不再歌唱　再也不会……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "未闻凯歌　未闻凯歌啊……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "再也……再也……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "听不见了　你的声音……"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "是……"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "明白了……"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "让我们一同歌唱吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "尽情地……"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "你也好好享受吧"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "为我歌唱吧……"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "没错！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "歌啊，歌啊……"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "必须调音才行"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "听到了……啊啊，啊啊……"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "听不见，听不见……"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "是否合您心意呢……"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "啊啊，果然……听不见……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "再见了……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "听不见，听不见！",
                "0_B801": "克里斯蒂娜，克里斯蒂娜！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "心爱的你。",
                "0_B811": "心爱的你。",
                "0_B812": "——呜呼，在此奏响地狱吧！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "献给你……"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "没有喝彩……没有喝彩……"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "来吧，演出时刻已至。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我的灵魂与声音在此……"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "不行啊　不行啊　目睹我丑陋容颜之人　已然──"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "我不会说原谅我……这绝不能被原谅……我的灵魂与声音将永远被囚禁……",
                "0_G051": "呜呼，克里斯蒂娜，克里斯蒂娜……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "美妙之声响起时　起源之时",
                "0_H011": "美丽从此开始——"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "被限制的时刻　不及永恒的短暂—— 来吧，走吧。让我们一起。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "拥有美妙声音的人儿啊　我们走吧"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我的主人　拥有美妙声音的人儿",
                "0_H041": "呜呼……"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "克里斯蒂娜…… 你的声音　宛若克里斯蒂娜"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "你的声音很美……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我诅咒丑陋之物　因此我诅咒自己"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我向圣杯祈愿　愿你成为最优秀的歌姬！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "歌姬啊　我会与你相伴"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "你才是歌姬　仍在人间的克里斯蒂娜"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "歌唱吧　歌唱吧　歌声嘹亮 歌唱爱　歌唱希望　歌唱死亡！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "如此美丽……你的歌声　美丽的形状……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "……相伴左右　随着歌声　永远　永远"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "你不能看我的面容……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "我的面具　为遮挡我的丑陋之物……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "你才是克里斯蒂娜　我所爱之声"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "让我倾听　你的声音吧",
                "0_H171": "向着我　向着我　向着我"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "别过来……",
                "0_H211": "别过来……",
                "0_H212": "南丁格尔……！"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "海德？",
                "0_H221": "啊啊　他并不坏……",
                "0_H222": "杰基尔？",
                "0_H223": "啊啊　他也不坏……"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "啊啊……",
                "0_H231": "他的旋律",
                "0_H232": "真不错"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "啊啊……",
                "0_H241": "她的旋律……",
                "0_H242": "相当不错……"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "……面具，您现在仍带着吧。",
                "0_H251": "非常感谢，御主。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "看见我面容之人 将会感到恐惧吧──你也亦然。"
            }
        }
    },
    "ChrVoice_ids": [
        "601300"
    ]
}
{
    "id": 504200,
    "collectionNo": 237,
    "name": "紫式部",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "这样如何？　呵呵呵。"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "这样——然后再这样吧。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "这里如此这般这样写。"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "丧衣色淡",
                "0_B051": "因遵制，",
                "0_B052": "袖泪成渊痛哭多。",
                "0_B053": "——『源氏物语·葵·物之怪』。"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "相逢江海上——呵呵呵。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "这是晴明大人直传。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "一口气写完吧？"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "尽管我不太擅长这种——"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "无比可悲——"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "无比可笑。呵呵呵。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "怎么会……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "明明……只写到一半……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "我认输了……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "嗯，好。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "哎呀！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "这样比较好。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "那请恕我放肆。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "创作灵感如泉涌！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "啊哇哇哇……呀！ 对不起……"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "预备！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "我想到了！　沙沙挥笔疾书。"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "把那边重新写过吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "夜半云中月，匆匆无影踪——"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "但愿能是个有趣的故事——"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "差不多就这样吧。嗯。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "我对道长大人不做评价……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "香子我，",
                "0_B801": "虽不熟咒。",
                "0_B802": "然若有心。",
                "0_B803": "——『源氏物语·葵·物之怪』。"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "沙沙挥笔疾书。"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "这段剧情就当做废案吧。"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "原来如此，魔力啊。",
                "0_G011": "是这种机制啊。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "衣裳……没有更换呢。",
                "0_G021": "稍微有些遗憾。呵呵呵。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我更换了衣裳。",
                "0_G031": "心情也发生了变化呢。呵呵。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "哎呀，这是——",
                "0_G041": "呵呵，居然是十二单衣！",
                "0_G042": "怎么样。适合我吗？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "词与文能表现、传达一切，甚至连时空都能跨越。",
                "0_G051": "这与空中璀璨的群星一样。",
                "0_G052": "哪怕无法成为永恒，但能将原本无形的感情化为有形——",
                "0_G053": "呜呼——"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "是吗，生日。",
                "0_H011": "真是恭喜了。",
                "0_H012": "那我就送您一份礼物吧。",
                "0_H013": "——这是为您准备的宝贵的一册。一定要认真阅读哦。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "活动。我知道我知道。",
                "0_H021": "用现代的说法就是——",
                "0_H022": "长期取材旅行……对吧！　我们走吧！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "您说的是任务吧。",
                "0_H031": "我明白了。",
                "0_H032": "用现代的说法，就是……取材吧？　我说得没错吧？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "御主与从者。",
                "0_H041": "嗯。我非常清楚。",
                "0_H042": "但除此之外，我也是迦勒底的图书馆司书，还请不要忘记。呵呵。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "尽管我很习惯侍奉他人……",
                "0_H051": "但对阴阳道的实力没有什么自信。",
                "0_H052": "虽说比生前要更会运用了——",
                "0_H053": "可如果拖累了您，还请见谅。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "我喜欢竹取物语，也喜欢伊势物语！",
                "0_H061": "古今集是我每晚都会阅读的，后撰集也是。",
                "0_H062": "哈姆雷特、李尔王、红楼梦、小美人鱼、冰雪女王……都如此美妙。",
                "0_H064": "我喜欢的有很多，今后会喜欢上的或许会有更多。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？",
                "0_H071": "让我想想……",
                "0_H072": "应该是不会爱惜书籍的人吧？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯是危险的东西，对吧？",
                "0_H081": "正所谓君子不立危墙之下哦，御主。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "欢迎光临。",
                "0_H091": "御主。",
                "0_H092": "您想要找书吗？",
                "0_H093": "我这里可以出借纸质书。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "您喜欢书吗？",
                "0_H101": "啊啊，那么……",
                "0_H102": "汲取无数言辞，将人们的历程作为食粮，您一定也会像夜空中的繁星一般闪耀吧。",
                "0_H103": "多么美妙。呵呵。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我是描写情感的英灵。",
                "0_H111": "只有人对人的思念之情，才是我要描写、要整理之物。",
                "0_H112": "用当代的话来说，吸引我的就是「书」。",
                "0_H113": "电子书？",
                "0_H114": "这可不行。还是纸质书最好。",
                "0_H115": "毕竟，这个才较为浓厚地继承了卷轴与书册那时候的氛围呀。",
                "0_H116": "呵呵呵，今天您想阅读什么呢？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "自我年轻丧夫之后，就开始撰写故事了。",
                "0_H121": "写的就是源氏物语。",
                "0_H122": "……嗯，对。撰写故事是多么的！",
                "0_H123": "多么的快乐。当我奋笔疾书时，太阳转眼间就落山了。",
                "0_H124": "别说一天了，甚至连季节的变化都会被抛在脑后……",
                "0_H125": "那真的是一段愉快的时光。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "现在的我，已经不会再去撰写新的故事了。",
                "0_H131": "最多只会重新编写过去的故事……或是写写日记罢了。",
                "0_H132": "我是从者。是刻在人理上的过去的香子，过去的紫式部的影子。",
                "0_H133": "再也不会吟诵新的和歌……",
                "0_H134": "……啊？　您觉得没有这种事？",
                "0_H135": "觉得像安徒生或是莎士比亚那样干脆顺其自然也完全没问题？",
                "0_H136": "呵呵，尽管我不是很懂——但御主，感谢您的体贴。",
                "0_H137": "那么……如果真的到了那种时候，",
                "0_H138": "就容我就不客气地撰写故事、吟诵诗歌吧。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "赖光大人。是秀美过人，但有些可怕的武家大人。",
                "0_H171": "与从前丝毫未变——",
                "0_H172": "……不，似乎有些……",
                "0_H173": "总觉得似乎有些不太一样……？"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "赖光大人！？",
                "0_H181": "那身打扮究竟是！　啊哇哇！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "聚集于大江山的童子们。",
                "0_H191": "真可怕，真可怕，众鬼们……",
                "0_H192": "请千万小心，御主。",
                "0_H193": "鬼乃鬼，非人也。",
                "0_H194": "不知是会被掠夺，还是被撕碎……若对象仅为生命，还算是幸运的。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "金时大人。赖光四天王的金时大人。",
                "0_H201": "真令人怀念。不过您遮住面容的那个是……",
                "0_H202": "呃，太阳眼镜。",
                "0_H203": "不，这我当然知道，但为何要这般？"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "俵藤太大人……！？",
                "0_H211": "不，藤原秀乡大人！",
                "0_H212": "居居……居然……",
                "0_H213": "我是香子，藤原香子，是藤原北家良门大人的后裔。",
                "0_H214": "啊哇哇哇……！"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "您最近心情可好，老师？",
                "0_H221": "……不不，请容我称呼您为老师。",
                "0_H222": "我随时恭候您向我的图书馆捐赠「新作」。",
                "0_H223": "我认为，一旦被编织而出的故事，",
                "0_H224": "都期待着自己被人阅读。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "请问，我可以听您讲故事吗？",
                "0_H231": "可以吗？"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "请问，福尔摩斯先生。",
                "0_H241": "您还有其他初版的血字的研究吧？",
                "0_H242": "能否请您……寄赠给我呢？",
                "0_H243": "我当然不会白要。请务必考虑。"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "那位一定很喜欢图书馆吧……"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "童谣——",
                "0_H261": "是这样啊，您是西洋的……！",
                "0_H262": "也就是说，您的身心，所有一切都是由感情与言辞编织而成的吧……",
                "0_H263": "啊啊……这是何等的……"
            }
        },
        "0_H270": {
            "name": "对话 14",
            "text": {
                "0_H270": "恺撒大人，您不打算写新的战记吗？",
                "0_H271": "您意下如何？"
            }
        },
        "0_H280": {
            "name": "对话 15",
            "text": {
                "0_H280": "刑部姬大人！",
                "0_H281": "我们来办读书会吧！"
            }
        },
        "0_H290": {
            "name": "对话 16",
            "text": {
                "0_H290": "渡边纲大人，是检非违使的……！",
                "0_H291": "我我……我没做什么违法的事哦？",
                "0_H292": "我怎么可能有什么道长大人的黑账本什么的嘛，啊哈哈……"
            }
        },
        "0_H300": {
            "name": "对话 17",
            "text": {
                "0_H300": "法师大人。具备了犹如野兽般的美……",
                "0_H301": "完全没变——",
                "0_H302": "真是……完全没变。"
            }
        },
        "0_I010": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 8",
            "text": {
                "0_I010": "平时我从不经手书籍以外的东西……",
                "0_I011": "这次情况特殊。",
                "0_I012": "就当是慷慨大放送吧！"
            }
        },
        "0_I020": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 9",
            "text": {
                "0_I020": "欢迎光临。",
                "0_I021": "这里可以挑选您中意的道具。",
                "0_I022": "都是些品质优良的商品……",
                "0_I023": "应该是这样，您觉得如何……？"
            }
        },
        "0_I030": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 10",
            "text": {
                "0_I030": "原来如此，您真是好眼光……",
                "0_I031": "呃，对不起。",
                "0_I032": "我有点不懂装懂了。"
            }
        },
        "0_I040": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 11",
            "text": {
                "0_I040": "明白了。",
                "0_I041": "但愿这件物品能帮上您的忙。"
            }
        },
        "0_I050": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 12",
            "text": {
                "0_I050": "明白了。",
                "0_I051": "话说，要不要休息一会儿呢？",
                "0_I052": "一说到休息，当然就是读书吧！",
                "0_I053": "……啊，您现在很忙吧，是这样吧。",
                "0_I054": "嗯……"
            }
        },
        "0_I060": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 13",
            "text": {
                "0_I060": "这个柜台已经关门了。",
                "0_I061": "啊，难道您忘了什么吗？"
            }
        },
        "0_I070": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 14",
            "text": {
                "0_I070": "辛苦了。",
                "0_I071": "尽管商店已经关门了，但我还会在地下图书馆。",
                "0_I072": "还请随时光临。"
            }
        },
        "0_I210": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 15",
            "text": {
                "0_I210": "我会为您声援的。",
                "0_I211": "请加油！"
            }
        },
        "0_I220": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 16",
            "text": {
                "0_I220": "破罐子破摔？",
                "0_I221": "不，请真心诚意地去做。",
                "0_I222": "但愿您的心意能够传达。"
            }
        },
        "0_I310": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 17",
            "text": {
                "0_I310": "情人节过得如何？",
                "0_I311": "看您的样子……",
                "0_I312": "呵呵，一定是获得了美妙的回忆吧！"
            }
        },
        "0_I320": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 18",
            "text": {
                "0_I320": "您似乎非常疲劳呢。",
                "0_I321": "请好好休息。"
            }
        },
        "0_I420": {
            "name": "情人节2020\n语音＆书信·来收集吧！～紫式部与7册咒本～ 19",
            "text": {
                "0_I420": "欢迎光临。",

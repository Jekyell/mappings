{
    "id": 505400,
    "collectionNo": 404,
    "name": "由井正雪",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "瞠目吧！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "vuur（炎）！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "消散吧！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "六瓣雪花，",
                "0_B051": "睑之彼方，",
                "0_B052": "神田连雀。",
                "0_B053": "——烈士徇名·不惜身命。"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "好了……"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "下一招是……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "由我由井正雪做你的对手。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "不用手下留情。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "无聊……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "我说过，不用手下留情……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "不能输……怎么能，输掉……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "啊啊，是吗……会在这里……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "嗯。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "知道了。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "我试试。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "宝具吗！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "我的，宝具……！"
            }
        },
        "0_B2410": {
            "name": "攻击 13",
            "text": {
                "0_B2410": "好好尝尝滋味吧！"
            }
        },
        "0_B2420": {
            "name": "攻击 14",
            "text": {
                "0_B2420": "这样就能……！"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "地水火风！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "倾注所有力量。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "哼！……消失吧。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "这样的话……"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "得手了！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "嗯……"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "森宗意轩老师如是说……"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "随时可以。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "拿命来吧。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "必须，始终如斯。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "你选错了挑战的对手呢。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "宗意轩，大人……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "使不出，力气来……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "刻印，激发。",
                "0_B801": "吾之思念，吾之血潮……",
                "0_B802": "愿能传达给您。",
                "0_B803": "——烈士徇名·不惜身命。"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "吾之元祖，",
                "0_B811": "吾之源头的各位啊，",
                "0_B812": "请赐予我力量。",
                "0_B813": "——烈士徇名·不惜身命。"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "认命吧！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "魔力上升……"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "赐我力量！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "……感激不尽。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "为我这样的人……",
                "0_G021": "倾注那么多魔力和资源，您不后悔吗？",
                "0_G022": "……是吗。",
                "0_G023": "那就好。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "……啊啊，令人心安呢。",
                "0_G031": "不。因为我生前经常穿这身打扮。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "灵基……",
                "0_G041": "力量充满全身。",
                "0_G042": "谢谢你，感觉比生前更能战斗了。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "……我真的，",
                "0_G051": "对您的耐心无言以对。",
                "0_G052": "啊，不，我这是在赞赏您。",
                "0_G053": "我想表达的……是我的赞赏。",
                "0_G054": "谢谢您给了我那么多力量。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "真的……可以吗？"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "嗯，谢谢你……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "为您的生日送上祝福。",
                "0_H011": "愿您今后一年幸福相随。",
                "0_H012": "……生日快乐，御主。",
                "0_H013": "今后我也会继续成为你的助力。",
                "0_H014": "……除此之外，还有什么想要的东西吗？",
                "0_H015": "但愿是我能够满足的东西……"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举行叫活动的事件。",
                "0_H021": "御主。",
                "0_H022": "恕我冒昧，我建议您前去参加。",
                "0_H023": "资源储备还是保持一定存量以上为好。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "那个……您真的可以，待在这里吗？",
                "0_H031": "明明还有工作要做……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "现在，我是从者啊。",
                "0_H041": "呵呵，不，只是觉得还会发生这种事呢。",
                "0_H042": "只是自说自话而已。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "……您在做什么呀，御主。",
                "0_H051": "写报告？",
                "0_H052": "原来如此，作战的报告啊。",
                "0_H053": "好，我来帮忙吧。我很擅长撰写。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "正确之物，美丽之物，善良之物。",
                "0_H061": "这就是我所爱的美德。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "邪恶之物，悲伤之物。",
                "0_H071": "我厌恶世间所有扭曲的存在。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "……万能的愿望机。",
                "0_H081": "盈月的根源，原型。",
                "0_H082": "若说我不想要，那肯定是在撒谎，但现在的我是您的从者。",
                "0_H083": "我不会做你不希望的事。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "是否有意学习兵法呢，御主？",
                "0_H091": "如何用兵、如何作战，这对您来说应该很有助益吧。",
                "0_H092": "虽说，那个……在坐拥太公望阁下与孔明阁下的迦勒底，或许没有我出马的余地……",
                "0_H093": "但或许由我这样凡人来教导，会让您更容易学会吧……"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "御主、御主！",
                "0_H101": "我刚才在图书馆里查到了一些资料……",
                "0_H102": "说在泛人类史中……由井正雪是位男性的名字……",
                "0_H103": "而且还是企图颠覆幕府的谋反之辈……！",
                "0_H104": "这，这是真的吗？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "……嗯。",
                "0_H111": "起初确实有些惊讶，但我现在已经接受了。",
                "0_H112": "我所生活的时代肯定是脱离泛人类史的事象吧。",
                "0_H113": "由井正雪原本并不是我，在神田连雀开设私塾的也不是我。",
                "0_H114": "……若说一点也不失落，那肯定是在撒谎。",
                "0_H115": "但我也终于解开了一些疑问。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我的一位弟子曾经说过。",
                "0_H121": "『由井正雪过于清廉，并非人类可及的程度。』",
                "0_H122": "……没错，我不是人类。",
                "0_H123": "是远涉异国学习了炼金术的吾师森宗意轩老师创造的人造人。",
                "0_H124": "我之所以不能无视世间的扭曲，之所以追求正确……",
                "0_H125": "肯定，是因为我乃是身处世间常理之外的存在吧。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "……我，并不是人。",
                "0_H131": "并不是世间自然产生的生命。",
                "0_H132": "即便如此，我依然追求正确，并为此而抗争。",
                "0_H133": "我觉得……这一切绝非错误。",
                "0_H134": "即使在死后，成为了刻于座上之灵魂碎片的现在，我仍这么相信。",
                "0_H135": "您会嘲笑这样的我吗？",
                "0_H136": "……",
                "0_H137": "…………是吗，谢谢您。"
            }
        },
        "0_H180": {
            "name": "对话 4",
            "text": {
                "0_H180": "大和武尊……",
                "0_H181": "对我的态度，好像比以前……",
                "0_H182": "更柔和了一些？　奇怪……"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H1800": "犹如我的执迷、怨念般的愿望……",
                "0_H1801": "真的给你们添了不少麻烦。",
                "0_H1802": "非常抱歉。",
                "0_H1803": "我……",
                "0_H1804": "……唔？",
                "0_H1805": "真好吃……",
                "0_H1806": "这，这是什么？",
                "0_H1807": "是食堂的面包？　刚出炉的？",
                "0_H1808": "比起这些，能陪您去食堂写报告吗……？",
                "0_H1809": "……嗯，我明白了，现在就这么办吧。"
            }
        },
        "0_H190": {
            "name": "对话 5",
            "text": {
                "0_H190": "枪兵！",
                "0_H191": "不，不对，失礼了。",
                "0_H192": "因为您……很像我的知己……",
                "0_H193": "贞德阁下。",
                "0_H194": "是吗，是正当灵基的……"
            }
        },
        "0_H200": {
            "name": "对话 6",
            "text": {
                "0_H200": "果然是枪兵……！",
                "0_H201": "那面容，那黑色的身姿！",
                "0_H202": "……你还记得我……不记得了啊。",
                "0_H203": "什么？　不是一个人？",
                "0_H204": "是……这样吗……？"
            }
        },
        "0_H210": {
            "name": "对话 7",
            "text": {
                "0_H210": "枪兵……吗？",
                "0_H211": "不，好小……",
                "0_H212": "贞德·Alter·Santa·Lily？",
                "0_H213": "什么？"
            }
        },
        "0_H220": {
            "name": "对话 8",
            "text": {
                "0_H220": "天草四郎阁下……！",
                "0_H221": "啊啊……！",
                "0_H222": "就是吾师在天草并肩战斗过的，那位天草四郎时贞阁下……！",
                "0_H223": "没想到，没想到竟然有幸见到您……",
                "0_H224": "是！",
                "0_H225": "既是吾师殉道的对象，我自当也该为您殉道！",
                "0_H226": "不，不，我决不能抬头……！"
            }
        },
        "0_H230": {
            "name": "对话 9",
            "text": {
                "0_H230": "义仲阁下的……",
                "0_H231": "原来如此，您就是巴御前啊。",
                "0_H232": "请务必允许我和您谈谈。没错，是关于义仲阁下。"
            }
        },
        "0_H240": {
            "name": "对话 10",
            "text": {
                "0_H240": "骑兵……？",
                "0_H241": "不，不，不对……",
                "0_H242": "是正当的，源赖光阁下……"
            }
        },
        "0_H250": {
            "name": "对话 11",
            "text": {
                "0_H250": "！！",
                "0_H251": "您是……",
                "0_H252": "啊啊，不知为何，我能看出来……",
                "0_H253": "难道说，您是爱因兹贝伦的……"
            }
        },
        "0_H260": {
            "name": "对话 12",
            "text": {
                "0_H260": "那个人……",
                "0_H261": "难道是人工生命体……？"
            }
        },
        "0_H270": {
            "name": "对话 13",
            "text": {
                "0_H270": "骑兵。",
                "0_H271": "不，丑御前。",
                "0_H272": "……我觉得终有一天有必要与你促膝而谈。",
                "0_H273": "你又如何？"
            }
        },
        "0_H280": {
            "name": "对话 14",
            "text": {
                "0_H280": "宫本伊织。",
                "0_H281": "他好像失去了记忆。",
                "0_H282": "……不，我没资格谈论呢。"
            }
        },
        "0_H290": {
            "name": "对话 15",
            "text": {
                "0_H290": "宫本，伊织……",
                "0_H291": "嗯，现在的你……",
                "0_H292": "…………"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我名为由井正雪。",
                "0_S011": "志在成为忠烈之士。",
                "0_S012": "……现在，应该称我为魔术师吧。",
                "0_S013": "今后还请多指教。"
            }
        },
        "0_S110": {
            "name": "召唤 (盈月剑风帖通关后)",
            "text": {
                "0_S110": "……没错，是我。",
                "0_S111": "由井正雪。",
                "0_S112": "作为最起码的赎罪，前来成为您的助力。",
                "0_S113": "职阶是魔术师。",
                "0_S114": "——今后，还请多指教。"
            }
        }
    },
    "ChrVoice_ids": [
        "505400"
    ]
}
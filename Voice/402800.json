{
    "id": 402800,
    "collectionNo": 205,
    "name": "伊凡雷帝",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "愚蠢之辈！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "这不是都迸发了吗！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "如噩梦般出现，",
                "0_B051": "如噩梦般杀戮。",
                "0_B052": "肃正之时到来。",
                "0_B053": "于吾旅路",
                "0_B054": "随行之兽！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "冰冻吧！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "沙皇不会错……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "和余玩一会儿吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "余很宽容。会为你留下尸骸的。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "为何要客气。吃得饱一点。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "不应该……不应该这样才对……"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "杀，杀，杀——！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "沙皇……错了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "是这样啊……已经到礼拜的时间了啊……"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "同感。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "手不要停。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "……汝是让余……去沉睡吧……"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "不能原谅。不能原谅，不能原谅，不能原谅——！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "区区草芥！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "在神前叩拜吧！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "去死，去死，去死！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "承受神罚吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "充盈吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "寻求绝对的服从吧。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "和余共同迈进吧，沿着杀戮的道路。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "余的方式并没有错误。 错误的，是汝等。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "余之胜利即为神之胜利。 一切均为了主，余之巡礼将会继续。"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "可恶，无礼之徒！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "余的儿子，余的妻子……对不起……余……余……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "吾之行军永恒持续。",
                "0_B801": "神之兽将踏碎一切，",
                "0_B802": "于吾旅路",
                "0_B803": "随行之兽！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "吾肉体之兽啊。",
                "0_B811": "尽情发挥汝等的力量吧。",
                "0_B812": "蹂躏、蹂躏、蹂躏一切！"
            }
        },
        "0_B820": {
            "name": "宝具 {0}",
            "text": {
                "0_B820": "将这无礼的惩罚，刻在自己身上吧。",
                "0_B821": "不要妨碍吾之巡礼！",
                "0_B823": "于吾旅路随行之兽！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "不准妨碍余之前行！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "余之鲜血中寄宿着雷电……！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "不够。",
                "0_G021": "还不够！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "搜集出来……",
                "0_G031": "将反叛者们都搜集出来……"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "连眼球都宛若雷火迸发啊！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "余之大愿的实现近在眼前……！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "是汝的生日吗……",
                "0_H011": "那必须要建造一座大教堂才行啊……"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "必须扩展领土才行。",
                "0_H021": "出击，余御驾亲征！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "礼拜的时间结束了。",
                "0_H031": "那接下来就是战斗的时间了。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "哪怕是御主，也该服从余。",
                "0_H041": "这样就能超越所有的幸福。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "余正是铲平所有国度的真正沙皇。",
                "0_H051": "汝乃是身为余之参谋见证一切之人。",
                "0_H052": "即为余之杖。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "问余爱什么？",
                "0_H061": "难道汝不知道吗？",
                "0_H062": "身为人的爱献给了阿纳斯塔西娅。",
                "0_H063": "身为沙皇的爱献给了神。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "污物是那些拽着自己的权力不放，不爱神明，",
                "0_H071": "在国内专横跋扈的贵族们。",
                "0_H072": "不服从王命的所有贵族，都是余的敌人。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯……问余打算向圣杯许什么愿望？",
                "0_H081": "还用问吗。",
                "0_H082": "希望这噩梦走向终焉。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "……没想到，余的契约者居然住在这种马棚里……"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "请原谅余说这里是马棚的评价。",
                "0_H101": "尽管余不喜欢清贫，但能够理解这份志向。",
                "0_H102": "犹如圣愚尼古拉过去那样。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "这样啊……汝世界中的余是个疯狂的皇帝啊。",
                "0_H111": "但是，也有一些值得庆幸的事。",
                "0_H112": "汝那边的余，应该不需要啃食兽肉吧？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "只要汝希望，",
                "0_H121": "余不会拒绝接受汝加入余之眷属。",
                "0_H122": "只不过，这样一来将无法再次穿上轻巧的衣服了。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "……没想到汝，",
                "0_H131": "仍不打算接受余的宠爱啊……",
                "0_H132": "但无妨。",
                "0_H133": "现在，余梦见在阳光普照的平原上汝驰骋的梦境。",
                "0_H134": "所有一切……这样就好。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "英雄王啊。",
                "0_H141": "身为王，却还渴望身为英杰的力量。",
                "0_H142": "神代时候还真是严苛呢。"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "尽管爱的形式与对余之爱妃阿纳斯塔西娅的不一样，但余也会向她倾注余的宠爱。",
                "0_H151": "直截了当地说，她就如同余宝贵的女儿。",
                "0_H152": "……不过，她或许会认为这是种麻烦吧……"
            }
        },
        "0_H1600": {
            "name": "牵绊Lv.4 (第2部3章intro通关后)",
            "text": {
                "0_H1600": "看来在余的身体里，异闻带与泛人类史的记忆混淆在一起了。",
                "0_H1601": "连自己为何会变成这个样子，都十分模糊不请。",
                "0_H1602": "……没事，在战斗中绝不会落于人后。应该没有问题。"
            }
        },
        "0_H170": {
            "name": "对话 6",
            "text": {
                "0_H170": "中华皇帝的源流啊。",
                "0_H171": "原来如此，这份霸气……寻常人定会被轻易吞没吧。"
            }
        },
        "0_H180": {
            "name": "对话 7",
            "text": {
                "0_H180": "拉斯普京……令帝国崩溃的可怕奸臣。",
                "0_H181": "然而，他为俄罗斯的历史打下了一个楔子也是不容置疑的事实。",
                "0_H182": "不管如何，汝务必要提防。",
                "0_H183": "因为这世间存在着连英雄之力都无法左右的话语怪物。"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (第2部3章intro通关后)",
            "text": {
                "0_H1800": "在那边的世界被太阳抛弃，而在这里的世界，则抛弃了爱。",
                "0_H1801": "虽然人生尽是失去，但余仍不打算停止巡礼。",
                "0_H1802": "你愿意跟随余一起来吗，御主。",
                "0_H1803": "……啊啊，谢谢。"
            }
        },
        "0_H190": {
            "name": "对话 8",
            "text": {
                "0_H190": "噢噢，是尼基季奇吗。",
                "0_H191": "是余都知晓其大名的英雄……",
                "0_H192": "但竟会借用女性的身体出现，多少令人有些惊讶。",
                "0_H193": "不过也罢。只要其力量仍保持万全，余就不会有失敬意。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "Rider，雷帝伊凡是也。",
                "0_S011": "已完成戴冠。",
                "0_S012": "惧怕余吧，",
                "0_S013": "叩拜余吧，",
                "0_S014": "惩罚余吧。"
            }
        }
    },
    "ChrVoice_ids": [
        "402800"
    ]
}
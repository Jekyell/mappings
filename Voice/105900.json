        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "好好看清楚。",
                "0_B801": "其为生命，祖为开辟之滴，成就星辰之存在。",
                "0_B802": "——绝技！",
                "0_B803": "——八岐怒涛！",
                "0_B804": "上啊啊啊啊啊！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "绝技启动。",
                "0_B811": "挡我者，悉数面伏于地吧。",
                "0_B812": "——全力一击！",
                "0_B813": "——八岐怒涛！",
                "0_B814": "唔————！"
            }
        },
        "0_B820": {
            "name": "宝具 4",
            "text": {
                "0_B820": "绝技启动。",
                "0_B821": "赌上我的名讳，拯救普天之下的一切吧。",
                "0_B822": "——嗯！",
                "0_B823": "天丛云剑拟似解放！",
                "0_B824": "大蛇·大波涛！"
            }
        },
        "0_B840": {

{
    "id": 200800,
    "collectionNo": 122,
    "name": "崔斯坦",
    "voices": {
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "歌唱痛苦，",
                "0_B051": "弹奏哀怨。",
                "0_B052": "痛哭幻奏……",
                "0_B053": "这就是我的箭矢。"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "请来这里。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "请将您的手递给我。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "让我们开始吧。悲伤的歌曲。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "让我们起舞吧。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "那就来一首镇魂之曲。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "悲伤终将消失……随风消逝，冰消雪融……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "再见了……无需悲伤。这是我的宿命。"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "啊啊……似乎在这个世界逗留时间过长了啊……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "好的。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "好吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "嗯。"
            }
        },
        "0_B2010": {
            "name": "攻击 10",
            "text": {
                "0_B2010": "真空斩！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "那就开始吧……"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "宝具起动。 用我的弓，弹奏悲伤的旋律。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "狂乱绽放吧，吾之菲尔诺特！"
            }
        },
        "0_B2440": {
            "name": "额外攻击 3",
            "text": {
                "0_B2440": "不想感受痛苦的话，就老实一点。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "斩裂！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "再怎么吼也没用！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "就是那里！"
            }
        },
        "0_B440": {
            "name": "额外攻击 1",
            "text": {
                "0_B440": "拧得粉碎！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "轻快地演奏吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "演奏悲伤的歌曲。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "那么，就全力陪你玩玩吧。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "虽然会很痛，但就当是上课的费用吧。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "明知赢不了的话，根本没必要再站起来了吧。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "很抱歉脏了您的眼，御主。 总之，胜利了。"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "还没完！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "呜，难以承受……！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "虽然……大意了……但还……没有……呜。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "痛苦的音色，哀怨的曲子。",
                "0_B801": "斩裂汝之血肉的乃是吾之悲伤。",
                "0_B802": "痛哭幻奏……再见了。"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "以死亡赎罪，以杀戮晏息。",
                "0_B811": "傲慢之罪，在此偿还吧。",
                "0_B812": "痛哭幻奏！"
            }
        },
        "0_B820": {
            "name": "宝具 {0}",
            "text": {
                "0_B820": "在吾之音色之前，万物劣化，物质静止。",
                "0_B821": "痛哭幻奏！",
                "0_B822": "只需斩断，就只剩东西而已了。"
            }
        },
        "0_B840": {
            "name": "额外攻击 2",
            "text": {
                "0_B840": "老实点，我会斩下你的脑袋。"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "演奏欢乐的歌曲。"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "演奏激烈的歌曲。"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "大切断！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "撕裂！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {
                "0_B930": "可悲……！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "看来等级提升了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "感谢您，我的御主。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "有种重生的感觉呢。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "仿佛能从纠缠的命运中逃脱。",
                "0_G041": "谢谢您，御主。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "居然能到达这等领域……",
                "0_G051": "仿佛连命运都能颠覆。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝您生日快乐。",
                "0_H011": "但愿您的未来能够幸福。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "啊啊，好像发生了什么事。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "御主，命运是残酷的。",
                "0_H031": "但不去旅行之人是无法抓住胜利机会的。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "嗯，只要您是一位正确的人，",
                "0_H041": "也是我的御主……",
                "0_H042": "我就会服从您。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "请不要过于信任……",
                "0_H051": "曾傲慢地舍弃了主人的我……"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "……爱会迷惑人心。",
                "0_H061": "虽深知这一点，我却依然无法对爱表示踌躇。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我特别讨厌口无遮拦的话语。",
                "0_H071": "比如，临走时甩下一句说他人不懂人心之类的话……"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯吗？",
                "0_H081": "如果过去的那位王有愿望的话。",
                "0_H082": "真希望为那位人实现……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "嗯，御主。",
                "0_H091": "有何贵干？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "哎呀，御主。",
                "0_H101": "您好像很累了吧。",
                "0_H102": "还是去休息一下为好。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我曾经居住的国度身陷战乱。",
                "0_H111": "而一位王为了夺回和平，挺身而出。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……骑士王。",
                "0_H121": "被这么称呼的王甚至不让身心休息，为了和平不断战斗。",
                "0_H122": "周围的人以及我，都认为王就是这样的存在。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "但是，我们错了。",
                "0_H131": "犯下了过于……致命的错误。",
                "0_H132": "御主，这次我不会再犯错了。",
                "0_H133": "我会与您一起战斗，一起分担痛苦。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "……！！",
                "0_H141": "什么……那位……骑士王居然……",
                "0_H142": "不，没，",
                "0_H143": "没什么。",
                "0_H144": "想必那位也不愿意见到这张不知羞耻的脸吧。",
                "0_H145": "请不用为我担心。"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "哎呀，这不是兰斯洛特吗！",
                "0_H151": "让我们继续优雅地谈论禁忌之恋吧。",
                "0_H152": "唔……周围人的视线好像有点……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "我的朋友，我的同胞。",
                "0_H161": "居然变成如此不堪入目的模样……",
                "0_H162": "但是，那也是兰斯的一面……",
                "0_H163": "至少让我为他弹奏一曲送他一程。"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "噢噢，高文卿。",
                "0_H171": "能再次集结于同一面旗帜之下，真值得庆幸。",
                "0_H172": "与身为阳光化身的你在一起的话，值得恐惧的东西其实没多少了呢。"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "莫德雷德卿……你还是那么凶暴呢。",
                "0_H181": "见到什么都想咬上去的样子，简直就像狼……",
                "0_H182": "不，狼好像太帅了一点吧。",
                "0_H183": "响尾蛇……拟鳄龟……",
                "0_H184": "王八！　没错，就像王八！",
                "0_H185": "……我道歉，所以能不能别高举克拉伦特？"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (第2部1章通关后)",
            "text": {
                "0_H1800": "不能屈服、不能认输的日子一定非常艰难而严苛吧。",
                "0_H1801": "当无能为力的时候，请交给我吧。",
                "0_H1802": "我会将您身上沉重的负担全部扫清。",
                "0_H1803": "这就是我身为从者的誓言。"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "加雷斯……你也来这里了啊。",
                "0_H191": "希望你的手不会被玷污。",
                "0_H192": "这次，我们一定……",
                "0_H193": "……唔，我刚才好像说了什么奇怪的话吧。",
                "0_H194": "请忘记吧。我也会忘记……"
            }
        },
        "0_H210": {
            "name": "对话 10",
            "text": {
                "0_H210": "贝德维尔。",
                "0_H211": "忠义的骑士、银臂的战士、圆桌的良心、圆桌的吐槽人……",
                "0_H212": "你能来真是帮大忙了。",
                "0_H213": "不，后半只是圆桌笑话。"
            }
        },
        "0_H220": {
            "name": "对话 11",
            "text": {
                "0_H220": "……什么？　希望配合我的竖琴唱歌？",
                "0_H221": "原来如此，这倒是无妨，但哪首曲子？",
                "0_H222": "恋爱是龙？",
                "0_H223": "……嗯，可以弹奏。",
                "0_H224": "只是，在那之前。",
                "0_H225": "我能先准备一下耳塞吗？"
            }
        },
        "0_H230": {
            "name": "对话 12",
            "text": {
                "0_H230": "每一天都那么的不平静呢。",
                "0_H231": "但愿我的竖琴能带给您一些慰藉。"
            }
        },
        "0_H2300": {
            "name": "对话 3 (第2部3章intro通关后)",
            "text": {
                "0_H2300": "我们是主从，是超越了忠诚的存在。",
                "0_H2301": "我希望这么认为，您觉得呢？",
                "0_H2302": "我愿意为您而死。",
                "0_H2303": "……当然只是个玩笑，嗯。"
            }
        },
        "0_H240": {
            "name": "对话 13",
            "text": {
                "0_H240": "珀西瓦尔卿。",
                "0_H241": "又加入一位光荣的圆桌骑士，",
                "0_H242": "是这么回事吧……",
                "0_H243": "另外，一事归一事，既然你找茬，我当然愿意奉陪，你这混蛋。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "圆桌骑士，哀叹的崔斯坦。",
                "0_S011": "服从召唤之令前来报到。",
                "0_S012": "但愿我能够为您效劳。",
                "0_S013": "请下令，御主。"
            }
        },
        "11_B080": {
            "name": "技能 1 鲁鲁夏威的鸡",
            "text": {
                "11_B080": "喜欢炸鸡块吗？"
            }
        },
        "11_B090": {
            "name": "技能 2 鲁鲁夏威的鸡",
            "text": {
                "11_B090": "犹如鸮面鹦鹉。"
            }
        },
        "11_B100": {
            "name": "开始 1 鲁鲁夏威的鸡",
            "text": {
                "11_B100": "开始吧。悲伤的……好吵啊……"
            }
        },
        "11_B110": {
            "name": "开始 2 鲁鲁夏威的鸡",
            "text": {
                "11_B110": "走吧，鸟儿们。"
            }
        },
        "11_B120": {
            "name": "胜利 1 鲁鲁夏威的鸡",
            "text": {
                "11_B120": "好响亮的鸣叫声呢。那我也来弹奏一曲吧。"
            }
        },
        "11_B130": {
            "name": "胜利 2 鲁鲁夏威的鸡",
            "text": {
                "11_B130": "要穿着这身衣服悲伤……还是相当困难的呢……"
            }
        },
        "11_B140": {
            "name": "受击 2 鲁鲁夏威的鸡",
            "text": {
                "11_B140": "好痛。"
            }
        },
        "11_B150": {
            "name": "受击 1 鲁鲁夏威的鸡",
            "text": {
                "11_B150": "好痛好痛好痛！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 鲁鲁夏威的鸡",
            "text": {
                "11_B160": "被，被干掉了……稍微……睡一……呼噜。"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 鲁鲁夏威的鸡",
            "text": {
                "11_B170": "向着天空振翅高飞……鸡群……"
            }
        },
        "11_B180": {
            "name": "指令卡 1 鲁鲁夏威的鸡",
            "text": {
                "11_B180": "我会做的。"
            }
        },
        "11_B190": {
            "name": "指令卡 2 鲁鲁夏威的鸡",
            "text": {
                "11_B190": "相当了得。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 鲁鲁夏威的鸡",
            "text": {
                "11_B200": "当然可以。"
            }
        },
        "11_B2010": {
            "name": "攻击 10 鲁鲁夏威的鸡",
            "text": {
                "11_B2010": "太死缠烂打了吧！"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 鲁鲁夏威的鸡",
            "text": {
                "11_B210": "我要放个大招了，稍微离远一些。"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 鲁鲁夏威的鸡",
            "text": {
                "11_B220": "要～射～击～了～。"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 鲁鲁夏威的鸡",
            "text": {
                "11_B230": "来一曲吧。"
            }
        },
        "11_B2440": {
            "name": "额外攻击 3 鲁鲁夏威的鸡",
            "text": {
                "11_B2440": "我可以原谅，但鸟又如何呢。"
            }
        },
        "11_B410": {
            "name": "攻击 4 鲁鲁夏威的鸡",
            "text": {
                "11_B410": "甜辣！"
            }
        },
        "11_B420": {
            "name": "攻击 5 鲁鲁夏威的鸡",
            "text": {
                "11_B420": "犹如照烧！"
            }
        },
        "11_B430": {
            "name": "攻击 6 鲁鲁夏威的鸡",
            "text": {
                "11_B430": "啄杀！"
            }
        },
        "11_B440": {
            "name": "额外攻击 1 鲁鲁夏威的鸡",
            "text": {
                "11_B440": "将线狠狠一拉！"
            }
        },
        "11_B480": {
            "name": "技能 3 鲁鲁夏威的鸡",
            "text": {
                "11_B480": "叮咚叮咚叮咚。"
            }
        },
        "11_B490": {
            "name": "技能 4 鲁鲁夏威的鸡",
            "text": {
                "11_B490": "那就一起来吧。"
            }
        },
        "11_B500": {
            "name": "开始 3 鲁鲁夏威的鸡",
            "text": {
                "11_B500": "哈哈哈，要啄就去啄敌人啦，好痛好痛。"
            }
        },
        "11_B510": {
            "name": "开始 4 鲁鲁夏威的鸡",
            "text": {
                "11_B510": "嗯，我刚醒。"
            }
        },
        "11_B520": {
            "name": "胜利 3 鲁鲁夏威的鸡",
            "text": {
                "11_B520": "获得胜利啦～♪　啦啦啦获得胜利啦～♪"
            }
        },
        "11_B530": {
            "name": "胜利 4 鲁鲁夏威的鸡",
            "text": {
                "11_B530": "南国的夕阳颇有滋味呢……比喻来说，没错……就像先行上映呢……"
            }
        },
        "11_B540": {
            "name": "受击 4 鲁鲁夏威的鸡",
            "text": {
                "11_B540": "哎哟。"
            }
        },
        "11_B550": {
            "name": "受击 3 鲁鲁夏威的鸡",
            "text": {
                "11_B550": "炸鸡——！"
            }
        },
        "11_B560": {
            "name": "无法战斗 3 鲁鲁夏威的鸡",
            "text": {
                "11_B560": "只是稍微开了些玩笑……不可大意……扑街。"
            }
        },
        "11_B840": {
            "name": "额外攻击 2 鲁鲁夏威的鸡",
            "text": {
                "11_B840": "没有睡着！"
            }
        },
        "11_B880": {
            "name": "技能 5 鲁鲁夏威的鸡",
            "text": {
                "11_B880": "是要退场了吧。"
            }
        },
        "11_B890": {
            "name": "技能 6 鲁鲁夏威的鸡",
            "text": {
                "11_B890": "与鸟一起。"
            }
        },
        "11_B910": {
            "name": "攻击 7 鲁鲁夏威的鸡",
            "text": {
                "11_B910": "抱歉如此高速！"
            }
        },
        "11_B920": {
            "name": "攻击 8 鲁鲁夏威的鸡",
            "text": {
                "11_B920": "四分五裂事件呢。"
            }
        },
        "11_B930": {
            "name": "攻击 9 鲁鲁夏威的鸡",
            "text": {
                "11_B930": "让你的脖子清凉一些吧！"
            }
        },
        "11_G010": {
            "name": "升级 鲁鲁夏威的鸡",
            "text": {
                "11_G010": "看来等级提升了。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 鲁鲁夏威的鸡",
            "text": {
                "11_G060": "灵衣开放。呵，简直有种释放自己的感觉。",
                "11_G061": "以及不知为何跟来的这些鸡。",
                "11_G062": "不，稍微等一下，为什么会出现这些鸡。",
                "11_G063": "真可怕呢，意料之外也该有点限度。",
                "11_G064": "但既然没办法，就给它们取个名字吧。",
                "11_G065": "莫德雷德一号、二号、三号。好。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 鲁鲁夏威的鸡",
            "text": {
                "11_H9000": "没想到穿着这身衣服还要前去战斗。",
                "11_H9001": "命运还真是奇妙呢。",
                "11_H9002": "但是总不能一直飘飘然的状态。",
                "11_H9003": "为了管理好其他肯定也很兴奋的骑士们，我必须狠下心肠。",
                "11_H9004": "哼，真是的，负责人实在辛苦呢。"
            }
        },
        "11_H9170": {
            "name": "对话 1 鲁鲁夏威的鸡",
            "text": {
                "11_H9170": "莫德雷德一号、二号、三号改名为约翰、太郎、皮埃尔。",
                "11_H9171": "因为名字的事暴露后被莫德雷德拿着克拉伦特四处追杀。"
            }
        }
    },
    "ChrVoice_ids": [
        "200800"
    ]
}
{
    "id": 704700,
    "collectionNo": 386,
    "name": "阿尔托莉雅·卡斯特",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "就是那边！　交叉！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "那边！　埃克特！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "抓住！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "清唱剧·光晕！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "对灾厄大仪式，开门。",
                "0_B051": "自伦戈米尼亚德，",
                "0_B052": "变奏为誓约胜利之剑。",
                "0_B053": "圣剑，拔刀！",
                "0_B054": "祭神啊，请宽恕吾等之罪——！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "要快乐一些，轻松一些哦！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "春日的悸动，要像这样。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "感觉完美！　好，出发吧！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "毕竟是夏天嘛，让我们欢闹吧！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "耶～，击掌～～！ 下次也按照这个状态上吧！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "没有受伤吧？ 是吗。因为我注意力全在自己身上，这下我放心了。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "好痛！？"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "你竟敢这么做！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "抱歉，到极限了！ 我还以为自己还能够继续呢～！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "对方的攻击好厉害啊！ 不过，下次我可不会输了哦！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是那边吧！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "是这边啊！？"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "我就知道！"
            }
        },
        "0_B2010": {
            "name": "攻击 10",
            "text": {
                "0_B2010": "团团转！"
            }
        },
        "0_B2020": {
            "name": "攻击 11",
            "text": {
                "0_B2020": "上吧，马赫！"
            }
        },
        "0_B2030": {
            "name": "攻击 12",
            "text": {
                "0_B2030": "突袭，强风！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "有罪之人，方可通过——"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "冬之玉座，春之希望——"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "终结设置！"
            }
        },
        "0_B2420": {
            "name": "攻击 14",
            "text": {
                "0_B2420": "踩着节拍！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "和声！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "乘着波浪，上啦！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "鲨鱼剑！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "自龙脉站起来吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "合唱就拜托了哦！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "大家，各就各位。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "要看好我华丽的动作哦！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "大家都很努力呢！ 那么，一起去吃饭吧！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "就算现实中没有那光，",
                "0_B801": "也不放弃，不气馁，",
                "0_B802": "强撑着，迈步前进。",
                "0_B803": "因为我，知道那希望。",
                "0_B804": "承继宿愿希望之剑。"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "让我们讲述一个故事吧。",
                "0_B811": "启程的誓言，救世的童话。",
                "0_B812": "无论羽翼几度染污，冬之鸟也会穿越暴雨。",
                "0_B813": "那份希望，不会消失！",
                "0_B814": "承继宿愿希望之剑！"
            }
        },
        "0_B840": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B840": "这么说来我好像是个魔术师哦！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "哎哟，此时发生了意外！"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "我可是魔术师，必须摄取糖分才行！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "预备，呜呜～～！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "老老实实好好听！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {
                "0_B930": "飞舞而上吧！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "唔哦～～，等级提升了～～！",
                "0_G011": "智慧超级增长了啊～～！"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "我有成长吗？　有？",
                "0_G111": "太棒了！",
                "0_G112": "就算职阶不同也是有办法的嘛！"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "我感觉自己开始熟悉号嘴的用法了！",
                "0_G121": "在能够吹出好听的声音之前，我会努力的！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日快乐！",
                "0_H011": "特别的日子就该准备特别的活动，综上所述，我预约了模拟装置室！",
                "0_H012": "蛋糕虽然不错，但战斗也很棒哦！",
                "0_H013": "去在高难度连续挑战中达成厉害的记录吧！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "活动绝赞举办中～～！",
                "0_H021": "仔细阅读信息，以万全的态势面对吧！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "哎哟，该登场了？",
                "0_H031": "那得先做好热身运动才行呢！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "御主是乐团首席，我是演奏者，对吧？",
                "0_H041": "让我们来一场精彩的演奏吧！"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "能和推心置腹的朋友一起去度假胜地游玩真是开心到爆了！",
                "0_H051": "唔～～～，一天时间过得太快了啦！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "以前喜欢的东西是巧克力……",
                "0_H061": "不知是否由于变成了狂战士……",
                "0_H062": "现在……想要打猎……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "有人来南国之后还考虑讨厌的东西吗？",
                "0_H071": "就算悲观的自己也肯定会变得乐观啦。",
                "0_H072": "……虽然，也是有一个例外的啦。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "希望圣杯实现的愿望依然没变！",
                "0_H081": "没错，只要那可恨的巴格子，",
                "0_H082": "还在！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "现在一切都安定下来了，所以重新打个招呼。",
                "0_H091": "我是阿尔托莉雅·卡斯特，这次是作为狂战士前来的！",
                "0_H092": "支援魔术太麻烦了，所以随手一扔封印起来，这次以直觉、直接的方式帮你的忙！",
                "0_H093": "没错——用我的梅林魔术！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "从者庆典就是相互展现艺术的祭典吧。",
                "0_H101": "虽然我不擅长绘画，但我觉得，用身体表现的舞蹈应该还有点机会！",
                "0_H102": "加油吧～！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "虽然想做的事有很多，但首先必须解决特异点。",
                "0_H111": "没关系，我听说迦勒底的夏天很漫长。",
                "0_H112": "让我们慢慢来吧！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "小号是阿瓦隆为我准备的。",
                "0_H121": "『虽然技术对音乐非常重要，但天赋也很重要。靠感觉想办法解决吧，这是你的拿手好戏吧？』",
                "0_H122": "诸如此类。虽然我不擅长用言语表达心情，但用音乐的话，应该就能传达毫不掩饰的心情了。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "哎，友爱点数已经攒了那么多了！？",
                "0_H131": "南国太可怕了吧！？　这不会显得我很好搞定吗！",
                "0_H132": "……不过，算了，也是。毕竟确实玩得很开心。",
                "0_H133": "迦勒底的你，谢谢了。",
                "0_H134": "狂战士也不赖呢。创造的每一段回忆都非常闪耀。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "说起夏天当然少不了怪谈吧！　好，阿尔托莉雅·卡斯特，开始说啦！",
                "0_H171": "「早上起床后，我不禁怀疑自己看错了。冬日女王穿上了泳装，以媲美模特的优雅身姿闯入了夏日乐园——」",
                "0_H172": "别说背脊发凉了，连脑袋都冻起来了，我说真的。"
            }
        },
        "0_H190": {
            "name": "对话 5",
            "text": {
                "0_H190": "加雷斯亲的夏装～～！",
                "0_H191": "呜哇，冲浪板好帅啊！　在海浪上站起需要保持的平衡一定很难吧！",
                "0_H192": "也让我挑战一下啦！"
            }
        },
        "0_H200": {
            "name": "对话 6",
            "text": {
                "0_H200": "喂，还能穿女仆装的吗！？　泳装上哪儿去了，泳装！",
                "0_H201": "我原本还期待充满牙之氏族风范的野性震撼的那种呢！",
                "0_H202": "啊～受不了，既然打算伺候，那我这边也麻烦你了哦！"
            }
        },
        "0_H210": {
            "name": "对话 7",
            "text": {
                "0_H210": "我和芭万·希之间虽然发生了不少事，但她也和其他妖精骑士一样变了呢。",
                "0_H211": "还是说这才是她的本性呢？",
                "0_H212": "我总觉得现在应该能够和她一起创作作品了。",
                "0_H213": "只是我不太能理解那身人偶装。"
            }
        },
        "0_H220": {
            "name": "对话 8",
            "text": {
                "0_H220": "巧克力妖精加南十字星加会拉小提琴加从者庆典的委员长，信息量过大脑子都要炸了啦……！",
                "0_H221": "不过嘛，虽然有很多头衔，但那百分百是诺克娜蕾吧。",
                "0_H222": "好嘞……我要去好好向她显摆自己舞蹈大师的英姿！"
            }
        },
        "0_H280": {
            "name": "对话 9",
            "text": {
                "0_H280": "虽然我知道风之妖精，但竟然还存在可被称作大气本身的精灵，太令人惊讶了。",
                "0_H281": "虽然由来和形态都和我们不同，但她“希望和大家和睦相处”的心情养育了生命吧。",
                "0_H282": "感谢你在愉快的夏日舞台选择了诺克娜蕾，旺吉娜。"
            }
        },
        "0_I010": {
            "name": "从者·夏日·庆典2024！ 16",
            "text": {
                "0_I010": "欢迎光临道具兑换所。",
                "0_I011": "哎呀？　准确地说应该叫直销商店吗？",
                "0_I012": "真是失礼了。毕竟我不太熟悉世俗生活。"
            }
        },
        "0_I020": {
            "name": "从者·夏日·庆典2024！ 17",
            "text": {
                "0_I020": "你正在努力获取资源吧。",
                "0_I021": "进一步说，你的行动关系到岛屿的和平。",
                "0_I022": "但是，千万不要逞强，要轻松一些。"
            }
        },
        "0_I030": {
            "name": "从者·夏日·庆典2024！ 18",
            "text": {
                "0_I030": "要这个吧，请收下。",
                "0_I031": "还有其他想要的东西吗？"
            }
        },
        "0_I040": {
            "name": "从者·夏日·庆典2024！ 19",
            "text": {
                "0_I040": "是用来成长的素材吧？",
                "0_I041": "对那位能为全体上非常厉害无敌的魔术师，或是宝具是很厉害单体攻击的狂战士使用吧。"
            }
        },
        "0_I050": {
            "name": "从者·夏日·庆典2024！ 20",
            "text": {
                "0_I050": "啊，惨了，弄丢了！",
                "0_I051": "……不，没什么。",
                "0_I052": "我是圣剑的守护者，完美的阿尔托莉雅·阿瓦隆。",
                "0_I053": "是，是真的哦？"
            }
        },
        "0_I060": {
            "name": "从者·夏日·庆典2024！ 21",
            "text": {
                "0_I060": "能量饮料、咖喱饭、牛排。",
                "0_I061": "这是将她脑海中描绘的原动力化作形态的产物。",
                "0_I062": "大家的感情会就此变得美丽。"
            }
        },
        "0_I070": {
            "name": "从者·夏日·庆典2024！ 22",
            "text": {
                "0_I070": "苏打棒冰……唔……",
                "0_I071": "这是给谁用的？",
                "0_I072": "算了，无所谓啦。"
            }
        },
        "0_I080": {
            "name": "从者·夏日·庆典2024！ 23",
            "text": {
                "0_I080": "直销商店关门了呢……",
                "0_I081": "虽说站在这个立场上无可奈何，但阿瓦隆的登场机会之少实属遗憾……",
                "0_I082": "啊，但还是有很多机会的吧。",
                "0_I083": "让我们在情人节再见吧！"
            }
        },
        "0_I090": {
            "name": "从者·夏日·庆典2024！ 24",
            "text": {
                "0_I090": "夏威托莉雅的生活怎么样？",
                "0_I091": "就算回忆迟早会淡去，但为了不让其褪色，请将相片保存在相册里哦。"
            }
        },
        "0_M010": {
            "name": "从者·夏日·庆典2024！ 13",
            "text": {
                "0_M010": "啊，惨了，清单被覆盖了啦，撤销撤销！",
                "0_M011": "……唉，已经不行了啦……",
                "0_M012": "我无法穿着不习惯的衣服做这种困难的工作啦……",
                "0_M013": "已经动不了了啦……"
            }
        },
        "0_M020": {
            "name": "从者·夏日·庆典2024！ 14",
            "text": {
                "0_M020": "客，客人，欢迎光临啾啾亭！",
                "0_M021": "……唉……只是正常看店而已，为什么视线都集中在我身上啦……",
                "0_M022": "但现在不是害羞的时候，必须尽快让克洛伊恢复原状才行！"
            }
        },
        "0_M030": {
            "name": "从者·夏日·庆典2024！ 15",
            "text": {
                "0_M030": "哎～，又来了？",
                "0_M031": "不过，这应该就是勤劳的人吧……",
                "0_M032": "当前状况是这种感觉，要再努力一下吗？"
            }
        },
        "0_M040": {
            "name": "从者·夏日·庆典2024！ 8",
            "text": {
                "0_M040": "听说漫画好像完成了哦！",
                "0_M041": "快去看看吧！"
            }
        },
        "0_M050": {
            "name": "从者·夏日·庆典2024！ 9",
            "text": {
                "0_M050": "又增加了一本新的阿尔托莉雅本！"
            }
        },
        "0_M060": {
            "name": "从者·夏日·庆典2024！ 10",
            "text": {
                "0_M060": "大家看上去都好开心啊！　我也要努力调查！"
            }
        },
        "0_N010": {
            "name": "从者·夏日·庆典2024！ 11",
            "text": {
                "0_N010": "你好！　我是传达进度的新人工作人员～！",
                "0_N011": "总之……要看看兑换道具清单吗？"
            }
        },
        "0_N020": {
            "name": "从者·夏日·庆典2024！ 12",
            "text": {
                "0_N020": "有时间的话，记得查看帮助信息哦！",
                "0_N021": "会认真阅读说明书的御主好帅啊～～！",
                "0_N022": "然后……稍后记得要偷偷告诉我哦？"
            }
        },
        "0_N030": {
            "name": "从者·夏日·庆典2024！ 3",
            "text": {
                "0_N030": "南国的冒险就此结束了。",
                "0_N031": "虽然没能成为名侦探，但和大家一起创造了很多回忆。",
                "0_N032": "夏日的回忆，闪闪发光呢。"
            }
        },
        "0_N040": {
            "name": "从者·夏日·庆典2024！ 4",
            "text": {
                "0_N040": "话说回来。虽然不知道说过多少次了，但都到最后了，我还是要说……",
                "0_N041": "夏威托莉雅这座岛的名字也太过分了吧！？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "你好！　我是听说发生了难办的事才特地赶来的阿尔托莉雅·卡斯特！",
                "0_S011": "以及既然是夏天，当然穿上了泳装！",
                "0_S012": "虽然我还不了解具体情况，但一切包在我身上吧！",
                "0_S013": "哎？　问我是什么职阶？",
                "0_S014": "这个嘛……",
                "0_S015": "哈哈……究竟是什么呢？"
            }
        },
        "1_B010": {
            "name": "攻击 1",
            "text": {
                "1_B010": "突击！　如何！"
            }
        },
        "1_B020": {
            "name": "攻击 2",
            "text": {
                "1_B020": "哈啊啊——！　发射！"
            }
        },
        "1_B030": {
            "name": "攻击 3",
            "text": {
                "1_B030": "盛大地，华丽地！"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "茁壮地，坚强地！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "对灾厄大仪式，开门。",
                "1_B051": "自伦戈米尼亚德，",
                "1_B052": "变奏为誓约胜利之剑。",
                "1_B053": "圣剑，拔刀！",
                "1_B054": "祭神啊，请宽恕吾等之罪——！"
            }
        },
        "1_B080": {
            "name": "技能 1",
            "text": {
                "1_B080": "配合着上吧！"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "无论什么时候，都要保持轻快！"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "先不管是不是合身！　开始了吧！"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "是，是战斗吧！　包，包在夏日妖精身上吧！"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "请牵起我的手，御主。我带你去安全的地方。"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "呼啊～，累死了～～……有平时三倍那么累！"
            }
        },
        "1_B140": {
            "name": "受击 2",
            "text": {
                "1_B140": "好痛！？"
            }
        },
        "1_B150": {
            "name": "受击 1",
            "text": {
                "1_B150": "你竟敢这么做！"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "就不该穿这种不熟悉的衣服呢……但只要可爱就可以了！"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "啊～真是的！　这身衣服防御力太弱了啦！"
            }
        },
        "1_B180": {
            "name": "指令卡 1",
            "text": {
                "1_B180": "唔，嗯！　我做！"
            }
        },
        "1_B190": {
            "name": "指令卡 2",
            "text": {
                "1_B190": "这，这样啊！　我去！"
            }
        },
        "1_B200": {
            "name": "指令卡 3",
            "text": {
                "1_B200": "好，好嘞！　出招啦！"
            }
        },
        "1_B2010": {
            "name": "攻击 10",
            "text": {
                "1_B2010": "把不好的地方切割掉吧！"
            }
        },
        "1_B2020": {
            "name": "攻击 11",
            "text": {
                "1_B2020": "仙境～～劈斩！"
            }
        },
        "1_B2030": {
            "name": "攻击 12",
            "text": {
                "1_B2030": "童话是这样的吗？"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "宝，宝具！？　穿这身放吗！？"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "夏之氏族，要上啦～！"
            }
        },
        "1_B230": {
            "name": "宝具卡 3",
            "text": {
                "1_B230": "愉快的时间，到此结束啦～！"
            }
        },
        "1_B2410": {
            "name": "攻击 13",
            "text": {
                "1_B2410": "动手吧！"
            }
        },
        "1_B2420": {
            "name": "攻击 14",
            "text": {
                "1_B2420": "合奏！"
            }
        },
        "1_B410": {
            "name": "攻击 4",
            "text": {
                "1_B410": "旋转，命中！"
            }
        },
        "1_B420": {
            "name": "攻击 5",
            "text": {
                "1_B420": "搞定吧！"
            }
        },
        "1_B430": {
            "name": "攻击 6",
            "text": {
                "1_B430": "剑之进行曲！"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "成长吧，米斯特汀！"
            }
        },
        "1_B480": {
            "name": "技能 3",
            "text": {
                "1_B480": "阿瓦隆……"
            }
        },
        "1_B490": {
            "name": "技能 4",
            "text": {
                "1_B490": "稍微休息一会儿……"
            }
        },
        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "和我想象的完全不一样啦～～！　这头饰真的有必要吗～～！？"
            }
        },
        "1_B520": {
            "name": "胜利 3",
            "text": {
                "1_B520": "战斗结束。拜，拜托下次用平时的我！"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "就算现实中没有那光，",
                "1_B801": "也不放弃，不气馁，",
                "1_B802": "强撑着，迈步前进。",
                "1_B803": "因为我，知道那希望。",
                "1_B804": "承继宿愿希望之剑。"
            }
        },
        "1_B810": {
            "name": "宝具 3",
            "text": {
                "1_B810": "让我们讲述一个故事吧。",
                "1_B811": "启程的誓言，救世的童话。",
                "1_B812": "无论羽翼几度染污，冬之鸟也会穿越暴雨。",
                "1_B813": "那份希望，不会消失！",
                "1_B814": "承继宿愿希望之剑！"
            }
        },
        "1_B840": {
            "name": "额外攻击 3",
            "text": {
                "1_B840": "发生了奇妙的事☆"
            }
        },
        "1_B880": {
            "name": "技能 5",
            "text": {
                "1_B880": "收到您的要求啦！"
            }
        },
        "1_B890": {
            "name": "技能 6",
            "text": {
                "1_B890": "结束后要大家一起，好吗！"
            }
        },
        "1_B910": {
            "name": "攻击 7",
            "text": {
                "1_B910": "跳跃，迸发！"
            }
        },
        "1_B930": {
            "name": "攻击 9",
            "text": {
                "1_B930": "这种其实也可以吧！"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "唔哦～～，等级提升了～～！",
                "1_G011": "智慧超级增长了啊～～！"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "我想你应该早就知道了，没有变化。",
                "1_G021": "要之后才能由这种『虽然很可爱但不该现在穿吧』的围裙风泳装换成风雅而华丽的打扮了……",
                "1_G022": "咦？　下次再临难道是阿瓦隆的我！？　这已经是我的最终泳装形态了吗！？"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "你一脸为什么会变成这样的表情呢。",
                "1_G031": "其实这件事有很深的隐情。",
                "1_G032": "详情请询问灵基第三的我。",
                "1_G033": "……就不能至少把头饰摘下来吗？",
                "1_G034": "摘不下来。",
                "1_G035": "这是最重要的隐喻？",
                "1_G036": "这样啊……",
                "1_G037": "可恶的奥伯龙……！"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "我有成长吗？　有？",
                "1_G111": "太棒了！",
                "1_G112": "就算职阶不同也是有办法的嘛！"
            }
        },
        "1_G120": {
            "name": "升级 3",
            "text": {
                "1_G120": "我感觉自己开始熟悉号嘴的用法了！",
                "1_G121": "在能够吹出好听的声音之前，我会努力的！"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "生日快乐！",
                "1_H011": "特别的日子就该准备特别的活动，综上所述，我预约了模拟装置室！",
                "1_H012": "蛋糕虽然不错，但战斗也很棒哦！",
                "1_H013": "去在高难度连续挑战中达成厉害的记录吧！"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "活动绝赞举办中～～！",
                "1_H021": "仔细阅读信息，以万全的态势面对吧！"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "穿，穿这身打扮出击吗～……",
                "1_H031": "不要把村正他们加入组队哦！？"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "这身打扮不适合跳舞吧。",
                "1_H041": "？　这样其实也不错？　就像偶像？",
                "1_H042": "——还，还有这种事吗！？"
            }
        },
        "1_H050": {
            "name": "对话 3 选择男性主人公时",
            "text": {
                "1_H050": "『欢迎光临，哥哥大人。』",
                "1_H051": "——好，这样就行了吧！？　因为你坚持，我才只说一遍哦！",
                "1_H052": "呜呜，感觉自己都不像自己了！　这就是兔子的诅咒吗！"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "以前喜欢的东西是巧克力……",
                "1_H061": "不知是否由于变成了狂战士……",
                "1_H062": "现在……想要打猎……"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "有人来南国之后还考虑讨厌的东西吗？",
                "1_H071": "就算悲观的自己也肯定会变得乐观啦。",
                "1_H072": "……虽然，也是有一个例外的啦。"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "希望圣杯实现的愿望依然没变！",
                "1_H081": "没错，只要那可恨的巴格子，",
                "1_H082": "还在！"
            }
        },
        "1_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H090": "现在一切都安定下来了，所以重新打个招呼。",
                "1_H091": "我是阿尔托莉雅·卡斯特，这次是作为狂战士前来的！",
                "1_H092": "支援魔术太麻烦了，所以随手一扔封印起来，这次以直觉、直接的方式帮你的忙！",
                "1_H093": "没错——用我的梅林魔术！"
            }
        },
        "1_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H100": "从者庆典就是相互展现艺术的祭典吧。",
                "1_H101": "虽然我不擅长绘画，但我觉得，用身体表现的舞蹈应该还有点机会！",
                "1_H102": "加油吧～！"
            }
        },
        "1_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H110": "虽然想做的事有很多，但首先必须解决特异点。",
                "1_H111": "没关系，我听说迦勒底的夏天很漫长。",
                "1_H112": "让我们慢慢来吧！"
            }
        },
        "1_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H120": "小号是阿瓦隆为我准备的。",
                "1_H121": "『虽然技术对音乐非常重要，但天赋也很重要。靠感觉想办法解决吧，这是你的拿手好戏吧？』",
                "1_H122": "诸如此类。虽然我不擅长用言语表达心情，但用音乐的话，应该就能传达毫不掩饰的心情了。"
            }
        },
        "1_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H130": "哎，友爱点数已经攒了那么多了！？",
                "1_H131": "南国太可怕了吧！？　这不会显得我很好搞定吗！",
                "1_H132": "……不过，算了，也是。毕竟确实玩得很开心。",
                "1_H133": "迦勒底的你，谢谢了。",
                "1_H134": "狂战士也不赖呢。创造的每一段回忆都非常闪耀。"
            }
        },
        "1_H170": {
            "name": "对话 4",
            "text": {
                "1_H170": "说起夏天当然少不了怪谈吧！　好，阿尔托莉雅·卡斯特，开始说啦！",
                "1_H171": "「早上起床后，我不禁怀疑自己看错了。冬日女王穿上了泳装，以媲美模特的优雅身姿闯入了夏日乐园——」",
                "1_H172": "别说背脊发凉了，连脑袋都冻起来了，我说真的。"
            }
        },
        "1_H190": {
            "name": "对话 5",
            "text": {
                "1_H190": "加雷斯亲的夏装～～！",
                "1_H191": "呜哇，冲浪板好帅啊！　在海浪上站起需要保持的平衡一定很难吧！",
                "1_H192": "也让我挑战一下啦！"
            }
        },
        "1_H200": {
            "name": "对话 6",
            "text": {
                "1_H200": "喂，还能穿女仆装的吗！？　泳装上哪儿去了，泳装！",
                "1_H201": "我原本还期待充满牙之氏族风范的野性震撼的那种呢！",
                "1_H202": "啊～受不了，既然打算伺候，那我这边也麻烦你了哦！"
            }
        },
        "1_H210": {
            "name": "对话 7",
            "text": {
                "1_H210": "我和芭万·希之间虽然发生了不少事，但她也和其他妖精骑士一样变了呢。",
                "1_H211": "还是说这才是她的本性呢？",
                "1_H212": "我总觉得现在应该能够和她一起创作作品了。",
                "1_H213": "只是我不太能理解那身人偶装。"
            }
        },
        "1_H220": {
            "name": "对话 8",
            "text": {
                "1_H220": "巧克力妖精加南十字星加会拉小提琴加从者庆典的委员长，信息量过大脑子都要炸了啦……！",
                "1_H221": "不过嘛，虽然有很多头衔，但那百分百是诺克娜蕾吧。",
                "1_H222": "好嘞……我要去好好向她显摆自己舞蹈大师的英姿！"
            }
        },
        "1_H280": {
            "name": "对话 9",
            "text": {
                "1_H280": "虽然我知道风之妖精，但竟然还存在可被称作大气本身的精灵，太令人惊讶了。",
                "1_H281": "虽然由来和形态都和我们不同，但她“希望和大家和睦相处”的心情养育了生命吧。",
                "1_H282": "感谢你在愉快的夏日舞台选择了诺克娜蕾，旺吉娜。"
            }
        },
        "1_H5050": {
            "name": "对话 3 选择女性主人公时",
            "text": {
                "1_H5050": "『欢迎光临，姐姐大人。』",
                "1_H5051": "——好，这样就行了吧！？　因为你坚持，我才只说一遍哦！",
                "1_H5052": "呜呜，感觉自己都不像自己了！　这就是兔子的诅咒吗！"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "是那边吧。"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "犹如羽毛。"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "在这边。"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "守护者们啊！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "龙脉烧却兵装，装填。",
                "2_B051": "圆桌圣枪，并列拔锚。",
                "2_B052": "祈祷之枪，救济之剑。",
                "2_B053": "在此，化作洗刷罪孽的光！",
                "2_B054": "承继宿愿希望之剑！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "仅作为消遣。"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "奏响留存心中的旋律。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "要战斗吗？　可以哦，我愿意奉陪。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "不用担心，因为无论何时，都可以锻造圣剑。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "玩得高兴吗？ 那就好，我也过得非常有意义。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "沙沙～，沙沙～。 夏日的海滩真不错。夕阳很美。"
            }
        },
        "2_B140": {
            "name": "受击 2",
            "text": {
                "2_B140": "危险。"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "是大浪警报吗？"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "哎，已经结束了……！？ 咳咳。不，我很冷静，应该迅速撤退。下次也拜托了。"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "很遗憾，只能到此为止了，御主。 下次在被打倒前打倒对方吧。"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "叫我吗？"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "很准确。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "期待已久。"
            }
        },
        "2_B2010": {
            "name": "攻击 10",
            "text": {
                "2_B2010": "交响乐！"
            }
        },
        "2_B2020": {
            "name": "攻击 11",
            "text": {
                "2_B2020": "犹如降雨。"
            }
        },
        "2_B2030": {
            "name": "攻击 12",
            "text": {
                "2_B2030": "莫杜尔。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "神域之技，至此。"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "宝具，展开。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "好吧。再次展现救济之技！"
            }
        },
        "2_B2410": {
            "name": "攻击 13",
            "text": {
                "2_B2410": "卡里德福洛斯。"
            }
        },
        "2_B2420": {
            "name": "攻击 14",
            "text": {
                "2_B2420": "一刀，两段！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "犹如疾风。"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "米斯特汀。"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "吸引你们的目光吧。"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "龙之悸动！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "比光，更锐利。"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "比火，更激烈。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "那么，就奏一曲。只要能为祭典添彩。"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "我有回应你的期待吗？ 我完全可以继续，后续也请随时叫我。"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "有平时两倍那么痛。狂战士真辛苦呢。"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "黑铁之城，神域之技。",
                "2_B801": "圆桌虽已成空席，",
                "2_B802": "圣枪却未有瑕疵。",
                "2_B803": "——即便远离乐园！",
                "2_B804": "救世的童话，今仍确实无疑！"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "致止境年幼的你，",
                "2_B811": "相信着你，仰望着你，",
                "2_B812": "如今的我仍在奔走。",
                "2_B813": "留存心中的星之悸动。",
                "2_B814": "承继宿愿希望之剑！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "如星辰般闪耀。"
            }
        },
        "2_B880": {
            "name": "技能 5",
            "text": {
                "2_B880": "可不只有圣剑哦。"
            }
        },
        "2_B890": {
            "name": "技能 6",
            "text": {
                "2_B890": "完成度很高。啊唔～。"
            }
        },
        "2_B910": {
            "name": "攻击 7",
            "text": {
                "2_B910": "十束剑。"
            }
        },
        "2_B920": {
            "name": "攻击 8",
            "text": {
                "2_B920": "海浪要来了哦。"
            }
        },
        "2_B930": {
            "name": "攻击 9",
            "text": {
                "2_B930": "阿瓦隆之门。"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "积累是非常重要的。",
                "2_G011": "让我们细致、仔细地成长吧。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "是的，非常抱歉。",
                "2_G041": "从此刻起，阿尔托莉雅·阿瓦隆将作为她的本质，代替阿尔托莉雅·卡斯特前来。",
                "2_G042": "本质这种说法或许有些偏差，但说起来话就长了，有空再解释吧。",
                "2_G043": "比起这些——这身打扮如何？",
                "2_G044": "我平时虽然身穿甲胄丝毫不裸露皮肤，可只要认真起来也能打扮成这样。",
                "2_G045": "无论是游泳展销会还是摄影都放马过来吧。"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "从漫长的坠落中苏醒过来，眼前已是南国之海——",
                "2_G051": "太好了，最后还是赶上了。看来阿瓦隆的我也不是魔鬼呢。",
                "2_G052": "休闲装和围裙打扮虽然都不错，但夏天大海的经典打扮还是泳装吧。",
                "2_G053": "虽然祭典迟早会结束，但现在就让我们忘记时间，与你共创闪耀的记忆吧。",
                "2_G054": "好嘞～，去覆盖更新那寒酸的夏日记忆吧～！"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "永远不变的形貌吗？",
                "2_G111": "承蒙夸奖，不胜惶恐。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "嗯，麻烦帮忙提升到极限。",
                "2_G121": "要提升到真正的极限哦？"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "今天是你生日吧。",
                "2_H011": "虽然我也考虑过是否该在星之内海郑重为你祝贺，但现在如你所见，状态比较轻松。",
                "2_H012": "何不在洋溢着人们笑容的泳池，像孩童般欢闹呢？"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "活动已经开始了，千万不要错过。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "要出击了吗？",
                "2_H031": "那么，就去赢得连完美都能破坏的压倒性胜利吧。",
                "2_H032": "毕竟我是狂战士嘛。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "还请尽管下令。",
                "2_H041": "即便身姿优雅，我也是圣剑的化身。",
                "2_H042": "我定会回应您的愿望。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "您或许并没有发现，但这身姿是御主期望的结果。",
                "2_H051": "所以，还请不要避而不视。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "喜欢的东西和阿尔托莉雅·卡斯特没什么差别。",
                "2_H061": "但跳舞的技术还是我更胜一筹。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西倒是没什么。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "我没有私欲，所以没有什么需要圣杯实现的愿望。",
                "2_H081": "只不过，我明白将贵重的东西交给我保管这种行为是很好的。",
                "2_H082": "这并不是什么无谓之举。"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "我是『圣剑的骑士』这一概念具现化的存在。",
                "2_H20091": "是受众多人希望如此的愿望，不具备共通名字的无名之王。",
                "2_H20092": "这么说或许有些夸张，所以就叫我A·A吧。",
                "2_H20093": "因为现在，是夏日的祭典啊。",
                "2_H20094": "我也打算表现出平日里双倍的轻快。"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "你问我和阿尔托莉雅·卡斯特是否能同时存在？",
                "2_H20101": "嗯，当然可以。",
                "2_H20102": "我基本会在魂之世界，想要干涉这边世界的时候才会以阿尔托莉雅·卡斯特的形式被召唤。",
                "2_H20103": "这种时候，当需要的是『A·A』，而不是『阿尔托莉雅·卡斯特』的时候，我就会出现在这个世界，但并不会因为我的存在，就无法召唤『阿尔托莉雅·卡斯特』了。",
                "2_H20104": "唔～，说起来果然很花时间呢……",
                "2_H20105": "靠感觉去理解吧。"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "『圣剑的骑士』这一概念，同时也意味着『圣剑的守护者』。",
                "2_H20111": "而这样的我能否做好狂战士职阶，多少令我有些不安。",
                "2_H20112": "不过，我是不会输给摩根的。"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "南国之海……",
                "2_H20121": "虽然我是出现在黄昏的存在，但黄昏也是分种类的。",
                "2_H20122": "不怀着悲伤，而是充满对一天的感谢与对明天希望的夕阳景色……",
                "2_H20123": "太美妙了。",
                "2_H20124": "边欣赏这种风景边品尝的晚餐一定别具一格吧。",
                "2_H20125": "——我这是在邀请你哦？"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H20130": "非常感谢您能为我提供这样的机会。",
                "2_H20131": "这个夏日对阿尔托莉雅·卡斯特而言，是“梦想的景象”，对我来说，则是“想送给她”的东西。",
                "2_H20132": "彼此的希望都获得了实现，算是双赢，对吧。",
                "2_H20133": "虽然迦勒底的召唤术称不上正当，但正因为不正当，才会出现这种故障。",
                "2_H20134": "错误并不是罪过，如何处理就取决于你们了。"
            }
        },
        "2_H230": {
            "name": "对话 4",
            "text": {
                "2_H230": "所谓奈落之虫，是出现在大树上的黑洞。是让生死失去价值的不断落下的坠落之梦。",
                "2_H231": "会将跃入那黑暗的她与落入空虚的爱丽丝重叠在一起看待，也是在所难免的吧？"
            }
        },
        "2_H240": {
            "name": "对话 5",
            "text": {
                "2_H240": "哈？　为什么村正先生不来游泳？",
                "2_H241": "没有泳装灵衣……？　那位的灵基第一状态不是和泳装没什么差别吗？",
                "2_H242": "立刻把他带过来。"
            }
        },
        "2_H250": {
            "name": "对话 6",
            "text": {
                "2_H250": "真祖的公主……",
                "2_H251": "是这颗星球的灵魂，也是作为卫星容器的镜子设计出来的存在。",
                "2_H252": "虽然只是作为灵基被召唤出来的，但她能在这里还是很令人安心的。",
                "2_H253": "——不，并不是指她是个可靠的伙伴。",
                "2_H254": "只是当她成为敌人时，会比作为伙伴时任性十倍。"
            }
        },
        "2_H260": {
            "name": "对话 7",
            "text": {
                "2_H260": "阿尔比恩应该不会游泳才对……",
                "2_H261": "原来如此，专精于空战，换上了绝对无法进入海洋的装备啊。",
                "2_H262": "泛人类史的阿尔托莉雅只能算龙种的混血，而她则是纯粹的龙。",
                "2_H263": "这种万能性、变化性虽然是首屈一指的……",
                "2_H264": "但未免太自由了吧？"
            }
        },
        "2_H270": {
            "name": "对话 8",
            "text": {
                "2_H270": "这次做的事有些对不起BB呢，请允许我借这个场合向她道歉。",
                "2_H271": "但是，她是每被打倒一次就会变得更强并重新站起来的不屈女主角。",
                "2_H272": "从长远角度来看，这次的屈辱对她或许还是件好事呢。"
            }
        },
        "2_H3070": {
            "name": "讨厌的东西 (2部7章通关后)",
            "text": {
                "2_H3070": "反感的东西依然是阿尔比恩，还要加上宇宙侵略体。",
                "2_H3071": "哎……因为我没想到那东西竟然如此像蜘蛛啊……"
            }
        }
    },
    "ChrVoice_ids": [
        "109827440",
        "109827540",
        "109827811",
        "704700"
    ]
}
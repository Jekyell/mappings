        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "自不净之园而来！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "“沉睡之门”的另一侧，",
                "0_B051": "降临所至乃幻梦境。",
                "0_B052": "灾厄的魔都，隐秘的严寒荒野，",
                "0_B053": "蕃神的孤峰，未知的绝巅！",
                "0_B054": "一旦到访，便无法离去。",
                "0_B055": "遥远的……幻梦境。"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "开启吧……门啊……！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "猫会制裁罪恶……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "吾知晓其门。汝不可关闭，呵呵……"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "Ia，Ia！　啊哈哈……"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "真蠢，碍事的家伙已经死了哦。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "听到夜鹰的啼鸣声了吗……？"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "啊啊……真粗暴……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "御主……我看不见你……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "快开门啊……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "说得对。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "真高明。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "做不到。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "哪怕痛苦，也要忍受哦，御主。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "啊啊……吾之胎内藏有白银之钥……"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "畏惧无尽之梦的深渊吧。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "是客人哦？　好好款待吧……"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "陪他玩玩。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "不能挑剔哦？"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "降临此处吧……父亲。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "Ia！　Eh-y-yahaahfhtagn！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "白色的帆船扬起船帆……"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "父亲……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "七十、七百级梦境阶梯。",
                "0_B801": "降临所至乃幻梦境。",
                "0_B802": "灾厄的魔都，隐秘的严寒荒野，",
                "0_B803": "蕃神的孤峰，未知的绝巅！",
                "0_B804": "其在梦境中静待来访。",
                "0_B805": "遥远的……幻梦境。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "啊啊，真好吃啊，御主。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "完全不够。",
                "0_G111": "请给我。多到能沐浴其中……"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "请让我保护你哦……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "今天是御主的生日吧……",
                "0_H011": "对不起。可怕魔女的礼物一定会破坏这喜庆的日子吧……",
                "0_H012": "如果您不介意的话……能允许我为您送上小小的祈祷吗？",
                "0_H013": "主与我们同在……",
                "0_H014": "请赐予御主与您所爱的人安宁祥和……",
                "0_H015": "不会被梦境迷惑，也不会泪湿面颊……唯有平静的沉睡……"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "收到不详的报告了哦，御主。",
                "0_H021": "也就是说，轮到我出场了。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "今天要开启位于何处的门呢？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我会化作迎接你的“门”……"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "怀着祈祷敲响大门吧……",
                "0_H051": "哪怕是尽头的地狱，我，也会回应。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西喜欢的东西……",
                "0_H061": "还是血淋淋的厚切牛排！",
                "0_H062": "餐刀一切不顾一切咬上去的感觉真让人按捺不住。",
                "0_H063": "……当然上面只是在开玩笑。",
                "0_H064": "难得的机会，我想品尝一下成熟口味的冰淇淋呢。",
                "0_H065": "在香草冰淇淋上淋上几滴白兰地。",
                "0_H066": "要不要悄悄去愿意偷偷给我吃这些的人的房间玩呢。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……",
                "0_H071": "唔……除了狂吠的狗以外……",
                "0_H072": "啊啊。",
                "0_H073": "但凡我发现闹钟，都会用锤子敲坏。",
                "0_H074": "因为竟然胆敢冷酷无情地妨碍安眠，简直是不可饶恕。",
                "0_H075": "对吧？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "指望梦想的力量简直荒谬……",
                "0_H081": "呵呵，究竟该怎么办呢。",
                "0_H082": "呵呵呵呵。",
                "0_H083": "啊啊……对不起，御主。",
                "0_H084": "我只是觉得好笑……",
                "0_H085": "呜……我才……没哭……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我扯下了蝴蝶的翅膀。",
                "0_H091": "活生生地、庄严地，犹如祭礼……",
                "0_H092": "怜悯与祈祷，绝望……恍惚。",
                "0_H093": "难道会有人不被这种景象所吸引吗……？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我们一起去夜晚的水边游泳吧，御主？",
                "0_H101": "借助微弱的星光。",
                "0_H102": "不知不觉连星光都消失了……",
                "0_H103": "只留下在这漆黑世界中交缠在一起的手指的触感。",
                "0_H104": "身为魔女的我，能漂浮在水上吗？",
                "0_H105": "还是说，伴随着脆弱泡沫的浮起，沉入深邃的水底呢……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "「♪～♪♪～♪～♪♪～♪～～♪♪…… Gloria in excelsis Deo……♪」",
                "0_H111": "……好美的旋律……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "御主您……想要我吗？　害怕我吗？",
                "0_H121": "还是想要将我破坏得体无完肤？",
                "0_H122": "如果异端的魔女会扰乱内心的平静，",
                "0_H123": "您可以一直避而不视……",
                "0_H124": "我……只要能依偎在您背后，就足够了……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "真正的自己……应该实现的理想……",
                "0_H131": "咯咯。我完全不在意这种轻浮的存在。",
                "0_H132": "大家都是为了与他人建立联系，而不停地说着虚伪的话语。",
                "0_H133": "比起这些，让我看看你的梦吧？",
                "0_H134": "只有你能抵达的梦境之国……",
                "0_H135": "我想要守护那个地方。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "另一个我……",
                "0_H171": "有时会令人不快到无法直视。真就是自我厌恶……",
                "0_H172": "不过没有比这更好的自我回顾的方法了吧。",
                "0_H173": "我果然还是个小孩呢……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "黑色外套的吉尔·德·雷先生？",
                "0_H181": "嗯，他非常友善。也会毫无保留地忠告我，对我很有帮助。",
                "0_H182": "还会把珍贵的书籍借给我，说一些关于神秘的有趣故事给我听。",
                "0_H183": "虽然还有些困难，但我逐渐开始理解他说的话了。",
                "0_H184": "唯一的缺点就是话题偶尔会跑偏，一说起献上活祭的问题，描述内容就会细致入微无比冗长……"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "呵呵呵……",
                "0_H191": "女主角XX小姐。",
                "0_H192": "一旦半夜里感到饥饿，",
                "0_H193": "偷偷潜入厨房的话，八成会撞上她。",
                "0_H194": "只要在一起吃面条时捉弄她，她就会时而大闹时而消沉，时而双眼熠熠生辉露出微笑……",
                "0_H195": "非常热闹，让人看不厌……"
            }
        },
        "0_M010": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 8",
            "text": {
                "0_M010": "辛苦了。",
                "0_M011": "感觉非常不错呢！"
            }
        },
        "0_M020": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 9",
            "text": {
                "0_M020": "来，收下你努力的结晶吧。",
                "0_M021": "如果累了……就休息一会儿吧？",
                "0_M022": "不介意的话，把我的膝盖当做短暂的午睡枕头吧。"
            }
        },
        "0_M030": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 10",
            "text": {
                "0_M030": "是新任务呢。",
                "0_M031": "出发，成为探寻神秘之人吧？"
            }
        },
        "0_M040": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 11",
            "text": {
                "0_M040": "好像有新关卡了呢。 探究就是要孜孜不倦踏踏实实前进，对吧？"
            }
        },
        "0_M060": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 12",
            "text": {
                "0_M060": "哎呀！",
                "0_M061": "泳装的虞美人小姐好像终于成了迦勒底的一员呢。",
                "0_M062": "今后一定会非常愉快吧！"
            }
        },
        "0_M070": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 13",
            "text": {
                "0_M070": "啊啊……终于到最后一个关卡了呢。",
                "0_M071": "这地方也会冷清下来了吧……",
                "0_M072": "好了，一路顺风。"
            }
        },
        "0_M080": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 14",
            "text": {
                "0_M080": "终于彻底完成了呢。 恭喜。我也要由衷祝贺您。 呜……请不要再回头，继续前进吧。 如果被您看到了我的表情，我会不好意思的……"
            }
        },
        "0_N010": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 1",
            "text": {
                "0_N010": "让我们齐心协力，一起努力吧？"
            }
        },
        "0_N020": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 2",
            "text": {
                "0_N020": "又冰又甜，要来一份柠檬水吗？"
            }
        },
        "0_N030": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 3",
            "text": {
                "0_N030": "太拼命的话……对身体可不好哦？"
            }
        },
        "0_N040": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 4",
            "text": {
                "0_N040": "和黑猫一起烤薄饼♪　喵♪",
                "0_N041": "把黑猫放在烤薄饼上♪　喵♪",
                "0_N042": "黑猫烤薄饼烤好了，",
                "0_N043": "黑猫烤薄饼～～～～♪　喵喵♪"
            }
        },
        "0_N050": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 5",
            "text": {
                "0_N050": "读过通知了吗？",
                "0_N051": "这样啊。那我就放心了。"
            }
        },
        "0_N060": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 6",
            "text": {
                "0_N060": "呼～～，辛苦了。",
                "0_N061": "好了，您的心脏里究竟残留着什么样的恐怖呢？",
                "0_N062": "呵呵呵。"
            }
        },
        "0_N070": {
            "name": "从者·夏令营！\n～迦勒底·惊悚之夜～ 7",
            "text": {
                "0_N070": "你看……",
                "0_N071": "夏天……也结束了呢……"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "你好，御主。",
                "0_S011": "Foreigner，阿比盖尔·威廉姆斯。",
                "0_S012": "……您在害怕吗？",
                "0_S013": "年幼的女孩就像道德观念尚未成熟的虫蛹。",
                "0_S014": "请您接纳像猫眼一样变化无常的我吧。",
                "0_S015": "让我们一起创造美丽的回忆吧……"
            }
        },
        "1_B010": {
            "name": "攻击 1",
            "text": {
                "1_B010": "好，拿过来～！"
            }
        },
        "1_B030": {
            "name": "攻击 3",
            "text": {
                "1_B030": "啊啊啊，对不起！"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "夏天，太阳， 烤薄饼！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "“沉睡之门”的另一侧，",
                "1_B051": "旅途巡回千之惊奇，所有神秘！",
                "1_B052": "失落的黄金都市，壮丽的永恒花都，",
                "1_B053": "诸神的御座，梦之尽头的极限之门！",
                "1_B054": "虽可造访，但无法离去。",
                "1_B055": "遥远的幻梦境！"
            }
        },
        "1_B080": {
            "name": "技能 1",
            "text": {
                "1_B080": "您期望的梦想是什么呢？"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "呀，吓了我一跳。"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "争斗是不好的啊……"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "我不会让你们痛苦的，请务必……"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "美妙之光之主已经降临……"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "没事吧，御主？"
            }
        },
        "1_B150": {
            "name": "受击 1",
            "text": {
                "1_B150": "我还能……继续……"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "御主……对不起……"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "因为……我是魔女吗……"
            }
        },
        "1_B180": {
            "name": "指令卡 1",
            "text": {
                "1_B180": "好。"
            }
        },
        "1_B190": {
            "name": "指令卡 2",
            "text": {
                "1_B190": "顺您心意。"
            }
        },
        "1_B200": {
            "name": "指令卡 3",
            "text": {
                "1_B200": "虽然有些不安……嗯。"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "呜呼，残酷的御主……"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "此刻，正是星辰的良机……！"
            }
        },
        "1_B230": {
            "name": "宝具卡 3",
            "text": {
                "1_B230": "统治一切的神明啊……"
            }
        },
        "1_B410": {
            "name": "攻击 4",
            "text": {
                "1_B410": "你要把你的重担卸下？"
            }
        },
        "1_B420": {
            "name": "攻击 5",
            "text": {
                "1_B420": "就是现在，诺修！ 就是现在，鲁撒！"
            }
        },
        "1_B430": {
            "name": "攻击 6",
            "text": {
                "1_B430": "打个招呼吧？"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "甜美的梦境啊到来吧…… 呀，或……或许有些粗鲁……"
            }
        },
        "1_B480": {
            "name": "技能 3",
            "text": {
                "1_B480": "去和猫和解……？"
            }
        },
        "1_B490": {
            "name": "技能 4",
            "text": {
                "1_B490": "鲁撒真是的，怎么连诺修也这样，拿你们没办法。"
            }
        },
        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "即便是斯芬克斯忘却的智慧，猫咪们也记得……"
            }
        },
        "1_B540": {
            "name": "受击 4",
            "text": {
                "1_B540": "好痛……"
            }
        },
        "1_B550": {
            "name": "受击 3",
            "text": {
                "1_B550": "只要稳住气势，就还不会输……"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "七十、七百级梦境阶梯。",
                "1_B801": "旅途巡回千之惊奇，所有神秘！",
                "1_B802": "失落的黄金都市，壮丽的永恒花都，",
                "1_B803": "诸神的御座，梦之尽头的极限之门！",
                "1_B804": "此刻打破禁忌，穿越无限。",
                "1_B805": "遥远的幻梦境！"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "不要勉强自己哦，御主。"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "您看，很可爱吧？",
                "1_G021": "不，不对……不是指我。",
                "1_G022": "我肩膀上的这孩子叫“诺修”。",
                "1_G023": "我脚边胖胖的那个叫“鲁撒”。",
                "1_G024": "它们俩关系非常好。",
                "1_G025": "对吧？　……对吧？",
                "1_G026": "……如果不给我肯定的回答，一会儿你们可就麻烦了哦。",
                "1_G027": "……嗯，非常好。"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "疯狂与理性的狭缝间，梦境与现实的彼方。",
                "1_G031": "跨越蔷薇的沉睡，被带领至古老存在们的宝座……",
                "1_G032": "此刻，我在此显现。",
                "1_G033": "Foreigner灵基也被更新了……",
                "1_G034": "这是好事？　还是坏事？",
                "1_G035": "呜呼……没将重要的御主置身于无法挽回的危险中吧……"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "谢谢，我会努力的。"
            }
        },
        "1_G120": {
            "name": "升级 3",
            "text": {
                "1_G120": "话说……能再给一点……",
                "1_G121": "不，没关系。"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "御主？　祝您生日快乐！",
                "1_H011": "虽然送您一首热闹的歌曲也不错……",
                "1_H012": "但如果您同意的话，我们今天办一个悠闲的茶话会吧？",
                "1_H013": "御主的家人，令人怀念的故乡，",
                "1_H014": "内心描绘的梦想……",
                "1_H015": "请都说来给我听听吧？"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "根据我的占梦……",
                "1_H021": "得出了前往陌生的土地可以获得幸运的结果哦。",
                "1_H022": "如果您不介意的话，我愿意奉陪。"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "让我们去完成日常的工作吧，御主？"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "我就是我哦，御主。",
                "1_H041": "是您找到的不谙世事的一介村姑阿比盖尔……",
                "1_H042": "只要这暂时的生命还在延续，就一直是这样。"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "旅途的伙伴……禁忌的魔女……",
                "1_H051": "小小的……恋……恋人……",
                "1_H052": "我好想成为您所期望的我啊……"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "最近我开始喜欢读书了。",
                "1_H061": "常常会读得入迷，一不小心就熬夜了……",
                "1_H062": "然后不知不觉祈祷时间到了，才慌慌张张地准备。",
                "1_H063": "我是个坏孩子吧？　呵呵。"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "与其说是讨厌，倒不如有件事让我很为难……",
                "1_H071": "我会不知不觉置身某处神殿中，穿着一件十分合身的衣服，被一群老人围在中间。",
                "1_H072": "给我一些老式的点心、海带茶，还有金枪鱼糖……？之类很多东西。",
                "1_H073": "虽然他们对我非常友善，但我完全听不懂他们的话……",
                "1_H074": "而且，这些孩子明明能听懂他们的话，却只会在心血来潮的时候告诉我内容。",
                "1_H075": "真是的。"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "我觉得“圣杯”一定不具备实现我愿望的力量与容量……",
                "1_H081": "这也是因为我做得不够好……"
            }
        },
        "1_H10090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H10090": "虽然我不会做梦……",
                "1_H10091": "但可以和梦境连接。",
                "1_H10092": "甚至是那些一旦踏入，就再也回不来的甜美而危险的梦境。"
            }
        },
        "1_H10100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H10100": "……这个？　这不是什么泳装哦？",
                "1_H10101": "这身衣服是宣扬神之教诲的“高位者（adeptus）”的礼服。",
                "1_H10102": "但是……令人无奈的是必须随身携带这个沉重的台座才行。",
                "1_H10103": "是为了适应夏天而调整了设计的反作用吗……",
                "1_H10104": "不，没有？　没什么事。"
            }
        },
        "1_H10110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H10110": "「♪～♪～♪～♪～♪♪～♪～♪～♪……♪」",
                "1_H10111": "……哎呀，御主。",
                "1_H10112": "我下意识哼了起来。",
                "1_H10113": "……好哀伤的歌。所以我喜欢。"
            }
        },
        "1_H10120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H10120": "塞勒姆的……阿比……？",
                "1_H10121": "称呼您为“团长”的女孩？",
                "1_H10122": "…………对不起……遗憾的是，这是现在的我无法实现的。",
                "1_H10123": "真奇怪。明明都是我的事……我居然会如此羡慕……",
                "1_H10124": "但是，等有意愿之后，请再说给我听吧……"
            }
        },
        "1_H10130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H10130": "请您一定要留在这里哦……？",
                "1_H10131": "请保持现在这样哦，御主。",
                "1_H10132": "保持脆弱的心灵……保持炙热的双眼。",
                "1_H10133": "可不能变成……像我这样的哦。"
            }
        },
        "1_H200": {
            "name": "对话 4",
            "text": {
                "1_H200": "比起自以为学习了知识变得聪明的自己……",
                "1_H201": "或许还是虽然犹豫却怀着真挚之心的我更为强大……",
                "1_H202": "作为御主的从者，我没有变弱吧……？"
            }
        },
        "1_H210": {
            "name": "对话 5",
            "text": {
                "1_H210": "英……英勇的大卫王……多少有些轻浮的大卫王……",
                "1_H211": "我应该和那位好好谈一次才行，嗯。",
                "1_H212": "关于神明……关于心灵的宁静……还有关于他是如何看待平时总是挂在嘴边的亚比煞小姐的事……"
            }
        },
        "1_H220": {
            "name": "对话 6",
            "text": {
                "1_H220": "我再次认识到她是个非常厉害的人。",
                "1_H221": "嗯……是葛饰北斋小姐。",
                "1_H222": "那个人用画笔流畅地描绘的作品会直接化作“梦境”。",
                "1_H223": "清醒状态下看到的梦，没错，是会实现的梦哦？",
                "1_H224": "难怪……身为从者之身，却能和魔术师或是敌人的那些怪物平分秋色地对抗呢……"
            }
        },
        "1_H230": {
            "name": "对话 7",
            "text": {
                "1_H230": "比方说哦，当身在迦勒底时哦。",
                "1_H231": "我有时会感觉到自己的梦之力量……会被什么人干涉……",
                "1_H232": "杨贵妃大人……对，玉玉小姐。",
                "1_H233": "那个人可以连接围绕北落师门的幻梦境……",
                "1_H234": "是星辰的彼方哦？　非常了不起哦？",
                "1_H235": "虽说她本人表示那是侍女们待命的“后宫”。",
                "1_H236": "但如果能得到允许的话，我真想去叨扰一次呢。"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "摇摆！　呵呵，做得漂亮！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "实在，对不起，哟！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "呵呵呵，真愉快！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "好了，来吧御主？ 呵呵，啊哈哈哈。"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "“沉睡之门”的另一侧。",
                "2_B051": "天行交错乃繁星之帆船。",
                "2_B052": "云幕摩天楼，猫咪休憩的村落，",
                "2_B053": "诸神的宫殿，冻土荒野之绝险！",
                "2_B054": "来访客人不愿归。",
                "2_B055": "遥远的幻梦境☆"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "要来这里吗？"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "轻飘飘，就像在做梦！"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "呵呵，御～主～～～♪"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "讨厌，真是个坏人呢。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "啊哈哈，稍微有点累了。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "我们一起去喝茶吧，御主？"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "真是个坏人呢……？"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "得赶快……起床才行……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "大家……都会死……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "悉听尊便。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "嗯，我知道。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "真的吗？"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "将这变成一次最美妙的假日吧？"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "帚星鲜明地闪烁着……"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "逐渐充盈……蔷薇的香气……"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "哎，没错，就这样！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "看招，扣杀得漂亮！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "鲁撒？"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "短暂的夏日一梦！ 哇，呵呵呵呵！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "一起游泳吧？"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "呵呵，猫之国度靠近了呢！"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "让这成为留在心里的回忆吧？"
            }
        },
        "2_B540": {
            "name": "受击 4",
            "text": {
                "2_B540": "真是的……"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "心满意足了吗……？"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "七十、七百级梦境阶梯。",
                "2_B801": "天行交错乃繁星之帆船。",
                "2_B802": "云幕摩天楼，猫咪休憩的村落，",
                "2_B803": "诸神的宫殿，冻土荒野之绝险！",
                "2_B804": "探索之辈心怀期待叩响大门。",
                "2_B805": "遥远的幻梦境☆"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "啊啊……好高兴。请让我帮上您的忙哦。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "呵呵。",
                "2_G041": "这件泳装还合适吗？",
                "2_G042": "虽然很不好意思，但只要御主喜欢，我就会非常高兴……",
                "2_G043": "我做梦都没有想到自己能在水边游泳，暴露的皮肤能沐浴在这种水晶般的日光中。",
                "2_G044": "无论何时何地，这美妙的时光如果能永远延续下去就好了……"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "哪怕那个时刻终将到来，您也不要悲伤哦，御主。",
                "2_G051": "无论是对遗忘，还是对分隔两地无法见面……",
                "2_G052": "大家都是神明赐予的赠礼。",
                "2_G053": "珍惜短暂的时光，怜爱微小的幸福。",
                "2_G054": "这就是我们的福音，是真正的宝物……",
                "2_G055": "我是这么认为的。"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "……我真想变成大人呢。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "呵呵，御主喜欢我吗？"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "祝您生日快乐，御主！",
                "2_H011": "我昨晚在拼命思考该送什么礼物……",
                "2_H012": "不过我能送给您的，只有非常微不足道的东西。",
                "2_H013": "御主自己挑选想要什么吧？",
                "2_H014": "第一种，是我今天一整天会成为御主的妈妈。尽情向我撒娇吧？",
                "2_H015": "第二种，是我会招待您品尝我亲手制作的烤薄饼。我会根据您的年龄放上同样数量的莓果作为点缀。",
                "2_H016": "……第三种，是一句话都不说，两个人安静地去海滩散步。",
                "2_H017": "啊……列出的这些好像全都成了我想做的事了呢……咯咯。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "鲁撒？　诺修？　出门了哦！"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "能不能带我去雪白的沙滩呢，御主？"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "身为从者忠实侍奉您的我……",
                "2_H041": "温柔地体谅我辛勤的御主。",
                "2_H042": "心灵之主究竟是哪个？",
                "2_H043": "在梦境的世界中一定是模糊不清的。",
                "2_H044": "而且……",
                "2_H045": "我们真正的主人，或许是猫咪哦。",
                "2_H046": "咯咯。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "只要御主能过上平静的日子，我就心满意足了。",
                "2_H051": "但是您一定会选择最为艰难的道路吧……",
                "2_H052": "所以哪怕只有短暂的一刻，我也会成为治愈您的梦境。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "说起喜欢的东西……",
                "2_H061": "当然是烤薄饼啦！",
                "2_H062": "呵呵。而我也明白了。在现代，人活着不是单靠烤薄饼的！",
                "2_H063": "松软的松饼、漂亮的可丽饼、四角形的格雷派饼、布利尼、御好烧。",
                "2_H064": "太美妙了，世界上充满了面粉……",
                "2_H065": "咦？　只有最后一个种类不同？",
                "2_H066": "难得有一个御主国家的蛋糕，遭到排挤也太可怜了……"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "喜欢就是讨厌，讨厌就是喜欢。",
                "2_H071": "远离某种存在，盲目地想要从视野中消除对方，也是因为内心被那个吸引。",
                "2_H072": "梦境则会暴露这种企图。",
                "2_H073": "泡沫之梦……有时候也会展现真实。",
                "2_H074": "所以……我对梦……"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "如果这颗行星上的一切都成为“梦境之国”的一部分就好了……",
                "2_H081": "这就是我想要托付给“圣杯”实现的愿望。但也是绝不会被原谅的罪孽……",
                "2_H082": "所以我才会希望。"
            }
        },
        "2_H20090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H20090": "泳装真是种非常好的东西呢！",
                "2_H20091": "因为这能让人们意识到，大家只是披着淡薄的常识，而实际上一直是赤裸裸的吧？"
            }
        },
        "2_H20100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H20100": "难得的机会，要不要挑战一下晒黑呢？",
                "2_H20101": "我总觉得现在这个灵基可以做到……",
                "2_H20102": "哪怕在夜里，小麦色皮肤的我肯定也会散发出太阳的香味吧。",
                "2_H20103": "御主要不要也一起来试试？"
            }
        },
        "2_H20110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H20110": "「♪……♪……♪♪……♪…… ♪……♪……♪♪……」",
                "2_H20111": "多么美妙的歌啊……仿佛身心都变得通透了……"
            }
        },
        "2_H20120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H20120": "我做了这么一个梦。",
                "2_H20121": "是去叨扰了御主的梦吗？",
                "2_H20122": "在一个阳光炽热的盛夏日子……",
                "2_H20123": "漂浮在碧绿色的波浪上，",
                "2_H20124": "品尝着冰凉的烤薄饼。",
                "2_H20125": "我明明从未吃过这种东西。",
                "2_H20126": "蔓越莓的开胃菜也非常酸。",
                "2_H20127": "但是，相当好吃。",
                "2_H20128": "多半是由于我和某个人在一起吧。",
                "2_H20129": "……是梦境吧。"
            }
        },
        "2_H20130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H20130": "话说，御主……？",
                "2_H20131": "如果带上我沉溺于隐秘之梦的话，我也会非常幸福哦。",
                "2_H20132": "无论是什么梦境，只要能和御主在一起就能幸福。",
                "2_H20133": "……但是哦？",
                "2_H20134": "等这颗星辰大放光明的时候……",
                "2_H20135": "真正迎来破晓的时候，您能发誓一定会回来吗？　说好了哦？"
            }
        },
        "2_H240": {
            "name": "对话 4",
            "text": {
                "2_H240": "Foreigner的阿比盖尔……",
                "2_H241": "毕竟我们是同一个人。",
                "2_H242": "只要把洋装和泳装交换，就会完全分辨不出来，令御主感到头疼吧？　呵呵呵。",
                "2_H243": "哎呀……御主能轻松分辨出来？",
                "2_H244": "唔……那现在的我究竟是哪个阿比？",
                "2_H245": "咯咯。如果回答不正确，我们或许会惩罚您哦？"
            }
        },
        "2_H250": {
            "name": "对话 5",
            "text": {
                "2_H250": "和那位北斋小姐一样，都穿泳装。",
                "2_H251": "有比这更值得高兴的事吗？",
                "2_H252": "啊，泳装打扮的时候，称呼她为阿荣小姐比较好吧？　御主？",
                "2_H253": "阿荣小姐真是的，当我身穿高位者（adeptus）打扮的时候，似乎总是一副坐立不安的样子。",
                "2_H254": "我都说了我不是什么仙女，但她总坚称天上的羽衣就是不可动摇的证据。",
                "2_H255": "咯咯。",
                "2_H256": "阿荣小姐从年轻时起，就是个非常可爱的人呢。"
            }
        },
        "2_H260": {
            "name": "对话 6",
            "text": {
                "2_H260": "隐约有股奇妙的气味？",
                "2_H261": "都是因为这个啦，“烤薄饼碗”！　别名“烤薄饼盖饭”。　是日本的传统食物！",
                "2_H262": "做法是从豹人小姐那里学会的！",
                "2_H263": "原本应该将“御好烧”盖在“米饭”上……",
                "2_H264": "但豹人小姐说『没事，烤薄饼也没什么差别啦』！",
                "2_H265": "只是，豹人小姐本人倒是——",
                "2_H266": "被突然出现的弓兵卫宫先生和阿尔托莉雅陛下带走了，之后就再也没见过她。",
                "2_H267": "那两位似乎都露出了一言难尽的表情呢……",
                "2_H268": "没错，就像御主您现在的表情……",
                "2_H269": "御……御主也来一份吧？"
            }
        }
    },
    "ChrVoice_ids": [
        "109820072",
        "2500700"
    ]
}
{
    "id": 2500200,
    "collectionNo": 198,
    "name": "葛饰北斋",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "不知深浅。"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "轰隆隆地雷鸣吧！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "Om Sudristih Sriye Svaha，",
                "0_B051": "Om Maha Sriye rbhave Svaha！",
                "0_B052": "看透万象之玄帝，",
                "0_B053": "由北辰拥八纮的尊星之王啊！",
                "0_B054": "奉上这浑身一笔！",
                "0_B055": "敬请垂览！",
                "0_B056": "富岳三十六景！",
                "0_B057": "神奈川冲浪里兴怀！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "彻底明白了！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "堂堂正正！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "该工作了啦，爹爹。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "交给我吧，我会画个透彻！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "非常好，非常好。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "你看，全部都搞定了。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "好痛。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "还挺能叫唤的。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "白画了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "何不化人魂　决起逍遥游　夏踏青原……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "来感觉了哦。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "没错。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "真出人意料。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "爹爹，该干活了。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "要充分蘸饱墨哦。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "来呀来呀，请观赏这绝景。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "翻眼皮吐舌头！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "你这怪家伙！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "爹爹，怎么又换雅号了啦！"
            }
        },
        "0_B880": {
            "name": "技能 4",
            "text": {
                "0_B880": "上上吉！"
            }
        },
        "0_B890": {
            "name": "技能 5",
            "text": {
                "0_B890": "潇潇洒洒。"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "重二，重二，章鱼，加二……（二、四、六、八、十）"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "哟，出手挺阔绰的呢，御主阁下。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "模仿花魁的散乱穿法适合我？",
                "0_G031": "干什么……啦，别这样，我……我会害羞的。",
                "0_G032": "……爹爹也闭嘴不准说话。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "哎，要庆祝生日吗。",
                "0_H011": "抱歉没法为你举办庆祝仪式……",
                "0_H012": "如何，我来为你画一张容貌变得老成聪慧的肖像吧，御主阁下。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "我听说现在去街头巷尾能看到新奇的东西哦？",
                "0_H021": "御主阁下，要不要去看看呢。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "要出门吗？",
                "0_H031": "好嘞，我去找另一只草鞋，等我一下。",
                "0_H032": "让我看看，到底扔哪里去了呢……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "对我和爹爹来说，只要能在任何地方画自己想画的绘画，且不会为温饱而苦就足够了啦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "好啦，差不多该换房间啦。",
                "0_H051": "御主阁下要不要也来我这里当食客？",
                "0_H052": "食客的食客，啊哈哈。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西，当然是美人啦。",
                "0_H061": "『迦勒底』这地方无论男女都不缺美人，让人作画很有兴致。",
                "0_H062": "还有就是甜酒和大福啦。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我受不了郁闷烦躁的氛围。",
                "0_H071": "这太不吉利了。对吧，爹爹。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我的愿望，当然是成为有真本事的人。",
                "0_H081": "只要再活5年，不，只要再活1年，我就能成为真正的画工。",
                "0_H082": "在那之前，我会画啊画啊不停地画，可不能轻易死掉啦。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我们一只加一个人才算“北斋”。",
                "0_H091": "如果只有我一个，也不过是普通的小丫头罢了。",
                "0_H092": "为了付食客的钱，我可以画破魔屏风图、漫画，",
                "0_H093": "甚至现代风的春宫图也行，什么图我都愿意接哦。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "咱爹爹因为磨墨磨过头了结果变成章鱼了？",
                "0_H101": "本人似乎表示手脚多反而更好……",
                "0_H102": "行吧，也无所谓啦。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "其实我也有『葛饰应为』这个画号啦。",
                "0_H111": "也会作为爹爹的助手，和一些好事之徒做生意啦。",
                "0_H112": "只不过我知道，就算我再怎么磨练技术，也无法超过爹爹啦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "偶尔哦，只是偶尔心血来潮，",
                "0_H121": "我会产生用和爹爹不一样的笔砚，画我自己想画的绘画的欲望。",
                "0_H122": "如果那样的话，你会买吗？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我并不是一个正常的“从者”吧。",
                "0_H131": "而你却……真是个好事之徒。我有点想认命了。",
                "0_H132": "总……总之，怎么说呢。",
                "0_H133": "就是别总是让我拿笔画画，我也对你画的画有点兴趣了。",
                "0_H134": "今……今后也请多关照哦。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "哦哦，原来所谓的『Foreigner』不止一个人啊。",
                "0_H141": "拥有一头黄金御发的南蛮女孩『阿比盖尔』。",
                "0_H142": "战战兢兢提心吊胆的，总是静不下来。",
                "0_H143": "唔？　让我别这么说，而是把她画成漫画吗？",
                "0_H144": "虽然很麻烦，但既然御主阁下这么说，那就没办法了……",
                "0_H145": "但是和南蛮人做生意可是连一文都不值哦？"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "这里有个长得像凸眼金鱼的男人吧？　好像叫什么吉尔·德·雷。",
                "0_H151": "那家伙惨叫着「噢噢，多么美妙，这不是什么神画师，而是邪神画师啊！」",
                "0_H152": "还拿来了供品，被我郑重拒绝了。",
                "0_H153": "但他还是哇哇哭个不停，为安抚他，我只能在他额头涂鸦，签了名。哦。",
                "0_H154": "画号是『紫色雁高』哦。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "嗝……哎哟哟，抱歉。",
                "0_H161": "稍微喝多了一点……",
                "0_H162": "在迦勒底啊，连大江山的鬼都好可爱呀。",
                "0_H163": "内在嘛，虽然确实是个怪物，哈哈……",
                "0_H164": "（抽泣）……啊啊，抱歉，变得那么感伤。",
                "0_H165": "还真不能在没醉酒的时候看见妹妹头的女童呢……"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "御……御主阁下？",
                "0_H171": "这里似乎有个打扮奇怪，或者说几乎一丝不挂的年轻武士，",
                "0_H172": "那个居然是九郎义经吗！？",
                "0_H173": "虽说坛之浦确实流行过一阵子，",
                "0_H174": "更重要的是我还曾经画过好几次九郎的伯父镇西八郎哦。",
                "0_H175": "作为马琴的读本『椿说弓张月』的插画。",
                "0_H176": "这样啊……要把八郎按照那个样子重新画啊……"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "名为吕布的唐人。",
                "0_H181": "就是那个高大到离谱的武将。",
                "0_H182": "我不过是问了那家伙关于我笔下关羽的事，结果你猜怎么样，",
                "0_H183": "完全听不懂他说什么！"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "真了不起啊，御主阁下。",
                "0_H191": "居然连怪童丸和赖光也在这里，太令人钦佩了。",
                "0_H192": "他们真适合消灭怪物。",
                "0_H193": "不过，我总觉得自己倒是可能成为那个怪物……",
                "0_H194": "反正也就这样吧。",
                "0_H195": "以血洗血，兵行诡道。",
                "0_H196": "……素材总是越多越好。"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "哎呀，那一定是天女。",
                "0_H201": "说搞错未免太失礼了。",
                "0_H202": "爹爹确实画过荼吉尼天以及殷的妲己，",
                "0_H203": "但那些都是参照唐国图版的样子吧。",
                "0_H204": "那不过是画工的幻想。",
                "0_H205": "迦勒底真是比读本还要离奇啊。"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "唉～～～～～～…………",
                "0_H211": "御主阁下真是太过分了。",
                "0_H212": "居然召唤出那种纯情小丫头。",
                "0_H213": "害我脸上都烧得红通通犹如燃烧的草原呀。",
                "0_H214": "唉，没办法，毕竟她姑且也备好了笔，一副打算相互理解的样子，成熟的我可得表示谦让才行。",
                "0_H216": "……你问我为什么会知道？",
                "0_H217": "那当然啦，因为她那边的爹爹和我爹爹内在完全一致，而且还连接着。",
                "0_H218": "到底什么情况啊？"
            }
        },
        "0_H220": {
            "name": "对话 12",
            "text": {
                "0_H220": "马琴老爷，以及时不时出现的阿路亲……吗。",
                "0_H221": "虽然不算保守，但希望衬托公公的做法确实很像那姑娘的性格呢。",
                "0_H222": "这下顽固老头就变成两个了。看来短期内只能由我夹在中间缓和气氛了呢。"
            }
        },
        "0_H240": {
            "name": "对话 13",
            "text": {
                "0_H240": "俗话说，美人三天就看腻吧？",
                "0_H241": "那当然了，光顾着装饰外表，味道平庸的美食，谁都会吃腻啦。",
                "0_H242": "在这个问题上，杨家的贵妃则每天都会表现出意料外色泽的图案。",
                "0_H243": "这种深不见底正可谓“降临者”呢。",
                "0_H244": "……我呀，打算在迦勒底的期间，抓住这绝世美女的“关键”，将其拆解到七零八落。",
                "0_H245": "记得要对她本人保密哦。",
                "0_H246": "……她酒品不好的评价，完全是无端揣测，纯粹是周围给她灌太多酒了哦？"
            }
        },
        "0_H250": {
            "name": "对话 14",
            "text": {
                "0_H250": "喂～，阿比～，阿比盖尔～。",
                "0_H251": "拜托偶尔给我做点烤薄饼和粥以外的夜宵啦。",
                "0_H252": "……不过这话还真说不出口呢。",
                "0_H253": "前不久她用不得了的日本料理招待我，害我差点挂了……"
            }
        },
        "0_N010": {
            "name": "南溟弓张八犬传 1",
            "text": {
                "0_N010": "啊～～，好好，是看店吧。",
                "0_N011": "我会干哦？　当然会干啦。",
                "0_N012": "对客人要赔笑，商品的品质也不能放松啦。",
                "0_N013": "……唔唉唉唉。"
            }
        },
        "0_N020": {
            "name": "南溟弓张八犬传 2",
            "text": {
                "0_N020": "辛苦啦，御主阁下！",
                "0_N021": "揉手揉脚，感激不尽。",
                "0_N022": "感激止于涕泪流鼻抽抽。"
            }
        },
        "0_N030": {
            "name": "南溟弓张八犬传 3",
            "text": {
                "0_N030": "辛辛苦苦，劳劳累累，艰艰难难。",
                "0_N031": "……话说这个……能不能改成“远程办公”啊？"
            }
        },
        "0_N040": {
            "name": "南溟弓张八犬传 4",
            "text": {
                "0_N040": "来了来了现在就来。",
                "0_N041": "感谢您远道光临。",
                "0_N042": "让我帮您拂去衣角的狗毛吧，需要我帮您揉揉肩膀吗？",
                "0_N043": "……唔？　违和感太强烈了？",
                "0_N044": "嘁……没有这种事吧？"
            }
        },
        "0_N050": {
            "name": "南溟弓张八犬传 5",
            "text": {
                "0_N050": "要问我不擅长什么，肯定就是看店吧。",
                "0_N051": "简直和我八字相克。",
                "0_N052": "但是，总不能让阿路亲一个人站着。",
                "0_N053": "就当做路边卖小人偶的时期，加把劲吧。"
            }
        },
        "0_N060": {
            "name": "南溟弓张八犬传 6",
            "text": {
                "0_N060": "……呼，差不多终于可以收工啦。",
                "0_N061": "御主阁下的成果还好吗？",
                "0_N062": "哎呀，完全不行吗？",
                "0_N063": "……爹爹好久没有这样为俗事而兴奋，以墨洗墨地全力奋斗了哦。",
                "0_N064": "当着马琴老爷的面，格外干劲十足，哈哈哈哈！",
                "0_N065": "唔呀！"
            }
        },
        "0_N070": {
            "name": "南溟弓张八犬传 7",
            "text": {
                "0_N070": "话说回来……",
                "0_N071": "要问选狗还是选猫的话，马琴老爷和阿路亲的泷泽家其实算猫派哦。",
                "0_N072": "虽然家里的鸟笼多到一串一串的。",
                "0_N073": "而在屋子里养狗的则是咱家。",
                "0_N074": "毕竟猫咪在手边蹭一下可就麻烦了呢。",
                "0_N075": "哈哈。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "葛饰北斋。只是个微不足道的画工。",
                "0_S011": "你就是『御主』阁下？",
                "0_S012": "哦～，是个挺有绘画感的人物呢。",
                "0_S013": "总之，我会随心所欲自由行动哦。"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "那当然啦！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "胡说什么呢！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "日本第一！"
            }
        },
        "2_B050": {
            "name": "宝具",
            "text": {
                "2_B050": "Ph’nglui ph’nglui，",
                "2_B051": "Om Sudristih Sriye Svaha，",
                "2_B052": "Wgah’nagl fhtagn！",
                "2_B053": "看透万象之玄帝，",
                "2_B054": "于星海之馆浅眠的天帝啊！",
                "2_B055": "奉上这浑身一笔！",
                "2_B056": "敬请垂览！",
                "2_B057": "富岳三十六景！",
                "2_B058": "神奈川异海里兴怀！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "原来如此啊。"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "要来一杯甘茶吗？"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "要开始工作了吧。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "Ph’nglui ph’nglui……"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "享受，享受。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "Fhtagn fhtagn……"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "只是有些痒痒呢……"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "我总是那么笨拙……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "何不化人魂　决起逍遥游　踏星之宫……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "请务必如此。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "自不用说。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "太无聊了。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "气势十足。"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "刻不容缓。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "Ia，Ia！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "未达神妙之境！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "敬谢不敏！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "雅号改变，叫异星蛸！"
            }
        },
        "2_B880": {
            "name": "技能 4",
            "text": {
                "2_B880": "太不像话了！"
            }
        },
        "2_B890": {
            "name": "技能 5",
            "text": {
                "2_B890": "哎呀，真奇怪……"
            }
        },
        "2_G010": {
            "name": "升级",
            "text": {
                "2_G010": "Ph’nglui ph’nglui……"
            }
        },
        "2_G040": {
            "name": "灵基再临 3",
            "text": {
                "2_G040": "还真是大方豪爽。",
                "2_G041": "仿佛能画尽森罗万象梦境幻想哦。",
                "2_G042": "这都是多亏了你啊，御主阁下。"
            }
        },
        "2_G050": {
            "name": "灵基再临 4",
            "text": {
                "2_G050": "Ph’nglui ph’nglui……",
                "2_G051": "呵呵，有种在梦中化为了仙女的感觉呢。",
                "2_G052": "我知道我不该觉得御主阁下可爱到令人想吃掉。",
                "2_G053": "但要不要趁势稍微吸一口呢？"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "哎，要庆祝生日吗。",
                "2_H011": "抱歉没法为你举办庆祝仪式……",
                "2_H012": "如何，我来为你画一张容貌变得老成聪慧的肖像吧，御主阁下。"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "哦，火灾？　追捕剧？",
                "2_H021": "什么都行，听上去挺有意思的。",
                "2_H022": "把我带去吧，御主阁下。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "我正打算去周游列国呢。",
                "2_H031": "正好顺便，也为御主的工作帮一把手吧，啊哈哈。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "我只要能在任何地方画自己想画的绘画，且不会为温饱而苦就足够了啦。",
                "2_H041": "但这样你就不好办了吧？"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "截稿日啊、细节要求啊、啰嗦的出版商什么的，",
                "2_H051": "如果真的没有了，我就没法讨生活了，这也挺让人头疼的呢。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "我什么都愿意画。",
                "2_H061": "完全没有好恶……哈哈，我骗你的。",
                "2_H062": "我最喜欢婀娜的女人与甜的东西了。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "我最讨厌那种郁闷烦躁的氛围了。求放过。",
                "2_H071": "实在是太不吉利了。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "我的愿望，当然是成为有真本事的人。",
                "2_H081": "只要再活5年，不，只要再活1年，我就能成为真正的画工。",
                "2_H082": "在那之前，我会画啊画啊不停地画，可不能轻易死掉啦。"
            }
        },
        "2_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H090": "我们一只加一个人才算“北斋”。",
                "2_H091": "如果只有我一个，也不过是普通的小丫头罢了。",
                "2_H092": "为了付食客的钱，我可以画破魔屏风图、漫画，",
                "2_H093": "甚至现代风的春宫图也行，什么图我都愿意接哦。"
            }
        },
        "2_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H100": "咱爹爹因为磨墨磨过头了结果变成章鱼了？",
                "2_H101": "本人似乎表示手脚多反而更好……",
                "2_H102": "行吧，也无所谓啦。"
            }
        },
        "2_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H110": "其实我也有『葛饰应为』这个画号啦。",
                "2_H111": "也会作为爹爹的助手，和一些好事之徒做生意啦。",
                "2_H112": "只不过我知道，就算我再怎么磨练技术，也无法超过爹爹啦。"
            }
        },
        "2_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H120": "偶尔哦，只是偶尔心血来潮，",
                "2_H121": "我会产生用和爹爹不一样的笔砚，画我自己想画的绘画的欲望。",
                "2_H122": "如果那样的话，你会买吗？"
            }
        },
        "2_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H130": "我并不是一个正常的“从者”吧。",
                "2_H131": "而你却……真是个好事之徒。我有点想认命了。",
                "2_H132": "总……总之，怎么说呢。",
                "2_H133": "就是别总是让我拿笔画画，我也对你画的画有点兴趣了。",
                "2_H134": "今……今后也请多关照哦。"
            }
        },
        "2_H140": {
            "name": "对话 4",
            "text": {
                "2_H140": "哦哦，原来所谓的『Foreigner』不止一个人啊。",
                "2_H141": "拥有一头黄金御发的南蛮女孩『阿比盖尔』。",
                "2_H142": "战战兢兢提心吊胆的，总是静不下来。",
                "2_H143": "唔？　让我别这么说，而是把她画成漫画吗？",
                "2_H144": "虽然很麻烦，但既然御主阁下这么说，那就没办法了……",
                "2_H145": "但是和南蛮人做生意可是连一文都不值哦？"
            }
        },
        "2_H150": {
            "name": "对话 5",
            "text": {
                "2_H150": "这里有个长得像凸眼金鱼的男人吧？　好像叫什么吉尔·德·雷。",
                "2_H151": "那家伙惨叫着「噢噢，多么美妙，这不是什么神画师，而是邪神画师啊！」",
                "2_H152": "还拿来了供品，被我郑重拒绝了。",
                "2_H153": "但他还是哇哇哭个不停，为安抚他，我只能在他额头涂鸦，签了名。",
                "2_H154": "画号是『紫色雁高』哦。"
            }
        },
        "2_H170": {
            "name": "对话 6",
            "text": {
                "2_H170": "御……御主阁下？",
                "2_H171": "这里似乎有个打扮奇怪，或者说几乎一丝不挂的年轻武士，",
                "2_H172": "那个居然是九郎义经吗！？",
                "2_H173": "虽说坛之浦确实流行过一阵子，",
                "2_H174": "更重要的是我还曾经画过好几次九郎的伯父镇西八郎哦。",
                "2_H175": "作为马琴的读本『椿说弓张月』的插画。",
                "2_H176": "这样啊……要把八郎按照那个样子重新画啊……"
            }
        },
        "2_H180": {
            "name": "对话 7",
            "text": {
                "2_H180": "名为吕布的唐人。",
                "2_H181": "就是那个高大到离谱的武将。",
                "2_H182": "我不过是问了那家伙关于我笔下关羽的事，结果你猜怎么样，",
                "2_H183": "完全听不懂他说什么！"
            }
        },
        "2_H190": {
            "name": "对话 8",
            "text": {
                "2_H190": "真了不起啊，御主阁下。",
                "2_H191": "居然连怪童丸和赖光也在这里，太令人钦佩了。",
                "2_H192": "他们真适合消灭怪物。",
                "2_H193": "不过，我总觉得自己倒是可能成为那个怪物……",
                "2_H194": "反正也就这样吧。",
                "2_H195": "以血洗血，兵行诡道。",
                "2_H196": "……素材总是越多越好。"
            }
        },
        "2_H200": {
            "name": "对话 9",
            "text": {
                "2_H200": "哎呀，那一定是天女。",
                "2_H201": "说搞错未免太失礼了。",
                "2_H202": "爹爹确实画过荼吉尼天以及殷的妲己，",
                "2_H203": "但那些都是参照唐国图版的样子吧。",
                "2_H204": "那不过是画工的幻想。",
                "2_H205": "迦勒底真是比读本还要离奇啊。"
            }
        },
        "2_H210": {
            "name": "对话 4",
            "text": {
                "2_H210": "啊哈哈。",
                "2_H211": "我居然又多了一个女儿吗，真有意思。",
                "2_H212": "蛮不讲理回娘家的女儿和调皮捣蛋的疯丫头。",
                "2_H213": "得麻烦御主阁下费心啦，还请多多指教哦！"
            }
        },
        "2_H220": {
            "name": "对话 5",
            "text": {
                "2_H220": "仓藏吗……",
                "2_H221": "终于给我露面了呢，曲亭马琴！",
                "2_H222": "向他那样提麻烦透顶要求的家伙前无古人后无来者啦。",
                "2_H223": "如果敢仗着自己的天赋胡言乱语，看我不把草鞋塞他嘴里去！",
                "2_H224": "不过……容貌是那个孝顺儿媳的话，还真让人难办呢……",
                "2_H225": "但在这个问题上反正彼此彼此啦。"
            }
        },
        "2_H280": {
            "name": "对话 6",
            "text": {
                "2_H280": "在这迦勒底，达·芬奇阁下分明那么忙碌，现在哪有什么时间画什么画啊。",
                "2_H281": "心里虽然有点过意不去，但身有梵高小姐在身旁着实令人安心。",
                "2_H282": "唔？　其他人也在奔放地干自己的事，所以不用在意？",
                "2_H283": "……哼，可不能这样啦。毕竟我这个可不是为了放松哦。"
            }
        },
        "2_H290": {
            "name": "对话 7",
            "text": {
                "2_H290": "那个怪……机关武者就是为朝！？",
                "2_H291": "呃，哦……好厉害啊。",
                "2_H292": "活得久，不对，当从者看来还是有好处的。",
                "2_H293": "周围的家伙瞬间就习惯了，只有我还最为惊讶真是太不好意思了。",
                "2_H294": "毕竟一看他的脸就心里一紧嘛。",
                "2_H295": "不管怎么说，还真可靠呢，镇西八郎。"
            }
        }
    },
    "ChrVoice_ids": [
        "2500200"
    ]
}
{
    "id": 503300,
    "collectionNo": 192,
    "name": "喀耳刻",
    "voices": {
        "0_B010": {
            "name": "攻击 1(真名确认后)",
            "text": {
                "0_B010": "看招！"
            }
        },
        "0_B020": {
            "name": "攻击 2(真名确认后)",
            "text": {
                "0_B020": "你看。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1(真名确认后)",
            "text": {
                "0_B040": "咬上去！"
            }
        },
        "0_B050": {
            "name": "宝具(真名确认后)",
            "text": {
                "0_B050": "让你们久等了，我心爱的仔猪们！",
                "0_B051": "为宴席做准备，举办狂宴款待客人吧！",
                "0_B052": "来吧，暴饮暴食！",
                "0_B053": "禁忌狂宴！",
                "0_B054": "呵呵，啊哈哈哈哈哈！"
            }
        },
        "0_B080": {
            "name": "技能 1(真名确认后)",
            "text": {
                "0_B080": "呵呵。我明白了。"
            }
        },
        "0_B090": {
            "name": "技能 2(真名确认后)",
            "text": {
                "0_B090": "想要喝麦粥吗？"
            }
        },
        "0_B100": {
            "name": "开始 1(真名确认后)",
            "text": {
                "0_B100": "呵呵。开始动手吧。"
            }
        },
        "0_B110": {
            "name": "开始 2(真名确认后)",
            "text": {
                "0_B110": "在这里建一座猪圈吧！"
            }
        },
        "0_B120": {
            "name": "胜利 1(真名确认后)",
            "text": {
                "0_B120": "很好很好，我会给你们奖赏的，仔猪。"
            }
        },
        "0_B130": {
            "name": "胜利 2(真名确认后)",
            "text": {
                "0_B130": "女神赫卡忒啊，向您表示感谢。"
            }
        },
        "0_B150": {
            "name": "受击 1(真名确认后)",
            "text": {
                "0_B150": "呜，还挺能干的嘛……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1(真名确认后)",
            "text": {
                "0_B160": "啊啊，输了啊～。"
            }
        },
        "0_B170": {
            "name": "无法战斗 2(真名确认后)",
            "text": {
                "0_B170": "很不甘心吧……"
            }
        },
        "0_B180": {
            "name": "指令卡 1(真名确认后)",
            "text": {
                "0_B180": "可以哦。"
            }
        },
        "0_B190": {
            "name": "指令卡 2(真名确认后)",
            "text": {
                "0_B190": "交给我吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3(真名确认后)",
            "text": {
                "0_B200": "唔哎，开玩笑吧？"
            }
        },
        "0_B210": {
            "name": "宝具卡 1(真名确认后)",
            "text": {
                "0_B210": "何不让我们享受一段甜蜜时光呢……"
            }
        },
        "0_B220": {
            "name": "宝具卡 2(真名确认后)",
            "text": {
                "0_B220": "献给女神赫卡忒！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3(真名确认后)",
            "text": {
                "0_B230": "我会回应你的爱，呵呵。"
            }
        },
        "0_B410": {
            "name": "攻击 4(真名确认后)",
            "text": {
                "0_B410": "你喜欢疼痛吗！"
            }
        },
        "0_B420": {
            "name": "攻击 5(真名确认后)",
            "text": {
                "0_B420": "魔力……开放！"
            }
        },
        "0_B430": {
            "name": "攻击 6(真名确认后)",
            "text": {
                "0_B430": "好嘞，冲啊！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2(真名确认后)",
            "text": {
                "0_B440": "由水底摇曳着出现吧！"
            }
        },
        "0_B480": {
            "name": "技能 3(真名确认后)",
            "text": {
                "0_B480": "我可是大魔女哦？"
            }
        },
        "0_B490": {
            "name": "技能 4(真名确认后)",
            "text": {
                "0_B490": "啊喂稍微等下糟了！"
            }
        },
        "0_G010": {
            "name": "升级(真名确认后)",
            "text": {
                "0_G010": "呵呵，魔力提升啦。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2(真名确认后)",
            "text": {
                "0_G020": "爱你爱到想把你变成猪哦？",
                "0_G021": "是真的。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1(真名确认后)",
            "text": {
                "0_G030": "以这个样子出现心情不禁有些复杂。",
                "0_G031": "这长发也很像少女，",
                "0_G032": "让……让人焦虑不安。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3(真名确认后)",
            "text": {
                "0_G040": "天有赫卡忒守护的辉煌，",
                "0_G041": "古代提坦的灵气在吾血脉中流淌。",
                "0_G042": "来吧，伟大睿智将成为喀耳刻之翼振翅高飞！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4(真名确认后)",
            "text": {
                "0_G050": "其实呀，我是魔女哦。",
                "0_G051": "你触碰了不该触碰的东西哦。",
                "0_G052": "呵呵，我再也不会放开你了。"
            }
        },
        "0_H010": {
            "name": "生日(真名确认后)",
            "text": {
                "0_H010": "祝你生日快乐，仔猪！",
                "0_H011": "我会准备一个盛大的宴会……",
                "0_H012": "哎，在那之前想先和我两个人单独小小庆祝一番？",
                "0_H013": "……算了，偶尔这样或许也不错。"
            }
        },
        "0_H020": {
            "name": "活动举行中(真名确认后)",
            "text": {
                "0_H020": "外面好吵闹啊。",
                "0_H021": "是祭典吗？　是宴会吗？"
            }
        },
        "0_H030": {
            "name": "对话 1(真名确认后)",
            "text": {
                "0_H030": "想走就走啊，撇下我一个人不管好啦！",
                "0_H031": "……不，抱歉，那个……带上我吧？",
                "0_H032": "好吗？"
            }
        },
        "0_H040": {
            "name": "对话 2(真名确认后)",
            "text": {
                "0_H040": "肚子饿不饿？"
            }
        },
        "0_H050": {
            "name": "对话 3(真名确认后)",
            "text": {
                "0_H050": "来，张嘴～。",
                "0_H051": "喝下这麦粥吧？",
                "0_H052": "……哈？　御主想喂给我喝？",
                "0_H053": "唔哎哎哎，开玩笑吧！？",
                "0_H054": "那……那就没办法了，就一次哦！",
                "0_H055": "啊，啊……"
            }
        },
        "0_H060": {
            "name": "喜欢的东西(真名确认后)",
            "text": {
                "0_H060": "这麦粥“休刻翁”啊，",
                "0_H061": "既是药，也是毒，是神的食物哦。",
                "0_H062": "呵呵。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西(真名确认后)",
            "text": {
                "0_H070": "奥德修斯？",
                "0_H071": "我可不认识。完全不记得呢。",
                "0_H072": "像那种愚蠢傲慢撒谎无药可救的男人什么的——"
            }
        },
        "0_H080": {
            "name": "关于圣杯(真名确认后)",
            "text": {
                "0_H080": "圣杯啊……",
                "0_H081": "只有无法实现的愿望，才是构成我魔女喀耳刻这个存在的要素……",
                "0_H082": "如果我这么说，你会笑我吗。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1(真名确认后)",
            "text": {
                "0_H090": "好。就在这里建一座猪圈吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2(真名确认后)",
            "text": {
                "0_H100": "呵呵，呵呵，呵呵，我全都理解了哦。",
                "0_H101": "你也想成为我的爱猪（仔猪）吧？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3(真名确认后)",
            "text": {
                "0_H110": "你喜欢比较丰满的……？",
                "0_H111": "尽管我对自己的技术很有自信……",
                "0_H112": "嗯，我会加油的……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4(真名确认后)",
            "text": {
                "0_H120": "我什么都愿意做，也愿意忍耐。",
                "0_H121": "只要你能和我在一起。",
                "0_H122": "所以……不要走啊……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5(真名确认后)",
            "text": {
                "0_H130": "没想到你真的把我带出来了……",
                "0_H131": "将被那座岛困住的我的灵魂。",
                "0_H132": "嗯，我愿意去任何地方。",
                "0_H133": "一起去旅行吧。",
                "0_H134": "躺在草原上做相同的梦吧。"
            }
        },
        "0_H140": {
            "name": "对话 4(真名确认后)",
            "text": {
                "0_H140": "美狄亚真可怜。",
                "0_H141": "那孩子的愚蠢在于荒废了女神赫卡忒的药术。",
                "0_H142": "可怜的孩子。",
                "0_H143": "真是……令人羡慕……"
            }
        },
        "0_H150": {
            "name": "对话 5(真名确认后)",
            "text": {
                "0_H150": "美狄亚·Lily啊……",
                "0_H151": "说实话那孩子让人很棘手。",
                "0_H152": "我不擅长对付那种。",
                "0_H153": "压力可是让我的羽毛都掉了哦。"
            }
        },
        "0_H160": {
            "name": "对话 6(真名确认后)",
            "text": {
                "0_H160": "呃……阿尔戈船的Archer，",
                "0_H161": "对对，阿塔兰忒。",
                "0_H162": "我和她莫名合得来。",
                "0_H163": "在岛上那时是个很好的聊天对象。",
                "0_H164": "虽然好像也会相互骂对方兴趣恶劣呢。"
            }
        },
        "0_H170": {
            "name": "对话 7(真名确认后)",
            "text": {
                "0_H170": "唔。",
                "0_H171": "是我妹妹帕西淮的孩子吧。",
                "0_H172": "就是长着牛角的那个。",
                "0_H173": "虽说我见过他……",
                "0_H174": "但到底是谁把这种毫无必要的事灌输给他的啊？",
                "0_H175": "能不能别毕恭毕敬地叫我「阿姨」啦！"
            }
        },
        "0_H180": {
            "name": "对话 8(真名确认后)",
            "text": {
                "0_H180": "我喜欢水手。",
                "0_H181": "就算像黑胡子那样的海盗也完全欢迎。",
                "0_H182": "只是稍微……",
                "0_H183": "因为他一直在闹腾说什么我是萝莉老太婆或是合法萝莉什么的，",
                "0_H184": "所以我把他变成猪，吊在厨房冷冻库里了，",
                "0_H185": "不能这么做吗……？"
            }
        },
        "0_H200": {
            "name": "对话 9(真名确认后)",
            "text": {
                "0_H200": "唔哎，那家伙啊，伊阿宋。",
                "0_H201": "尽管我早就预测到他迟早会被召唤……",
                "0_H202": "不过，既然令我介意的年轻的那个美狄亚很高兴……也罢。",
                "0_H203": "那我就期待他好好发挥船员英雄的本领吧……",
                "0_H204": "哎，Saber？"
            }
        },
        "0_H210": {
            "name": "对话 10(真名确认后)",
            "text": {
                "0_H210": "唔，呜呜……呼哈……",
                "0_H211": "嗯，我不会逃跑也不会躲避。",
                "0_H212": "我已经能微笑着面对他了。",
                "0_H213": "……只不过，我也不是铁石青铜做的。",
                "0_H214": "也有想要独处的时候。"
            }
        },
        "0_I010": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 1",
            "text": {
                "0_I010": "「Kyke Stock Aeaea」今天也在精神十足地营业中！　呵呵。"
            }
        },
        "0_I020": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 2",
            "text": {
                "0_I020": "仔仔仔仔仔，仔猪～♪",
                "0_I021": "爱爱爱爱爱，艾尤～♪",
                "0_I022": "喔哇啊！　既既……既然来了就叫我一声啊！？"
            }
        },
        "0_I030": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 3",
            "text": {
                "0_I030": "这可是大魔女的工房哦？",
                "0_I031": "严苛挑选出来的考究素材可是我最引以为傲的！"
            }
        },
        "0_I040": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 4",
            "text": {
                "0_I040": "谢谢你～！",
                "0_I041": "附赠你一个鹰之魔女特制的追加配菜吧。",
                "0_I042": "好毒和坏毒，你想要哪种？"
            }
        },
        "0_I050": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 5",
            "text": {
                "0_I050": "哎？　你说想要谁？",
                "0_I051": "……什么嘛，是要兑换素材啊。",
                "0_I052": "好啦好啦，知道啦知道啦。"
            }
        },
        "0_I060": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 6",
            "text": {
                "0_I060": "啊～好开心啊～。",
                "0_I061": "我的性格好像很适合这种开店的工作呢。",
                "0_I062": "你也不要忘记兑换哦。",
                "0_I063": "呵呵。又赢了呢。"
            }
        },
        "0_I070": {
            "name": "艾尤的春风\n～魔女和愉快的伙伴们与崭新的冒险～ 7",
            "text": {
                "0_I070": "呵呵。所以说这次也通过休刻翁变成三高仔猪吧！",
                "0_I071": "你差不多也该吃腻经典口味和应季风味口味了吧？",
                "0_I072": "所以趁此机会听取大魔女的建议一口气提升等级吧。不用客气啦。",
                "0_I073": "这是目标成为世界通用休刻翁大师的仔猪应尽的义务。",
                "0_I074": "我会手把手教会你包括与最强一盘相符的状况以及餐具摆放在内的秘传中的秘传！",
                "0_I075": "没错，就算断定其为休刻翁道的奥义传授也毫不为过。你看，我觉得美狄亚应该说没错没错地表示同意哦！",
                "0_I076": "好了，首先让我们慢慢地……",
                "0_I077": "哎，只剩5秒左右了！？",
                "0_I078": "喂，啊啊，基本全部跳过，秘方用高速神言把那个这样那样弄一下，然后把这附近生长的东西随便点这么搞！",
                "0_I079": "很好，搞定了！　给，战斗结……"
            }
        },
        "0_S010": {
            "name": "召唤(真名确认后)",
            "text": {
                "0_S010": "哟。我不会再让你感到寂寞了。",
                "0_S011": "因为你邀请了我魔女喀耳刻哦。",
                "0_S012": "呵呵。"
            }
        }
    },

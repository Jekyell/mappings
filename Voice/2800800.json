            }
        },
        "0_B2020": {
            "name": "アタック 11",
            "text": {
                "0_B2020": "Vive l'Empereur（万岁）！"
            }
        },
        "0_B2030": {
            "name": "アタック 12",
            "text": {
                "0_B2030": "Uthgos plam'f Hypnos asgu'i……"
            }
        },
        "0_B210": {
            "name": "宝具カード 1",
            "text": {
                "0_B210": "我的王子殿下……"
            }
        },
        "0_B220": {
            "name": "宝具カード 2",
            "text": {
                "0_B220": "哈利路亚！"
            }
        },
        "0_B230": {
            "name": "宝具カード 3",
            "text": {
                "0_B230": "白雪堆积，星辰闪耀……"
            }
        },
        "0_B2410": {
            "name": "アタック 13",
            "text": {
                "0_B2410": "甜美，甜美，甜到下巴都要掉下来！"
            }
        },
        "0_B2420": {
            "name": "アタック 14",
            "text": {
                "0_B2420": "稍微pique（刺步）一下。"
            }
        },
        "0_B2440": {
            "name": "エクストラアタック 4",
            "text": {
                "0_B2440": "啊哈，圣诞，圣诞圣诞快乐！"

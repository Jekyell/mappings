        "0_B030": {
            "name": "アタック 3",
            "text": {
                "0_B030": "Flic-flac（拍打连接步）♪　Flip-flop（啪嗒啪嗒）♪"
            }
        },
        "0_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "0_B040": "滚动，梦想啊，Assemble(双起双落跳)！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "群星清澈闪耀的今夜，",
                "0_B051": "糖果王国敞开大门。",
                "0_B052": "连接伸来的指尖，",
                "0_B053": "与悸动之心的桥梁。",
                "0_B054": "为翩然起舞的两人送上即将绽放的祝福之花。",
                "0_B055": "渴盼朦胧之梦！"
            }
        },
        "0_B080": {
            "name": "スキル 1",
            "text": {
                "0_B080": "梦幻的阿拉伯风☆"
            }
        },
        "0_B090": {
            "name": "スキル 2",
            "text": {
                "0_B090": "闪耀的星辰☆"
            }
        },
        "0_B100": {
            "name": "開始 1",
            "text": {
                "0_B100": "愿意和我一起舞蹈吗？"
            }
        },
        "0_B110": {
            "name": "開始 2",
            "text": {
                "0_B110": "美妙的宴会开始了吧？"
            }
        },
        "0_B120": {
            "name": "勝利 1",
            "text": {
                "0_B120": "好孩子就该给好礼物。坏孩子……要给坏礼物？"
            }
        },
        "0_B130": {
            "name": "勝利 2",
            "text": {
                "0_B130": "We wish you a Merry Christmas（我们祝你圣诞快乐）……"
            }
        },
        "0_B140": {
            "name": "ダメージ 2",
            "text": {
                "0_B140": "哎呀！"
            }
        },
        "0_B150": {
            "name": "ダメージ 1",
            "text": {
                "0_B150": "真讨厌……"
            }
        },
        "0_B160": {
            "name": "戦闘不能 1",
            "text": {
                "0_B160": "回去吧……回地狱深渊……"
            }
        },
        "0_B170": {
            "name": "戦闘不能 2",
            "text": {
                "0_B170": "呜呼……听到了吗？　谢幕声……"
            }

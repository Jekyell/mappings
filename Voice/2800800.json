        },
        "0_B520": {
            "name": "勝利 3",
            "text": {
                "0_B520": "来吧，御主，开始吃点心吧。"
            }
        },
        "0_B530": {
            "name": "勝利 4",
            "text": {
                "0_B530": "跳得还好吗？"
            }
        },
        "0_B540": {
            "name": "ダメージ 4",
            "text": {
                "0_B540": "呀！"
            }
        },
        "0_B550": {
            "name": "ダメージ 3",
            "text": {
                "0_B550": "该怎么办……"
            }
        },
        "0_B560": {
            "name": "戦闘不能 3",
            "text": {
                "0_B560": "再见了……王子殿下。"
            }
        },
        "0_B570": {
            "name": "戦闘不能 4",
            "text": {
                "0_B570": "看来……已经无法再舞蹈了……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "王子终将到来，",
                "0_B801": "纵是短暂的梦境，",
                "0_B802": "渴盼已久的夜会，",
                "0_B803": "滑过脸颊的微苦泪珠……",
                "0_B804": "这珍贵难忘的舞蹈，是你赠予的礼物。",
                "0_B805": "渴盼朦胧之梦……"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "神圣的钟声，热情的尾声回荡。",
                "0_B811": "在此宣告誓言，",
                "0_B812": "邀请前来白银之园。",
                "0_B813": "这是赠与你的，幸福的礼物！",
                "0_B814": "渴盼朦胧之梦！"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "欢迎光临！　邀请来到糖果之国！"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "就像圣尼古拉大人一样。"
            }
        },
        "0_B890": {
            "name": "スキル 6",
            "text": {
                "0_B890": "我们到来（我们三王）……"
            }
        },
        "0_B910": {
            "name": "アタック 7",
            "text": {
                "0_B910": "Un、deux、trois（一、二、三）。"
            }
        },
        "0_B920": {
            "name": "アタック 8",
            "text": {
                "0_B920": "Jingle all the way（一路响叮当）！♪"
            }
        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "Un pour tous，tous pour un（人人为我，我为人人）！"
            }
        },

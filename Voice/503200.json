{
    "id": 503200,
    "collectionNo": 175,
    "name": "尼禄·克劳狄乌斯",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "射击～！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "化作大朵鲜花凋零吧！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "站在那里不准动～！",
                "0_B031": "射击～！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "这轨迹犹如星辰！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "剧场由大海而来——",
                "0_B051": "奢华！　庄严！　且流丽！",
                "0_B052": "见证吧，此乃讴歌荣耀的辉煌——",
                "0_B053": "即为，余的黄金剧场！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "这是独占夏日的话题吧！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "现在正是表现的时候吧，包在余身上吧！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "开启灼热夏日，热情舞台吧！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "被余迷得神魂颠倒？ 唔嗯唔嗯，汝这诚实的家伙～！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "真是场不错的演出！ 敬请期待下次公演吧！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "唔，唔嗯……或许有些花哨过头了……"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "汝做什么啊！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "这才是夏日！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "蠢货，余刚想要动真格啊～！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "稍微有些累了，余先去休息了。"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "要闪耀了哦！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "化为余之剧场的锈迹吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "真是群不懂礼数的客人啊。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "奥林匹亚喝彩！ 为祭典大声欢呼吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "准备好了吧？ 已经无人可阻止余了哦～！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "咚咚咚咚，咚～！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "斩击皇帝，出击！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "啾哇～，啾哇～，烟花咚～！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "曼妙～，曼妙～，曼妙身材～♪"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "尽情享受吧，这才是至高的彩虹……",
                "0_B802": "呵呵呵！",
                "0_B801": "那是何人？　美女？　罗马？　当然是余哦？",
                "0_B803": "讴歌荣耀的黄金剧场！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "等级提升！",
                "0_G011": "尼禄的歌唱力提升！",
                "0_G012": "再说一遍，",
                "0_G013": "歌唱力大大提升～！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "这就是大事面前的小事。",
                "0_G021": "这次没有什么变化，忍耐一下吧。",
                "0_G022": "要期待下次哦！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "此乃这次的亮点！",
                "0_G031": "非魔术礼装，而是剧场礼装！",
                "0_G032": "看上去酷似炮门的东西是管乐器，汝就不用深究啦。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "哼……也难怪汝会看入迷。余也觉得很厉害。",
                "0_G041": "余的造型品味在状态绝佳时状态绝佳哦！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "还喜欢夏日的剧场吗，御主啊。",
                "0_G051": "Illustrius是表现华美的词语。",
                "0_G052": "只要这个夏日与大家的声援尚在，水中剧场的辉煌就会延续。",
                "0_G053": "不，最大的声援者就在面前呢！",
                "0_G054": "唔嗯，那夏日一定会继续下去！",
                "0_G055": "距余之落日的到来还早得很呢！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "余知道哦，是汝之生日吧？",
                "0_H011": "包在余身上吧，余会安排人送堆积如山的南国山珍海味过来！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "呵呵呵。活动好像开始了吧……",
                "0_H021": "在剧场的规模上余是不会输的。",
                "0_H022": "我们快去抢占吧，御主！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "啊～，啊～。嗯～！",
                "0_H031": "咳咳！　正如汝之所见，余随时都做好了准备哦？",
                "0_H032": "好好领教Caster的歌唱力吧！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "就算穿着泳装，余也是万能的皇帝。",
                "0_H041": "在夏日与海边，没有余做不到的事！",
                "0_H042": "赶海也大可包在余的身上！"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "由于此次乃休假，因此余比平时更为自由。在各种方面都是。",
                "0_H051": "就算是从者，也难以抵抗夏日的魔力。",
                "0_H052": "别以为一两划令咒可以阻止余哦，御主！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……自不用说。",
                "0_H061": "是在水边依然闪耀的那朵红色蔷薇……",
                "0_H062": "至于指的是谁，余就不用说了吧？　嗯？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……没血没泪，一点都不平易近人的女仆吧。",
                "0_H071": "尽管和她合得来，但一点都不可爱！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "不要提圣杯的事了。这会糟蹋难得的祭典兴奋感。",
                "0_H081": "余固然渴望繁荣，但不想享受欲望。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "终于盼到备受期待的泳装啦！",
                "0_H091": "这段过程好漫长啊……",
                "0_H092": "被C狐抢先的时候余差点就要Magna voluisse magnum啦！",
                "0_H093": "但快看啊，这美妙的健康美感！",
                "0_H094": "赢啦！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "话说关于余为什么会是Caster的问题。",
                "0_H101": "当余还是皇帝的时候，罗马有个名为西门的魔术师。",
                "0_H102": "他是个言辞恶劣、态度恶劣、性格也很恶劣的令人头疼的男人，但余很中意。",
                "0_H103": "当时，余时不时会向他学习魔术。",
                "0_H104": "……并不是什么强行变成Caster的哦？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "黄金剧场乃是根据余的印象设计、建造出来的。",
                "0_H111": "既然如此，将剧场改造成符合夏日氛围的风格当然也不在话下！",
                "0_H112": "听说御主的故乡有各式各样的泳池吧。",
                "0_H113": "吾等罗马在浴室方面可谓人类最强。余只想让汝等见识一下其证明。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "唔嗯唔嗯……噢噢，眼皮都撑不住了。",
                "0_H121": "假期真好……",
                "0_H122": "在炽热的阳光中，在皮肤上涂上防晒油，享用沉甸甸的果实，手指被椰子蟹夹住……",
                "0_H123": "不，最后那个是多余的……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "一起去游泳吧，御主！",
                "0_H131": "尽管伟大的罗马皇帝人数众多，但没有比余更适合夏日的皇帝了！",
                "0_H132": "这身泳装，这份热情全是为了余之契约者！",
                "0_H133": "整座闪耀的剧场都在祝福这个夏日！"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "唔，湿透的衬衫未免太不知廉耻了……！",
                "0_H141": "但这才不愧为余的对手。这种不顾一切只想获胜的态度，余很喜欢。",
                "0_H142": "只是不知道这剧场礼装会不会允许呢？"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "连伊丽莎白都穿泳装！",
                "0_H151": "唔嗯，唔嗯，确实是能表现她那鳗鱼身材的充满攻击力的捷径。",
                "0_H152": "余很喜欢！　但必须挑选合适的尺寸才行！",
                "0_H153": "……话虽如此，那个肩垫有些难以理解……",
                "0_H154": "简直就像里约热内卢狂欢节的装扮……为什么啊……"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "怎么连那个冷血女仆也在这里啊～～～！",
                "0_H161": "御主，应该尽快放逐那家伙哦？",
                "0_H162": "余说想要喝椰奶，让她为余拿来后，",
                "0_H163": "汝猜她对余做了些什么？",
                "0_H164": "那坚硬的椰子，居然在余的面前，凄惨地爆炸了……！"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "御主，刚才余在海边见到一个披着绿色斗篷戴着王冠，难以形容，",
                "0_H171": "但又面带太阳般灿烂笑容连射着水炮的从者，汝认识吗？",
                "0_H172": "由于只是远远眺望，没能打招呼，但那是个不错的从者。和余长得有点像。",
                "0_H173": "真希望能将那样的美少女加入余的麾下……唔？",
                "0_H174": "汝说余已经认识那个从者了？",
                "0_H175": "不可能啦～。"
            }
        },
        "0_M610": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 1",
            "text": {
                "0_M610": "碍事者就该被打飞！　唔嗯，竞争就该这样才行！ 同感。就算是赛车比赛，也不能手下留情。"
            }
        },
        "0_M620": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 2",
            "text": {
                "0_M620": "要继续比赛了。快坐进来！ 唔，这次余负责支援吗！？ 不过算了，就让余见识一下汝的实力吧！"
            }
        },
        "0_M630": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 3",
            "text": {
                "0_M630": "怎么会这样！　余好开心啊！ 干得挺漂亮的嘛，汝这冷血女仆！ 哼。你也是，剧场女。 我甚至觉得你作为同伴未免太浪费了。"
            }
        },
        "0_M640": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 4",
            "text": {
                "0_M640": "唔唔唔，为何余要做这种事～！ 手不要停。不好好劳动的人没得吃。"
            }
        },
        "0_M650": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 5",
            "text": {
                "0_M650": "仔细看清楚，勺子就该这么用……！ 这不可能……居然是……二刀流……！？"
            }
        },
        "0_M660": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 6",
            "text": {
                "0_M660": "累死了。余想要泡澡。 在逃离这里之前只能忍耐。 ……我也一直在忍耐。"
            }
        },
        "0_M670": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 7",
            "text": {
                "0_M670": "包在余的身上吧！　这是吾等的胜利！",
                "0_M671": "碍事者一律用荷枪实弹消灭。这是赛车的基本。"
            }
        },
        "0_M680": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 8",
            "text": {
                "0_M680": "我们的驾驶中不存在刹车。给我记住。",
                "0_M681": "不，余好歹也是会踩刹车的啊？",
                "0_M682": "汝这家伙，难道是野猪吗？",
                "0_M683": "别提魔猪的话题……！"
            }
        },
        "0_M690": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 9",
            "text": {
                "0_M690": "奔跑吧玫瑰啊～　犹如那玫瑰～",
                "0_M691": "向着那月之海～划桨划桨♪",
                "0_M692": "哼哼哼～　哼哼哼～……♪",
                "0_M693": "哼哼哼　哼哼～……♪",
                "0_M694": "唔嗯，吾等二人兴致高昂！"
            }
        },
        "0_M700": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 10",
            "text": {
                "0_M700": "穿过墙壁了啊。终于要与对手见面了。",
                "0_M701": "唔嗯，汝先出去吧。",
                "0_M702": "余的眼睛还有点干涩。"
            }
        },
        "0_M710": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 11",
            "text": {
                "0_M710": "任务完成。拿把新勺子过来。",
                "0_M711": "差不多也该走私点铲子过来了吧？"
            }
        },
        "0_M720": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 12",
            "text": {
                "0_M720": "噢噢，看到光了！　这次也成功了呢！",
                "0_M721": "……唔？　抱歉，我睡着了。",
                "0_M722": "女仆也是需要休息的。"
            }
        },
        "0_M730": {
            "name": "难解难分·夏日锦标赛！\r\n队伍对话 13",
            "text": {
                "0_M730": "最后一个检查点，干得漂亮！",
                "0_M731": "谢谢大家啦。谢谢～！",
                "0_M732": "冷血女仆，汝也来挥挥手吧！",
                "0_M733": "这可是好不容易的凯旋！",
                "0_M734": "这……这样吗？　你在笑什么。",
                "0_M735": "我好歹也能感受氛围，听到声援也是会高兴的。",
                "0_M736": "这样啊，那余也很高兴！",
                "0_M737": "那开始吧，预备～",
                "0_M738": "啊，嗯。预备～",
                "0_M739": "祝贺，完·全·胜·利！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "为这突然的泳装感到惊讶吗？　一定很惊讶吧？",
                "0_S011": "唔嗯，余也高兴地心跳不已！",
                "0_S012": "Caster，尼禄·克劳狄乌斯，应召唤而来！",
                "0_S013": "何不让我们铸就一个难忘的夏日呢！"
            }
        }
    },
    "ChrVoice_ids": [
        "503200",
        "9010000"
    ]
}
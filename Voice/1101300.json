        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "今天是你的生日吗？",
                "0_H011": "哎，是吗。",
                "0_H012": "想要礼物吗？",
                "0_H013": "还是希望我祝贺你呢？",
                "0_H014": "……呵呵，不行～，哪种我都不会给你。",
                "0_H015": "这种事还是去拜托其他灵基的我吧！",
                "0_H016": "和我玩耍，只会让你的生日变得血淋淋！",
                "0_H017": "啊哈哈哈哈！",
                "0_H018": "……如果即便这样你都不介意的话，我就陪你玩。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "活动？",
                "0_H021": "好啊，我喜欢热闹的事。",
                "0_H022": "让我们开心地玩个够吧！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "我没什么干劲呢。",
                "0_H031": "因为你总是装好孩子嘛。",
                "0_H032": "……这么一来帮你也没什么意义啦。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "御主就像是监狱看守呢。",
                "0_H041": "……没有这种事？",
                "0_H042": "呵呵。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主。",
                "0_H051": "……只是叫叫你而已哦。",
                "0_H052": "有什么问题吗？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "我喜欢泡澡哦。",
                "0_H062": "你应该知道吧？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌一切。",
                "0_H071": "憎恨一切。",
                "0_H072": "你明白吗？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "把圣杯拿远点",
                "0_H081": "因为我会向鲜血与复仇许愿。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我讨厌大家啦……",
                "0_H091": "每个人都令我憎恨到无以复加。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "大家杀了我、嘲弄我，然后还杀了那些孩子们。",
                "0_H101": "所以，我无法饶恕。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "要小心哦，御主。",
                "0_H111": "虽不知道另一个我有多乖巧，但这个我可是个坏孩子哦。",
                "0_H112": "一个不注意……",
                "0_H113": "就不知道结果会如何了哦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "你长着一双漂亮的眼睛。",
                "0_H121": "不知道能坚持到什么时候呢。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我无法饶恕。",
                "0_H131": "我会将自己得到的绝望，尽数奉还给世界。",
                "0_H132": "……但有你在的话，或许就无法实现了。"
            }
        },
        "0_H1600": {
            "name": "牵绊Lv.4 (奏章Ⅱ通关后)",
            "text": {
                "0_H1600": "你长着一双漂亮的眼睛。",
                "0_H1601": "但我知道。",
                "0_H1602": "当时，你的眼中也有火焰。",
                "0_H1603": "和我们一样。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "没有面包的话，就吃点心吧。",
                "0_H171": "吃得撑撑的，然后从内向外炸裂死掉吧。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "大家，全都死掉就好了……"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (奏章Ⅱ通关后)",
            "text": {
                "0_H1800": "我无法饶恕。",
                "0_H1801": "我会将自己得到的绝望，尽数奉还给世界。",
                "0_H1802": "……你能理解这种心情吧？",
                "0_H1803": "呵呵。",
                "0_H1804": "你不愿意首肯啊，御主。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "我也讨厌您哦，皇帝陛下！",
                "0_H191": "浸染了我们鲜血的绒毯踩起来感觉如何？",
                "0_H192": "啊哈哈哈！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "伯爵，您好。",
                "0_H201": "……啊啊，抱歉，您已经是沉默不语的影子了呢。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "我不想见……",
                "0_H211": "阿马德乌斯……"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "影子，残火。",
                "0_H221": "再见了，再见了。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "我，我不擅长应对……",
                "0_H231": "欧罗巴大人……"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "……身为好孩子的我，玛丽·安托瓦内特。",
                "0_H241": "事到如今，",
                "0_H242": "我也没什么话可以和她说了。",
                "0_H243": "要喝茶吗？"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "喝茶的时间？",
                "0_H251": "这可不关我的事。",
                "0_H252": "去问那一个玛丽好了。",
                "0_H253": "哼。"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "您好，基督山伯爵。",
                "0_H261": "虽说彼此所剩时间都很短暂……",
                "0_H262": "但请多关照哦。",
                "0_H263": "……什么？　红茶里有毒？",
                "0_H264": "啊，被发现了啊，啊哈哈！"
            }
        },
        "0_H3070": {
            "name": "讨厌的东西 (奏章Ⅱ通关后)",
            "text": {
                "0_H3070": "讨厌一切。",
                "0_H3071": "憎恨一切。",
                "0_H3072": "……你其实也明白吧？",
                "0_H3073": "血泪，与不灭火焰的滋味。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我是玛丽。",
                "0_S011": "玛丽·安托瓦内特哦。",
                "0_S012": "我最讨厌最讨厌大家了。",
                "0_S013": "也最讨厌你了！",
                "0_S014": "呵呵，呵呵呵呵呵！"
            }
        },
        "0_S110": {
            "name": "召唤 (奏章Ⅱ通关后)",
            "text": {
                "0_S110": "哎呀，是你啊。",
                "0_S111": "呼嗯。",
                "0_S112": "……好吧。",
                "0_S113": "由王权的死亡与绝望孕育而出的一朵黑色之花！",
                "0_S114": "就让我玛丽·安托瓦内特……",
                "0_S115": "确认你是否能驾驭我吧。"
            }
        },
        "1_B010": {
            "name": "攻击 1",
            "text": {

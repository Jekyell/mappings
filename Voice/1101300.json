        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "欣赏死之舞蹈吧。"
            }
        },
        "1_B510": {
            "name": "开始 4",
            "text": {
                "1_B510": "败者法国终将不幸（Vae Victis France）……"
            }
        },
        "1_B520": {
            "name": "胜利 3",
            "text": {
                "1_B520": "再见了。在地狱尽情受苦吧。"
            }
        },
        "1_B530": {
            "name": "胜利 4",
            "text": {
                "1_B530": "败者终将不幸（Vae Victis）。诅咒所有一切。"
            }
        },
        "1_B540": {
            "name": "受击 4",
            "text": {
                "1_B540": "区区疼痛！"
            }
        },
        "1_B560": {
            "name": "无法战斗 3",
            "text": {
                "1_B560": "御主，我先走一步了……"
            }
        },
        "1_B570": {
            "name": "无法战斗 4",
            "text": {
                "1_B570": "再，见，了……"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "不能去爱任何人，",
                "1_B801": "不能饶恕任何人！",
                "1_B802": "————悲剧流转·黑华葬列！",
                "1_B803": "啊啊，诅咒所有一切！"
            }
        },
        "1_B810": {
            "name": "宝具 3",
            "text": {
                "1_B810": "不可饶恕，不可饶恕，",
                "1_B811": "绝对，不可饶恕！",
                "1_B812": "——这世间的所有一切！",
                "1_B813": "都腐朽溃烂吧！"
            }
        },
        "1_B840": {
            "name": "额外攻击 3",
            "text": {
                "1_B840": "嘲讽的……断头台！"
            }
        },
        "1_B880": {
            "name": "技能 5",
            "text": {
                "1_B880": "败者终将不幸（Vae Victis）？"
            }
        },
        "1_B890": {
            "name": "技能 6",
            "text": {
                "1_B890": "要不要尝尝点心？"
            }
        },
        "1_B910": {
            "name": "攻击 7",
            "text": {
                "1_B910": "心情如何？"
            }
        },
        "1_B920": {
            "name": "攻击 8",
            "text": {
                "1_B920": "失礼了。"
            }
        },
        "1_B930": {
            "name": "攻击 9",
            "text": {
                "1_B930": "这个给你。"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "呵呵……"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "像这样注入魔力真的没问题吗。",
                "1_G021": "呵呵。"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "虽然怀着恶作剧的心情变成了当时的模样，",
                "1_G031": "啊啊……",
                "1_G032": "我还是更适应这个样子。"
            }
        },
        "1_G040": {
            "name": "灵基再临 3",
            "text": {
                "1_G040": "……那么，请允许我再次自我介绍。",
                "1_G041": "绝望的漆黑之花，愤怒与悲哀的凝聚。",
                "1_G042": "复仇者。",
                "1_G043": "……玛丽·安托瓦内特哦。"
            }
        },
        "1_G050": {
            "name": "灵基再临 4",
            "text": {
                "1_G050": "啊啊，真遗憾！",
                "1_G051": "给了我那么多资源的您……",
                "1_G052": "竟然不愿意陪我一起复仇。",
                "1_G053": "您真是个罪孽深重的人呢，御主。"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "可以哦。"
            }
        },
        "1_G120": {
            "name": "升级 3",
            "text": {
                "1_G120": "尽管献上吧。"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "哎呀，哎呀，今天是你的生日吧。",
                "1_H011": "竟然诞生在如此扭曲、残酷、可怕的世界上……",
                "1_H012": "真可怜……",
                "1_H013": "因为很可怜，所以由我来安慰你吧。",
                "1_H014": "好乖好乖。",
                "1_H015": "打起精神来。",
                "1_H016": "即便路途的前方没有光……",
                "1_H017": "也还留有黑暗。"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "我也讨厌节日的疯狂。",
                "1_H021": "但如果你要去的话，我愿意奉陪哦。",
                "1_H022": "是打算杀了谁吧？"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "要去哪儿？",
                "1_H031": "可以哦。",
                "1_H032": "然后呢，想要杀什么——"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "我很有自知之明哦。",
                "1_H041": "我是囚徒，而御主是……",
                "1_H042": "……唔，怎么了？",
                "1_H043": "你没有这个意思？",
                "1_H044": "呵呵。"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "……我只是在看你的脸。",
                "1_H051": "有意见吗？"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "嗯，我喜欢泡澡。",
                "1_H061": "要一起泡吗？",
                "1_H062": "我不介意哦。"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "讨厌、厌恶一切。",
                "1_H071": "包括你。",
                "1_H072": "包括我。"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "圣杯，十分美妙吧。",
                "1_H081": "与可怕的世界相符的，可怕的杯子。",
                "1_H082": "向其许下的愿望，总是会，极尽扭曲……"
            }
        },
        "1_H10090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H10090": "我不会饶恕任何人。",
                "1_H10091": "每个人都让我憎恨到无以复加。",
                "1_H10092": "和那个我截然相反呢。"
            }
        },
        "1_H10100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H10100": "你要夺回人理吧？",
                "1_H10101": "另一个我肯定会非常高兴吧。",
                "1_H10102": "但是，我不高兴。",
                "1_H10103": "我希望一切都能毁灭。",
                "1_H10104": "是真的哦。"
            }
        },
        "1_H10110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H10110": "你也很辛苦呢，御主。",
                "1_H10111": "为了人理……不惜拿出我这样扭曲的灵基。",
                "1_H10112": "一定要小心哦。",
                "1_H10113": "我是诅咒世界的复仇者。",
                "1_H10114": "额外职阶，复仇者啊。"
            }
        },
        "1_H10120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H10120": "当你的眼瞳终将浑浊，浮现出火焰的时候……",
                "1_H10121": "我会将你，连同世界一起杀掉。"
            }
        },
        "1_H10130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H10130": "你是不断前进绝不放弃的人吧。",
                "1_H10131": "但是，呵呵。",
                "1_H10132": "你能坚持不懈到什么时候呢……？",
                "1_H10133": "如果改变主意的话，一定要告诉我哦。",
                "1_H10134": "我会温柔的斩下你的首级。"
            }
        },
        "1_H11600": {
            "name": "牵绊Lv.4 (奏章Ⅱ通关后)",
            "text": {
                "1_H11600": "当你的眼瞳终将浑浊，浮现出火焰的时候……",
                "1_H11601": "我会将你，连同世界一起杀掉。",
                "1_H11602": "啊啊，真令人期待！",
                "1_H11603": "因为，你已经一度差点成为火焰了吧！"
            }
        },
        "1_H11800": {
            "name": "牵绊Lv.5 (奏章Ⅱ通关后)",
            "text": {
                "1_H11800": "我不会杀你。",
                "1_H11801": "不播撒诅咒，不被憎恨所囚禁，也不会用火焰灼烧世界与自己。",
                "1_H11802": "所以，你不会变得和我们一样。",
                "1_H11803": "……我终于明白了。",
                "1_H11804": "啊啊，真遗憾。",
                "1_H11805": "真是，真是太遗憾了……"
            }
        },
        "1_H270": {
            "name": "对话 4",
            "text": {
                "1_H270": "没有面包？",
                "1_H271": "那就去死吧。",
                "1_H272": "我才不会给你点心呢。",
                "1_H273": "饥饿、干渴、腐朽……在地狱嘶喊吧。"
            }
        },
        "1_H280": {
            "name": "对话 5",
            "text": {
                "1_H280": "什么事？",
                "1_H281": "想要杀谁吗？"
            }
        },
        "1_H290": {
            "name": "对话 6",
            "text": {
                "1_H290": "您好，皇帝陛下。",
                "1_H291": "……我是不会特殊对待您的，请放心。",
                "1_H292": "我会公平地、公正地，憎恨所有一切。"
            }
        },
        "1_H300": {
            "name": "对话 7",
            "text": {
                "1_H300": "伯爵，伯爵？",
                "1_H301": "……真的只剩下影子了呢。",
                "1_H302": "再见了。",
                "1_H303": "有朝一日，让我们一起喝茶吧。"
            }
        },
        "1_H3070": {
            "name": "讨厌的东西 (奏章Ⅱ通关后)",
            "text": {
                "1_H3070": "讨厌、厌恶一切。",
                "1_H3071": "包括你。",
                "1_H3072": "包括我。",
                "1_H3073": "……怎么样？　令人怀念？",
                "1_H3074": "这就是曾一度差点灼烧你的，复仇者之火。"
            }
        },
        "1_H310": {
            "name": "对话 8",
            "text": {
                "1_H310": "阿马德乌斯……",
                "1_H311": "什么都别说。",
                "1_H312": "什么都，别说。",
                "1_H313": "……求你了。"
            }
        },
        "1_H330": {
            "name": "对话 9",
            "text": {
                "1_H330": "欧，欧罗巴大人。",
                "1_H331": "您，您好。",
                "1_H332": "呜呜……",
                "1_H333": "果然不擅长应对呢……"
            }
        },
        "1_H340": {
            "name": "对话 10",
            "text": {
                "1_H340": "……受人爱戴的王后，白百合的玛丽·安托瓦内特。",
                "1_H341": "是吗。",
                "1_H342": "我，会像那样微笑啊。"
            }
        },
        "1_H350": {
            "name": "对话 11",
            "text": {
                "1_H350": "不，不用了。",
                "1_H351": "我是不会和你们喝茶的。",
                "1_H352": "对不起啦。",
                "1_H353": "……对不起。"
            }
        },
        "1_H360": {
            "name": "对话 12",
            "text": {
                "1_H360": "到这里来吧，可爱的孩子们。",
                "1_H361": "我是不会弄痛你们的……",
                "1_H362": "是真的哦……"
            }
        },
        "1_H370": {
            "name": "对话 13",
            "text": {
                "1_H370": "基督山伯爵！",
                "1_H371": "你快来这边……",
                "1_H372": "不好？",
                "1_H373": "是吗。呵呵，说得也是呢。",
                "1_H374": "或许很快就会离开，但就让我们好好相处吧。"
            }
        }
    },
    "ChrVoice_ids": [
        "1101300"
    ]
}
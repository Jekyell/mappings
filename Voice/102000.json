{
    "id": 102000,
    "collectionNo": 121,
    "name": "兰斯洛特",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "看，这个如何！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "抵达尽头，超越界限。",
                "0_B051": "彼方之王啊，",
                "0_B052": "见证这光吧！",
                "0_B053": "缚锁全断·过重湖光！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "仙女的佑护。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "就是这里！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "很遗憾，我并没有灵巧到可以放水。觉悟吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "倘若你要挡路，那我不会手下留情。上！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "此身乃奉献给骑士王之物，决不允许失败。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "可别怪我。要恨就恨这强悍吧。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "……王啊……若能再见您一面……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "还没完……我还不能在这里倒下……呜……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "交给我吧。"
            }
        },
        "0_B2010": {
            "name": "攻击 10",
            "text": {
                "0_B2010": "区区这种水平！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "向吾王宣誓！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "阿隆戴特，加速……！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "上，吾之圣剑！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "与湖中仙女一起……"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "没用的！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "斩断！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "容我排除你们！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "在圆桌的名下！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "圣剑啊，与我同在！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "绝不掉以轻心。全力以赴！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "兰斯洛特在此……上！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "是个强者。但我是不能输的。"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "不好……！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "闪耀如同水面，",
                "0_B801": "熊熊燃烧，吾之圣剑！",
                "0_B802": "缚锁全断·过重湖光！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "赌上圆桌的名义！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "快退下吧！"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "给我让开！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "一刀！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {
                "0_B930": "重新来过吧！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "好像又变强了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "这真是一件好事。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "……原来如此，变得更强了。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "感觉如何，御主？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "没想到能够再次见到如此光景……",
                "0_G051": "御主，我兰斯洛特十分感谢您！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝您生日快乐，御主。",
                "0_H011": "但愿今天是个好日子。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "看来发生了什么。 还是去看看为好吧。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "现在应该出门了吧？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我是剑。 只要您的指示还正确，我的剑刃就不会指向御主。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "请向着正确的道路前进。",
                "0_H051": "只要您这样做，我就会忠实于您。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "足以引火焚身的恋爱。",
                "0_H061": "除此以外，我拥有的也就只有剑技了吧。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "不，我没有谈论自己讨厌之物的资格。 严格说起来的话，应该是我自己。",
                "0_H071": "希望王能够对如此不贞的我降下制裁……"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我只希望王能够制裁我这个罪人。",
                "0_H081": "我希望圣杯实现的愿望只有这个。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "御主，怎么了？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "嬉闹也要适可而止。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "真令人头疼。 作为我的御主，我希望您能堂堂正正。",
                "0_H111": "……不，我失言了。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "没想到我会为和您一起战斗感到如此喜悦。",
                "0_H121": "御主，我一定会守护您的。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "您是我值得献上自己剑的人。",
                "0_H131": "这一生，能让我有产生这种想法的，只有骑士王和您。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "……不，别。 那个，我没脸去见那位……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "多么邪恶……！",
                "0_H151": "但将王逼到如此境地的正是我们啊……！"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "啊啊，高文卿。",
                "0_H161": "对了，我们下一盘棋一较高下如何？"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "是谁？　哎，莫德雷德？",
                "0_H171": "……哎？　不，等下等下！",
                "0_H172": "你是莫德雷德？",
                "0_H173": "你吗！？"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "噢噢，吾友崔斯坦卿。",
                "0_H181": "你还是老样子，一边哀叹一边将人射穿或是斩裂呢。",
                "0_H182": "我由衷庆幸你不是我的敌人。",
                "0_H183": "话说回来，我们还是另找个地方聊恋爱问题吧！"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (第2部3章intro通关后)",
            "text": {
                "0_H1800": "吾之剑，吾之命，吾之魂。",
                "0_H1801": "即便献上一切，也要为拯救人理而迈进。",
                "0_H1802": "御主，只要与您在一起，我的剑就绝不会犯错。"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "加雷斯吗。",
                "0_H191": "我很期待你能建立无愧于圆桌骑士之名的功勋。",
                "0_H192": "……除此以外，我应该没有任何资格对你说什么了吧。",
                "0_H193": "你的勇气、你的慈爱，正是我们有时会失去的东西。",
                "0_H194": "别变得像我这样哦，加雷斯。",
                "0_H195": "对于曾经身为我侍从的你，这是我能给出的对你有益的最后忠告了。",
                "0_H196": "该向你学习的，应该是我才对。"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "贝德维尔吗！",
                "0_H201": "你帮了我很多。",
                "0_H202": "让我们再次作为圆桌的骑士并肩战斗吧。"
            }
        },
        "0_H220": {
            "name": "对话 11",
            "text": {
                "0_H220": "吾王，骑士王啊！",
                "0_H221": "我对能再次在这里见到您感到无比荣幸。",
                "0_H222": "……是的，现在不是说什么没脸见您这种话的时候了。",
                "0_H223": "我定将挺起胸膛，高举这把剑奔赴战场。"
            }
        },
        "0_H230": {
            "name": "对话 12",
            "text": {
                "0_H230": "好久不见，珀西瓦尔！",
                "0_H231": "你那圣枪光辉依然未变，你那凛然的眼瞳也一如往昔。",
                "0_H232": "那就像过去那样，与你共同杀出一条血路吧。",
                "0_H233": "没什么，对我们来说，这已经稀疏平常了。",
                "0_H234": "对吧？"
            }
        },
        "0_H240": {
            "name": "对话 13",
            "text": {
                "0_H240": "您就是堂·吉诃德阁下啊。当然，我也知道您的大名。",
                "0_H241": "无论如何，您都是位勇敢的骑士。",
                "0_H242": "我是这么坚信的。哎呀，签名？",
                "0_H243": "唔，那我可以求一个您的签名作为交换吗？",
                "0_H244": "哈哈哈，感觉可以去显摆一下了。"
            }
        },
        "0_H250": {
            "name": "对话 14",
            "text": {
                "0_H250": "莫德雷德卿吗。",
                "0_H251": "听说你的精彩表现在奥林波斯帮上了大忙。",
                "0_H252": "那既是我也不是我？",
                "0_H253": "哈哈哈，这话虽然没错，但是……我还是觉得这值得骄傲哦。"
            }
        },
        "0_H260": {
            "name": "对话 15",
            "text": {
                "0_H260": "那就是妖精国的兰斯洛特……妖精骑士梅柳齐娜啊。",
                "0_H261": "既然不是获得了龙种的力量，而是本身就是龙种的话，她那压倒性的力量也就能够理解了。",
                "0_H262": "只不过，既然她借用了我的名字，我当然也要具备配得上让她借用的力量才行。",
                "0_H263": "好，高文卿！　先开始训练吧！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，Saber。兰斯洛特，前来报到。",
                "0_S011": "虽然只有短暂的时间，但吾之剑将献给御主您。"
            }
        },
        "11_B010": {
            "name": "攻击 1 夏日最强骑士",
            "text": {
                "11_B010": "夏日！"
            }
        },
        "11_B020": {
            "name": "攻击 2 夏日最强骑士",
            "text": {
                "11_B020": "大海！"
            }
        },
        "11_B030": {
            "name": "攻击 3 夏日最强骑士",
            "text": {
                "11_B030": "看招！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 夏日最强骑士",
            "text": {
                "11_B040": "假期斩击，上！"
            }
        },
        "11_B080": {
            "name": "技能 1 夏日最强骑士",
            "text": {
                "11_B080": "麻烦来点加护！"
            }
        },
        "11_B090": {
            "name": "技能 2 夏日最强骑士",
            "text": {
                "11_B090": "嗨起来——！"
            }
        },
        "11_B100": {
            "name": "开始 1 夏日最强骑士",
            "text": {
                "11_B100": "没事，虽说没穿铠甲，但行动也更轻便了！"
            }
        },
        "11_B110": {
            "name": "开始 2 夏日最强骑士",
            "text": {
                "11_B110": "呀嗬！　各位，觉悟吧！　现在的我情绪高涨！"
            }
        },
        "11_B120": {
            "name": "胜利 1 夏日最强骑士",
            "text": {
                "11_B120": "这身肉体是献给骑士王的！ ……这种说法好像会产生歧义吧……"
            }
        },
        "11_B130": {
            "name": "胜利 2 夏日最强骑士",
            "text": {
                "11_B130": "大意了吧。 就算身穿泳装，我也很强。"
            }
        },
        "11_B140": {
            "name": "受击 2 夏日最强骑士",
            "text": {
                "11_B140": "哎哟。"
            }
        },
        "11_B150": {
            "name": "受击 1 夏日最强骑士",
            "text": {
                "11_B150": "泳装未免有些难防……！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 夏日最强骑士",
            "text": {
                "11_B160": "以，以这身打扮无法战斗……太羞耻了……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 夏日最强骑士",
            "text": {
                "11_B170": "欢闹过头了吗……呜。"
            }
        },
        "11_B180": {
            "name": "指令卡 1 夏日最强骑士",
            "text": {
                "11_B180": "动手吧！"
            }
        },
        "11_B190": {
            "name": "指令卡 2 夏日最强骑士",
            "text": {
                "11_B190": "交给我吧。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 夏日最强骑士",
            "text": {
                "11_B200": "干杯！"
            }
        },
        "11_B2010": {
            "name": "攻击 10 夏日最强骑士",
            "text": {
                "11_B2010": "阳光也不是敌手！"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 夏日最强骑士",
            "text": {
                "11_B210": "虽然这身打扮，但上宝具！"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 夏日最强骑士",
            "text": {
                "11_B220": "怀着一夏的悲伤回忆。"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 夏日最强骑士",
            "text": {
                "11_B230": "结束一切吧……"
            }
        },
        "11_B250": {
            "name": "宝具卡 4 夏日最强骑士",
            "text": {
                "11_B250": "向这身泳装与圣剑起誓！"
            }
        },
        "11_B410": {
            "name": "攻击 4 夏日最强骑士",
            "text": {
                "11_B410": "干劲十足！"
            }
        },
        "11_B420": {
            "name": "攻击 5 夏日最强骑士",
            "text": {
                "11_B420": "晒伤！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 夏日最强骑士",
            "text": {
                "11_B440": "夏日阿隆戴特！"
            }
        },
        "11_B480": {
            "name": "技能 3 夏日最强骑士",
            "text": {
                "11_B480": "吾王，敬请见证！"
            }
        },
        "11_B490": {
            "name": "技能 4 夏日最强骑士",
            "text": {
                "11_B490": "哪怕身着泳装！"
            }
        },
        "11_B500": {
            "name": "开始 3 夏日最强骑士",
            "text": {
                "11_B500": "我连裸体战斗的经验都有，与之相较，能有遮挡已经不错了！"
            }
        },
        "11_B510": {
            "name": "开始 4 夏日最强骑士",
            "text": {
                "11_B510": "兰斯洛特，身穿泳装在此。……出击！"
            }
        },
        "11_B520": {
            "name": "胜利 3 夏日最强骑士",
            "text": {
                "11_B520": "没错，就算这身打扮，也不能输啊……！"
            }
        },
        "11_B550": {
            "name": "受击 3 夏日最强骑士",
            "text": {
                "11_B550": "好痛好痛好痛！"
            }
        },
        "11_B840": {
            "name": "额外攻击 3 夏日最强骑士",
            "text": {

    "name": "坂田金时",
    "voices": {
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "真扫兴，给我消失吧……！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "消失吧，",
                "0_B051": "必杀——",
                "0_B052": "黄金冲击！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "你还真没得救呢。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "连骨到髓都去死吧……！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "哦，不妨去一趟地狱看看吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "我可没什么慈悲能给留给恶人哦。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "拿下一城了哟，御主！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "真无聊。去从头开始锻炼吧。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "Go……lden！！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "……是早饭……没吃饱吗……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "……区区敌人，还挺Golden的嘛……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "哦。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "包在我身上吧。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "华丽地上吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "Golden！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "Big Golden！"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "Thunderbolt！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "OK！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "Golden 台风！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "这招……如何！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "抽支烟抽支烟……呼～！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "做给你看好了！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "我要蛮干了。抱歉啦。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "稍微认真一点好了。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "所谓的Golden，就该这样做。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "OK，complete了！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "哎哟哟，抱歉了，御主……！"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "啊？　糟了，搞砸了吗……！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "要上了，",
                "0_B801": "必杀——",
                "0_B802": "——黄金大两断！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "这是！　Golden！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "真不错。我不讨厌锻炼。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "真来电。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "挺来劲的嘛。我很中意你哦，头儿！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "状况不错。 难得的版本升级，玩得更豪华一些吧。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "真是个美妙的夜晚。没错，狗屎到不行的美妙夜晚。",
                "0_G051": "这样的夜晚──让我想起了大江山的皎洁白月。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "啊啊？ 你的生日？那真是值得庆祝。",
                "0_H011": "出去吃顿肉吧。我请客，我请客啦。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "哟哟，一群气血过剩的傻子们都集中在一起了呢。 同为傻子，我们也走吧，御主？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "啥，没事干啊。 那就去健身房啦，健身房！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "哟，是御主啊。 御主这称呼真有种Golden的味道呢。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "头儿负责作战指挥，我负责实干。 简单明了，不是很好嘛。我很期待你哦，头儿。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？  当然孩子啦。孩子就该是被保护的存在啦！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……就是不太擅长应付的东西吧？ 我是个傻瓜，不擅长思考麻烦事啦。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "我压根没见过圣杯，不过那啥，是黄金吧？",
                "0_H081": "那就不可能是什么坏东西。 无论里面是啥，外表不是很棒嘛！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "别叫我金时，叫我Golden。",
                "0_H091": "并不是说我对自己的名字有意见，这是一种感觉的问题。你懂吗？",
                "0_H092": "不是意思相同或是礼节的问题，而是灵魂啦！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "如果我以Rider现界，就能更让你看到极致完美Cool Golden的熊号了……",
                "0_H101": "那家伙可是非常不得了的恶魔怪物机器哦。因为身为一只熊，它居然能变形哦？",
                "0_H102": "哪来的会变形的金刚啊！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "知道那种熊玩具吗？很小的。在你们的时代商店有卖的那种。",
                "0_H111": "那玩意儿Golden可爱的说……！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "不过这次的干架真是夸张到赞啊。",
                "0_H121": "老子也算经历过不少大风大雨，还没见过这种规模的斗殴呢。",
                "0_H122": "没事，放轻松上吧，头儿。反正死活都是第一次，也没啥差别吧？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "你就放一百个心吧，头儿。你想办的事就由我来实现。",
                "0_H131": "只要是为了你，哪怕面对雷神，我坂田金时也会把他胖揍一顿。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "惹女孩子哭泣可一点都不Golden啊……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "啊？　怎么了？"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "啥事？ ……哎哟喂，脸也靠得太近了啦……"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "啊……肚子Golden饿扁了……"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "我的战斗方式稍微有些变化？",
                "0_H181": "嗯，没错。正可谓Golden upgrading哦！",
                "0_H182": "华丽地上吧！"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "一块儿去较量较量吧。如何！"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "啊？　干嘛啦。",
                "0_H201": "问我是不是变得更强壮了……我本来就很强壮。",
                "0_H202": "要试试看吗？",
                "0_H203": "我会把你一拳打到老天爷那边去的哦！",
                "0_H204": "……别，别。不是马上就想动手啦！",
                "0_H205": "在到模拟战斗装置前忍耐一下！"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "赖光老大！　怎么样，状况还好吧。",
                "0_H211": "应该已经熟悉迦勒底……哈？　问我御主怎么了？",
                "0_H212": "啊，啊～，嗯，没错。",
                "0_H213": "礼貌是Golden重要的事呢，哈，哈哈……"
            }
        },
        "0_H220": {
            "name": "对话 12",
            "text": {
                "0_H220": "赖光老大，不，我不是那个，我是那个头发没梳起来的我！",
                "0_H221": "我不是那个啦！"
            }
        },
        "0_H230": {
            "name": "对话 13",
            "text": {
                "0_H230": "喂，茨木。",
                "0_H231": "我放在食堂保管的哪个……呃，哦，没了。",
                "0_H232": "哈哈！　保管在这里的巧克力也消失了！"
            }
        },
        "0_H240": {
            "name": "对话 14",
            "text": {
                "0_H240": "喂，EVIL Wind！",
                "0_H241": "你的宝具还是超级Cool啊。",
                "0_H242": "不错不错。",
                "0_H243": "好嘞，让我靠近点好好看看！"
            }
        },
        "0_H260": {
            "name": "对话 15",
            "text": {
                "0_H260": "哦，狐狸。你有没有看到我的新皮带？",
                "0_H261": "不是不同职阶的问题，是新型的豪华版的那个……",
                "0_H262": "不，不是啦。不是献给狐狸的贡品哦？",
                "0_H263": "……很像太阳？",
                "0_H264": "不是啦，那只是Golden的装饰……啊！"
            }
        },
        "0_H270": {
            "name": "对话 16",
            "text": {
                "0_H270": "是这样啊，你……是那家伙的妈妈啊……",
                "0_H271": "那这应该算久违的重逢吧！",
                "0_H272": "还真是So golden！",
                "0_H273": "让我们举办庆祝宴会吧！"
            }
        },
        "0_H280": {
            "name": "对话 17",
            "text": {
                "0_H280": "你……",
                "0_H281": "不，没什么。"
            }
        },
        "0_H300": {
            "name": "对话 18",
            "text": {
                "0_H300": "又来了个衣着单薄的从者……",
                "0_H301": "叫鬼救……啥来着。",
                "0_H302": "噢？　向我搭话了。",
                "0_H303": "哦，噢！　Nice to meet you！"
            }
        },
        "0_H310": {
            "name": "对话 19",
            "text": {
                "0_H310": "哟，奥德修斯！　再让我坐一下那个啦！　好不好嘛！",
                "0_H311": "稍微坐一下就好了。",
                "0_H312": "……OK？",
                "0_H313": "太棒了Golden！！　孩子们，获得允许了。我们上！"
            }
        },
        "0_H320": {
            "name": "对话 20",
            "text": {
                "0_H320": "欧罗巴大姐！　拜托了！",
                "0_H321": "让我和您那儿的塔罗斯比试一下力气吧！　拜托了……！",
                "0_H322": "唔？　不要叫「大姐」，叫「奶奶」就行了？",
                "0_H323": "这个……Really？"
            }
        },
        "0_H330": {
            "name": "对话 21",
            "text": {
                "0_H330": "心里明明知道这里不是御山……",
                "0_H331": "但还是热血沸腾。",
                "0_H332": "哎哎没办法了！",
                "0_H333": "我说你，对力气很有自信吧。那和我用相扑较量一下吧！"
            }
        },
        "0_H340": {
            "name": "对话 22",
            "text": {
                "0_H340": "伊吹童子——",
                "0_H341": "一看到你，总觉得看到了老熟人。这是为什么呢？",
                "0_H342": "算了，反正你我共有同一个御主，多多关照啦！",
                "0_H343": "……唔。什么事？　赤龙的？",
                "0_H344": "啊，老爹怎么……",
                "0_H345": "唔哦！　靠……靠太近了！",
                "0_H346": "脸靠太近了……而且味道好香啊……",
                "0_H347": "唔，这味道好像似曾相识……？",
                "0_H348": "不不不！　总之你先离我远一点啦！"
            }
        },
        "0_H350": {
            "name": "对话 23",
            "text": {
                "0_H350": "道满你这家伙，那身打扮是什么……？",
                "0_H351": "夏天的灵衣？　你吗？",
                "0_H352": "哎——，真不知道该怎么说。你倒变得相当圆滑了呢，芦屋道满。",
                "0_H353": "不，这才是你的本性吗？",
                "0_H354": "算了，无关紧要啦！",
                "0_H355": "要将御主的暑假搞得尽可能热闹一些哦！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "哟，你好。不好意思，这阵子要承蒙你照顾了。",
                "0_S011": "你叫我黄金就行了。"
            }
        },
        "11_B080": {
            "name": "技能 1 平安武者装束",
            "text": {
                "11_B080": "使出力气来吧！"
            }
        },
        "11_B090": {
            "name": "技能 2 平安武者装束",
            "text": {
                "11_B090": "你就去一趟月亮公公哪儿吧……！"
            }
        },
        "11_B100": {
            "name": "开始 1 平安武者装束",
            "text": {
                "11_B100": "出击！　赖光家臣，坂田金时！"
            }
        },
        "11_B110": {
            "name": "开始 2 平安武者装束",
            "text": {
                "11_B110": "来和我比力气吧！"
            }
        },
        "11_B120": {
            "name": "胜利 1 平安武者装束",
            "text": {
                "11_B120": "不在话下。"
            }
        },
        "11_B130": {
            "name": "胜利 2 平安武者装束",
            "text": {
                "11_B130": "老子赢了！　哈哈！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 平安武者装束",
            "text": {
                "11_B160": "肚子饿了……抱歉啦……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 平安武者装束",
            "text": {
                "11_B170": "以敌人而言……可恶，输了！"
            }
        },
        "11_B190": {
            "name": "指令卡 2 平安武者装束",
            "text": {
                "11_B190": "这样啊！"
            }
        },
        "11_B200": {
            "name": "指令卡 3 平安武者装束",
            "text": {
                "11_B200": "明白啦！"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 平安武者装束",
            "text": {
                "11_B210": "主马佑，出击！"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 平安武者装束",
            "text": {
                "11_B220": "要爆发了哦？"
            }
        },
        "11_B430": {
            "name": "攻击 6 平安武者装束",
            "text": {
                "11_B430": "足柄旋风！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 平安武者装束",
            "text": {
                "11_B440": "八卦——准备！"
            }
        },
        "11_B480": {
            "name": "技能 3 平安武者装束",
            "text": {
                "11_B480": "好嘞……！"
            }
        },
        "11_B500": {
            "name": "开始 3 平安武者装束",
            "text": {
                "11_B500": "胆敢挡路，就强行冲过去！"
            }
        },
        "11_B520": {
            "name": "胜利 3 平安武者装束",
            "text": {
                "11_B520": "你也不赖哦……"
            }
        },
        "11_B840": {
            "name": "额外攻击 3 平安武者装束",
            "text": {
                "11_B840": "就用这招——！"
            }
        },
        "11_G010": {
            "name": "升级 平安武者装束",
            "text": {
                "11_G010": "真不错。我不讨厌锻炼。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 平安武者装束",
            "text": {
                "11_G060": "嘿嘿，真奇怪。",
                "11_G061": "明明是早就穿习惯的装束……",
                "11_G062": "现在穿上总觉得有些不好意思呢！"
            }
        },
        "11_H9000": {

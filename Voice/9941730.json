{
    "id": 9941730,
    "collectionNo": 240,
    "name": "BeastⅢ／L",
    "voices": {
        "0_B010": {
            "name": "0_B010",
            "text": {
                "0_B010": "宇宙充满了我的爱。"
            }
        },
        "0_B020": {
            "name": "0_B020",
            "text": {
                "0_B020": "你喜欢哪个我？"
            }
        },
        "0_B030": {
            "name": "0_B030",
            "text": {
                "0_B030": "堕落吧。"
            }
        },
        "0_B050": {
            "name": "0_B050",
            "text": {
                "0_B050": "我乃烦恼之化身，第六天帝。"
            }
        },
        "0_B051": {
            "name": "0_B051",
            "text": {
                "0_B051": "观音之火并非永恒，而是用以填满冰冷虚无的色度。"
            }
        },
        "0_B052": {
            "name": "0_B052",
            "text": {
                "0_B052": "沉沦摇曳吧，于这欲望的枯海。"
            }
        },
        "0_B053": {
            "name": "0_B053",
            "text": {
                "0_B053": "摩罗·阿婆卢陀"
            }
        },
        "0_B080": {
            "name": "0_B080",
            "text": {
                "0_B080": "照亮无名之辈的，是爱之光。"
            }
        },
        "0_B090": {
            "name": "0_B090",
            "text": {
                "0_B090": "烦恼无量誓愿度。"
            }
        },
        "0_B140": {
            "name": "0_B140",
            "text": {
                "0_B140": "哈啊……"
            }
        },
        "0_B150": {
            "name": "0_B150",
            "text": {
                "0_B150": "真是多余的爱呢。"
            }
        },
        "0_B160": {
            "name": "0_B160",
            "text": {
                "0_B160": "为什么，为什么……人类应该无法逃离爱的快乐才对。"
            }
        },
        "0_B170": {
            "name": "0_B170",
            "text": {
                "0_B170": "反抗堕落之炎，不过是徒劳。无限的宇宙与无限的爱，早已没有区别。人类本就会失去呼吸。哈啊，『溺爱』这词说得真好。沉溺于爱，在无尽堕落的快乐中窒息吧。人类。"
            }
        },
        "0_B300": {
            "name": "0_B300",
            "text": {
                "0_B300": "轮回·迦摩，此乃终止轮回的爱之世界，舍弃一切顾虑的堕落之邀。于吾身此般宇宙中，欲爱将覆盖一切，烦恼将化为执着。罗伽、摩那、基罗娑、阿斯罗婆，就让我将这一切都引渡至菩提之下吧。魔罗·阿瓦卢陀。"
            }
        },
        "0_B410": {
            "name": "0_B410",
            "text": {
                "0_B410": "只要你想要，要多少都行。"
            }
        }
    },
    "ChrVoice_ids": []
}
{
    "id": 300700,
    "collectionNo": 20,
    "name": "库·丘林〔Prototype〕",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "看招！！"
            }
        },
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "吃这招！！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "贯通。刺透。穿过！",
                "0_B051": "贯穿之朱枪！！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "要上啰……？"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "来吧，让我们开始吧？"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "赶快把事情了结吧――"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "……无聊的对手。让人提不起劲来。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "相当不错哦？"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "枪不太好呢，枪……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "真是的，该说没有运气……吗？"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "知道啦！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "包在我身上吧。"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "那就让我们动手吧。"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "开始进入状态了呢。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "我也开始习惯这躯体了呢――"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "不错呢，真合适！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "让你领教一下阿尔斯特纯熟的技巧！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "太完美了。真是无可挑剔――没想到你居然能做到这种程度啊……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日快乐。 今天是你的生日吧？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举行有意思的活动呢。 难得的机会，你也去参加吧。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "你打算在这儿待到啥时候？ 快去拯救世界啦。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "你是御主啊―― 一定要熟练运用我哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我和康纳尔王相处得还算不错。",
                "0_H051": "如果能和你合得来就好了。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "你有什么喜欢的动物吗？",
                "0_H061": "我的话，让我想想――狗吧。尤其是猎犬非常好。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "虽然说不上讨厌，但女人这种存在只会带来厄运。",
                "0_H071": "但所谓的好女人，连这种要素都会成为她们的魅力的一部分。 真是相当棘手吧？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "你知道达格达的大锅吗？",
                "0_H081": "我猜那东西就是圣杯的原型……不过具体怎样就不清楚了。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "看样子我和你挺合得来的。 ――你就好好运用我吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "和我不同的其他库·丘林？",
                "0_H101": "这或许也并非不可能。毕竟英灵和人类有很大的区别嘛。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "就算是出自「同一英雄」的英灵，也不能保证每次来的都是同一个。",
                "0_H111": "没事，很快就会被我替代的。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "假设有一个人。根据不同的时期，这个人也会成为和原来区别很大的「别人」。",
                "0_H121": "些许的时期不同会造成致命的差异。",
                "0_H122": "……如果我能更早发现这一点就好了。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "你感觉有点像斯卡哈呢。 不，就是有这种感觉啦，感觉。",
                "0_H131": "不是指外表啦……啊……我以前也有过这种感觉吧。"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "……以前成为我御主的那个女人啊…… 不。还是不说了。"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "库林的猎犬就是指我。 啊啊，我可是会咬死敌人哦。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "我的枪只有在真名解放时才是「我真正的枪」啊…… 真是的。"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "弗格斯叔父曾说过哦。 所谓的女人就是身体…… 不，还是算了。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我是阿尔斯特的库·丘林。 作为Lancer，那就请你多指教啰。"
            }
        }
    },
    "ChrVoice_ids": [
        "300700"
    ]
}
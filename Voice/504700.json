{
    "id": 504700,
    "collectionNo": 340,
    "name": "大黑天",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "净化吧。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "信者得福！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "遇到猫叫该怎么办……",
                "0_B051": "把灯熄掉吧……",
                "0_B052": "感谢各位的光临。",
                "0_B053": "还请注意脚下……"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "侍奉侍奉！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "还请看好了！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "看来需要清扫呢。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "开始准备晚餐吧！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "饭已经煮好了。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "老鼠才是最强的！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "米饭要煮好啦～！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "老爷……非常抱歉……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "怪我信仰不够虔诚……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白了。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "好！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "内者富良富良，外者须夫须夫！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "饭团准备好啦～！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "南无大黑天！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "归命顶礼！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "辛苦了！ 要吃饭吗？　还是洗澡呢？"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "大老爷，干得太精彩了！ 了不起，天才！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "不能在这里被消灭…… 很快就会繁殖出来给你看的……！"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "好不容易煮好的饭…… 要冷掉了……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "鼠净土即将闭园！",
                "0_B801": "期待各位的再次光临。",
                "0_B802": "如果各位能够活着回去的话。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "感觉又要增加一只了。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "多亏了您，感觉个体数量又能增加了。",
                "0_G021": "下一套衣服目前正在剪裁，还请稍候片刻。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我换了一身衣服。",
                "0_G031": "实在是找不到其他不会对大老爷失礼的服装了……",
                "0_G032": "其他都是迷你裙女仆服或是泳装女仆服那类下流的服装……"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "您感觉如何，这是将第一套服装重新剪裁而成的护士服。",
                "0_G041": "遗憾的是，我们没有任何医疗相关的知识……",
                "0_G042": "简单的手术程度应该可以通过模仿来完成吧，肯定没问题！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "十分感谢您让我们成长到如此程度。",
                "0_G051": "多亏了您，我们的个体数量也在顺利递增。",
                "0_G052": "我们蔓延整个迦勒底的日子应该不远了吧。",
                "0_G053": "我们今后也会为了大老爷的幸福而全力侍奉您。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "感觉会诞生一只健壮的个体！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "大老爷，祝您生日快乐！",
                "0_H011": "我们准备了这个代替蛋糕！",
                "0_H012": "这是用砂糖捏的饭团！",
                "0_H013": "有很多，还请尽情品尝吧！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举行什么值得庆祝的祭典哦。",
                "0_H021": "大老爷要不要也去参加呢？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "为了让大黑天的福德遍布世间，我们走吧，大老爷！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "从者是侍奉他人之人。",
                "0_H041": "理应侍奉照顾大老爷。",
                "0_H042": "其他各位从者也应该多多侍奉。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "没能召唤到吾等的主人·大黑天，而是召唤了我们这些无能之辈，可大老爷却依然爽快地接纳了我们。",
                "0_H051": "我们会用真心诚意的侍奉来回应您的这种善良温柔。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的存在，当然是吾等的主人·大黑天，以及其主人大老爷。",
                "0_H061": "只要侍奉的主人幸福，我们别无所……",
                "0_H062": "哎，奶酪？　在哪儿，在哪儿啊！？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？",
                "0_H071": "我们的职责是让地球充满幸福。",
                "0_H072": "会慈悲地对待这世间的一切，不会憎恨某个个体。",
                "0_H073": "猫另当别论。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "只要有了圣杯的魔力，我们一定能蔓延整个世界吧！",
                "0_H081": "我们大黑鼠可以做到每个人类身边都有一只24小时提供侍奉。",
                "0_H082": "这是多么美妙啊！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我们的名字吗？",
                "0_H091": "我们并没有什么个体名，但为了方便起见，每只都被分配了编号。",
                "0_H092": "我是1号，这位是2号。",
                "0_H093": "大老爷想怎么称呼我们都可以哦。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "大老爷您饲养的那只叫芙芙的生物，是什么生物啊？",
                "0_H101": "不是猫吧？",
                "0_H102": "太好了，这下我们能放心繁殖了。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我们始终注意保持干净。",
                "0_H111": "我们在吾等之主·大黑天的身边积累了长期的修行。",
                "0_H112": "不会像一般老鼠那样顺应自己的欲望行……",
                "0_H113": "啊呜……快要生了……！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "大老爷，关于我们的房间，现在已经十分拥挤了……",
                "0_H121": "即便我们挖地向下扩张，也追不上增长的速度……",
                "0_H122": "提这种要求实在不好意思，但能否麻烦您为我们准备其他的房间呢。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "照顾大老爷的工作变得越来越困难了……",
                "0_H131": "由于个体数过多，我们实在是处理不过来。",
                "0_H132": "只能说我们陷入了严重的老爷不足状态。",
                "0_H133": "我们也想侍奉其他的从者，请问可以吗？"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "摩诃迦罗（大黑天）……？",
                "0_H171": "对不起，我们什么都没听见。还请询问本人吧。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "自称猫咪虽然不值得称道，但那个人并没有罪过。",
                "0_H181": "全都是猫的错。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "从那位可爱的小姐身上能感受到和我们相通的部分。",
                "0_H191": "尤其是讨厌猫咪的方面，特别美妙！"
            }
        },
        "0_I010": {
            "name": "连续武剧神话 密西西比·神话编撰者们 1",
            "text": {
                "0_I010": "欢迎光临，大老爷。",
                "0_I011": "丰年丰收，宾客临门，欢迎来到百货店净土。",
                "0_I012": "这里准备了大量好商品～！",
                "0_I013": "当然，还是要收钱的哦？"
            }
        },
        "0_I020": {
            "name": "连续武剧神话 密西西比·神话编撰者们 2",
            "text": {
                "0_I020": "大老爷！　来得正是时候！",
                "0_I021": "我们刚煮好了饭。",
                "0_I022": "现在就做饭团。",
                "0_I023": "要加什么料？",
                "0_I024": "梅干、干木鱼、腌鲑鱼、海带、腌菜、咸菜、鱼籽……",
                "0_I025": "哎？　更想要宝物吗？",
                "0_I026": "放心吧，这饭团是我们的心意。",
                "0_I027": "是免费的服务。",
                "0_I028": "还请尽情品尝。"
            }
        },
        "0_I030": {
            "name": "连续武剧神话 密西西比·神话编撰者们 3",
            "text": {
                "0_I030": "开始兑换宝物吧～！",
                "0_I031": "骨碌骨碌滚咚咚掉！"
            }
        },
        "0_I040": {
            "name": "连续武剧神话 密西西比·神话编撰者们 4",
            "text": {
                "0_I040": "不愧是大老爷，真有眼光。",
                "0_I041": "那么……",
                "0_I042": "骨碌骨碌滚咚咚掉！"
            }
        },
        "0_I050": {
            "name": "连续武剧神话 密西西比·神话编撰者们 5",
            "text": {
                "0_I050": "啊呜……卖光啦……！",
                "0_I051": "没什么大不了的，还差得远。五谷丰登的恩惠是无穷无尽的。",
                "0_I052": "要大量提供啦～！",
                "0_I053": "预备～！",
                "0_I054": "骨碌骨碌滚咚咚掉！"
            }
        },
        "0_I060": {
            "name": "连续武剧神话 密西西比·神话编撰者们 6",
            "text": {
                "0_I060": "承蒙您光顾的百货店净土，就此关店。",
                "0_I061": "如果有缘，今后或许还能在什么祭典上见到吧。",
                "0_I062": "我们也会期待那一天的到来。",
                "0_I063": "那么，大老爷——",
                "0_I064": "期待您的再次光临。"
            }
        },
        "0_I070": {
            "name": "连续武剧神话 密西西比·神话编撰者们 7",
            "text": {
                "0_I070": "大老爷真是位好老爷，值得我们侍奉。",
                "0_I071": "有缘的话，我们定将再次全力侍奉您！",
                "0_I072": "当然，这是我们一族的整体意见——",
                "0_I073": "我们全体成员都将倾尽全力为您效力！",
                "0_I074": "工作、侍奉、诶呀呀！",
                "0_I075": "繁荣、增加、大老爷！",
                "0_I076": "深深祈祷有缘再为您服务——"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "初次见面，大老爷。我们是作为吾之主人·大黑天的代理来到此处的。",
                "0_S011": "吾之主人工作繁忙，无法回应您的召唤……",
                "0_S012": "因而吩咐我们这些作为仆从的大黑鼠们前来为大老爷效力。",
                "0_S013": "或许能力薄弱，但还请您多多关照。"
            }
        }
    },
    "ChrVoice_ids": [
        "504700"
    ]
}
{
    "id": 305700,
    "collectionNo": 433,
    "name": "ビショーネ",
    "voices": {
        "0_B010": {
            "name": "アタック 1",
            "text": {
                "0_B010": "连同身体，飞扑！"
            }
        },
        "0_B020": {
            "name": "アタック 2",
            "text": {
                "0_B020": "特技，可以喷毒～！"
            }
        },
        "0_B030": {
            "name": "アタック 3",
            "text": {
                "0_B030": "玉石俱碎……我可不愿意！"
            }
        },
        "0_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "0_B040": "倾注力量！　米兰风，连击！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "咬合开始。",
                "0_B051": "以枯萎丛林之毒，于米兰之地营巢。",
                "0_B052": "蛮勇的骑士不在，蛇安心地暴食！",
                "0_B053": "米兰蛇龙·纹章咬合！"
            }
        },
        "0_B080": {
            "name": "スキル 1",
            "text": {
                "0_B080": "请，请不要一直盯着我～。"
            }
        },
        "0_B090": {
            "name": "スキル 2",
            "text": {
                "0_B090": "我会全力以赴！"
            }
        },
        "0_B100": {
            "name": "開始 1",
            "text": {
                "0_B100": "自我展示的机会，我是不会放过的！"
            }
        },
        "0_B110": {
            "name": "開始 2",
            "text": {
                "0_B110": "好，好紧张啊……！"
            }
        },
        "0_B120": {
            "name": "勝利 1",
            "text": {
                "0_B120": "我感觉抓住了机会！"
            }
        },
        "0_B130": {
            "name": "勝利 2",
            "text": {
                "0_B130": "感谢您给与我时间！"
            }
        },
        "0_B140": {
            "name": "ダメージ 2",
            "text": {
                "0_B140": "危机！"
            }
        },
        "0_B150": {
            "name": "ダメージ 1",
            "text": {
                "0_B150": "履历空白期！"
            }
        },
        "0_B160": {
            "name": "戦闘不能 1",
            "text": {
                "0_B160": "这或许是一次不合时宜的参加……！"
            }
        },
        "0_B170": {
            "name": "戦闘不能 2",
            "text": {
                "0_B170": "我们没有缘分？　怎么会……"
            }
        },
        "0_B180": {
            "name": "コマンドカード 1",
            "text": {
                "0_B180": "我会工作！"
            }
        },
        "0_B190": {
            "name": "コマンドカード 2",
            "text": {
                "0_B190": "我很精神！"
            }
        },
        "0_B200": {
            "name": "コマンドカード 3",
            "text": {
                "0_B200": "我有干劲！"
            }
        },
        "0_B2010": {
            "name": "アタック 10",
            "text": {
                "0_B2010": "感觉会被礼仪老师训斥！"
            }
        },
        "0_B2020": {
            "name": "アタック 11",
            "text": {
                "0_B2020": "咿呀，这，这不是这个意思！……对吧？"
            }
        },
        "0_B2030": {
            "name": "アタック 12",
            "text": {
                "0_B2030": "害怕得不敢看结果！　但是偷瞄。"
            }
        },
        "0_B210": {
            "name": "宝具カード 1",
            "text": {
                "0_B210": "突如其来的表现机会！"
            }
        },
        "0_B220": {
            "name": "宝具カード 2",
            "text": {
                "0_B220": "受到期待了……？"
            }
        },
        "0_B230": {
            "name": "宝具カード 3",
            "text": {
                "0_B230": "紧张，突破极限！"
            }
        },
        "0_B2410": {
            "name": "アタック 13",
            "text": {
                "0_B2410": "来呀来呀，这个这个！　你应该明白吧！？"
            }
        },
        "0_B2420": {
            "name": "アタック 14",
            "text": {
                "0_B2420": "鼓起勇气，这样！"
            }
        },
        "0_B250": {
            "name": "宝具カード 4",
            "text": {
                "0_B250": "自由时间，到来！"
            }
        },
        "0_B410": {
            "name": "アタック 4",
            "text": {
                "0_B410": "意外以尾巴为傲！"
            }
        },
        "0_B420": {
            "name": "アタック 5",
            "text": {
                "0_B420": "哇！？哇哇！？呜……"
            }
        },
        "0_B430": {
            "name": "アタック 6",
            "text": {
                "0_B430": "抱，抱歉背对你！"
            }
        },
        "0_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "0_B440": "在当地还是有很多人怕我的！"
            }
        },
        "0_B480": {
            "name": "スキル 3",
            "text": {
                "0_B480": "唔，啊唔啊唔……"
            }
        },
        "0_B490": {
            "name": "スキル 4",
            "text": {
                "0_B490": "请，请问您有什么问题吗……？"
            }
        },
        "0_B500": {
            "name": "開始 3",
            "text": {
                "0_B500": "我是比肖内，请多关照！"
            }
        },
        "0_B510": {
            "name": "開始 4",
            "text": {
                "0_B510": "在被敦促前，决不能坐下……！"
            }
        },
        "0_B520": {
            "name": "勝利 3",
            "text": {
                "0_B520": "感觉，有戏……！"
            }
        },
        "0_B530": {
            "name": "勝利 4",
            "text": {
                "0_B530": "感觉能抓住美好的未来了！"
            }
        },
        "0_B540": {
            "name": "ダメージ 4",
            "text": {
                "0_B540": "不是啦！"
            }
        },
        "0_B550": {
            "name": "ダメージ 3",
            "text": {
                "0_B550": "压迫面试！"
            }
        },
        "0_B560": {
            "name": "戦闘不能 3",
            "text": {
                "0_B560": "我可以重新挑战吗……？"
            }
        },
        "0_B570": {
            "name": "戦闘不能 4",
            "text": {
                "0_B570": "我不想接到拒绝邮件啊……！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "我要上了！",
                "0_B801": "我是比肖内，居住在米兰的蛇亦是龙。",
                "0_B802": "人啊，不要靠近，如果你还惜命！",
                "0_B803": "米兰蛇龙·纹章咬合！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "为贵司带来利益！",
                "0_B811": "虽然有些不安，但还是要摆出得意的表情想办法！",
                "0_B812": "新人作风的冒失肯定是我唯一的武器！",
                "0_B813": "务必请您正式聘用我！"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "业务！　穿刺、鞭打、喷毒！"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "我是不会手下留情的！"
            }
        },
        "0_B890": {
            "name": "スキル 6",
            "text": {
                "0_B890": "职业社会人，应该这样休息……"
            }
        },
        "0_B910": {
            "name": "アタック 7",
            "text": {
                "0_B910": "大家都知道的纹章光线！"
            }
        },
        "0_B920": {
            "name": "アタック 8",
            "text": {
                "0_B920": "我有毒之吐息的执照！"
            }
        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "奋勇突击！　……对不起！"
            }
        },
        "0_G010": {
            "name": "レベルアップ 1",
            "text": {
                "0_G010": "感觉距离被录用又近了一步？",
                "0_G011": "对吧？"
            }
        },
        "0_G020": {
            "name": "霊基再臨 2",
            "text": {
                "0_G020": "呼。是我误会了啊，太好了……",
                "0_G021": "不过，御主会不会觉得这种布章太孩子气了？",
                "0_G022": "并不觉得……？",
                "0_G023": "哎嘿嘿。那真是太好了！",
                "0_G024": "下次我干脆多做几个以御主为主题的布章吧。"
            }
        },
        "0_G030": {
            "name": "霊基再臨 1",
            "text": {
                "0_G030": "如果被告知可以穿得随意一点，会不会有点不好办呢……？",
                "0_G031": "啊！？　但御主还是平时那身打扮……",
                "0_G032": "难道这也是正式录用考试的一部分！？",
                "0_G033": "我，我可能应该穿得更正式一点吧……"
            }
        },
        "0_G110": {
            "name": "レベルアップ 2",
            "text": {
                "0_G110": "我等的就是这项福利！"
            }
        },
        "0_G120": {
            "name": "レベルアップ 3",
            "text": {
                "0_G120": "好大方……！",
                "0_G121": "来这里果然是正确的选择！"
            }
        },
        "0_H010": {
            "name": "誕生日",
            "text": {
                "0_H010": "今天是您生日吗？　祝您生日快乐！",
                "0_H011": "啊。难道这就是传说中「该送上司什么生日礼物才好」的难题降临了……！？",
                "0_H012": "请，请稍等一下！",
                "0_H013": "能给我一点做功课的时间吗？"
            }
        },
        "0_H020": {
            "name": "イベント開催中",
            "text": {
                "0_H020": "外面好像有些热闹呢。",
                "0_H021": "是所谓的……繁忙期吗？"
            }
        },
        "0_H030": {
            "name": "会話 1",
            "text": {
                "0_H030": "我觉得内勤非常重要，但光靠这个应该不行！",
                "0_H031": "请务必教导我外勤的精髓！",
                "0_H032": "我会靠毅力跟上的！"
            }
        },
        "0_H040": {
            "name": "会話 2",
            "text": {
                "0_H040": "上司的命令必须绝对服从……这种作风是错误的！",
                "0_H041": "其实还是稍微费点工夫、有点粗心大意的人不容易让人忽视，最终会得到长期宠爱吧～。",
                "0_H042": "这本「受宠新人手册」上就是这么写的！",
                "0_H043": "虽然作者就是我。"
            }
        },
        "0_H050": {
            "name": "会話 3",
            "text": {
                "0_H050": "御主是我的直属上司，也是前辈……",
                "0_H051": "而且对相当于处于正式员工试用期阶段的我来说，还是我的面试官……！",
                "0_H052": "所以我会全力以赴！",
                "0_H053": "努力展示自己能干的一面！"
            }
        },
        "0_H060": {
            "name": "好きなこと",
            "text": {
                "0_H060": "喜欢的东西是平稳而安定的生活！",
                "0_H061": "如果在此基础上还能用干净的水洗个澡，再慢慢冬眠的话，那就太完美了……",
                "0_H062": "会，会不会过于奢望了？"
            }
        },
        "0_H070": {
            "name": "嫌いなこと",
            "text": {
                "0_H070": "与其说是讨厌，倒不如说有一件我无论如何都不擅长应付的东西……",
                "0_H071": "那个……我应付不了男人的裸体！",
                "0_H072": "只要一看到，面孔就会瞬间发烫，感到羞耻！",
                "0_H073": "如果能改的话，真希望能改掉……呜呜。"
            }
        },
        "0_H080": {
            "name": "聖杯について",
            "text": {
                "0_H080": "如果有圣杯的话，我的愿望……应该还是正式录用……不，是终身雇用……就职一辈子……？",
                "0_H081": "唔？　这也意味着……",
                "0_H082": "啊哇哇哇！　刚才当我没说！",
                "0_H083": "总，总之，我会靠自己的力量努力的！"
            }
        },
        "0_H090": {
            "name": "絆Lv.1",
            "text": {
                "0_H090": "呜呜。",
                "0_H091": "我真的能干好吗……",
                "0_H092": "胃都开始痛了……"
            }
        },
        "0_H100": {
            "name": "絆Lv.2",
            "text": {
                "0_H100": "呼。意外好像开始习惯工作了。",
                "0_H101": "……啊。",
                "0_H102": "不，不是的，我决没有看不起贵公司的意思！",
                "0_H103": "请千万别终止我的雇用合同啊～！"
            }
        },
        "0_H110": {
            "name": "絆Lv.3",
            "text": {
                "0_H110": "这里的员工食堂水准真的好高啊～。",
                "0_H111": "好～，好幸福……"
            }
        },
        "0_H120": {
            "name": "絆Lv.4",
            "text": {
                "0_H120": "早上好！",
                "0_H121": "一上班就能看到御主，我就觉得今天一定会是个好日子。哎嘿嘿。",
                "0_H122": "今天也请多多指导、鞭策！"
            }
        },
        "0_H130": {
            "name": "絆Lv.5",
            "text": {
                "0_H130": "辛苦了！",
                "0_H131": "……那个，不知道以我的立场这么建议是否合适……",
                "0_H132": "但，但如果可以的话，下班路上要不要闷一杯维生素饮料？",
                "0_H133": "就是所谓的，那个，共饮社交！",
                "0_H134": "哎？　可以？",
                "0_H135": "太好了！"
            }
        },
        "0_H170": {
            "name": "会話 4",
            "text": {
                "0_H170": "那个，有个很像能干女秘书的人邀请我加入叫ＮＦＦ服务（？）的地方……",
                "0_H171": "为什么呢。",
                "0_H172": "我隐约嗅到了黑心企业的味道……"
            }
        },
        "0_H180": {
            "name": "会話 5",
            "text": {
                "0_H180": "在这里总会碰到裸着上半身的男人，害我平时都不能掉以轻心……",
                "0_H181": "尤其是那个叫贝奥武夫的人非常危险！",
                "0_H182": "不，不仅总是裸着上半身，还是个屠龙者！",
                "0_H183": "屠龙这种事已经无可奈何了，但能否至少让他把衣服穿上……？"
            }
        },
        "0_H190": {
            "name": "会話 6",
            "text": {
                "0_H190": "这里有个叫罗兰的人吧？",
                "0_H191": "我觉得那个人多半跟我有什么仇怨……",
                "0_H192": "总是突然开始脱衣服，我每次只能拔腿就跑……",
                "0_H193": "哎？　你说那个人看到谁都会脱？",
                "0_H194": "……",
                "0_H195": "……为什么……？"
            }
        },
        "0_H200": {
            "name": "会話 7",
            "text": {
                "0_H200": "有，有翼之蛇，神明～！？",
                "0_H201": "叩，叩拜～！"
            }
        },
        "0_H210": {
            "name": "会話 8",
            "text": {
                "0_H210": "干旱的邪龙，弗栗多……",
                "0_H211": "咿咿，超级大人物～！",
                "0_H212": "请务必……不要拿我跟能与神明打架的人物相比……"
            }
        },
        "0_H220": {
            "name": "会話 9",
            "text": {
                "0_H220": "那姑娘……是叫梅柳齐娜亲吗？",
                "0_H221": "那么娇小可爱，一定和我一样是新员工从者吧。",
                "0_H222": "好想和她打好关系啊。"
            }
        },
        "0_H270": {
            "name": "会話 10",
            "text": {
                "0_H270": "那个，有件事让我一直以来都很疑惑……",
                "0_H271": "因为实在忍不住了，我可以问吗？",
                "0_H272": "归根结底，伊丽亲前辈到底有多少位啊？",
                "0_H273": "我至今还没搞清楚哪个职阶的伊丽亲前辈是独立存在的，哪个职阶的只是换了套衣服……",
                "0_H274": "哎？『伊丽是普遍存在的……？』",
                "0_H275": "越，越来越搞不懂啦～！"
            }
        },
        "0_H280": {
            "name": "会話 11",
            "text": {
                "0_H280": "美杜莎部长，早上好！",
                "0_H281": "今天没有什么业务联系！　告，告辞了！",
                "0_H282": "呼，她还是那么帅气呢。",
                "0_H283": "如果我努力的话，总有一天也能成为那样的职业女性……",
                "0_H284": "然后把那种性感的西装穿得得体……！",
                "0_H285": "……唔，背后的部分可能有些难……"
            }
        },
        "0_H290": {
            "name": "会話 12",
            "text": {
                "0_H290": "哇，阿斯克勒庇俄斯医生！",
                "0_H291": "没，没问题啦，我每天都有好好刷牙！",
                "0_H292": "是，是真的哦～？"
            }
        },
        "0_I010": {
            "name": "育て！ マイ・リトル・ドラゴン 1",
            "text": {
                "0_I010": "欢，欢迎光临！",
                "0_I011": "呜呜……感觉这个任务对新人来说过于重要了……",
                "0_I012": "不，不要紧！　交给我的工作我一定会认真完成的！"
            }
        },
        "0_I020": {
            "name": "育て！ マイ・リトル・ドラゴン 2",
            "text": {
                "0_I020": "是这个吧！",
                "0_I021": "递上。",
                "0_I022": "是这个吧！",
                "0_I023": "递上……",
                "0_I024": "呼～……",
                "0_I025": "啊？",
                "0_I026": "您，您看到了吗？　看到我练习当店员的样子了！"
            }
        },
        "0_I030": {
            "name": "育て！ マイ・リトル・ドラゴン 3",
            "text": {
                "0_I030": "是这个吧，请拿好！",
                "0_I031": "完，完美……",
                "0_I032": "练习好像收获成果了！",
                "0_I033": "哎嘿嘿。"
            }
        },
        "0_I040": {
            "name": "育て！ マイ・リトル・ドラゴン 4",
            "text": {
                "0_I040": "把商品稳稳地递出去，也不会算错找零！",
                "0_I041": "难不成我……其实很有当店员龙的才能……？"
            }
        },
        "0_I050": {
            "name": "育て！ マイ・リトル・ドラゴン 5",
            "text": {
                "0_I050": "感谢您的惠顾！",
                "0_I051": "那个～，如果太久不来，我会忍不住怀疑自己上次是不是搞砸了什么，然后陷入沮丧……",
                "0_I052": "所以如果可能的话，请尽快再来哦！　好吗！"
            }
        },
        "0_I060": {
            "name": "育て！ マイ・リトル・ドラゴン 6",
            "text": {
                "0_I060": "我曾是毫无经验的新人，肯定还有很多做得不够好的地方……",
                "0_I061": "但我很高兴能被分配到这家店！",
                "0_I062": "感谢您的光顾！"
            }
        },
        "0_I070": {
            "name": "育て！ マイ・リトル・ドラゴン 7",
            "text": {
                "0_I070": "「完美地完成了店员职责的我」……",
                "0_I071": "呵呵，这也是喜欢多变的我的可能性之一吧。",
                "0_I072": "没错，因为这里不会有敌人来袭，所以与其说我完全不讨厌，简直可以说太棒了！",
                "0_I073": "今后如果再任命我来这里的话，我也很乐意效力！"
            }
        },
        "0_S010": {
            "name": "召喚",
            "text": {
                "0_S010": "我想来贵公司的动机，是被贵公司的自由企业文化吸引，以及想要过上安心、安定的生活！",
                "0_S011": "……",
                "0_S012": "咿哎哎，我还没自我介绍！",
                "0_S013": "我是比肖内，大概是来自米兰那一带的比肖内！",
                "0_S014": "请多关照！",
                "0_S015": "……呜呜。明明第一印象才是最重要的。我却搞砸了～……"
            }
        },
        "2_B010": {
            "name": "アタック 1",
            "text": {
                "2_B010": "请不要靠近。"
            }
        },
        "2_B020": {
            "name": "アタック 2",
            "text": {
                "2_B020": "这是擅长空中的我。"
            }
        },
        "2_B030": {
            "name": "アタック 3",
            "text": {
                "2_B030": "来吧，赤热与厚重！"
            }
        },
        "2_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "2_B040": "我是无限的龙，拥有众多可能性的龙！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "闪耀的王冠。",
                "2_B051": "吾乃喷毒之蛇，也是无限的圆环之龙。",
                "2_B052": "轮转吧，半回路，然龙气将孵卵。",
                "2_B053": "有鳞幻想。"
            }
        },
        "2_B080": {
            "name": "スキル 1",
            "text": {
                "2_B080": "呼噜……啊？"
            }
        },
        "2_B090": {
            "name": "スキル 2",
            "text": {
                "2_B090": "赤色元素，启动！"
            }
        },
        "2_B100": {
            "name": "開始 1",
            "text": {
                "2_B100": "我是，不会让你后悔的。"
            }
        },
        "2_B110": {
            "name": "開始 2",
            "text": {
                "2_B110": "让你见识我的力量。"
            }
        },
        "2_B120": {
            "name": "勝利 1",
            "text": {
                "2_B120": "或许差不多，可以有后辈了！"
            }
        },
        "2_B130": {
            "name": "勝利 2",
            "text": {
                "2_B130": "这就是……充实感……！"
            }
        },
        "2_B140": {
            "name": "ダメージ 2",
            "text": {
                "2_B140": "好痛。"
            }
        },
        "2_B150": {
            "name": "ダメージ 1",
            "text": {
                "2_B150": "如果能……卸掉攻击就好了！"
            }
        },
        "2_B160": {
            "name": "戦闘不能 1",
            "text": {
                "2_B160": "竟然，还不够……！"
            }
        },
        "2_B170": {
            "name": "戦闘不能 2",
            "text": {
                "2_B170": "在，无限圆环之中……"
            }
        },
        "2_B180": {
            "name": "コマンドカード 1",
            "text": {
                "2_B180": "好。"
            }
        },
        "2_B190": {
            "name": "コマンドカード 2",
            "text": {
                "2_B190": "交给我吧。"
            }
        },
        "2_B200": {
            "name": "コマンドカード 3",
            "text": {
                "2_B200": "必须证明才行。"
            }
        },
        "2_B2010": {
            "name": "アタック 10",
            "text": {
                "2_B2010": "汉普郡的贪龙。"
            }
        },
        "2_B2020": {
            "name": "アタック 11",
            "text": {
                "2_B2020": "假想无限回路，轮转。"
            }
        },
        "2_B2030": {
            "name": "アタック 12",
            "text": {
                "2_B2030": "为喜悦而兴奋的地龙。"
            }
        },
        "2_B210": {
            "name": "宝具カード 1",
            "text": {
                "2_B210": "放一百个心吧！"
            }
        },
        "2_B220": {
            "name": "宝具カード 2",
            "text": {
                "2_B220": "和迄今为止不一样了。"
            }
        },
        "2_B230": {
            "name": "宝具カード 3",
            "text": {
                "2_B230": "乌洛波洛斯元素，启动。"
            }
        },
        "2_B2410": {
            "name": "アタック 13",
            "text": {
                "2_B2410": "维夫尔之翼。"
            }
        },
        "2_B2420": {
            "name": "アタック 14",
            "text": {
                "2_B2420": "加尔古耶的波涛。"
            }
        },
        "2_B250": {
            "name": "宝具カード 4",
            "text": {
                "2_B250": "放心吧，御主。"
            }
        },
        "2_B410": {
            "name": "アタック 4",
            "text": {
                "2_B410": "一口吞下去。"
            }
        },
        "2_B420": {
            "name": "アタック 5",
            "text": {
                "2_B420": "用过剩的力量，这样！"
            }
        },
        "2_B430": {
            "name": "アタック 6",
            "text": {
                "2_B430": "蛇会出现在意外的地方。"
            }
        },
        "2_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "2_B440": "这是迅速的我、炙热的我、喷毒的我！"
            }
        },
        "2_B480": {
            "name": "スキル 3",
            "text": {
                "2_B480": "翼翅元素，启动！"
            }
        },
        "2_B490": {
            "name": "スキル 4",
            "text": {
                "2_B490": "不慌不忙地摆出大人物风范……"
            }
        },
        "2_B500": {
            "name": "開始 3",
            "text": {
                "2_B500": "没有更换职业哦。"
            }
        },
        "2_B510": {
            "name": "開始 4",
            "text": {
                "2_B510": "Buongiorno（早上好）。"
            }
        },
        "2_B520": {
            "name": "勝利 3",
            "text": {
                "2_B520": "不能大意，对吧！"
            }
        },
        "2_B530": {
            "name": "勝利 4",
            "text": {
                "2_B530": "如果，能帮上御主。"
            }
        },
        "2_B540": {
            "name": "ダメージ 4",
            "text": {
                "2_B540": "有点本事！"
            }
        },
        "2_B550": {
            "name": "ダメージ 3",
            "text": {
                "2_B550": "没，没有效果哦！？"
            }
        },
        "2_B560": {
            "name": "戦闘不能 3",
            "text": {
                "2_B560": "果然，这种可能性……"
            }
        },
        "2_B570": {
            "name": "戦闘不能 4",
            "text": {
                "2_B570": "没法那么顺利吗……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "有鳞幻想。",
                "2_B801": "吾身为众多蛇与龙，人们畏惧的神秘之形。",
                "2_B802": "刻于骑士之系谱，有翼毒蛇爬行吞噬。",
                "2_B803": "有鳞幻想·纹章龙。"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "乌洛波洛斯元素。",
                "2_B811": "以圆环龙的力量，轮转高贵的无限。",
                "2_B812": "出现吧，蛇。我肯定即为此。",
                "2_B813": "有鳞幻想。"
            }
        },
        "2_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "2_B840": "高速轮转！　转变至转变！　嘿！"
            }
        },
        "2_B880": {
            "name": "スキル 5",
            "text": {
                "2_B880": "赤龙之力！"
            }
        },
        "2_B890": {
            "name": "スキル 6",
            "text": {
                "2_B890": "连接林德虫。"
            }
        },
        "2_B910": {
            "name": "アタック 7",
            "text": {
                "2_B910": "龙翼，显现！"
            }
        },
        "2_B920": {
            "name": "アタック 8",
            "text": {
                "2_B920": "林德虫的拥抱。"
            }
        },
        "2_B930": {
            "name": "アタック 9",
            "text": {
                "2_B930": "朗威顿之尾。"
            }
        },
        "2_G010": {
            "name": "レベルアップ 1",
            "text": {
                "2_G010": "成长不会停止。"
            }
        },
        "2_G040": {
            "name": "霊基再臨 1",
            "text": {
                "2_G040": "啊啊——我明白了。",
                "2_G041": "我是众多的龙与蛇。",
                "2_G042": "是戴上王冠的米兰之蛇、被骑士斩杀的无名之龙，也是相噬乌洛波洛斯的一半。",
                "2_G043": "无限圆环的构成要素，流转可能性的体现——",
                "2_G044": "虽然我依然是我，但如果……",
                "2_G045": "您能稍稍承认我还算是个可靠新人的话，我会很高兴的。"
            }
        },
        "2_G050": {
            "name": "霊基再臨 2",
            "text": {
                "2_G050": "御主为什么会对记性差、做事笨拙、都不知有什么优点的我……",
                "2_G051": "如此用心地培养呢？",
                "2_G052": "难道是因为……",
                "2_G053": "不，不行！",
                "2_G054": "这是在把工作上的关系往私人情感上套，是会导致误会的不可取的失败情况！",
                "2_G055": "职场关系会因此变得扭曲的，一定要小心！",
                "2_G056": "……不过，把这份心情像宝石一样悄悄珍藏在自己心里……应该可以被允许吧？"
            }
        },
        "2_G110": {
            "name": "レベルアップ 2",
            "text": {
                "2_G110": "可能性，还远没有到头……"
            }
        },
        "2_G120": {
            "name": "レベルアップ 3",
            "text": {
                "2_G120": "更深……更强……"
            }
        },
        "2_H010": {
            "name": "誕生日",
            "text": {
                "2_H010": "今天是您生日吗？　祝您生日快乐！",
                "2_H011": "啊。难道这就是传说中「该送上司什么生日礼物才好」的难题降临了……！？",
                "2_H012": "请，请稍等一下！",
                "2_H013": "能给我一点做功课的时间吗？"
            }
        },
        "2_H020": {
            "name": "イベント開催中",
            "text": {
                "2_H020": "外面好像有些热闹呢。",
                "2_H021": "是所谓的……繁忙期吗？"
            }
        },
        "2_H030": {
            "name": "会話 1",
            "text": {
                "2_H030": "我觉得内勤非常重要，但光靠这个应该不行！",
                "2_H031": "请务必教导我外勤的精髓！",
                "2_H032": "我会靠毅力跟上的！"
            }
        },
        "2_H040": {
            "name": "会話 2",
            "text": {
                "2_H040": "上司的命令必须绝对服从……这种作风是错误的！",
                "2_H041": "其实还是稍微费点工夫、有点粗心大意的人不容易让人忽视，最终会得到长期宠爱吧～。",
                "2_H042": "这本「受宠新人手册」上就是这么写的！",
                "2_H043": "虽然作者就是我。"
            }
        },
        "2_H050": {
            "name": "会話 3",
            "text": {
                "2_H050": "御主是我的直属上司，也是前辈……",
                "2_H051": "而且对相当于处于正式员工试用期阶段的我来说，还是我的面试官……！",
                "2_H052": "所以我会全力以赴！",
                "2_H053": "努力展示自己能干的一面！"
            }
        },
        "2_H060": {
            "name": "好きなこと",
            "text": {
                "2_H060": "喜欢的东西是平稳而安定的生活！",
                "2_H061": "如果在此基础上还能用干净的水洗个澡，再慢慢冬眠的话，那就太完美了……",
                "2_H062": "会，会不会过于奢望了？"
            }
        },
        "2_H070": {
            "name": "嫌いなこと",
            "text": {
                "2_H070": "与其说是讨厌，倒不如说有一件我无论如何都不擅长应付的东西……",
                "2_H071": "那个……我应付不了男人的裸体！",
                "2_H072": "只要一看到，面孔就会瞬间发烫，感到羞耻！",
                "2_H073": "如果能改的话，真希望能改掉……呜呜。"
            }
        },
        "2_H080": {
            "name": "聖杯について",
            "text": {
                "2_H080": "如果有圣杯的话，我的愿望……应该还是正式录用……不，是终身雇用……就职一辈子……？",
                "2_H081": "唔？　这也意味着……",
                "2_H082": "啊哇哇哇！　刚才当我没说！",
                "2_H083": "总，总之，我会靠自己的力量努力的！"
            }
        },
        "2_H090": {
            "name": "絆Lv.1",
            "text": {
                "2_H090": "呜呜。",
                "2_H091": "我真的能干好吗……",
                "2_H092": "胃都开始痛了……"
            }
        },
        "2_H100": {
            "name": "絆Lv.2",
            "text": {
                "2_H100": "呼。意外好像开始习惯工作了。",
                "2_H101": "……啊。",
                "2_H102": "不，不是的，我决没有看不起贵公司的意思！",
                "2_H103": "请千万别终止我的雇用合同啊～！"
            }
        },
        "2_H110": {
            "name": "絆Lv.3",
            "text": {
                "2_H110": "这里的员工食堂水准真的好高啊～。",
                "2_H111": "好～，好幸福……"
            }
        },
        "2_H120": {
            "name": "絆Lv.4",
            "text": {
                "2_H120": "早上好！",
                "2_H121": "一上班就能看到御主，我就觉得今天一定会是个好日子。哎嘿嘿。",
                "2_H122": "今天也请多多指导、鞭策！"
            }
        },
        "2_H130": {
            "name": "絆Lv.5",
            "text": {
                "2_H130": "辛苦了！",
                "2_H131": "……那个，不知道以我的立场这么建议是否合适……",
                "2_H132": "但，但如果可以的话，下班路上要不要闷一杯维生素饮料？",
                "2_H133": "就是所谓的，那个，共饮社交！",
                "2_H134": "哎？　可以？",
                "2_H135": "太好了！"
            }
        },
        "2_H170": {
            "name": "会話 4",
            "text": {
                "2_H170": "那个，有个很像能干女秘书的人邀请我加入叫ＮＦＦ服务（？）的地方……",
                "2_H171": "为什么呢。",
                "2_H172": "我隐约嗅到了黑心企业的味道……"
            }
        },
        "2_H180": {
            "name": "会話 5",
            "text": {
                "2_H180": "在这里总会碰到裸着上半身的男人，害我平时都不能掉以轻心……",
                "2_H181": "尤其是那个叫贝奥武夫的人非常危险！",
                "2_H182": "不，不仅总是裸着上半身，还是个屠龙者！",
                "2_H183": "屠龙这种事已经无可奈何了，但能否至少让他把衣服穿上……？"
            }
        },
        "2_H190": {
            "name": "会話 6",
            "text": {
                "2_H190": "这里有个叫罗兰的人吧？",
                "2_H191": "我觉得那个人多半跟我有什么仇怨……",
                "2_H192": "总是突然开始脱衣服，我每次只能拔腿就跑……",
                "2_H193": "哎？　你说那个人看到谁都会脱？",
                "2_H194": "……",
                "2_H195": "……为什么……？"
            }
        },
        "2_H230": {
            "name": "会話 7",
            "text": {
                "2_H230": "梅柳亲竟然是超强的龙啊，不要啦！",
                "2_H231": "我本想摸摸她的头，结果遭到反击壁咚，我哭丧着脸解释，却反被她摸头说「呵呵，好可爱」！",
                "2_H232": "……但是，既然打好关系的目的达成了，或许也不错……？"
            }
        },
        "2_H240": {
            "name": "会話 8",
            "text": {
                "2_H240": "自从乌洛波洛斯的力量显现之后，炼金术师和学者系的人盯着我看的频率越来越高了……",
                "2_H241": "真的不要紧吗，不会是想把我挖走，或者换个部门吧！",
                "2_H242": "",
                "2_H243": "因，因为我非常喜欢目前御主直属的部门，那个……请不要抛弃我啦～！"
            }
        },
        "2_H250": {
            "name": "会話 9",
            "text": {
                "2_H250": "就算具备乌洛波洛斯的要素，我还是不清楚自己的长处是什么。",
                "2_H251": "相比之下，清姬亲那种坚定不移、独一无二的力量非常厉害，令人羡慕。",
                "2_H252": "啊，不对，与其说是嫉妒……更像是崇拜吧。"
            }
        },
        "2_H270": {
            "name": "会話 10",
            "text": {
                "2_H270": "那个，有件事让我一直以来都很疑惑……",
                "2_H271": "因为实在忍不住了，我可以问吗？",
                "2_H272": "归根结底，伊丽亲前辈到底有多少位啊？",
                "2_H273": "我至今还没搞清楚哪个职阶的伊丽亲前辈是独立存在的，哪个职阶的只是换了套衣服……",
                "2_H274": "哎？『伊丽是普遍存在的……？』",
                "2_H275": "越，越来越搞不懂啦～！"
            }
        },
        "2_H280": {
            "name": "会話 11",
            "text": {
                "2_H280": "美杜莎部长，早上好！",
                "2_H281": "今天没有什么业务联系！　告，告辞了！",
                "2_H282": "呼，她还是那么帅气呢。",
                "2_H283": "如果我努力的话，总有一天也能成为那样的职业女性……",
                "2_H284": "然后把那种性感的西装穿得得体……！",
                "2_H285": "……唔，背后的部分可能有些难……"
            }
        },
        "2_H290": {
            "name": "会話 12",
            "text": {

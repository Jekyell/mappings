{
    "id": 2301000,
    "collectionNo": 422,
    "name": "特诺奇蒂特兰",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "这种程度，应该不要紧吧？"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "给你来点恩惠之雨……！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "老老实实被严重损毁吧。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "让你见识一下新武装吧！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "真实的躯体，降临此处。",
                "0_B051": "这建筑真不错，合体吧！",
                "0_B052": "这就是夏日的决战都市！　重装甲决战都市！",
                "0_B053": "——姊妹都市同盟缔结了吧？"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "有什么，想要的东西吗？"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "为你扩大夏天，吧。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "竟然如此缺乏水分。 在干燥的地方就是我一个人的天下了，对吧。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "请不要妨碍我观光。 我正在视察蜜月，呢。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "因为是夏天，所以心情翻倍，还增加了动作。 要三倍夸奖我。好吗？"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "就算离开了自己的地域，输出也不会降低。 因为有我在的地方就是水之都。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "能防住哦。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "由我，来承受……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "毕竟是注重外表的服装，会输掉也是在所难免的……对吧？"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "这种程度的失败不算输。要代我报一箭之仇哦，好吗？"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "真强硬，呢。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "真大胆，呢。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "太心急了，呢。"
            }
        },
        "0_B2010": {
            "name": "攻击 10",
            "text": {
                "0_B2010": "燃起火来。"
            }
        },
        "0_B2020": {
            "name": "攻击 11",
            "text": {
                "0_B2020": "强大、聪明的伊斯卡利！"
            }
        },
        "0_B2030": {
            "name": "攻击 12",
            "text": {
                "0_B2030": "犹如美丽的战士！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "为了居住在这个城市的人们……！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "兴奋就到此为止了，哦。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "想要见识，我新的筐体吗？"
            }
        },
        "0_B2410": {
            "name": "攻击 13",
            "text": {
                "0_B2410": "转身的托纳蒂乌！"
            }
        },
        "0_B2420": {
            "name": "攻击 14",
            "text": {
                "0_B2420": "过于华丽了，对吧！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "只有闯入才算夏天吧？"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "石之鳄！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "也要干力气活才行！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "现在是叩拜于祭坛之时，哦！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "去大道吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "羡慕，吗？"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "可以沉溺于我的，只有我的神官，哦。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "如你所见，我游泳的本事也是最棒的。 如果你想要的话，我愿意手把手教你，哦。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "给与污染水源之人以死亡。",
                "0_B801": "吾等乃是守护人类生活的意志！",
                "0_B802": "侵略者将被打飞到伊鲁维卡·亚特尔！",
                "0_B803": "——如何？　有没有看入迷呢？"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "阿兹特兰！",
                "0_B811": "巨大筐体换装，吾之心脏力量全开！",
                "0_B812": "世界最大的重型机械！　双神挖掘机！",
                "0_B813": "——哈啊，太棒了——"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "都因为你过于小看水流了！"
            }
        },
        "0_B880": {
            "name": "技能 5",
            "text": {
                "0_B880": "是想被冲走，吗？"
            }
        },
        "0_B890": {
            "name": "技能 6",
            "text": {
                "0_B890": "让你，多喝一点。"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "切断，手脚！"
            }
        },
        "0_B920": {
            "name": "攻击 8",
            "text": {
                "0_B920": "科约尔夏乌基！"
            }
        },
        "0_B930": {
            "name": "攻击 9",
            "text": {
                "0_B930": "去吧，奥塞洛特尔！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "等级提升了呢。",
                "0_G011": "我保证提供更为美好的假期。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "接下来会变成什么样的我，真令人期待。",
                "0_G021": "难得来一次沙漠，异国风情的服装也不错吧。",
                "0_G022": "褐色的皮肤与丝绸的面纱……",
                "0_G023": "越来越有魅力了，对吧？"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我换上了现代、高雅的装束。",
                "0_G031": "那句话怎么说来着，入乡随俗吗？",
                "0_G032": "是不是合适到令人目眩神迷了？",
                "0_G033": "呵呵。度假可不只有游泳，哦？"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "确定接下来的安排了吗？",
                "0_G111": "不准注意其他从者，好吗？"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "滋润度提升了。",
                "0_G121": "今年的我毫无疑问是世界遗产级别的，对吧。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日啊。",
                "0_H011": "那就去湖边吧。光在神殿深处可没意思。",
                "0_H012": "在夜晚空无一人的岸边，畅谈这一年发生的事——",
                "0_H013": "毕竟想对你说的话语多到湖水都满溢了。",
                "0_H014": "天亮之前可别想回去，好吗？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "现在正在，举办活动哦。",
                "0_H021": "一定要多赚一点哦，神官。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "空气好干燥啊。",
                "0_H031": "对皮肤不好，去提高一下湿度吧。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我不会强迫你……",
                "0_H041": "但身为神官，应该无论何时都要献上祈祷，对吧？"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "事到如今还需要确认什么和御主间的关系吗？",
                "0_H051": "你返回的地方，那就是我。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西和平时没什么差别。不过这次倒是多了个新兴趣。",
                "0_H061": "……标新立异的建筑也不赖呢。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西吗？",
                "0_H071": "是干燥的空气。",
                "0_H072": "唯独这方面让我动用了特拉洛克神的权能。",
                "0_H073": "还有嘛……",
                "0_H074": "对了。",
                "0_H075": "是把宝贵的水拿去做蠢事的女人，真是糟透了！　要禁止那种人进入啦，彻底禁止！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "啊啊……我也想在自己的都市里建一座宏伟的画框神殿……",
                "0_H081": "还有脚链型的美术馆……",
                "0_H082": "有圣杯的话就能实现了吗……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "这次我不会使用雨之神或战斗之神的名号。",
                "0_H091": "从头到尾都将身为特诺奇蒂特兰陪在你身边。",
                "0_H092": "……唔。一脸不够满意的表情呢。",
                "0_H093": "我很快就会让你说不出那种话来。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "你问我皮肤不会被晒黑吗？",
                "0_H101": "因为我是水属性的精灵，不会被晒黑。",
                "0_H102": "但灵基再临的话，应该就能变成褐色了。",
                "0_H103": "毕竟我原本出身南美嘛。",
                "0_H104": "……应该可以。但愿可以……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "你会保养自己的身体吗？",
                "0_H111": "玩了一整天后，睡前一定好好按摩哦。",
                "0_H112": "哪怕只是揉一揉小腿，也有效果。",
                "0_H113": "哈？　打算叫客房服务？",
                "0_H114": "你傻吗？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "写作强敌读作朋友。",
                "0_H121": "对我来说，著名观光胜地就是这样的存在。",
                "0_H122": "我必须周游世界上所有都市，了解它们的价值才行。",
                "0_H123": "如果比它们优秀，就能为阿兹特克人民的时间感到骄傲。如果不如它们，就要为报答他们的时间而学习。",
                "0_H124": "总之，拿不到第一我是不会善罢甘休的。",
                "0_H125": "无论是作为都市，还是作为恋人，对吧？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我大致了解现代观光胜地的质量了。",
                "0_H131": "娱乐设施固然需要，但最关键的还是建筑呢。",
                "0_H132": "能够一览那座都市特有的建筑群，畅想其历史才是观光真正的乐趣。",
                "0_H133": "明白了吗？",
                "0_H134": "……一定要认真培养我哦。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "哼，这下终于能和她一较高下。",
                "0_H171": "还问谁，当然是小刑……刑部姬啦。",
                "0_H172": "说到泳装和枪，肯定只有生存游戏了吧？"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "当我问尼基季奇究竟是豹子还是猫时，她总是用一如既往的笑容回答我『是战士哦』。",
                "0_H181": "真遗憾，呢。",
                "0_H182": "如果是豹战士的话，特斯卡特利波卡神一定很高兴吧。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "ＸＸ·Alter。这身充满夏日风情的小麦色皮肤，相当不错。",
                "0_H191": "或许应该倾尽全力跟你打一场。",
                "0_H192": "身为都市，总该经历一下与巨大机器人和机动兵器的战斗才行。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "女王都市系统……",
                "0_H201": "即便都市毁灭，只要基础的妖精作为心脏转生，都市也会随之重生……",
                "0_H202": "真是充满威胁的仪式，呢。",
                "0_H203": "库库尔坎也是，异闻带的文明也不容小觑，呢。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "我以前误会克莱恩小姐了。",
                "0_H211": "这次的服装就是接纳她的建议制作的。",
                "0_H212": "虽然她平时……可一旦投身工作，她的那种专注、气迫与信念，足以被称为勇者。",
                "0_H213": "年轻时一定是位充满了梦想与希望的美丽少女吧。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "英灵中虽然有很多自以为是的家伙，但也有值得尊敬的人。",
                "0_H221": "深爱土地、深爱人民、深爱雨水。",
                "0_H222": "……那身白色礼服犹如一双美丽的鸟儿。",
                "0_H223": "如果要与征氏姊妹一起出征的话，请务必叫上我，好吗？"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "偶尔会有一只圆滚滚的青鸟飞来，",
                "0_H231": "「这位姐姐真有魅力。如何，要不要和我一起去进攻死之溪谷呢。就像壮志雁云的独行侠那种。",
                "0_H232": "话说这里湿气好重啊。」",
                "0_H233": "像这样向我搭话，我可以把他碾碎吗？"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "我的别名是梅斯特利亚潘，月之湖。",
                "0_H241": "当我来到月面，确实会比其他从者更适应月球吧。",
                "0_H242": "结果便成了月面特诺奇蒂特兰，兔女郎形态。",
                "0_H243": "我不生气，我还挺喜欢的。",
                "0_H244": "毕竟——这很刺激，对吧？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "对我的神官来说理所当然，对吧。",
                "0_S011": "从者，特诺奇蒂特兰。",
                "0_S012": "这次不需要隐藏名字，所以一开始就报真正的名字了。",
                "0_S013": "因为用假名可提不了申请，对吧。",
                "0_S014": "[fontSize -]？　问我为什么是月之癌？",
                "0_S015": "……真是的。别忘了我的传说啊。",
                "0_S016": "我可是世界上最美的水上都市，月之湖哦。"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "这种程度就忍一下吧。"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "稍微干净点了吗？"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "欢迎来到夜之泳池。"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "消失吧！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "用新型的蹂躏。",
                "2_B051": "不会因为是垃圾就轻易消灭。",
                "2_B052": "在找到办法解决之前会好好折磨你！",
                "2_B053": "——月之海或许，也不赖。"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "想要的东西，全部没收♡"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "让你成为月亮的俘虏。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "竟然有那么多暴徒。 看来需要以维持治安为名义的调教，呢。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "与神官散步中。 胆敢妨碍神明恋爱之人，会死哦。"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "现在的我是守护、统治都市的存在。 你可以更为崇拜我，哦。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "兔子才是月面最强的生物。 没有兔耳的存在迟早会变成奴隶阶级，要有思想准备。"
            }
        },
        "2_B140": {
            "name": "受击 2",
            "text": {
                "2_B140": "真粗暴♡"
            }
        },
        "2_B150": {
            "name": "受击 1",
            "text": {
                "2_B150": "喂，住手！　耳朵要……！"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "损伤巨大。这是必须立刻修理的重大情况。 把我带去吧，好吗？"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "哪怕沦为兔子之躯，阿兹特克的尊严也不会改变。 ——我一定会报一箭之仇的。"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "强硬。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "大胆。"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "心急。"
            }
        },
        "2_B2010": {
            "name": "攻击 10",
            "text": {
                "2_B2010": "逼入绝境吧。"
            }
        },
        "2_B2020": {
            "name": "攻击 11",
            "text": {
                "2_B2020": "要上了，哦！"
            }
        },
        "2_B2030": {
            "name": "攻击 12",
            "text": {
                "2_B2030": "在那边，吗！"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "我的居民，不会给你的。"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "什么嘛。签证到期了啊。"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "立刻驱离。做好思想准备，哦？"
            }
        },
        "2_B2420": {
            "name": "攻击 14",
            "text": {
                "2_B2420": "梅斯特利亚潘！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "能逃得掉，吗？"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "要保持城市清洁！"
            }
        },
        "2_B430": {
            "name": "攻击 6",
            "text": {
                "2_B430": "要珍惜住家！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "真是顽固的污渍，呢！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "我要收缴市民税了。"
            }
        },
        "2_B490": {
            "name": "技能 4",
            "text": {
                "2_B490": "向我讨要吧。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "是被豢养至死，还是溺水而亡，自己选择，吧。"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "战斗结束，回收材料。 ……要用那些钱呼唤我，哦。"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "冠婚祭典，开始。",
                "2_B801": "仰望女神楚楚可怜的身姿吧！",
                "2_B802": "飞沫，震撼月亮！　重装甲决战都市！",
                "2_B803": "——可以收下这个画框吗？"
            }
        },
        "2_B810": {
            "name": "宝具 3",
            "text": {
                "2_B810": "找到高尔夫球了。",
                "2_B811": "应该可以玩吧？",
                "2_B812": "偶尔也该运动一下这身躯才行。",
                "2_B813": "双神挖掘机。",
                "2_B814": "——飞到月之肚脐了吗？"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "边边角角，彻底地！"
            }
        },
        "2_B880": {
            "name": "技能 5",
            "text": {
                "2_B880": "现在是休息时间，哦。"
            }
        },
        "2_B890": {
            "name": "技能 6",
            "text": {
                "2_B890": "泳池肯定比沙漠好，对吧？"
            }
        },
        "2_B910": {
            "name": "攻击 7",
            "text": {
                "2_B910": "给我，出去！"
            }
        },
        "2_B920": {
            "name": "攻击 8",
            "text": {
                "2_B920": "黑曜刀！"
            }
        },
        "2_B930": {
            "name": "攻击 9",
            "text": {
                "2_B930": "四处逃窜。"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "等级提升了呢。",
                "2_G011": "我保证提供更为美好的假期。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "有什么好惊讶的？",
                "2_G041": "这是月之正装，也是绝对统治者的证明。",
                "2_G042": "没错，是万人羡慕的月之兔女郎形态♡",
                "2_G043": "跳跃力变成２倍，耐孤独力减半，要求神官的贡献度变成３倍。",
                "2_G044": "愿意搬家吗？　愿意了吧？",
                "2_G045": "那请在这份文件上签字。是移居申请。",
                "2_G046": "我再也不会放跑你了，认命吧，好吗？"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "这次的都市开发要到此为止了，呢。",
                "2_G051": "竟然能在度假期间发现全新的可能性，不愧是我。",
                "2_G052": "……不过，最被这个形象震惊到的倒是我。",
                "2_G053": "月之湖的精灵竟然会变成兔子，连阿兹特克的神话中也没出现过。",
                "2_G054": "话说回来，据说最近似乎开始时兴远程工作，也就是居家办公了。",
                "2_G055": "既在公司任职又能留在家里，这真不错。",
                "2_G056": "要一直保持这样，哦。"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "确定接下来的安排了吗？",
                "2_G111": "不准注意其他从者，好吗？"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "滋润度提升了。",
                "2_G121": "今年的我毫无疑问是世界遗产级别的，对吧。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "生日啊。",
                "2_H011": "那就去湖边吧。光在神殿深处可没意思。",
                "2_H012": "在夜晚空无一人的岸边，畅谈这一年发生的事——",
                "2_H013": "毕竟想对你说的话语多到湖水都满溢了。",
                "2_H014": "天亮之前可别想回去，好吗？"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "现在正在，举办活动哦。",
                "2_H021": "一定要多赚一点哦，神官。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "空气好干燥啊。",
                "2_H031": "对皮肤不好，去提高一下湿度吧。"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "我不会强迫你……",
                "2_H041": "但身为神官，应该无论何时都要献上祈祷，对吧？"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "事到如今还需要确认什么和御主间的关系吗？",
                "2_H051": "你返回的地方，那就是我。"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "喜欢的东西和平时没什么差别。不过这次倒是多了个新兴趣。",
                "2_H061": "……标新立异的建筑也不赖呢。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西吗？",
                "2_H071": "是干燥的空气。",
                "2_H072": "唯独这方面让我动用了特拉洛克神的权能。",
                "2_H073": "还有嘛……",
                "2_H074": "对了。",
                "2_H075": "是把宝贵的水拿去做蠢事的女人，真是糟透了！　要禁止那种人进入啦，彻底禁止！"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "啊啊……我也想在自己的都市里建一座宏伟的画框神殿……",
                "2_H081": "还有脚链型的美术馆……",
                "2_H082": "有圣杯的话就能实现了吗……"
            }
        },
        "2_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H090": "这次我不会使用雨之神或战斗之神的名号。",
                "2_H091": "从头到尾都将身为特诺奇蒂特兰陪在你身边。",
                "2_H092": "……唔。一脸不够满意的表情呢。",
                "2_H093": "我很快就会让你说不出那种话来。"
            }
        },
        "2_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H100": "你问我皮肤不会被晒黑吗？",
                "2_H101": "因为我是水属性的精灵，不会被晒黑。",
                "2_H102": "但灵基再临的话，应该就能变成褐色了。",
                "2_H103": "毕竟我原本出身南美嘛。",
                "2_H104": "……应该可以。但愿可以……"
            }
        },
        "2_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H110": "你会保养自己的身体吗？",
                "2_H111": "玩了一整天后，睡前一定好好按摩哦。",
                "2_H112": "哪怕只是揉一揉小腿，也有效果。",
                "2_H113": "哈？　打算叫客房服务？",
                "2_H114": "你傻吗？"
            }
        },
        "2_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H120": "写作强敌读作朋友。",
                "2_H121": "对我来说，著名观光胜地就是这样的存在。",
                "2_H122": "我必须周游世界上所有都市，了解它们的价值才行。",
                "2_H123": "如果比它们优秀，就能为阿兹特克人民的时间感到骄傲。如果不如它们，就要为报答他们的时间而学习。",
                "2_H124": "总之，拿不到第一我是不会善罢甘休的。",
                "2_H125": "无论是作为都市，还是作为恋人，对吧？"
            }
        },
        "2_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H130": "我大致了解现代观光胜地的质量了。",
                "2_H131": "娱乐设施固然需要，但最关键的还是建筑呢。",
                "2_H132": "能够一览那座都市特有的建筑群，畅想其历史才是观光真正的乐趣。",
                "2_H133": "明白了吗？",
                "2_H134": "……一定要认真培养我哦。"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "哼，这下终于能和她一较高下。",
                "2_H171": "还问谁，当然是小刑……刑部姬啦。",
                "2_H172": "说到泳装和枪，肯定只有生存游戏了吧？"
            }
        },
        "2_H180": {
            "name": "对话 5",
            "text": {
                "2_H180": "当我问尼基季奇究竟是豹子还是猫时，她总是用一如既往的笑容回答我『是战士哦』。",
                "2_H181": "真遗憾，呢。",
                "2_H182": "如果是豹战士的话，特斯卡特利波卡神一定很高兴吧。"
            }
        },
        "2_H190": {
            "name": "对话 6",
            "text": {
                "2_H190": "ＸＸ·Alter。这身充满夏日风情的小麦色皮肤，相当不错。",
                "2_H191": "或许应该倾尽全力跟你打一场。",
                "2_H192": "身为都市，总该经历一下与巨大机器人和机动兵器的战斗才行。"
            }
        },
        "2_H200": {
            "name": "对话 7",
            "text": {
                "2_H200": "女王都市系统……",
                "2_H201": "即便都市毁灭，只要基础的妖精作为心脏转生，都市也会随之重生……",
                "2_H202": "真是充满威胁的仪式，呢。",
                "2_H203": "库库尔坎也是，异闻带的文明也不容小觑，呢。"
            }
        },
        "2_H210": {
            "name": "对话 8",
            "text": {
                "2_H210": "我以前误会克莱恩小姐了。",
                "2_H211": "这次的服装就是接纳她的建议制作的。",
                "2_H212": "虽然她平时……可一旦投身工作，她的那种专注、气迫与信念，足以被称为勇者。",
                "2_H213": "年轻时一定是位充满了梦想与希望的美丽少女吧。"
            }
        },
        "2_H220": {
            "name": "对话 9",
            "text": {
                "2_H220": "英灵中虽然有很多自以为是的家伙，但也有值得尊敬的人。",
                "2_H221": "深爱土地、深爱人民、深爱雨水。",
                "2_H222": "……那身白色礼服犹如一双美丽的鸟儿。",
                "2_H223": "如果要与征氏姊妹一起出征的话，请务必叫上我，好吗？"
            }
        },
        "2_H230": {
            "name": "对话 10",
            "text": {
                "2_H230": "偶尔会有一只圆滚滚的青鸟飞来，",
                "2_H231": "「这位姐姐真有魅力。如何，要不要和我一起去进攻死之溪谷呢。就像壮志雁云的独行侠那种。",
                "2_H232": "话说这里湿气好重啊。」",
                "2_H233": "像这样向我搭话，我可以把他碾碎吗？"
            }
        },
        "2_H240": {
            "name": "对话 11",
            "text": {
                "2_H240": "我的别名是梅斯特利亚潘，月之湖。",
                "2_H241": "当我来到月面，确实会比其他从者更适应月球吧。",
                "2_H242": "结果便成了月面特诺奇蒂特兰，兔女郎形态。",
                "2_H243": "我不生气，我还挺喜欢的。",
                "2_H244": "毕竟——这很刺激，对吧？"
            }
        }
    },
    "ChrVoice_ids": [
        "2301000"
    ]
}
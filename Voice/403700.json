{
    "id": 403700,
    "collectionNo": 296,
    "name": "尼莫",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "特里同——！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "打飞吧！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "绝不手下留情！"
            }
        },
        "0_B040": {
            "name": "额外攻击 2",
            "text": {
                "0_B040": "水兵们，出击！",
                "0_B5440": "预备， 鹦鹉螺号滑行！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "起航！",
                "0_B051": "发现目标！",
                "0_B052": "本舰正上方！",
                "0_B053": "内燃机，最大输出！",
                "0_B054": "生命保护最大化！",
                "0_B055": "蛋卷烧，完成！",
                "0_B056": "好，急速上浮！",
                "0_B057": "吾等乃是海神之矛！",
                "0_B058": "吾将远征，鹦鹉螺的大冲角！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "嘿咻。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "要补充氧气了，稍等片刻。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "发现敌影。全员，各就各位！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "声呐出现反应。各位，准备好了吗？"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "战斗结束。 继续航海吧，御主。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "……战斗得不错。 偶尔恢复成人鱼或许也…… 不，太羞耻了，还是算了。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "尼莫船长不屈服……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "将我的帽子……和鹦鹉螺一起……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "非常抱歉。撤退……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "能看到。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "能听到。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "我知道。"
            }
        },
        "0_B2010": {
            "name": "攻击 9",
            "text": {
                "0_B2010": "这边这边！"
            }
        },
        "0_B2020": {
            "name": "攻击 10",
            "text": {
                "0_B2020": "看招～！ 抓紧～！ 好重好重！"
            }
        },
        "0_B2030": {
            "name": "攻击 11",
            "text": {
                "0_B2030": "哎～！ 船长太会差遣人了～！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "系好安全带！ 鹦鹉螺号要上了！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "可以。是时候让你见识我的真本事了。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "前进！　我就在等这刻！ ……啊，不，没什么。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "让大海变得干净哦！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "伟大的父亲啊！"
            }
        },
        "0_B440": {
            "name": "额外攻击 4",
            "text": {
                "0_B440": "小型舰，起航！",
                "0_B2440": "预备， 鹦鹉螺号滑行！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "好。海风真不错。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "想听海鸥的歌声吗？"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "航海要冷静。接触要洒脱。明白了吗？"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "唉……好想发射鱼雷啊……"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "喵喵叫的是黑尾鸥。 不过也是海鸥的同伴。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "特里同引擎，启动！",
                "0_B801": "瞄准好啦！",
                "0_B802": "计算完成～！",
                "0_B803": "欧啦，转起来吧！",
                "0_B804": "舰内设备锁定！",
                "0_B805": "布丁可以吗？",
                "0_B806": "允许！",
                "0_B807": "尼莫船长决不原谅扰乱大海之人！",
                "0_B808": "出发——",
                "0_B809": "吾将远征，鹦鹉螺的大冲角！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "最终确认，点名！",
                "0_B811": "大致对了！",
                "0_B812": "大致没问题～。",
                "0_B813": "大致可行！",
                "0_B814": "大致完美！",
                "0_B815": "不做评论！",
                "0_B816": "啊～，真拿你们没办法！",
                "0_B817": "孤注一掷吧，",
                "0_B818": "吾将远征，鹦鹉螺的大冲角！"
            }
        },
        "0_B910": {
            "name": "攻击 8",
            "text": {
                "0_B910": "鱼雷，发射——！",
                "0_B930": "耶～！ 动手啦～！ 结束啦～！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "性能提升。 像丝足鱼一样顽强。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "像海鳝一样巍然不动。",
                "0_G021": "像鳄鱼一样不解风情。",
                "0_G022": "……反正我也没报什么期待，没什么大不了的。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "……怎么了。不就是换套衣服而已嘛，不需要发表什么感想哦。",
                "0_G031": "毕竟我有些明白你是认真的了，所以我也会稍微拿出一点干劲来。",
                "0_G032": "不要一直打量我啦。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "……好怀念啊。这斗篷以前是在典礼时使用的。",
                "0_G041": "虽然始终摆出一副船长的架子，但尼莫一般是沉默寡言性格阴暗的。",
                "0_G042": "若非特殊的时候，是不会如此盛装打扮的。",
                "0_G043": "怎么样？　还合适吗，御主？"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "这么大清早就来望风，辛苦啦。",
                "0_G051": "习惯鹦鹉螺号的生活了吗？　有些冷吧？　来这边坐一会儿吧。",
                "0_G052": "你看，太阳从水平线升起了。我喜欢这种在安静的雨中庄严切换的昏暗破晓。",
                "0_G053": "以前我都是和鸟儿们一起眺望的，今后和你一起眺望或许也不错。",
                "0_G054": "虽然不知道既是特里同也是尼莫的我能坚持到什么时候——",
                "0_G055": "但直到你的航海结束的那刻为止，我们都像这样一起看海吧。",
                "0_G056": "——我一定会保护你，御主。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "谢谢。",
                "0_G111": "……下次要不要去看看我喜欢的海底火山？"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "呜哇，等级提升了呀！",
                "0_G121": "我们什么事都可以帮忙，有事随时吩咐哦！"
            }
        },
        "0_G130": {
            "name": "升级 4",
            "text": {
                "0_G130": "嗨，你好。",
                "0_G131": "灵基的成长也提升了我的干劲。",
                "0_G132": "呵呵呵，这样研究应该也能有所进展了。"
            }
        },
        "0_G140": {
            "name": "升级 5",
            "text": {
                "0_G140": "非常感谢，御主。",
                "0_G141": "药不够的时候我可以开，您随时都可以来医务室。"
            }
        },
        "0_G150": {
            "name": "升级 6",
            "text": {
                "0_G150": "烘焙坊等级提升♡",
                "0_G151": "请随时享用刚出炉的美味面包～☆"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "我知道，今天是你的生日吧？",
                "0_H011": "这一年来辛苦了你了，御主。",
                "0_H012": "虽然算不上是犒劳……",
                "0_H013": "今天就稍微放纵一下吧。",
                "0_H014": "来，说出你的愿望吧。",
                "0_H015": "我可以带你去世界上任何一个港口。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "说是现在正在举办活动哦。 总之，记得适可而止哦。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "到巡航时间了哦，御主。",
                "0_H031": "在海上漂流一会儿吧。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "这是非常合理的，另外，符合道德的命令我会照办。",
                "0_H041": "残酷的作战我不会听从。",
                "0_H042": "胡来的作战……这个嘛，应该要看情况了吧。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我不善言辞，所以不会说非必要的话。",
                "0_H051": "并不是讨厌御主，这点还请不要误会。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西 1",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "自由的大海、海螺的造型、音乐演奏与建造基地。",
                "0_H062": "甜点喜欢布丁。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西 1",
            "text": {
                "0_H070": "讨厌的东西是侵略与欺负弱者，偷渡与老鼠。",
                "0_H071": "凯妮斯……与其说讨厌她，倒不如说是不擅长应付她吧。"
            }
        },
        "0_H080": {
            "name": "关于圣杯 1",
            "text": {
                "0_H080": "就算有圣杯我也不会用。因为会把所有东西都变成黄金吧？",
                "0_H081": "金色的鹦鹉螺号也太没品了吧。",
                "0_H082": "这种一目了然的宝物还是给伊阿宋吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "被希翁召唤的我不知为何成了你的从者。命运还真是难料呢。",
                "0_H091": "……不，这种情况下，应该说希翁的召唤方法太奇怪了吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "只要遵守舰内的规矩，我就不会有意见哦。",
                "0_H101": "因为我虽然身负船长的立场，但我也是热爱自由的特里同。",
                "0_H102": "……这点你该不会忘了吧？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "多亏了希翁亲授的分割思考，我可以创造多个自己的分身。",
                "0_H111": "水兵们是喜欢打杂的我，教授是埋头兴趣的我，类似这种。",
                "0_H112": "每种性格都不一样，但都是尼莫。和他们打好关系或许有好处哦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "男性也会有女性的嗜好，女性也会有男性的嗜好。",
                "0_H121": "与此相同，尼莫系列分成了少年体与少女体。",
                "0_H122": "至于谁属于哪种，我就不说了。",
                "0_H123": "……你应该明白吧？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "等在下个港口停泊时，陪我去市场。",
                "0_H131": "靠我一个人能买的东西是有极限的，你差不多也该参加鹦鹉螺的整备了。",
                "0_H132": "……真是的。不这么说你就不陪我一起去，你究竟有没有身为我御主的自觉啊？"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "船长所需的素质是稳定性。",
                "0_H171": "关键时候才有能力这种事，与其说是航海，还不如说是冒险。",
                "0_H172": "……不过，也正是这部分招水兵们喜爱吧。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "巴沙洛缪的谨慎非常可靠。",
                "0_H181": "长得帅气，做事一丝不苟，船舶起名品味也好。",
                "0_H182": "……但是，为什么那么废柴呢……"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "呜哇，凯妮斯也在啊！？",
                "0_H191": "讨厌讨厌，我们讨厌那家伙！　粗野、嗓门大，还不打扫卫生！",
                "0_H192": "好嘞，下次把她的枪偷换成拖把吧！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "哎，那就是俄里翁啊。名不虚传，肌肉发达。",
                "0_H201": "……嘁。",
                "0_H202": "真好……真羡慕……如果我也有男性的身体就好了……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "一看到她，不仅会感到骄傲，还会感到愧疚。",
                "0_H211": "对我来说，她既是我罪孽的证明，也是我的弹劾者。",
                "0_H212": "……啊，摔倒了。",
                "0_H213": "呜哇，端着的红茶泼到头上了。连锁推倒了垃圾箱。",
                "0_H214": "……嗯。从各种方面来说，都无法从她身上移开视线呢。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "达·芬奇小姐让我学到了很多知识。",
                "0_H221": "Shadow Border和鹦鹉螺号。既然控制了陆地与海洋，那剩下的课题就是天空了吧。",
                "0_H222": "……但是，稍微有些担心呢。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "这里好像有个冠以海中恶魔为名的从者呢。",
                "0_H231": "当着我的面竟敢夸下如此海口……",
                "0_H232": "那是什么！？",
                "0_H233": "竟然将企鹅像鱼雷一样发射出去了……！？",
                "0_H234": "——可恶，竟然还有这招！"
            }
        },
        "0_H250": {
            "name": "对话 11",
            "text": {
                "0_H250": "御主。你知道对人类来说，海底是比宇宙还要未知的世界吗？",
                "0_H251": "知道的话就好办了。了解海洋的我要说的话只有一句。",
                "0_H252": "——可不能把章鱼摆上餐桌哦。"
            }
        },
        "0_H260": {
            "name": "对话 12",
            "text": {
                "0_H260": "……怎么回事，忽然打了个冷战。",
                "0_H261": "就像成了被摆在市场上的金枪鱼，或是被摆在盘子上的沙丁鱼……",
                "0_H262": "哎？　通过吃下人鱼获得不老不死的修女？",
                "0_H263": "——好，全速逃离。"
            }
        },
        "0_H270": {
            "name": "对话 13",
            "text": {
                "0_H270": "梵高也在啊。",
                "0_H271": "……那太好了。",
                "0_H272": "我和她都是名为幻灵的模糊不清的存在。",
                "0_H273": "希望你能维系住她，不要让她化为泡沫消失哦。"
            }
        },
        "0_H280": {
            "name": "对话 14",
            "text": {
                "0_H280": "库库尔坎虽然看上去自由奔放，但内在十分细腻。",
                "0_H281": "对于攻击Storm Border的那件事，她一早就跑来向我道歉了，之后在我面前时也会显得心情低落哦。",
                "0_H282": "虽然的确曾一度被她击沉，但之后她也帮了我们，其实不用那么在意啦。"
            }
        },
        "0_H290": {
            "name": "对话 15",
            "text": {
                "0_H290": "嗯，我们和Protea是一起泡海水浴的伙伴！",
                "0_H291": "海洋里巨大的生物一点也不稀罕，况且Protea的游泳方式对鱼儿们很友善，我非常喜欢！",
                "0_H292": "偶尔还会玩玩怪兽游戏～！　嘎嗷～！",
                "0_H293": "啊，但是Alter稍微有些欺负人，一定要小心哦……",
                "0_H294": "我们中有好几个都遭遇过很可怕的经历哦……"
            }
        },
        "0_H300": {
            "name": "对话 16",
            "text": {
                "0_H300": "特诺奇蒂特兰经常向我发起挑战……",
                "0_H301": "为什么她那么针对我呢……",
                "0_H302": "哎？　对优秀建筑物的对抗意识？　只是因为作为竞争对手无法无视？",
                "0_H303": "这样啊。既然如此，不奉陪的话，未免会有些失礼呢。"
            }
        },
        "0_H3060": {
            "name": "喜欢的东西 2",
            "text": {
                "0_H3060": "喜欢的东西啊？",
                "0_H3061": "我喜欢冰凉地面的触感，蝠鲼的游泳方式，或是真空管之类的。",
                "0_H3062": "但最令我高兴的，还是有人能理睬我吧。",
                "0_H3063": "……要保护我哦？"
            }
        },
        "0_H3070": {
            "name": "讨厌的东西 2",
            "text": {
                "0_H3070": "讨厌的东西？　没什么啦。",
                "0_H3071": "啊，但船长那家伙令人火大。",
                "0_H3072": "一旦进港，那家伙就会偷偷外出去吃甜点哦！？",
                "0_H3073": "我明明还在舔着岩盐照顾引擎，这也太不公平了吧！？"
            }
        },
        "0_H3080": {
            "name": "关于圣杯 2",
            "text": {
                "0_H3080": "哎～，什么不要圣杯啊，船长真会装模作样！",
                "0_H3081": "我觉得圣杯非常棒哦。",
                "0_H3082": "因为可以变出无限的面粉吧？",
                "0_H3083": "这样就可以做好多～～好多美味的面包了～！"
            }
        },
        "0_H310": {
            "name": "对话 17",
            "text": {
                "0_H310": "嗯，报告～。",
                "0_H311": "这次依照梅柳齐娜小姐的需求，在Storm Border的甲板上新设置了旋转式弹射器～。",
                "0_H312": "是希翁小姐和我负责的，嗯。",
                "0_H313": "虽然没有表现出来，但船长内心应该也很兴奋～。",
                "0_H314": "毕竟航母，就是浪漫啊～。"
            }
        },
        "0_H320": {
            "name": "对话 18",
            "text": {
                "0_H320": "深海是比宇宙更难观测、调查的世界。那里存在着人类常识无法解释的生物。",
                "0_H321": "我曾在海底见过各式各样的生命。",
                "0_H322": "比如已经可以算发电站的巨大水母巴西琉斯，以及自远古时期活到现在巨大种族穆尔克拉肯。",
                "0_H323": "遭遇时都没有发生战斗，勉强得以逃离。",
                "0_H324": "那家伙也和它们一样。",
                "0_H325": "有需要的时候尽管叫我。即使将灵子鱼雷全部射空，我也会去救你的。"
            }
        },
        "0_H330": {
            "name": "对话 19",
            "text": {
                "0_H330": "麻烦御主替我转告特诺奇蒂特兰。",
                "0_H331": "潜航艇既不会变成人形，更不会合体。",
                "0_H332": "她为什么唯独那么敌视潜航艇呢？",
                "0_H333": "不是还有奥德修斯、欧罗巴大人他们吗？"
            }
        },
        "0_H3560": {
            "name": "喜欢的东西 3",
            "text": {
                "0_H3560": "我来我来～！",
                "0_H3561": "喜欢的东西是整备洗涤打扫浴室搬运鱼雷！",
                "0_H3562": "还有要为了午饭而活～！"
            }
        },
        "0_H3570": {
            "name": "讨厌的东西 3",
            "text": {
                "0_H3570": "讨厌的东西吗？　我不太擅长应付讨厌药的人。熬夜的人也是。",
                "0_H3571": "御主是不会这么做的吧？"
            }
        },
        "0_I010": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 7",
            "text": {
                "0_I010": "呀～，欢迎光临。",
                "0_I011": "欢迎来到兑换所，御主。美味的面包也有哦～。"
            }
        },
        "0_I020": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 2",
            "text": {
                "0_I020": "本舰从现在开始进入警戒态势。",
                "0_I021": "为了准备战斗，要做好从者的强化。"
            }
        },
        "0_I030": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 18",
            "text": {
                "0_I030": "感谢您的购买。",
                "0_I031": "……哎嘿嘿，感觉会忍不住笑起来呢。",
                "0_I032": "我一直很想说一次这种话☆"
            }
        },
        "0_I040": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 19",
            "text": {
                "0_I040": "话说，你有好好吃饭吗？",
                "0_I041": "没有食欲的时候记得来厨房哦。",
                "0_I042": "我会请你吃刚烤好的面包。"
            }
        },
        "0_I060": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 20",
            "text": {
                "0_I060": "干得漂亮。多亏了你的奋斗，作战结束了。我们胜利了。",
                "0_I061": "……客套话就说到这里吧。",
                "0_I062": "虽然发生了不少问题，但这趟航海依然让人受益良多。",
                "0_I063": "作为船长，我感到很自豪哦，御主。"
            }
        },
        "0_I070": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 21",
            "text": {
                "0_I070": "等鹦鹉螺号返回船坞之后，这兑换所就要关闭了。",
                "0_I071": "记得把剩下的物资一个不剩全部兑换掉哦。",
                "0_I072": "……不过，我也不觉得你会把在虚数空间获得的道具留在身边吧？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "……虽说事到如今再自我介绍未免有些奇怪……",
                "0_S011": "既是海神波塞冬与安菲特里忒之子特里同，也是驾驶潜水艇鹦鹉螺号的“子虚乌有之人”……",
                "0_S012": "虽然有些复杂，但你叫我尼莫船长就可以了。请多关照，御主。"
            }
        }
    },

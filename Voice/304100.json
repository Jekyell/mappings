{
    "id": 304100,
    "collectionNo": 279,
    "name": "凯妮斯",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "吃我这招！！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "这招……",
                "0_B041": "如何！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "飞翔吧，飞翔吧，飞翔吧……！",
                "0_B051": "无论多远，我都能到达！",
                "0_B052": "对吧！",
                "0_B053": "——飞翔吧，吾之金色大翼！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "我会做给你看！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "我很强！　你会死！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "我是！　神灵！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "我这儿早就做好准备了。动手吧。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "哈哈！　是我赢了！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "你就死在那里吧！"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "哎哟！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "可恶……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "嘁……什么嘛……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "继续……来啊……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "噢。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "我觉得可以。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "烦死了。"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "哈哈哈！　就让你见识一下吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "明白了！　御主！"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "我已经忍不住了！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "去死吧！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "看招！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "吃我这招……",
                "0_B441": "然后去死吧你这该死的！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "剑刃对我一概没有作用！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "杀！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "哈哈哈哈哈哈，杀！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "这是理所当然的结果吧！"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "没有人！　能赢过我！"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "根本没用！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "不错不错！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "可恶……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "我，我……怎么会……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "你看到了吗，波塞冬！",
                "0_B801": "无论是谁，都无法触及我的高度！",
                "0_B802": "振翅高飞吧！",
                "0_B803": "——飞翔吧，吾之金色大翼！！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "波塞冬……",
                "0_B841": "风暴！！！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "是供品吗！　哈哈！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "什么？　发生什么了吗？",
                "0_G021": "啊啊，灵基再临。外形有时候也不会改变呢。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "嗯，我更喜欢这个样子。",
                "0_G031": "那就再次请你多关照啦。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "我的确是神灵，但无论你为我做什么，都不会得到灵验的哦！",
                "0_G111": "你还是有这点觉悟为好。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "真不错，进贡啊。",
                "0_G121": "你是那啥吗，那种类型的人吗？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日？　谁的？",
                "0_H011": "……你的？",
                "0_H012": "哈？",
                "0_H013": "…………这……这种事你应该早点说啦！",
                "0_H014": "真是的。那总得准备些什么才行吧……啊……但是我什么都没有呢……",
                "0_H015": "好，我明白了。我会陪你一整天。",
                "0_H016": "虽然不至于到言听计从的地步，但我可以一定程度内听你的话哦？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "现在好像在办什么有意思的事呢！",
                "0_H021": "喂，御主！",
                "0_H022": "这是叫……活动吧。既然参加，就要全力以赴哦！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "闲～得～慌～。",
                "0_H031": "出门转转吧，喂，御主！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "从者和御主的关系真是简单易懂。",
                "0_H041": "来，向我下令吧。",
                "0_H042": "我会把碍事的家伙全部排除。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我觉得所谓的主从关系不太准确……",
                "0_H051": "我隐约觉得，御主和从者似乎不是那样的存在。",
                "0_H052": "应该更为……该怎么说呢……",
                "0_H053": "总之就是这种感觉。",
                "0_H054": "究竟是为什么呢？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "鸟非常棒吧。我喜欢。",
                "0_H061": "……不是啦，我说的不是鸡肉。你这笨蛋。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我不太喜欢海。",
                "0_H071": "海给我去死。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯啊……",
                "0_H081": "我不是很理解。",
                "0_H082": "不过，应该可以用来盛酒吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "啊？",
                "0_H091": "我不是告诉你我是神灵凯妮斯了嘛。",
                "0_H092": "哈？　……传承中的僭主凯妮斯不是神明？",
                "0_H093": "确实如此。我曾经自称过王，但没有自称过神明。",
                "0_H094": "你问我那为什么？",
                "0_H095": "这很简单。",
                "0_H096": "话说，有酒或是肉吗？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "……呼，当世的酒也不赖嘛。",
                "0_H101": "然后呢，刚才说到哪儿来着？",
                "0_H102": "啊啊，是神灵什么的吧。",
                "0_H103": "这很简单。",
                "0_H104": "因为我获得了那个狗屎海混蛋……海神波塞冬的佑护。是神的力量。",
                "0_H105": "而且，那家伙居然把自己的一部分都给了我。是三叉戟。海神的三叉戟。相当于海神波塞冬的象征啦。",
                "0_H106": "把那玩意儿交给了我就意味着……",
                "0_H107": "虽说让人恼火，但我实质上已经是海洋之神了。",
                "0_H108": "哈哈哈！　你说是吧！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "波塞冬的事？",
                "0_H111": "我才不聊呢。",
                "0_H112": "为什么我要聊那个狗屎家伙的事……",
                "0_H113": "啊？",
                "0_H114": "既然得到了他的佑护和三叉戟，我们的关系一定非同一般？",
                "0_H115": "哈哈哈哈哈哈哈哈哈哈。",
                "0_H116": "小心我杀了你。你不准再多问一句了。否则我会杀了你哦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我……",
                "0_H121": "喜欢看海。这是很久以前的事了……",
                "0_H122": "但是，某一天，我在海边……",
                "0_H123": "………………不，没什么。",
                "0_H124": "我……色萨利的女人凯妮斯，一定已经在那天死了……",
                "0_H125": "然后时而被称为僭主，时为被称为英雄之一的凯纽斯诞生了吧。",
                "0_H126": "然而，之所以像这样……",
                "0_H127": "以凯妮斯之名现界——",
                "0_H128": "究竟是为什么呢……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "哈？　……像女人？　我？　你搞错了吧？",
                "0_H131": "哈哈哈哈。你这话说得真有意思。",
                "0_H132": "你试着再说一次看看。我会在你张口之前把你的脑袋砍掉。",
                "0_H133": "…………唔。不是在开玩笑？",
                "0_H134": "这样啊。那么，那个……应该是凯妮斯的残留之类的吧。",
                "0_H135": "不，我现在的真名确实是凯妮斯。真是的。实在是太麻烦了。",
                "0_H136": "……你那是什么表情。喂。喂，我说你。刨根问题或是想象都随你的便……",
                "0_H137": "但唯独不要表示同情哦，御主。",
                "0_H138": "如果被自己人同情——",
                "0_H139": "我会忍不住杀掉你的。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "……哎，你们在这儿的关系是这种感觉的啊。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "哟，美狄亚！",
                "0_H181": "我当然一眼就认出来了啦。",
                "0_H182": "哈？　和平时的差别？　有吗？？"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "你这样子还真是……",
                "0_H191": "不，这模样也不赖。",
                "0_H192": "严格说来，我还挺喜欢的。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "啊啊，原来如此。军神阿瑞斯的眷属吗？",
                "0_H201": "哎～！　是吗是吗！"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "……你这家伙，是什么？",
                "0_H211": "让我莫名有种不安……你这家伙究竟是什么？"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "神明这种存在真是讨厌呢。",
                "0_H221": "不管是老的还是新的，没一个好东西。",
                "0_H222": "话说御主，我能杀了他们吗？"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "………………居然自愿变成女人的样子？",
                "0_H231": "什么？",
                "0_H232": "外形无关紧要，对你来说一切都是随心所欲的？",
                "0_H233": "哎。",
                "0_H234": "呃，哎……迦勒底还真有些奇怪的家伙呢。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "噢，莫德雷德！",
                "0_H241": "我们去模拟装置吧！",
                "0_H242": "不，我没打算找豆芽菜那家伙啦。"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "不要移开视线啦，我在说你啦，说你。",
                "0_H251": "啊？……暴露？",
                "0_H252": "这……这和暴露没关系吧。反正我是不死身，穿什么都没关系吧！",
                "0_H253": "比起这个，你好歹也算是个王吧。",
                "0_H254": "我的意思是你应该打起点精神来。",
                "0_H255": "啊，算了。",
                "0_H256": "跟我来。去模拟装置啦，模拟装置。我要从头开始锻炼你。"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "哎～，坂田金时啊。",
                "0_H261": "那家伙……感觉不太一样。",
                "0_H262": "虽说我不知道他本性如何，但出身完全就是个少爷。",
                "0_H263": "我是不会让他加入迦勒底不良组的。"
            }
        },
        "0_H270": {
            "name": "对话 14",
            "text": {
                "0_H270": "惨了，是玛尔达！",
                "0_H271": "来了个啰嗦的家伙。那家伙对各种问题都很啰嗦……",
                "0_H272": "好嘞，开溜！"
            }
        },
        "0_H280": {
            "name": "对话 15",
            "text": {
                "0_H280": "武藏？",
                "0_H281": "我知道。是个啰嗦、天真、胡闹的家伙。",
                "0_H282": "虽说我不承认人类的剑士，但那家伙算是个合格的英灵哦。",
                "0_H283": "不过，从和我很投缘这点来看，那家伙也不是个正经的家伙吧！"
            }
        },
        "0_H300": {
            "name": "对话 16",
            "text": {
                "0_H300": "找到你了哦，特～里～同～！",
                "0_H301": "好不容易见到你，让我们叙叙旧——",
                "0_H302": "哈！？　你的样子是怎么回事！？　完全是个小孩子吧！？　而且还穿着女孩子的衣服！",
                "0_H303": "哈？　只是制服？",
                "0_H304": "看上去像女孩子纯属我的爱好！？",
                "0_H305": "怎么可能啦，真是的，我一点都不觉得你这样帅气！"
            }
        },
        "0_H310": {
            "name": "对话 17",
            "text": {
                "0_H310": "不，不对吧……人鱼应该更加……",
                "0_H311": "总之不对吧……",
                "0_H312": "可恶，感觉一看到梵高心情就会低落……",
                "0_H313": "难道我在为那家伙担心吗……？"
            }
        },
        "0_H320": {
            "name": "对话 18",
            "text": {
                "0_H320": "肯定不对吧，你是人鱼什么的肯定有问题吧！？",
                "0_H321": "连希腊的海里都很难找到你这种程度的怪物啊！？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我是凯妮斯。",
                "0_S011": "真名凯妮斯，是神灵。",
                "0_S012": "…………嘁。什么凯妮斯啊。别开玩笑了。"
            }
        },
        "0_S110": {
            "name": "召唤 (第2部5章奥林波斯篇通关后)",
            "text": {
                "0_S110": "真名凯妮斯，是神灵。",
                "0_S111": "好像是所谓的Lancer。请多关照啦。",
                "0_S112": "……唔。",
                "0_S113": "怎么，是你啊！",
                "0_S114": "哈哈。什么嘛，你果然需要我啊？",
                "0_S115": "真拿你没办法，我愿意奉陪！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "波塞冬……",
                "2_B041": "风暴！！！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "更高，更高，再高一些……！",
                "2_B051": "我甚至能到达繁星之海！",
                "2_B052": "振翅飞翔！",
                "2_B053": "——飞翔吧，吾之金色大翼！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "寄宿于我的双臂上吧，力量！"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "我的武勇，不靠武器！"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "——我真的是认真的。 上吧。"
            }
        },
        "2_B110": {
            "name": "开始 2",
            "text": {
                "2_B110": "强者——僭主凯妮斯，出击！"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "这是理所当然的结果。"
            }
        },
        "2_B130": {
            "name": "胜利 2",
            "text": {
                "2_B130": "胜利的滋味如何，御主。"
            }
        },
        "2_B160": {
            "name": "无法战斗 1",
            "text": {
                "2_B160": "抱歉……我……"
            }
        },
        "2_B170": {
            "name": "无法战斗 2",
            "text": {
                "2_B170": "明明正进入佳境……"
            }
        },
        "2_B180": {
            "name": "指令卡 1",
            "text": {
                "2_B180": "噢。"
            }
        },
        "2_B190": {
            "name": "指令卡 2",
            "text": {
                "2_B190": "这样也不错！"
            }
        },
        "2_B200": {
            "name": "指令卡 3",
            "text": {
                "2_B200": "偶尔可以。"
            }
        },
        "2_B210": {
            "name": "宝具卡 1",
            "text": {
                "2_B210": "嗯，我会做给你看！"
            }
        },
        "2_B220": {
            "name": "宝具卡 2",
            "text": {
                "2_B220": "明白了！"
            }
        },
        "2_B230": {
            "name": "宝具卡 3",
            "text": {
                "2_B230": "凯妮斯出击！"
            }
        },
        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "就用这招！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "如何！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "海洋啊！",
                "2_B441": "风暴啊！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "哈哈！　我的肉体是无敌的！"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "哈哈！　我会全力以赴！"
            }
        },
        "2_B510": {
            "name": "开始 4",
            "text": {
                "2_B510": "哈哈哈哈哈哈！ ——做好思想准备了吗？"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "……没什么值得高兴的。"
            }
        },
        "2_B530": {
            "name": "胜利 4",
            "text": {
                "2_B530": "我是不是太亢奋了？"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "还挺能干的嘛……！"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "挨了一招，不错的呢……"
            }
        },
        "2_B570": {
            "name": "无法战斗 4",
            "text": {
                "2_B570": "这种事偶尔也会……对吧……？"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "我是自由的！",
                "2_B801": "无论是海洋、大地，都无法留住我！",
                "2_B802": "看好了！",
                "2_B803": "——飞翔吧，吾之金色大翼！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "就用这招……",
                "2_B841": "彻底！　击碎！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "是供品吗！　哈哈！"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "……哈哈！　没错，就是这个。",
                "2_G041": "正所谓神灵凯妮斯在此。",
                "2_G042": "看看这个，波塞冬的三叉戟！　是正牌货！",
                "2_G043": "佑护实体化的黄金铠甲，以及三叉戟！",
                "2_G044": "只要凑齐了这些，我！",
                "2_G045": "就是货真价实的不死身神灵凯妮斯大人了！"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "虽然我确实说自己是神灵……",
                "2_G051": "哈哈哈，你居然把我拉到了这种领域啊。",
                "2_G052": "居然有如此好事的家伙。",
                "2_G053": "起初我还以为你只是个喜欢上供的笨蛋呢……",
                "2_G054": "笨到极致也是相当了得的嘛。",
                "2_G055": "我还挺喜欢这种的！",
                "2_G056": "今后也请多指教啦，御主！"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "我的确是神灵，但无论你为我做什么，都不会得到灵验的哦！",
                "2_G111": "你还是有这点觉悟为好。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "真不错，进贡啊。",
                "2_G121": "你是那啥吗，那种类型的人吗？"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "生日？　谁的？",
                "2_H011": "……你的？",
                "2_H012": "哈？",
                "2_H013": "…………这……这种事你应该早点说啦！",
                "2_H014": "真是的。那总得准备些什么才行吧……啊……但是我什么都没有呢……",
                "2_H015": "好，我明白了。我会陪你一整天。",
                "2_H016": "虽然不至于到言听计从的地步，但我可以一定程度内听你的话哦？"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "现在好像在办什么有意思的事呢！",
                "2_H021": "喂，御主！",
                "2_H022": "这是叫……活动吧。既然参加，就要全力以赴哦！"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "闲～得～慌～。",
                "2_H031": "出门转转吧，喂，御主！"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "从者和御主的关系真是简单易懂。",
                "2_H041": "来，向我下令吧。",
                "2_H042": "我会把碍事的家伙全部排除。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "我觉得所谓的主从关系不太准确……",
                "2_H051": "我隐约觉得，御主和从者似乎不是那样的存在。",
                "2_H052": "应该更为……该怎么说呢……",
                "2_H053": "总之就是这种感觉。",
                "2_H054": "究竟是为什么呢？"
            }
        },
        "2_H060": {
            "name": "喜欢的东西",
            "text": {
                "2_H060": "鸟非常棒吧。我喜欢。",
                "2_H061": "……不是啦，我说的不是鸡肉。你这笨蛋。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "我不太喜欢海。",
                "2_H071": "海给我去死。"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "圣杯啊……",
                "2_H081": "我不是很理解。",
                "2_H082": "不过，应该可以用来盛酒吧。"
            }
        },
        "2_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H090": "啊？",
                "2_H091": "我不是告诉你我是神灵凯妮斯了嘛。",
                "2_H092": "哈？　……传承中的僭主凯妮斯不是神明？",
                "2_H093": "确实如此。我曾经自称过王，但没有自称过神明。",
                "2_H094": "你问我那为什么？",
                "2_H095": "这很简单。",
                "2_H096": "话说，有酒或是肉吗？"
            }
        },
        "2_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H100": "……呼，当世的酒也不赖嘛。",
                "2_H101": "然后呢，刚才说到哪儿来着？",
                "2_H102": "啊啊，是神灵什么的吧。",
                "2_H103": "这很简单。",
                "2_H104": "因为我获得了那个狗屎海混蛋……海神波塞冬的佑护。是神的力量。",
                "2_H105": "而且，那家伙居然把自己的一部分都给了我。是三叉戟。海神的三叉戟。相当于海神波塞冬的象征啦。",
                "2_H106": "把那玩意儿交给了我就意味着……",
                "2_H107": "虽说让人恼火，但我实质上已经是海洋之神了。",
                "2_H108": "哈哈哈！　你说是吧！"
            }
        },
        "2_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H110": "波塞冬的事？",
                "2_H111": "我才不聊呢。",
                "2_H112": "为什么我要聊那个狗屎家伙的事……",
                "2_H113": "啊？",
                "2_H114": "既然得到了他的佑护和三叉戟，我们的关系一定非同一般？",
                "2_H115": "哈哈哈哈哈哈哈哈哈哈。",
                "2_H116": "小心我杀了你。你不准再多问一句了。否则我会杀了你哦。"
            }
        },
        "2_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H120": "我……",
                "2_H121": "喜欢看海。这是很久以前的事了……",
                "2_H122": "但是，某一天，我在海边……",
                "2_H123": "………………不，没什么。",
                "2_H124": "我……色萨利的女人凯妮斯，一定已经在那天死了……",
                "2_H125": "然后时而被称为僭主，时为被称为英雄之一的凯纽斯诞生了吧。",
                "2_H126": "然而，之所以像这样……",
                "2_H127": "以凯妮斯之名现界——",
                "2_H128": "究竟是为什么呢……"
            }
        },
        "2_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H130": "哈？　……像女人？　我？　你搞错了吧？",
                "2_H131": "哈哈哈哈。你这话说得真有意思。",
                "2_H132": "你试着再说一次看看。我会在你张口之前把你的脑袋砍掉。",
                "2_H133": "…………唔。不是在开玩笑？",
                "2_H134": "这样啊。那么，那个……应该是凯妮斯的残留之类的吧。",
                "2_H135": "不，我现在的真名确实是凯妮斯。真是的。实在是太麻烦了。",
                "2_H136": "……你那是什么表情。喂。喂，我说你。刨根问题或是想象都随你的便……",
                "2_H137": "但唯独不要表示同情哦，御主。",
                "2_H138": "如果被自己人同情——",
                "2_H139": "我会忍不住杀掉你的。"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "……哎，你们在这儿的关系是这种感觉的啊。"
            }
        },
        "2_H180": {
            "name": "对话 5",
            "text": {
                "2_H180": "哟，美狄亚！",
                "2_H181": "我当然一眼就认出来了啦。",
                "2_H182": "哈？　和平时的差别？　有吗？？"
            }
        },
        "2_H190": {
            "name": "对话 6",
            "text": {
                "2_H190": "你这样子还真是……",
                "2_H191": "不，这模样也不赖。",
                "2_H192": "严格说来，我还挺喜欢的。"
            }
        },
        "2_H200": {
            "name": "对话 7",
            "text": {
                "2_H200": "啊啊，原来如此。军神阿瑞斯的眷属吗？",
                "2_H201": "哎～！　是吗是吗！"
            }
        },
        "2_H210": {
            "name": "对话 8",
            "text": {
                "2_H210": "……你这家伙，是什么？",
                "2_H211": "让我莫名有种不安……你这家伙究竟是什么？"
            }
        },
        "2_H220": {
            "name": "对话 9",
            "text": {
                "2_H220": "神明这种存在真是讨厌呢。",
                "2_H221": "不管是老的还是新的，没一个好东西。",
                "2_H222": "话说御主，我能杀了他们吗？"
            }
        },
        "2_H230": {
            "name": "对话 10",
            "text": {
                "2_H230": "………………居然自愿变成女人的样子？",
                "2_H231": "什么？",
                "2_H232": "外形无关紧要，对你来说一切都是随心所欲的？",
                "2_H233": "哎。",
                "2_H234": "呃，哎……迦勒底还真有些奇怪的家伙呢。"
            }
        },
        "2_H2300": {
            "name": "对话 3 (第3再临后)",
            "text": {
                "2_H2300": "人类会将超越人类的存在作为兵器来使用……",
                "2_H2301": "我起初对这件事非常恼火。觉得他们凭什么那么自以为是。",
                "2_H2302": "……不过嘛，这也要看人。",
                "2_H2303": "如果对象是超越我的笨蛋，那奉陪到最后也不是件坏事。"
            }
        },
        "2_H240": {
            "name": "对话 11",
            "text": {
                "2_H240": "噢，莫德雷德！",
                "2_H241": "我们去模拟装置吧！",
                "2_H242": "不，我没打算找豆芽菜那家伙啦。"
            }
        },
        "2_H250": {
            "name": "对话 12",
            "text": {
                "2_H250": "不要移开视线啦，我在说你啦，说你。",
                "2_H251": "啊？……暴露？",
                "2_H252": "这……这和暴露没关系吧。反正我是不死身，穿什么都没关系吧！",
                "2_H253": "比起这个，你好歹也算是个王吧。",
                "2_H254": "我的意思是你应该打起点精神来。",
                "2_H255": "啊，算了。",
                "2_H256": "跟我来。去模拟装置啦，模拟装置。我要从头开始锻炼你。"
            }
        },
        "2_H260": {
            "name": "对话 13",
            "text": {
                "2_H260": "哎～，坂田金时啊。",
                "2_H261": "那家伙……感觉不太一样。",
                "2_H262": "虽说我不知道他本性如何，但出身完全就是个少爷。",
                "2_H263": "我是不会让他加入迦勒底不良组的。"
            }
        },
        "2_H270": {
            "name": "对话 14",
            "text": {
                "2_H270": "惨了，是玛尔达！",
                "2_H271": "来了个啰嗦的家伙。那家伙对各种问题都很啰嗦……",
                "2_H272": "好嘞，开溜！"
            }
        },
        "2_H280": {
            "name": "对话 15",
            "text": {
                "2_H280": "武藏？",
                "2_H281": "我知道。是个啰嗦、天真、胡闹的家伙。",
                "2_H282": "虽说我不承认人类的剑士，但那家伙算是个合格的英灵哦。",
                "2_H283": "不过，从和我很投缘这点来看，那家伙也不是个正经的家伙吧！"
            }
        },
        "2_H300": {
            "name": "对话 16",
            "text": {
                "2_H300": "找到你了哦，特～里～同～！",
                "2_H301": "好不容易见到你，让我们叙叙旧——",
                "2_H302": "哈！？　你的样子是怎么回事！？　完全是个小孩子吧！？　而且还穿着女孩子的衣服！",
                "2_H303": "哈？　只是制服？",
                "2_H304": "看上去像女孩子纯属我的爱好！？",
                "2_H305": "怎么可能啦，真是的，我一点都不觉得你这样帅气！"
            }
        },
        "2_H310": {
            "name": "对话 17",
            "text": {
                "2_H310": "不，不对吧……人鱼应该更加……",
                "2_H311": "总之不对吧……",
                "2_H312": "可恶，感觉一看到梵高心情就会低落……",
                "2_H313": "难道我在为那家伙担心吗……？"
            }
        },
        "2_H320": {
            "name": "对话 18",
            "text": {
                "2_H320": "肯定不对吧，你是人鱼什么的肯定有问题吧！？",
                "2_H321": "连希腊的海里都很难找到你这种程度的怪物啊！？"
            }
        }
    },
    "ChrVoice_ids": [
        "304100"
    ]
}
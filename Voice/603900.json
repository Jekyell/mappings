{
    "id": 603900,
    "collectionNo": 243,
    "name": "格蕾",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "好嘞来了！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "拜托了，亚德！"
            }
        },
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "亚德！",
                "0_B041": "包在我身上吧！",
                "0_B042": "用这招！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "拟似人格停止。",
                "0_B051": "魔力收集率，突破规定值。",
                "0_B052": "第二阶段极限解除开始。",
                "0_B053": "圣枪……拔锚……",
                "0_B054": "闪耀于……终焉之枪……！！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "让你们见识一下……守墓人的秘法……！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "哎哟，是这样的命令呀！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "是……我也会……全力以赴……！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "咿嘻嘻嘻嘻！ 心情愉悦吧！　按捺不住了吧！ 让我吃个饱吧，格蕾！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "嗯……都是多亏了大家，总算是成功了……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "太棒了！　成功了！ 咿嘻嘻嘻，大胜利呢！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "我们必须……坚持才行……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "非常抱歉……请容我……先去休息了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "该死的，这家伙不好对付……格蕾……先撤退……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "我会做！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "斩裂！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "吃掉你哦！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "黑暗……欢腾……"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "渴望……堕落……"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "Gray……Rave……"
            }
        },
        "0_B250": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B250": "Crave……Deprave……"
            }
        },
        "0_B260": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B260": "亚德，解开封印！ 好嘞！"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "一剑之隔的话！",
                "0_B441": "用这招吧！",
                "0_B442": "拜托了！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "为生者与死者双方指引方向……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "古老的神秘啊，消亡吧。",
                "0_B801": "让这简陋的谜题全都归于虚无吧。",
                "0_B802": "第二阶段极限解除开始。",
                "0_B803": "……圣枪……拔锚！",
                "0_B804": "闪耀于终焉之枪……！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "等级提升……这感觉很不可思议。",
                "0_G011": "但是，真是太感谢您了……！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "感谢您为我做到这种地步。",
                "0_G021": "请问……有没有令您感到枯燥乏味呢？",
                "0_G022": "如果是的话，真对不起。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "对不起……",
                "0_G031": "其他的从者或许能改变外形，但我却只有这些。",
                "0_G032": "啊，但力量还是有所提升的！　我会尽最大努力！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "你还真是个好事之徒！",
                "0_G041": "居然将如此贵重的物品赐予这种阴沉的守墓人！",
                "0_G042": "你……你给我闭嘴啦，亚德！",
                "0_G043": "啊啊，但是能感到力量变充实了。",
                "0_G044": "亚德也是因为感受到了，才会像刚才那样胡言乱语啦。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "……真的抵达了。",
                "0_G051": "这景色就是我。",
                "0_G052": "是被您一路引领到这里来的我。",
                "0_G053": "真是……真是太感谢了。",
                "0_G054": "咿嘻嘻嘻！　我也必须感谢你才行！",
                "0_G055": "不过你干得还真不错呢！",
                "0_G056": "今后也请多关照啦！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "听说今天是您的生日。",
                "0_H011": "尽管我已经连祈福的话都记不清了……",
                "0_H012": "即便如此，还是要祝福您。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "外面好像发生了什么！",
                "0_H021": "请问，要不要一起去看看……！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "尽管我也喜欢在房间里悠闲地待着，但总觉得差不多该出门了……！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "对，您是我的御主，我是从者。",
                "0_H041": "请不用客气地对我下令吧！",
                "0_H042": "嘻嘻嘻！　你也可以命令我哦，御主？"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "主从关系……",
                "0_H051": "是啊，我已经习惯师傅和入室弟子那样的关系了。",
                "0_H052": "但是，我可以只称呼那位为师傅吗？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……",
                "0_H061": "安静的房间、烟卷的味道、古书的味道，还有大家热闹交谈的教室。",
                "0_H062": "……或许有些矛盾，但我喜欢这些。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西……",
                "0_H071": "呃，明明已经死了，却在地面行走的……",
                "0_H072": "我讨厌那种东西。",
                "0_H073": "还有……那个……",
                "0_H074": "我以前……有一点点讨厌这张脸。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "希望圣杯实现的愿望吗？",
                "0_H081": "如果……如果能被允许……",
                "0_H082": "希望能与师傅、莱妮丝小姐、以及埃尔梅罗教室的各位……",
                "0_H083": "不，没什么。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "请问……",
                "0_H091": "如果可能的话，您能厌恶我的容貌吗……？"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "关于这容貌吗……",
                "0_H101": "您见过其他容貌相似的人？",
                "0_H102": "如果是这样，我相信我总有一天会告诉您的。",
                "0_H103": "……啊，就是让你再等等啦。",
                "0_H104": "不要说这种不解风情的事啦，御主！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "嗯……我的容貌原本不属于我。",
                "0_H111": "因为我的故乡想要重现亚瑟王……想要重现在遥远过去已经死去的王……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "问我现在是不是还讨厌这容貌？",
                "0_H121": "不，不讨厌了。",
                "0_H122": "只是稍微有点……不太习惯……",
                "0_H123": "还有师傅也讨厌这容貌的事令我非常高兴。",
                "0_H124": "所以，才会给师傅提了个强人所难的请求。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "现在我也会摘下这兜帽，但并不会讨厌自己。",
                "0_H131": "嗯，一定是和师傅或莱妮丝小姐一样……",
                "0_H132": "由于您在身边……会令我非常高兴吧。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "啊啊……继续持有圣枪的可能性。",
                "0_H171": "我明白。那也是我可能出现的形态。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "亚瑟王也有各种侧面呢。",
                "0_H181": "嗯，我明白……"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "那就是……真正的亚瑟王……",
                "0_H191": "啊啊，我的故乡一直在追求的……",
                "0_H192": "终点……"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "无论在什么地方，只要师傅在就放心了。",
                "0_H201": "但是，无论在什么地方，师傅都眉头紧锁呢。",
                "0_H202": "如果有时间的话，真希望至少能为他梳个头发呢。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "请……请……请……请！　请问！",
                "0_H211": "那位征服王也在这里啊！！",
                "0_H212": "好厉害！　太厉害了！",
                "0_H213": "得赶快去通知师傅才行！"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "对，我是师傅的入室弟子……",
                "0_H221": "哎哎，您也是征服王！？",
                "0_H222": "不，只是没想到您会以这种形态被召唤……！",
                "0_H223": "那个，还请您多多指教……"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "嗯。莱妮丝小姐邀请我去参加茶会……！！",
                "0_H231": "每次都会准备最顶级的点心和茶，那些到底是从哪里找来的呢？"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "尽管我知道这与我认识的那位不一样……",
                "0_H241": "但该怎么说呢，这位天秤女神与那位的言行举止都一模一样。"
            }
        },
        "0_I010": {
            "name": "淑女·莱妮丝事件簿 1",
            "text": {
                "0_I010": "欢迎光临！",
                "0_I011": "那个……这里似乎能够兑换道具。",
                "0_I012": "我会为您介绍，如果有什么中意的东西，还请拿去吧。"
            }
        },
        "0_I020": {
            "name": "淑女·莱妮丝事件簿 2",
            "text": {
                "0_I020": "这是我与莱妮丝小姐一起搜集来的道具。",
                "0_I021": "莱妮丝小姐说……就算顾客是弟子，也要一概抬价。",
                "0_I022": "那个，对不起。"
            }
        },
        "0_I030": {
            "name": "淑女·莱妮丝事件簿 3",
            "text": {
                "0_I030": "是挑选这个嘛？",
                "0_I031": "给，请收下……！",
                "0_I032": "但愿这能在您的旅途上帮上忙。"
            }
        },
        "0_I040": {
            "name": "淑女·莱妮丝事件簿 4",
            "text": {
                "0_I040": "尽管我对魔术不甚了解。",
                "0_I041": "但既然您想要这个，那一定是有用处的东西……！"
            }
        },
        "0_I050": {
            "name": "淑女·莱妮丝事件簿 5",
            "text": {
                "0_I050": "看店的工作也变得逐渐有趣了。",
                "0_I051": "只要给您这个就行了吧！"
            }
        },
        "0_I060": {
            "name": "淑女·莱妮丝事件簿 6",
            "text": {
                "0_I060": "随着拼缀伦敦的落幕，这家店也要关门了。",
                "0_I061": "令人有些依依不舍呢。",
                "0_I062": "……请不要忘记兑换道具。"
            }
        },
        "0_I070": {
            "name": "淑女·莱妮丝事件簿 7",
            "text": {
                "0_I070": "啊啊，您又来了啊。",
                "0_I071": "嗯，再过没多久，这场梦就要苏醒了。",
                "0_I072": "在那之前，一切都请您随意……"
            }
        },
        "0_I080": {
            "name": "淑女·莱妮丝事件簿 8",
            "text": {
                "0_I080": "呃，我记得这个商品应该在……",
                "0_I081": "是这个吧！",
                "0_I082": "啊啊，就是这个，亚德！",
                "0_I083": "对不起，我还没能全部记清楚……"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "因为还是很担心师傅，所以就跟来了。",
                "0_S011": "嗯，尽管我身为从者来说全是些例外，也不成熟，",
                "0_S012": "但请容我伴您一起，完成这趟旅程……！"
            }
        },
        "0_S020": {
            "name": "期间限定加入",
            "text": {
                "0_S020": "我是……守墓人格蕾。",
                "0_S021": "和普通的从者情况不太一样，",
                "0_S022": "或许还多少会给您添麻烦，还请多多关照……！"
            }
        }
    },
    "ChrVoice_ids": [
        "603900"
    ]
}
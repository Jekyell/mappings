{
    "id": 702300,
    "collectionNo": 114,
    "name": "源赖光",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "疼吗？"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "上——抱歉了！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "到来吧，",
                "0_B053": "吾之忠臣、",
                "0_B056": "吾之手足、",
                "0_B057": "吾之铠甲。",
                "0_B058": "正如所见，",
                "0_B059": "此乃四天王——",
                "0_B054": "牛王招雷·",
                "0_B055": "天网恢恢！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "简直就像玩耍。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "请见证吧。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "——诛伐，执行。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "要注意不能太过火哦？"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "呼……战斗还真是粗暴的存在呢……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "帮上您的忙了吗？ 呵呵，那再好不过了。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "挺能干的嘛……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "我居然会……太羞愧了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "失策了……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "明白了。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "哎呀呀。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "嗯，我可是您的刀。"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "只要您下令。我赖光愿意化为鬼！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "消失吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "太骄傲了吧。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "请觉悟吧。"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "居然又一次……失败了……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "这就是所谓的……掉以轻心吧……！"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "来吧，四天王！",
                "0_B801": "……不，牛头天王的神使啊。",
                "0_B804": "化作吾记忆之形——",
                "0_B802": "上。",
                "0_B803": "牛王招雷·天网恢恢！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "牛王招雷·天网恢恢！",
                "0_B811": "呵呵……",
                "0_B812": "啊哈哈哈哈！",
                "0_B813": "无名小卒，化为尘土吧！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "成功了！",
                "0_G011": "您看到了吗，御主？",
                "0_G012": "我的状态非常好♪"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "哎呀……可以吗？",
                "0_G021": "居然为我使用这样的……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "虽然很不好意思……",
                "0_G031": "但我非常高兴。",
                "0_G032": "虽然会变得有些调皮捣蛋，",
                "0_G033": "但请不要讨厌我哦？"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "哎呀！",
                "0_G041": "又是新的战斗装束！",
                "0_G042": "……呵呵。被人期待真是一件高兴的事。",
                "0_G043": "我会继续努力的，御主。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "啊啊……居然变得这么强，您居然如此爱着我……",
                "0_G051": "明明应该高兴，但却有些难受……",
                "0_G052": "我是因血而疯狂的丑（牛）之魔性——",
                "0_G053": "像我这样的女人，真的能够在您的身旁吗……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "呵呵，请到这里来吧，御主。",
                "0_H011": "听说生日就该由家人一起庆祝。",
                "0_H012": "请不要客气，今天您就将我当做母亲，尽情对我撒娇吧。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "哎呀……",
                "0_H021": "我好像听到祭典的乐器声了呢？",
                "0_H022": "好像很有意思。我们走吧！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "俗话说，孩子犹如风之子。",
                "0_H031": "我会陪伴在您的身边，何不出去走走呢？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "御主……御主……",
                "0_H041": "呵呵，真不错。",
                "0_H042": "不是身为将领，而是作为一个女人侍奉他人，",
                "0_H043": "这感觉真新鲜。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "我希望能与您……",
                "0_H051": "缔结犹如我和四天王那样可以促膝长谈的深厚关系。",
                "0_H052": "……请问，是否会给您添麻烦呢？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……",
                "0_H061": "是金锷饼吧。",
                "0_H062": "哎，不是指吃的东西吗？",
                "0_H063": "十……十分抱歉！",
                "0_H064": "……真是的，我也太丢人了……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西吗？",
                "0_H071": "自不用说。我厌恶的东西只有一种。",
                "0_H072": "那就是企图接近我所爱之人的坏虫子。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "希望圣杯实现的愿望吗？",
                "0_H081": "应该是——",
                "0_H082": "被母子之爱所充斥的和平的世间吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我应该是Berserker吧……",
                "0_H091": "我早已做好这个职阶会被人如恶鬼罗刹般恐惧的思想准备……",
                "0_H092": "但您居然愿意与不称职的我缔结契约，实在是感激不尽。",
                "0_H093": "战斗就交给我吧，御主。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "虽说战斗是我的本分，但比起战斗，我更喜欢像这样聊天。",
                "0_H101": "因为战斗一点都不快乐。",
                "0_H102": "我可不喜欢仔细地碾碎那些蝼蚁般的敌人呢。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "那个，话说……我们已经相互深入了解了，",
                "0_H111": "差不多……是否可以不称您为御主，",
                "0_H112": "而称呼您为亲爱的呢……？"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "呵呵。请来这里。",
                "0_H121": "如果您累了，要不要枕在我的膝盖上。这可是源氏的膝枕。",
                "0_H122": "太好了，还舒服吗？",
                "0_H123": "话说御主。我是个独占欲很强的女人。",
                "0_H124": "所以如果您在其他女人那里做这种事，我可是会伤心的哦？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我向您发誓，御主。",
                "0_H131": "我爱您犹如爱自己的孩子。",
                "0_H132": "所以……希望你也不要背叛妈妈我。",
                "0_H133": "如果真的发生了那种事，",
                "0_H134": "我甚至不知道自己会做出什么事来……"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "哎呀，金时，你怎么举止又那么粗暴……",
                "0_H141": "为……为什么要逃跑啦！？",
                "0_H142": "妈妈我是为你着想才会这么说的……",
                "0_H143": "呜呜呜。",
                "0_H144": "被这么嫌弃，妈妈我都要哭了……"
            }
        },
        "0_H150": {
            "name": "对话 5",
            "text": {
                "0_H150": "我感到了虫子的气息……瘦小的鬼女居然如此碍眼。",
                "0_H151": "呵呵，没事啦。",
                "0_H152": "就算我真的想灭了她，也会找一个您与金时看不到的地方处理的。"
            }
        },
        "0_H160": {
            "name": "对话 6",
            "text": {
                "0_H160": "最近，听说御主和金时的身边有只金色的虫子……",
                "0_H161": "不，怎么会呢……您说对吧。"
            }
        },
        "0_H170": {
            "name": "对话 7",
            "text": {
                "0_H170": "那个，其他的我似乎给您添麻烦了……",
                "0_H171": "非常抱歉。",
                "0_H172": "武藏大人，还请您多多包涵。"
            }
        },
        "0_H180": {
            "name": "对话 8",
            "text": {
                "0_H180": "……怎么，虫子就别看这里，别呼吸，别存在。",
                "0_H181": "做不到的话就由我来碾碎。",
                "0_H183": "什么？　感谢关照？　你在说什么……",
                "0_H184": "啊啊，是其他的我和其他的你……",
                "0_H185": "不，这应该是这边的我无法知晓的事吧。",
                "0_H186": "但是，嗯，没错。……上次有劳你了。"
            }
        },
        "0_H190": {
            "name": "对话 9",
            "text": {
                "0_H190": "好久不见，香子大人。",
                "0_H191": "您在这里也打算撰写美妙的故事吗？"
            }
        },
        "0_H200": {
            "name": "对话 10",
            "text": {
                "0_H200": "哎呀，哎呀，连纲也来了啊！",
                "0_H201": "在这里也要请你多指教了呀。一定要和御主和金时好好相处哦。呵呵呵……",
                "0_H202": "哎？　说我氛围柔和了不少？　哎呀……",
                "0_H203": "这一定是你多心了吧。"
            }
        },
        "0_H210": {
            "name": "对话 11",
            "text": {
                "0_H210": "芦屋道满……连鬼也会吞噬的非人之物。"
            }
        },
        "0_I010": {
            "name": "复刻:天魔御伽草子 鬼岛 9",
            "text": {
                "0_I010": "欢迎光临。",
                "0_I011": "这里能根据您的武勋获得相应的报酬。",
                "0_I012": "来吧，您究竟建立了多少武勋呢？"
            }
        },
        "0_I020": {
            "name": "复刻:天魔御伽草子 鬼岛 10",
            "text": {
                "0_I020": "以奖励回报工作。用恩义回报奉献。",
                "0_I021": "嗯，没错，这是理所当然的。后世的武士似乎也是这么认为的吧。",
                "0_I022": "所以你也应该得到相应的报酬。"
            }
        },
        "0_I030": {
            "name": "复刻:天魔御伽草子 鬼岛 11",
            "text": {
                "0_I030": "有没有充分屠戮鬼众呢？",
                "0_I031": "哎呀……这是何等勤勉！",
                "0_I032": "呵呵呵，金时和四天王其他人自不用说，你似乎也能成为非常优秀的武士呢。"
            }
        },
        "0_I040": {
            "name": "复刻:天魔御伽草子 鬼岛 12",
            "text": {
                "0_I040": "明白了。请收下吧。",
                "0_I041": "呵呵。还要继续努力哦。"
            }
        },
        "0_I050": {
            "name": "复刻:天魔御伽草子 鬼岛 13",
            "text": {
                "0_I050": "您选了一件好东西呢。",
                "0_I051": "但愿这能为您更添一份力量。",
                "0_I052": "来吧，让我们继续剿灭众鬼吧。"
            }
        },
        "0_I060": {
            "name": "复刻:天魔御伽草子 鬼岛 14",
            "text": {
                "0_I060": "给，收下吧。这是你的。",
                "0_I061": "一定要好好使用哦。"
            }
        },
        "0_I110": {
            "name": "复刻:天魔御伽草子 鬼岛 15",
            "text": {
                "0_I110": "你的战斗将到此结束。",
                "0_I111": "但是，为了下次战斗，一定要记得将武勋换成报酬哦。"
            }
        },
        "0_I120": {
            "name": "复刻:天魔御伽草子 鬼岛 16",
            "text": {
                "0_I120": "虽然战斗已经结束了，但阵地还在这里。",
                "0_I121": "来吧，是否还有没有领取的报酬呢？"
            }
        },
        "0_N010": {
            "name": "天魔御伽草子 鬼岛 1",
            "text": {
                "0_N010": "欢迎来到鬼岛。",
                "0_N011": "欢迎你，异乡的御主大人。",
                "0_N012": "请一定要好好玩哦？"
            }
        },
        "0_N020": {
            "name": "天魔御伽草子 鬼岛 2",
            "text": {
                "0_N020": "除鬼的进展怎么样了？",
                "0_N021": "……呵呵，这样啊。那太好了。",
                "0_N022": "一定要将鬼一个不留全部咔嚓掉哦。"
            }
        },
        "0_N030": {
            "name": "天魔御伽草子 鬼岛 3",
            "text": {
                "0_N030": "哎？　除鬼的窍门吗？",
                "0_N031": "很简单。最后都不能掉以轻心，冷酷无情的消灭他们就行了。"
            }
        },
        "0_N040": {
            "name": "天魔御伽草子 鬼岛 4",
            "text": {
                "0_N040": "告知战况的说明文字是很重要的。一定要仔细阅读。",
                "0_N041": "是叫……Information……吧？",
                "0_N042": "全是些有益的内容。"
            }
        },
        "0_N050": {
            "name": "天魔御伽草子 鬼岛 5",
            "text": {
                "0_N050": "哎呀，沾满了那么多鲜血。",
                "0_N051": "你还真是顽皮呢……",
                "0_N052": "长着一张乖巧的脸，还真不能小看呢，呵呵。"
            }
        },
        "0_N060": {
            "name": "天魔御伽草子 鬼岛 6",
            "text": {
                "0_N060": "你明明已经听过皆大欢喜这句台词了啊——",
                "0_N061": "把什么东西遗忘在鬼的宝物库了吗？"
            }
        },
        "0_N070": {
            "name": "天魔御伽草子 鬼岛 7",
            "text": {
                "0_N070": "对这次除鬼的经历还满意吗？",
                "0_N071": "以你的功勋，获得的金银财宝是理所当然的权利。",
                "0_N072": "请注意，可别忘了拿走哦？"
            }
        },
        "0_N080": {
            "name": "天魔御伽草子 鬼岛 8",
            "text": {
                "0_N080": "啊啊……鬼岛就要崩溃了……",
                "0_N081": "但是，我还会再逗留在这里一会儿——",
                "0_N082": "请务必不要忘记交换以及领取战果哦。"
            }
        },
        "0_N110": {
            "name": "复刻:天魔御伽草子 鬼岛 1",
            "text": {
                "0_N110": "这次的驱鬼也是吾等源氏威严庄重的战斗之一。",
                "0_N111": "对，没错。不会放跑任何一只。"
            }
        },
        "0_N120": {
            "name": "复刻:天魔御伽草子 鬼岛 2",
            "text": {
                "0_N120": "一旦开始驱鬼，就要贯彻到最后。",
                "0_N121": "认真仔细，直到炉灶深处——",
                "0_N122": "啊，不，这话不是我说的，我记得好像是纲的话……"
            }
        },
        "0_N130": {
            "name": "复刻:天魔御伽草子 鬼岛 3",
            "text": {
                "0_N130": "异乡的御主大人。",
                "0_N131": "听好了，每天都要勤于锻炼，勤于提升，勤于除鬼。",
                "0_N132": "一定要努力哦。"
            }
        },
        "0_N140": {
            "name": "复刻:天魔御伽草子 鬼岛 4",
            "text": {
                "0_N140": "欢迎回来。",
                "0_N141": "来，先把溅到的血擦干净吧。"
            }
        },
        "0_N150": {
            "name": "复刻:天魔御伽草子 鬼岛 5",
            "text": {
                "0_N150": "哎呀呀，什么事？",

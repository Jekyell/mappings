        "11_H010": {
            "name": "生日 女武神琳德",
            "text": {
                "11_H010": "今天是你的生日。",
                "11_H011": "我知道哦，因为我们是同步的嘛。",
                "11_H012": "那么，送你卢恩的加护……不对，送你礼物吧。",
                "11_H013": "该送什么好呢～？"
            }
        },
        "11_H020": {
            "name": "活动举行中 女武神琳德",
            "text": {
                "11_H020": "活动，是类似祭典的存在吧。",
                "11_H021": "那我们走吧。",
                "11_H022": "你必须积累经验才行。"
            }
        },
        "11_H030": {
            "name": "对话 1 女武神琳德",
            "text": {
                "11_H030": "准备没问题～。",
                "11_H031": "无论战斗，还是探索，什么都没问题～。"
            }
        },
        "11_H040": {
            "name": "对话 2 女武神琳德",
            "text": {
                "11_H040": "御主很精神吧。",
                "11_H041": "有没有吃很多好吃的东西？"
            }
        },
        "11_H050": {
            "name": "对话 3 女武神琳德",
            "text": {
                "11_H050": "御主和所谓勇士的感觉稍微有些……",
                "11_H051": "不，没什么～。"
            }
        },
        "11_H060": {
            "name": "喜欢的东西 女武神琳德",
            "text": {
                "11_H060": "喜欢的东西？",
                "11_H061": "恩赫里亚，勇士的灵魂，理所当然。",
                "11_H062": "还有就是姐姐大人们。",
                "11_H063": "布伦希尔德姐姐大人就在这里吧～。",
                "11_H064": "呵呵呵呵。"
            }
        },
        "11_H070": {
            "name": "讨厌的东西 女武神琳德",
            "text": {
                "11_H070": "讨厌的东西，没有吧～。",
                "11_H071": "我们没有这个定义，所以无法切身体会。",
                "11_H072": "人类有很多讨厌的东西吧，我知道。"
            }
        },
        "11_H080": {
            "name": "关于圣杯 女武神琳德",
            "text": {
                "11_H080": "圣杯很有效果呢。",
                "11_H081": "愿望、追求、纷争、战斗。",
                "11_H082": "很容易产生勇士吧？"
            }
        },
        "11_H10090": {
            "name": "牵绊Lv.1 女武神琳德",
            "text": {
                "11_H10090": "你和斯露德关系很好啊。",
                "11_H10091": "哎～，是吗，是这样啊。"
            }
        },
        "11_H10100": {
            "name": "牵绊Lv.2 女武神琳德",
            "text": {
                "11_H10100": "如果你希望我和斯露德交换，记得随时告诉我哦～。",
                "11_H10101": "本机只会服从输入。",
                "11_H10102": "有时间的话，要不维护一下枪吧。"
            }
        },
        "11_H10110": {
            "name": "牵绊Lv.3 女武神琳德",
            "text": {
                "11_H10110": "这里有好多美味的东西呢。",
                "11_H10111": "美味是一件美好的事。",
                "11_H10112": "大多数生物只要吃到美味的东西就会精神起来吧～。",
                "11_H10113": "虽然我们和生物有些不太一样，但只要吃到美味的东西就会有精神。",
                "11_H10114": "……大概吧。"
            }
        },
        "11_H10120": {
            "name": "牵绊Lv.4 女武神琳德",
            "text": {
                "11_H10120": "我原本的工作应该是作战构筑。",
                "11_H10121": "如果说斯露德是指挥官的话，我就是参谋。",
                "11_H10122": "……唔？",
                "11_H10123": "问我是不是不擅长直接战斗？",
                "11_H10124": "并不会不擅长哦。战斗性能和其他姊妹们一模一样。"
            }
        },
        "11_H10130": {
            "name": "牵绊Lv.5 女武神琳德",
            "text": {
                "11_H10130": "真的要选本机——选我吗？　当真？",
                "11_H10131": "我可以随时换成斯露德哦？",
                "11_H10132": "……这样啊。",
                "11_H10133": "是吗。",
                "11_H10134": "呵呵，是吗～。",
                "11_H10135": "哎？　我看上去心情不错？　是这样吗～。"
            }
        },
        "11_H170": {
            "name": "对话 4 女武神琳德",
            "text": {
                "11_H170": "布伦希尔德姐姐大人！",
                "11_H171": "真，真的在呢……真的……",
                "11_H172": "好厉害……侧脸和笑容都好美……",
                "11_H173": "御主，谢谢您，真的谢谢您。",
                "11_H174": "啊～，快流鼻血了……",
                "11_H175": "感觉要习惯这个还得花点时间……"
            }
        },
        "11_H180": {
            "name": "对话 5 女武神琳德",
            "text": {
                "11_H180": "感知勇士的存在了。",
                "11_H181": "啊，自动劝诱模式快启动了……",
                "11_H182": "该怎么办呢～，坐立不安。"
            }
        },
        "11_H190": {
            "name": "对话 6 女武神琳德",
            "text": {
                "11_H190": "齐格鲁德在这里吧。",
                "11_H191": "啊，看到了。",
                "11_H192": "……唔～。",
                "11_H193": "御主，给我一阵子开枪许可吧？"
            }
        },
        "11_H200": {
            "name": "对话 7 女武神琳德",
            "text": {
                "11_H200": "阿特利大人……",
                "11_H201": "嗯，我有和斯露德她们同步，所以知道。",
                "11_H202": "姐姐大人的姐姐大人……"
            }
        },
        "11_H210": {
            "name": "对话 8 女武神琳德",
            "text": {
                "11_H210": "斯卡蒂大人，斯卡蒂大人。",
                "11_H211": "给我冰淇淋。",
                "11_H212": "……太棒了～，拿到了～。",
                "11_H213": "啊唔啊唔。",
                "11_H214": "御主要不要也来一份？"
            }
        },
        "11_H220": {
            "name": "对话 9 女武神琳德",
            "text": {
                "11_H220": "确认对灵基的侵蚀，自动防御机能发动。",
                "11_H221": "……哎？　问我发生了什么？",
                "11_H222": "是那个啦，芦屋道满那家伙悄悄地发来了恶性情报，所以被我烧了而已。",
                "11_H223": "没关系没关系～。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 女武神琳德",
            "text": {
                "11_H9000": "斯露德现在睡了。",
                "11_H9001": "我们的灵基比较特殊……",
                "11_H9002": "一方醒来时，另一方就会沉睡。"
            }
        }
    },
    "ChrVoice_ids": [
        "604500"
    ]
}
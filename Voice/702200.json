{
    "id": 702200,
    "collectionNo": 116,
    "name": "茨木童子",
    "voices": {
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "吃这招如何……！"
            }
        },
        "0_B050": {
            "name": "宝具",
            "text": {
                "0_B050": "咯咯……",
                "0_B051": "以奸计而断，",
                "0_B052": "失而复得的右臂化为怪异！",
                "0_B053": "疾驰吧，丛原火！",
                "0_B054": "罗生门大怨起！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "那就动真格吧……"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "不爽……不爽……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "喂，刚才汝嘲笑吾矮小吧？"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "鲜血沸腾……完全就是字面含义！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "快确认战利品吧。一文钱都别拉下哦？"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "凌辱果然令人心情愉快。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "该死……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "失手了……酒吞……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "鬼就注定……会被打倒吗……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "哦？"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "汝还真好事。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "然。"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "让汝等领教真正鬼的模样吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "呵呵。你倒是可以试试看能否看穿！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "只要吞噬就能化为血肉。",
                "0_G011": "这是理所当然的定律。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "唔……",
                "0_G021": "真是奇妙的感觉。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "哎呀。",
                "0_G031": "一说到鬼就该是衣衫褴褛的吧。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "呵呵呵，",
                "0_G041": "以这身豪华的装束攻击京……",
                "0_G042": "只要想想就觉得好愉快啊！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "鬼就是清贫的存在。",
                "0_G051": "因此会掠夺、吞噬。",
                "0_G052": "但是——",
                "0_G053": "现在确实已经吃饱了哦。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "此乃汝诞生之日啊。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "酒、点心、盛装的女人——",
                "0_H021": "祭典的日子才更值得掠夺吧？"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "汝是否明白，",
                "0_H031": "没有比无聊没事干的鬼更危险的存在了哦？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "居然将吾视为随从……",
                "0_H041": "真是个不知天高地厚的家伙啊，",
                "0_H042": "御主。",
                "0_H043": "但吾很喜欢这种愚蠢和自大哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "吾乃率领大江众鬼的魁首。",
                "0_H051": "不习惯服从他人。",
                "0_H052": "汝可要千万谨慎，可别被割断喉咙哦。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "是酒吞。",
                "0_H062": "共同成长，共同生活，共同向京发起数次袭击的同胞——",
                "0_H063": "啊啊，真想再见到她……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "吾不喜欢放毒的酒。",
                "0_H071": "也不喜欢将这酒用于除鬼的卑鄙武士。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "能实现愿望的杯子——",
                "0_H081": "想必是连京之御所中也没有的宝物中的宝物。",
                "0_H082": "……鬼就该夺取宝物，",
                "0_H083": "接下来该做的早已决定了吧？"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "吾乃鬼，汝乃人。",
                "0_H091": "敬畏吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "真是个奇妙的人。",
                "0_H101": "鬼就是吃人、侵犯人、夺走人类财产的邪恶之徒。",
                "0_H102": "居然想与鬼交谈，这可没有任何好处哦。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "还真是个好事之人呢。",
                "0_H111": "居然和吾这样的鬼相交如友……",
                "0_H112": "若非是大傻瓜，就是——"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "吾还是承认吧，吾对汝产生了兴趣。",
                "0_H121": "何不把酒言欢呢。",
                "0_H122": "……既然想要与鬼成为朋友，汝该不会说什么自己不会喝吧？"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "汝……",
                "0_H131": "虽为人类，但吾相当中意汝。",
                "0_H132": "要不要来大江山，",
                "0_H133": "和吾、酒吞以及鬼们一起愉快地生活呢？"
            }
        },
        "0_H140": {
            "name": "对话 4",
            "text": {
                "0_H140": "咯咯……",
                "0_H141": "没想到在此处也能与酒吞在一起啊。",
                "0_H142": "实乃幸运……！"
            }
        },
        "0_H170": {
            "name": "对话 5",
            "text": {
                "0_H170": "呜哇！　吾吓了一跳！",
                "0_H171": "那边那位该不会就是护法少女吧！",
                "0_H172": "酒……酒吞！　酒吞快来看啊！　是货真价实的护法少女啊！"
            }
        },
        "0_H180": {
            "name": "对话 6",
            "text": {
                "0_H180": "怎么。为什么。为什么纲在！　为什么那家伙会……！",
                "0_H181": "呵呵，不……不过无妨。这次吾也定会更胜一筹。",
                "0_H182": "……什么？　输给他了？",
                "0_H183": "不可能。不可能啦！",
                "0_H184": "应该……不可能……"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "吾名为茨木童子。",
                "0_S011": "乃大江山之鬼的魁首。"
            }
        }
    },
    "ChrVoice_ids": [
        "702200"
    ]
}
{
    "id": 603400,
    "collectionNo": 218,
    "name": "牛若丸",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "爽快地笑！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "痛快地玩！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "愉快地学！"
            }
        },
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "鞍马天狗，到此拜访！ ……开玩笑的啦。"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "自在天眼——",
                "0_B051": "呵呵，你难道以为只有山上有天狗吗？",
                "0_B052": "这就是我全力戏水！",
                "0_B053": "天狗之羽团扇·暴风！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "兴奋天狗！（Exci-tengu）"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "让你见识一下天狗的高鼻子吧！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "玩耍才是修行。 牛若丸，回归童心！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "用尽全力玩个痛快，主人！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "哈～，真开心♪ 夏日的海洋真棒呢！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "惩罚愚蠢的人类也是天狗的职责！"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "这才有玩的价值……"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "还真是个粗暴的家伙呢……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "居然会玩累了，看来我还差得远呢…… 明天再请多指教哦。"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "不甘心……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "交给我吧！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "从空中解决吗！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "从海上进攻吗！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "真的可以这么玩吗？"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "天衣无缝的天狗石，出击，堂堂正正地打过去！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "Pom Poko Rin！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "呵呵，接下来在那附近搞恶作剧……"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "那个敌人是不是比我强……？"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "太阳……落山了……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "比猿猴更敏捷，比狸猫更变幻自如。",
                "0_B801": "而最重要的，是比天狗更有天狗感！",
                "0_B802": "这也就是我，鞍马山的牛若丸是也！",
                "0_B803": "……好开心啊！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "哪怕在玩耍过程中也不会有损自己的英姿。",
                "0_G011": "因为我是主人的护卫嘛！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "呃，不知道视线该往哪儿搁，您这话还真奇怪。",
                "0_G021": "在我看来，这身衣服比平时的甲胄还要紧身贴身啊。",
                "0_G022": "尤其是……嗯，胸部附近。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我终于也恢复正常状态了。",
                "0_G031": "我是不会输给其他人的，所以差不多就现在换衣服吧。",
                "0_G032": "摸索摸索，好了……嗯，有种从冬毛换成了夏毛的感觉。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "主人～！",
                "0_G041": "为了能更为自在地玩耍，也就是为了更努力地修行，我换上了更有天狗风格的装束啦！",
                "0_G042": "呵呵呵，这样一来，无论是森林树木上，还是笼罩着雾气的湖面上，都会是我新的游戏场所！",
                "0_G043": "来吧，主人，让我们一起玩个痛快吧！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "连山上的动物们，也没有能陪我玩耍——能陪我修行到这个程度的存在。",
                "0_G051": "我由衷……向您表示感谢，主人。",
                "0_G052": "然后……如果可能的话，今后也请……一直……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "今天是主人的生日啊，真是可喜可贺！",
                "0_H011": "那今天，我牛若愿意陪您玩任何游戏！",
                "0_H012": "……呃，若说这和平时的我没什么不同的话，或许确实没错。",
                "0_H013": "还是说——要我久违地赠送什么人的首级给您比较好……？",
                "0_H014": "哎，还是决定和我玩？　我好高兴啊！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "主人，主人！",
                "0_H021": "难……难难难难不成现在正在举办祭典？",
                "0_H022": "我想和主人一起去玩，想去想去啦！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "主人，我还没玩够呢！",
                "0_H031": "赶快去下一个游戏场所吧！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "主人就是主人，但对现在的我来说，既是游戏同伴，也是修行同伴。",
                "0_H041": "嗯，就算无事可做也不用担心！",
                "0_H042": "全都包在甚至能将鞍马山视为游乐园的我牛若身上吧！"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "尽管成了类似山中天狗般的存在，",
                "0_H051": "但我并未忘记主从关系。",
                "0_H052": "具体说来……",
                "0_H053": "无论玩什么样的游戏，都一定要让主人享受比我更高的乐趣！",
                "0_H054": "觉悟吧！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "让我想想，和山里的动物们……",
                "0_H062": "或许这不值得称道，但我在修行时代很喜欢恶作剧。",
                "0_H063": "第一次从师傅那里顺来这把团扇的时候……",
                "0_H064": "（沉默）……咳咳，还是不回忆那段过去了……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "平时我不太喜欢高处，但现在的我反而最喜欢高处了。",
                "0_H071": "因此……让我想想。",
                "0_H072": "讨厌的东西，呃。纯粹就是敌人吧？"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "如果能向圣杯许什么愿望的话吗？",
                "0_H081": "我只要能和自己由衷想和对方一起玩的对象一起玩耍，就心满意足了……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "唔，居然只要在南国换上泳装，就有种接近修行时代的我的感觉啊。",
                "0_H091": "而这则是当时偷偷借用过好几次的师父的团扇……",
                "0_H092": "真是不可思议呢。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "嗯，师傅和鞍马天狗的……关系吗？",
                "0_H101": "呵呵呵，那我就用沉默是金来回答吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我从以前起就一直有这个想法，主人很有玩耍的才能呢，太棒了！",
                "0_H111": "看来一定是经历了相当艰苦的修行吧。",
                "0_H112": "您究竟被要求做了些什么呢？",
                "0_H113": "是捉迷藏或是抽陀螺之类的吗？",
                "0_H114": "我真希望您能教我一些现代的游戏呢！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "啊哈哈，啊哈哈哈哈哈！",
                "0_H121": "哈……高兴得都汗流浃背了呢。",
                "0_H122": "不过，穿着泳装无论擦拭还是冲洗都很便利，真好！",
                "0_H123": "…………我总觉得布料可以再少一点啊……",
                "0_H124": "不行吗，这样啊……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "……我有种奇妙的感觉。我在鞍马山上的玩伴都是动物们，我只有一个人。",
                "0_H131": "……然而，现在却像这样无论去哪儿都和主人在一起。",
                "0_H132": "无论跑到哪里，无论怎么四处乱窜，回过头来，总能看到主人在对我笑……",
                "0_H133": "这种心情应该怎么形容呢。",
                "0_H134": "总觉得这里轻飘飘的，有种充实的感觉……",
                "0_H135": "如果是熊太郎和平吉的话，一定会因为这种感觉直接想要冬眠了吧。",
                "0_H136": "但不用担心，我不会冬眠。",
                "0_H137": "因为一旦睡着了，不就没法玩了嘛！　冬天也有冬天的游戏！",
                "0_H138": "好期待呀，主人！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "海伦娜阁下在看到我的时候说什么「感到了强烈的伟大灵魂」。",
                "0_H171": "伟大灵魂……究竟是什么呢……？"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "汪汪，汪，汪汪，嗷呜～！",
                "0_H181": "……唔唔……真是只沉默的狼阁下呢。",
                "0_H182": "还是说，果然是语言有问题呢。",
                "0_H183": "……哎？　啊，我的动物会话只是靠感觉来交流而已。",
                "0_H184": "因为我是天才嘛！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "哦，你的身材还是那么高大呢。",
                "0_H191": "以你这样的尺寸，应该能走着横跨到那边的岛上去吧？",
                "0_H192": "好嘞，万事都需要尝试，让我坐在你肩膀上，常陆坊！",
                "0_H193": "哎哟，我叫错名字了吗？",
                "0_H194": "不，没叫错吧？",
                "0_H195": "你怎么看？　嗯～？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "换上泳装之后，奇妙的是那段修行时代仿佛又重新回到了我身边……",
                "0_S011": "在鞍马山上，师傅教导我游玩就是修行。",
                "0_S012": "因此这才是现在的正道。",
                "0_S013": "主人，还请您多多和牛若我一起玩哦！"
            }
        }
    },
    "ChrVoice_ids": [
        "603400"
    ]
}
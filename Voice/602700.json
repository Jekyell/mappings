{
    "id": 602700,
    "collectionNo": 170,
    "name": "武则天",
    "voices": {
        "0_B010": {
            "name": "攻击 1(真名确认后)",
            "text": {
                "0_B010": "上吧～！"
            }
        },
        "0_B020": {
            "name": "攻击 2(真名确认后)",
            "text": {
                "0_B020": "痛吗？"
            }
        },
        "0_B030": {
            "name": "攻击 3(真名确认后)",
            "text": {
                "0_B030": "高兴吧！"
            }
        },
        "0_B050": {
            "name": "宝具 1(真名确认后)",
            "text": {
                "0_B050": "可憎的并非你，而是你内心的罪！",
                "0_B051": "朕之法，会用痛苦之臂将这所有罪拖出来！",
                "0_B052": "好好品尝这滋味吧！",
                "0_B053": "……咿哈。"
            }
        },
        "0_B080": {
            "name": "技能 1(真名确认后)",
            "text": {
                "0_B080": "别让朕多费工夫。"
            }
        },
        "0_B090": {
            "name": "技能 2(真名确认后)",
            "text": {
                "0_B090": "这就是所谓的力量提升！"
            }
        },
        "0_B100": {
            "name": "开始 1(真名确认后)",
            "text": {
                "0_B100": "圣神皇帝闪亮登场的说！"
            }
        },
        "0_B110": {
            "name": "开始 2(真名确认后)",
            "text": {
                "0_B110": "头抬的太高了！　注意你的举止！"
            }
        },
        "0_B120": {
            "name": "胜利 1(真名确认后)",
            "text": {
                "0_B120": "今天的拷问到此结束！"
            }
        },
        "0_B130": {
            "name": "胜利 2(真名确认后)",
            "text": {
                "0_B130": "你成了奠定好国家的基石了。睡吧……"
            }
        },
        "0_B140": {
            "name": "受击 2(真名确认后)",
            "text": {
                "0_B140": "呜！？　喂！"
            }
        },
        "0_B150": {
            "name": "受击 1(真名确认后)",
            "text": {
                "0_B150": "无礼到令人震惊！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1(真名确认后)",
            "text": {
                "0_B160": "朕要求重新来过！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2(真名确认后)",
            "text": {
                "0_B170": "难道还不够努力吗……？"
            }
        },
        "0_B180": {
            "name": "指令卡 1(真名确认后)",
            "text": {
                "0_B180": "行刑！"
            }
        },
        "0_B190": {
            "name": "指令卡 2(真名确认后)",
            "text": {
                "0_B190": "悔罪吧。"
            }
        },
        "0_B200": {
            "name": "指令卡 3(真名确认后)",
            "text": {
                "0_B200": "哭也已经晚了！"
            }
        },
        "0_B210": {
            "name": "宝具卡(真名确认后)",
            "text": {
                "0_B210": "惩罚的时间到了。"
            }
        },
        "0_B480": {
            "name": "技能 3(真名确认后)",
            "text": {
                "0_B480": "你应该……很期待吧？"
            }
        },
        "0_B800": {
            "name": "宝具 2(真名确认后)",
            "text": {
                "0_B800": "虽然不能总那么高兴……但你很幸运哦？",
                "0_B801": "因为你可以比任何人都近距离观赏朕执行公务！",
                "0_B802": "拷问！　拷问！　复拷问哦！",
                "0_B803": "咿呵呵呵呵～。"
            }
        },
        "0_G010": {
            "name": "升级(真名确认后)",
            "text": {
                "0_G010": "嗯嗯，干得漂亮！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2(真名确认后)",
            "text": {
                "0_G020": "嗯，这是理所当然的。",
                "0_G021": "甚至可以说做得太晚了。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1(真名确认后)",
            "text": {
                "0_G030": "还远远不够！",
                "0_G031": "服饰就该越豪华越好！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3(真名确认后)",
            "text": {
                "0_G040": "唔，逐渐恢复朕的威信了。",
                "0_G041": "身为皇帝，就该这样才对。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4(真名确认后)",
            "text": {
                "0_G050": "这才是真正圣神皇帝的身姿！",
                "0_G051": "——如何，唔，如何？",
                "0_G052": "你大可表现得更为惊讶和喜悦哦？",
                "0_G053": "应该说，快这么做。"
            }
        },
        "0_H010": {
            "name": "生日(真名确认后)",
            "text": {
                "0_H010": "生日啊。",
                "0_H011": "朕会为你安排礼物的，要什么你随便说。"
            }
        },
        "0_H020": {
            "name": "活动举行中(真名确认后)",
            "text": {
                "0_H020": "什么，未经朕允许就举办节日了！？",
                "0_H021": "岂有此理，现在就去确认！"
            }
        },
        "0_H030": {
            "name": "对话 1(真名确认后)",
            "text": {
                "0_H030": "来。偶尔去看看民间的样子，也是为政者的职责哦。"
            }
        },
        "0_H040": {
            "name": "对话 2(真名确认后)",
            "text": {
                "0_H040": "你是可以让朕侍奉你的存在哦。",
                "0_H041": "就不能表现出一些自己才是世界支配者的表情吗？"
            }
        },
        "0_H050": {
            "name": "对话 3(真名确认后)",
            "text": {
                "0_H050": "有才之人就该被重用，无才之人自然该居于能人之下。",
                "0_H051": "和出身家世无关，就该这样。",
                "0_H052": "……虽然有很多人不明白。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西(真名确认后)",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "当然是自己的国家啦！",
                "0_H062": "还喜欢改一些名字。因为容易厌倦。",
                "0_H063": "唔，朕的名号也差不多该改了吧。",
                "0_H064": "超英雄大皇帝……之类的如何……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西(真名确认后)",
            "text": {
                "0_H070": "讨厌的东西是猫和幽……",
                "0_H071": "啊！？",
                "0_H072": "朕……朕为什么非要将自己的弱点告诉你才行！"
            }
        },
        "0_H080": {
            "name": "关于圣杯(真名确认后)",
            "text": {
                "0_H080": "圣杯啊……许下想再次得到自己国家的愿望固然简单。",
                "0_H081": "但还是要靠自己的力量获得才行吧？"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1(真名确认后)",
            "text": {
                "0_H090": "朕会称呼你为御主。",
                "0_H091": "但与之无关，谁的地位更高是早已注定了的。",
                "0_H092": "可别忘了对朕的崇敬哦！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2(真名确认后)",
            "text": {
                "0_H100": "御主，无聊，好无聊啊～。",
                "0_H101": "就没有什么关于不听话从者的密告情报吗？"
            }
        },
        "0_H10130": {
            "name": "牵绊Lv.5(真名确认后)",
            "text": {
                "0_H10130": "朕……一直在独自统治国家",
                "0_H10131": "虽然存在有名无实的愚王与功利世俗的僧侣，但没试过与真正可靠的人一起统治国家。",
                "0_H10132": "所以，那个……下一个国家朕想找一个能共同统治的人……",
                "0_H10133": "哎，别让朕说得那么明白啊！　理解一下啊，你这傻瓜！"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3(真名确认后)",
            "text": {
                "0_H110": "最近总是能感受到御主的视线呢……",
                "0_H111": "呵呵呵。朕的妖艳让你忍不住了吗？",
                "0_H112": "朕还真是个有罪过的女人呢。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4(真名确认后)",
            "text": {
                "0_H120": "御主，那个……今天倒是可以破例……允许你夸奖朕哦？",
                "0_H121": "比如说，摸摸朕的头，或者给朕甜甜的糖果什么的！",
                "0_H122": "好了来吧，不要客气！　来吧！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "居然如此重用连真名都不知道的从者，你太愚蠢了。",
                "0_H131": "但是……这份愚蠢应该得到回报才行。",
                "0_H132": "好，朕迟早会赐予你一个国家。",
                "0_H133": "……呵呵，如果知道了朕的名字，你就不会把这当成戏言了哦？"
            }
        },
        "0_H140": {
            "name": "对话 4(真名确认后)",
            "text": {
                "0_H140": "唔唔，猫……有个很像猫的家伙。",
                "0_H141": "嘴里叫着汪，完全不明白什么意思……",
                "0_H142": "朕……朕一点都不害怕哦！　只是讨厌罢了！"
            }
        },
        "0_H150": {
            "name": "对话 5(真名确认后)",
            "text": {
                "0_H150": "唔唔，猫……有个很像猫的家伙。",
                "0_H151": "……是虎斑猫吗……",
                "0_H152": "那凶恶的钩爪是……真……真可怕……一点，也不可怕！",
                "0_H153": "朕只是想要在下次的拷问中借用一下罢了！"
            }
        },
        "0_H160": {
            "name": "对话 6(真名确认后)",
            "text": {
                "0_H160": "那个女人……该不会连朕也想暗杀吧……盯。"
            }
        },
        "0_H170": {
            "name": "对话 7(真名确认后)",
            "text": {
                "0_H170": "哦，那就是秦的始皇帝啊。",
                "0_H171": "首先统一了广阔的中华大地乃是毋庸置疑的丰功伟业。",
                "0_H172": "就连朕也无法失了礼数呢……",
                "0_H173": "有机会的话，真想和他好好探讨国家的统治与政策呢。"
            }
        },
        "0_H180": {
            "name": "对话 8(真名确认后)",
            "text": {
                "0_H180": "什么，你就是那位兰陵王吗。",
                "0_H181": "生前侍奉的是个愚蠢的主公，还真是辛苦你了呢……",
                "0_H182": "但是，朕可是对驾驭部下非常有自信的女帝哦。",
                "0_H183": "如何？　要不要易主侍奉朕呢？"
            }
        },
        "0_H190": {
            "name": "对话 9(真名确认后)",
            "text": {
                "0_H190": "你是赤兔马……不，吕布……？",
                "0_H191": "哎，这都不重要啦！",
                "0_H192": "总之，朕赐予你可以被朕骑乘的荣誉。",
                "0_H193": "哼，出发！",
                "0_H195": "啊，喂，等下，太快了……",
                "0_H196": "呜哇？"
            }
        },
        "0_S010": {
            "name": "召唤(真名确认后)",
            "text": {
                "0_S010": "朕之名乃武则天。",
                "0_S011": "居然胆敢使役朕……",
                "0_S012": "……无礼到反而让朕产生兴趣了哦？"
            }
        }
    },
    "ChrVoice_ids": [
        "602700",
        "602710"
    ]
}
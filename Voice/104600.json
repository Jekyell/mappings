        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "敬仰吧。",
                "0_B801": "崇拜吧。",
                "0_B802": "在此认识到，世间存在超越人类智慧的存在。",
                "0_B803": "——神威拔刀！",
                "0_B804": "——『神剑·草那艺之大刀』！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "现世与常世。",
                "0_B811": "领教其边界有多么脆弱而空虚吧。",
                "0_B812": "——神威拔刀！",
                "0_B813": "——『神剑·草那艺之大刀』！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "这是奖赏！"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "这相当不错♪"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "你诞生的日子……这样啊。",
                "0_H011": "虽非余的职责，但余会为你送上祝福。",
                "0_H012": "高兴吧高兴吧。快点快点。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "祭典非常好。",
                "0_H021": "什么，活动？",
                "0_H022": "好，去散步吧。走。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "怎么，想外出吗？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "噢噢，乖孩子，乖孩子。",
                "0_H041": "呵呵呵呵呵呵。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "所谓的魔力通路连接在一起还真奇怪。",
                "0_H051": "有种痒痒的感觉……",
                "0_H052": "算了，应该类似脖子上套枷锁吧。呵呵。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "酒很好，非常好。",
                "0_H061": "但是，为什么。迦勒底这个地方不用酒招待余。",
                "0_H062": "到底打算干嘛啊……",
                "0_H063": "孩子不可以喝酒？　什么？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "先叮嘱你一句。",
                "0_H071": "余睡觉时不准叫醒余。",
                "0_H072": "不能原谅。余不喜欢。",
                "0_H073": "知道了吗？",
                "0_H074": "……嗯，答得好。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "能实现愿望的杯子。",
                "0_H081": "那还真是。",
                "0_H082": "余不要。还是人类努力去获得吧。",
                "0_H083": "不过，作为盛酒的容器倒是不错。",
                "0_H084": "呀哈哈！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "和余散步开心吗？",
                "0_H091": "……唔？",
                "0_H092": "呵呵，和余四处走动那都是散步吧。",
                "0_H093": "是带着你到处逛的余之散步。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "过来过来。",
                "0_H101": "来，到这里来。",
                "0_H102": "由余来摸摸你的头吧。",
                "0_H103": "……好了，差不多到散步的时间了吧。",
                "0_H104": "好，我们走吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "你饲料还够吗？",
                "0_H111": "嗯，有在好好吃饭啊。",
                "0_H112": "那就好。",
                "0_H113": "照看饮食情况也算饲主的职责嘛，嗯嗯。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……哦？　哦？",
                "0_H121": "你发现了呀。",
                "0_H122": "没错，对余来说，你不过是余心血来潮饲养的一只生物罢了。",
                "0_H123": "此乃这个形象的余特别准备的玩法。",
                "0_H124": "至于蜕皮之后的余……这个嘛……呵呵……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "过来，过来。呵呵，不用害怕。",
                "0_H131": "余是不会弄坏你的。",
                "0_H132": "因为这灵基还没品尝到甜美，不足以用那种会弄坏的玩法。",
                "0_H133": "好了……",
                "0_H134": "你。这件事暂且不提，要不要试着提升余的灵基呢？",
                "0_H135": "余会教会你何谓甜美。",
                "0_H136": "……什么？　保持这样比较好？",
                "0_H137": "呀哈哈哈哈哈！　你还真是个好事之徒呢！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "有蛇的气息。",
                "0_H171": "哦，是外国的蛇神吗……？",
                "0_H172": "那当然非见不可！",
                "0_H173": "和野槌蛇们一起！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "啊。刚才有个女忍者吧。",
                "0_H181": "……大蛇的诅咒？",
                "0_H182": "有意思。",
                "0_H183": "那个女人看到了余的眼睛却防住了。呵呵，有意思。",
                "0_H184": "让她当余的妹妹吧。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "有鬼在吗……？",
                "0_H191": "嗯，真令人怀念。",
                "0_H192": "真可爱。呵呵呵。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "明明……是鬼，什么人啊。",
                "0_H201": "真是个麻烦的家伙。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "刚才那边是不是有什么？",
                "0_H211": "有种类似雾气的，又似乎令人怀念……",
                "0_H212": "唔。酒吞童子……？",
                "0_H213": "鬼？　那是什么。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "赤龙的气味……",
                "0_H221": "什么。那个金发碧眼的小鬼，究竟是什么人。",
                "0_H222": "唔嗯。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "——余乃伊吹童子。",
                "0_S011": "所谓的御主就是你吗？",
                "0_S012": "是吗。",
                "0_S013": "嗯。不想死的话，还是趁现在尽早讨好余为好。",
                "0_S014": "毕竟蜕皮后的余可不像这个形象的余那么有兴致陪人玩耍哦？"
            }
        },
        "11_B020": {
            "name": "攻击 2 大蛇神的分御灵",
            "text": {
                "11_B020": "雷电！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 大蛇神的分御灵",
            "text": {
                "11_B040": "看啊，灾祸就在此处！"
            }
        },
        "11_B080": {
            "name": "技能 1 大蛇神的分御灵",
            "text": {
                "11_B080": "空、岚、雷。"
            }
        },
        "11_B090": {
            "name": "技能 2 大蛇神的分御灵",
            "text": {
                "11_B090": "神鸣之音……！"
            }
        },
        "11_B110": {
            "name": "开始 2 大蛇神的分御灵",
            "text": {
                "11_B110": "现世与……常世……"
            }
        },
        "11_B120": {
            "name": "胜利 1 大蛇神的分御灵",
            "text": {
                "11_B120": "呵，呵呵呵。愉快。"
            }
        },
        "11_B130": {
            "name": "胜利 2 大蛇神的分御灵",
            "text": {
                "11_B130": "愉快，愉快。"
            }
        },
        "11_B150": {
            "name": "受击 1 大蛇神的分御灵",
            "text": {
                "11_B150": "愉快……！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 大蛇神的分御灵",
            "text": {
                "11_B160": "唔……脆弱的，身躯……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 大蛇神的分御灵",
            "text": {
                "11_B170": "沉睡……剩下的，就交给你了。"
            }
        },
        "11_B180": {
            "name": "指令卡 1 大蛇神的分御灵",
            "text": {
                "11_B180": "嗯……"
            }
        },
        "11_B190": {
            "name": "指令卡 2 大蛇神的分御灵",
            "text": {
                "11_B190": "这样啊，这样啊。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 大蛇神的分御灵",
            "text": {
                "11_B200": "这个嘛……"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 大蛇神的分御灵",
            "text": {
                "11_B210": "你说了吧。哈哈！"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 大蛇神的分御灵",
            "text": {
                "11_B220": "那么，就拔刀吧。"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 大蛇神的分御灵",
            "text": {
                "11_B230": "不允许你移开视线。"
            }
        },
        "11_B410": {
            "name": "攻击 4 大蛇神的分御灵",
            "text": {
                "11_B410": "斩！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 大蛇神的分御灵",
            "text": {
                "11_B440": "一头两头三头，呵呵呵！"
            }
        },
        "11_B480": {
            "name": "技能 3 大蛇神的分御灵",
            "text": {
                "11_B480": "畏惧吧。"
            }
        },
        "11_B490": {
            "name": "技能 4 大蛇神的分御灵",
            "text": {
                "11_B490": "休想……逃跑。"
            }
        },
        "11_B500": {
            "name": "开始 3 大蛇神的分御灵",
            "text": {
                "11_B500": "祭品啊。唔……"
            }
        },
        "11_B510": {
            "name": "开始 4 大蛇神的分御灵",
            "text": {
                "11_B510": "无妨。允许你……挣扎。"
            }
        },
        "11_B520": {
            "name": "胜利 3 大蛇神的分御灵",
            "text": {
                "11_B520": "生命，如此脆弱。"
            }
        },
        "11_B530": {
            "name": "胜利 4 大蛇神的分御灵",
            "text": {
                "11_B530": "机会难得—— 黄泉归还再来一战，如何？"
            }
        },
        "11_B540": {
            "name": "受击 4 大蛇神的分御灵",
            "text": {
                "11_B540": "有点痒。"
            }
        },
        "11_B550": {
            "name": "受击 3 大蛇神的分御灵",
            "text": {
                "11_B550": "这是……！"
            }
        },
        "11_B560": {
            "name": "无法战斗 3 大蛇神的分御灵",
            "text": {
                "11_B560": "不错，这样，真不错……"
            }
        },
        "11_B570": {
            "name": "无法战斗 4 大蛇神的分御灵",
            "text": {
                "11_B570": "唔……魔力不够……"
            }
        },
        "11_B840": {
            "name": "额外攻击 3 大蛇神的分御灵",
            "text": {
                "11_B840": "风、炎、雷——"
            }
        },
        "11_G010": {
            "name": "升级 1 大蛇神的分御灵",
            "text": {
                "11_G010": "活祭，也无妨哦。"
            }
        },
        "11_G060": {
            "name": "灵衣开放 大蛇神的分御灵",
            "text": {
                "11_G060": "——呵，呵呵，呵呵呵。",
                "11_G061": "汝召唤了余吧。明明可以满足于那个我……",
                "11_G062": "现世的生命是何等愚蠢啊。",
                "11_G063": "呵呵，你，已经沉沦了吧？",
                "11_G064": "俗话说人会抓住救命稻草，呵呵。",
                "11_G065": "但你居然想要触碰神……？",
                "11_G066": "哈哈，愉快，愉快！"
            }
        },
        "11_G110": {
            "name": "升级 2 大蛇神的分御灵",
            "text": {
                "11_G110": "无妨，供奉吧。"
            }
        },
        "11_G120": {
            "name": "升级 3 大蛇神的分御灵",
            "text": {
                "11_G120": "继续供奉吧。"
            }
        },
        "11_H010": {
            "name": "生日",
            "text": {
                "11_H010": "呵，呵呵，呵呵呵。",
                "11_H011": "是你的生命出现于现世的日子啊。",
                "11_H012": "余不会祝福，去拜托其他和魂吧。",
                "11_H013": "你已然身在世间灾祸之一的身旁。",
                "11_H014": "还是努力在黑暗中定睛凝神，坚持走到临终那刻吧。"
            }
        },
        "11_H020": {
            "name": "活动举行中",
            "text": {
                "11_H020": "听到祭典的乐曲声了……",
                "11_H021": "这样啊，是祭典呀。",
                "11_H022": "偶尔一次也无妨。走吧。"
            }
        },
        "11_H030": {
            "name": "对话 1",
            "text": {
                "11_H030": "要外出走走。",
                "11_H031": "……让余陪你？",
                "11_H032": "这样啊。"
            }
        },
        "11_H040": {
            "name": "对话 2",
            "text": {
                "11_H040": "余，乃是神明。",
                "11_H041": "允许汝等尊崇。",
                "11_H042": "允许汝等敬畏。"
            }
        },
        "11_H050": {
            "name": "对话 3",
            "text": {
                "11_H050": "余乃是世间万物的灾祸。",
                "11_H051": "也是汝等的灾祸。",
                "11_H052": "奉献吧，或者——",
                "11_H053": "挣扎吧。"
            }
        },
        "11_H060": {
            "name": "喜欢的东西",
            "text": {
                "11_H060": "余并不觉得现世有多少意义。虽然不觉得有……",
                "11_H061": "但有时，会被一些东西吸引住目光。",
                "11_H062": "强大的存在……不……",
                "11_H063": "弱小的存在……不……",
                "11_H064": "无论何种，余都会被闪耀的东西吸引住目光。"
            }
        },
        "11_H070": {
            "name": "讨厌的东西",
            "text": {
                "11_H070": "由于余，乃是神明……",
                "11_H071": "因此不能允许不敬。"
            }
        },
        "11_H080": {
            "name": "关于圣杯",
            "text": {
                "11_H080": "神圣的酒杯？",
                "11_H081": "呵呵，好吧。献上吧。",
                "11_H082": "余乃是八岐大蛇分出的首级，自然觉得酒更适合当供品。",
                "11_H083": "神圣的酒杯，应该会是不错的供品吧。"
            }
        },
        "11_H170": {
            "name": "对话 4",
            "text": {
                "11_H170": "外国的蛇……",
                "11_H171": "有各式各样的呢。",
                "11_H172": "有接近余的，也有和余相去甚远的。"
            }
        },
        "11_H180": {
            "name": "对话 5",
            "text": {
                "11_H180": "甲贺三郎的后裔……",
                "11_H181": "真是个有意思的人。",
                "11_H182": "她……得好好疼爱才行。"
            }
        },
        "11_H190": {
            "name": "对话 6",
            "text": {
                "11_H190": "鬼啊。山中之物。",
                "11_H191": "多么令人怀念。"
            }
        },
        "11_H200": {
            "name": "对话 7",
            "text": {
                "11_H200": "源某人……以人类的形态操控雷电的女人。",
                "11_H201": "那个混着某种存在吧？",
                "11_H202": "唔，不过怎么了……",
                "11_H203": "余究竟有哪里是明令禁止的呢……？"
            }
        },
        "11_H210": {
            "name": "对话 8",
            "text": {
                "11_H210": "刚才，向余说话的究竟是何人？",
                "11_H211": "能听到声音。就从那附近传来的……",
                "11_H212": "什么。酒吞童子？",
                "11_H213": "……你在说什么呢。那处只有无名之暗，如雾气般的存在。",
                "11_H214": "岂会说出话语。"
            }
        },
        "11_H2100": {
            "name": "对话 1 (牵绊Lv.5以上)",
            "text": {
                "11_H2100": "走吧。",
                "11_H2101": "待在家里固然不错，但外面也不错。"
            }
        },
        "11_H220": {
            "name": "对话 9",
            "text": {
                "11_H220": "那个金发的小家伙……余似乎在哪儿……",
                "11_H221": "唔……",
                "11_H222": "啊啊，是这样啊。",
                "11_H223": "愉快，愉快。"
            }
        },
        "11_H230": {
            "name": "对话 10",
            "text": {
                "11_H230": "那个金发碧眼的小家伙……",
                "11_H231": "样子变了好多呢？",
                "11_H232": "呵呵。这样子也甚是有趣。",
                "11_H233": "非常愉快。"
            }
        },
        "11_H30090": {
            "name": "牵绊Lv.1",
            "text": {
                "11_H30090": "唔……",
                "11_H30091": "你……这张脸有些似曾相识……",
                "11_H30092": "唔。"
            }
        },
        "11_H30100": {
            "name": "牵绊Lv.2",
            "text": {
                "11_H30100": "唔……人类竟然未献祭品就随意叫余。",
                "11_H30101": "怎么？　不知天高地厚可是一种恶哦。",
                "11_H30102": "干脆吃掉你一条手臂……",
                "11_H30103": "唔？　不，你……",
                "11_H30104": "是……御主吗？",
                "11_H30105": "哎，真是的。人类真难分辨。"
            }
        },
        "11_H30110": {
            "name": "牵绊Lv.3",
            "text": {
                "11_H30110": "还是完全无法分辨。",
                "11_H30111": "……不过感到高兴吧。余已经记住一点气味了。",
                "11_H30112": "你是……",
                "11_H30113": "唔～……唔……",
                "11_H30114": "不，下次有机会再说吧。"
            }
        },
        "11_H30120": {
            "name": "牵绊Lv.4",
            "text": {
                "11_H30120": "唔？　啊啊，没错。余在看你。",
                "11_H30121": "……嗯。……呵呵。记住了哦，记住了。",
                "11_H30122": "你就是，你，御主。",
                "11_H30123": "虽然还是无法分辨容貌，但能辨认气味。逐渐能辨认了。",
                "11_H30124": "或许是由于在现世同行的缘故吧。",
                "11_H30125": "……愉快。",
                "11_H30126": "余，我，竟然能记住人类的个体。",
                "11_H30127": "这可不多见哦……"
            }
        },
        "11_H30130": {
            "name": "牵绊Lv.5",
            "text": {
                "11_H30130": "到这里来，余不会吃了你啦……",
                "11_H30131": "不，区区一条手臂一条腿的程度应该不要紧吧。",
                "11_H30132": "……要紧？",
                "11_H30133": "这样啊。这事暂且不提，你呀，看来是时候了。",
                "11_H30134": "余来教你何谓常世的甜美吧。",
                "11_H30135": "没事……不用那样颤抖……"
            }
        },
        "11_H31800": {
            "name": "牵绊Lv.5 (地狱界曼荼罗 平安京通关后)",
            "text": {
                "11_H31800": "让余听听你的声音。",
                "11_H31801": "……啊啊，就是这声音。",
                "11_H31802": "那你应该就是当时的人类之子吧。",
                "11_H31803": "与金发碧眼的小家伙一起撕裂了所谓的罗刹王，没错吧。",
                "11_H31804": "原来如此。愉快，真愉快。",
                "11_H31805": "难怪余会被召唤。呵呵，这样啊，这样啊——",
                "11_H31806": "现在仍凭借着生命于现世前行吗。",
                "11_H31807": "这样啊……"
            }
        },
        "1_B020": {
            "name": "攻击 2",
            "text": {
                "1_B020": "雷电！"
            }
        },
        "1_B030": {
            "name": "攻击 3",
            "text": {
                "1_B030": "对不起啦～！"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "出击，三蠕虫！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "畏惧吧。",
                "1_B051": "颤抖吧。",
                "1_B052": "灾祸现在正要降临你们身上。",
                "1_B053": "——神威拔刀！",
                "1_B054": "——『神剑·草那艺之大刀』！"
            }
        },
        "1_B080": {
            "name": "技能 1",
            "text": {
                "1_B080": "轻重有些难掌握呢。"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "我·要·上·了·哟！"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "呵，呵，呵呵呵……哎？"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "好不容易诞生于世……真可惜。"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "呵，呵，呵呵。真开心啊……"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "啊啊，真开心。"
            }
        },
        "1_B140": {
            "name": "受击 2",
            "text": {
                "1_B140": "好痛。"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "我可不是在……开玩笑……"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "啊～，真舒服啊……"
            }
        },
        "1_B180": {
            "name": "指令卡 1",
            "text": {
                "1_B180": "好～。"
            }
        },
        "1_B190": {
            "name": "指令卡 2",
            "text": {
                "1_B190": "这挺好呢。"
            }
        },
        "1_B200": {
            "name": "指令卡 3",
            "text": {
                "1_B200": "这样也可以呢。"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "没问题！　御主！"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "那我要拔刀了哦。"
            }
        },
        "1_B230": {
            "name": "宝具卡 3",
            "text": {
                "1_B230": "可不要移开视线哦。"
            }
        },
        "1_B420": {
            "name": "攻击 5",
            "text": {
                "1_B420": "这样！"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "不好意思，但爸爸，拜托啦！"
            }
        },
        "1_B480": {
            "name": "技能 3",
            "text": {
                "1_B480": "——对不起啦。"
            }
        },
        "1_B490": {
            "name": "技能 4",
            "text": {
                "1_B490": "把魔力哔哔哔地～。"
            }
        },
        "1_B500": {
            "name": "开始 3",
            "text": {
                "1_B500": "祭品什么的，现在已经不流行了吧？"
            }
        },
        "1_B510": {
            "name": "开始 4",
            "text": {
                "1_B510": "一定要取悦我哦！　尽量！"
            }
        },
        "1_B520": {
            "name": "胜利 3",
            "text": {
                "1_B520": "你们真脆弱呢……"
            }
        },
        "1_B530": {
            "name": "胜利 4",
            "text": {
                "1_B530": "晚安，好好睡吧。"
            }
        },
        "1_B540": {
            "name": "受击 4",
            "text": {
                "1_B540": "哎呀！"
            }
        },
        "1_B550": {
            "name": "受击 3",
            "text": {
                "1_B550": "不会吧……！"
            }
        },
        "1_B560": {
            "name": "无法战斗 3",
            "text": {
                "1_B560": "累死我了……"
            }
        },
        "1_B570": {
            "name": "无法战斗 4",
            "text": {
                "1_B570": "好想喝啤酒啊……"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "敬仰吧。",
                "1_B801": "崇拜吧。",
                "1_B802": "在此认识到，世间存在超越人类智慧的存在。",
                "1_B803": "——神威拔刀！",
                "1_B804": "——『神剑·草那艺之大刀』！"
            }
        },
        "1_B810": {
            "name": "宝具 3",
            "text": {
                "1_B810": "现世与常世。",
                "1_B811": "领教其边界有多么脆弱而空虚吧。",
                "1_B812": "——神威拔刀！",
                "1_B813": "——『神剑·草那艺之大刀』！"
            }
        },
        "1_B840": {
            "name": "额外攻击 3",
            "text": {
                "1_B840": "风、炎，以及雷！"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "可不要用什么活祭哦？"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "还……还不会脱啦？",
                "1_G021": "好不容易挑选了当世的衣服嘛……",
                "1_G022": "啊，怎么？　想·让·我·脱·掉·呀？",
                "1_G023": "啊哈哈，这样啊～。"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "——是汝召唤了余吧。",
                "1_G031": "唔……呵呵，开玩笑啦。",
                "1_G032": "不要那么紧张啦。",
                "1_G033": "是吗，这里就是迦勒底呀。",
                "1_G034": "哎……？　似乎挺有意思的。",
                "1_G035": "今后也请多关照啦，御主。"
            }
        },
        "1_G040": {
            "name": "灵基再临 3",
            "text": {
                "1_G040": "既然灵基恢复到这个程度……",
                "1_G041": "不用原来的形象可不行呢。",
                "1_G042": "啊～，我还挺喜欢针织衫呢～。",
                "1_G043": "唔？　怎么了怎么了？",
                "1_G044": "看到神明闪耀的身躯，心脏砰砰乱跳了吗？",
                "1_G045": "我开玩笑的啦。"
            }
        },
        "1_G050": {
            "name": "灵基再临 4",
            "text": {
                "1_G050": "呵，呵呵呵呵。",
                "1_G051": "——辛苦你，如此供奉。",
                "1_G052": "余得好好夸奖你。",
                "1_G053": "感到高兴吧。余如此评价现世之物可是非常稀罕的。",
                "1_G054": "……无妨，再靠近一些。",
                "1_G055": "余是不会亏待你的。",
                "1_G056": "呵，呵呵，呵呵呵呵……"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "啊，可能还挺舒服的。"
            }
        },
        "1_G120": {
            "name": "升级 3",
            "text": {
                "1_G120": "有点想喝了……"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "生日快乐！",
                "1_H011": "快做那个啦，呃，就是在蛋糕上插上很多蜡烛的那个。",
                "1_H012": "哎？　机会难得，让我也来？",
                "1_H013": "哎，哎～但是我又不了解什么生日……",
                "1_H014": "啊，所以才一起吗。这真不错。",
                "1_H015": "那用战斗模式来吧。",
                "1_H016": "我·要·上·了·哟～！"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "活动！　祭典！",
                "1_H021": "真棒。那我们去玩吧。"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "要不要外出逛逛？",
                "1_H031": "去呼吸一下外面的空气吧。"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "你是御主。",
                "1_H041": "我是从者。",
                "1_H042": "嗯嗯，这我都知道啦。",
                "1_H043": "安啦安啦！",
                "1_H044": "……刚才那种说法很现代风吧？"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "我有让自己的举止尽量像一个从者哦？",
                "1_H051": "这是真的哦？　如果有什么错误就对不起啦？",
                "1_H052": "呃，比方说，战斗以外的时候，我会让身高保持180公分的模式……",
                "1_H053": "这样没问题吧？"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "喜欢的东西？",
                "1_H061": "那当然是酒啦！",
                "1_H062": "日本酒和烧酒、啤酒和葡萄酒、威士忌和白兰地、伏特加和龙舌兰酒、绍兴酒和艾尔酒全都喜欢～。",
                "1_H063": "还有美少年和美少女！　另外还有强者！",
                "1_H064": "如果是既强又纯情的就更好了……",
                "1_H065": "喜欢到想把对方吃掉！",
                "1_H066": "问我什么意思？",
                "1_H067": "那当然是双重！　含义！　对吧！"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "讨厌的东西？",
                "1_H071": "唔～。唔～～。",
                "1_H072": "唔～～～？"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "圣杯啊。",
                "1_H081": "我其实并不……",
                "1_H082": "啊～，不过那个，是金色的吧？",
                "1_H083": "是闪光的？　差不多啦。",
                "1_H084": "那个可以拿来当引诱那小家伙上钩的诱饵吧？"
            }
        },
        "1_H10090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H10090": "好不容易现界一次，",
                "1_H10091": "世界却变成了这个样子……对吧？",
                "1_H10092": "不过，也行啦。",
                "1_H10093": "这样我就会跟你走。",
                "1_H10094": "你会去各种各样的地方吧？",
                "1_H10095": "那我应该也会很高兴吧。"
            }
        },
        "1_H10100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H10100": "你真能走呢！",
                "1_H10101": "啊，这姑且是在夸奖你啦。",
                "1_H10102": "人类都会这样前行吗。还是说，你比较特别呢？",
                "1_H10103": "毕竟你召唤了我，应该是后者吧。",
                "1_H10104": "对，一定是这样。"
            }
        },
        "1_H10110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H10110": "偶尔也要好好休息一下才行哦？",
                "1_H10111": "让我想想，旅行……就没意义了吧。",
                "1_H10112": "总之还是滚来滚去休息吧。",
                "1_H10113": "怎么样？",
                "1_H10114": "我很擅长滚来滚去哦～。",
                "1_H10115": "那当然～，毕竟我是蛇嘛～。"
            }
        },
        "1_H10120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H10120": "啊，喂，有酒吗？",
                "1_H10121": "哎呀我平时就在喝啦，当然在喝啦。",
                "1_H10122": "不过姐姐我呀，希望晚上喝酒的时候，能有可以为我斟酒的同伴呢。",
                "1_H10123": "唔～，斟酒什么的虽然有些过时～。",
                "1_H10124": "但我毕竟是古老的神明嘛。过时才对！",
                "1_H10125": "哎？　没醉没醉。",
                "1_H10126": "我这才喝了三桶啦，嗝。"
            }
        },
        "1_H10130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H10130": "唔？",
                "1_H10131": "啊，抱歉。我刚才在盯着你看。",
                "1_H10132": "毕竟我有蛇的性质嘛，会盯着猎物看。",
                "1_H10133": "……当然这些话都是玩笑啦，玩笑。",
                "1_H10134": "不，也不算玩笑呢。因为我记住了你。",
                "1_H10135": "想要吃掉祭品或许会有些困难……",
                "1_H10136": "但我应该至少可以见证你前行的旅程吧。"
            }
        },
        "1_H170": {
            "name": "对话 4",
            "text": {
                "1_H170": "啊，你好～。",
                "1_H171": "您是异国的蛇神呀。",
                "1_H172": "我也是蛇，大家和睦相处吧！"
            }
        },
        "1_H180": {
            "name": "对话 5",
            "text": {
                "1_H180": "啊～！　是甲贺那个！　三亲家那个！",
                "1_H181": "啊～啊～啊～啊～。哎，竟然连那么之后的后代都被诅咒了呀……那还真是难受呢。",
                "1_H182": "哎呀，心情方面哦，也就心情方面哦，我倒也不是不能不能不能理解……",
                "1_H183": "哇。怎么了，突然哭起来……",
                "1_H184": "啊，抱住我了。",
                "1_H185": "啊啊……好乖好乖。一定很怕吧，好乖好乖。"
            }
        },
        "1_H190": {
            "name": "对话 6",
            "text": {
                "1_H190": "鬼。稍微有些古老的鬼啊。",
                "1_H191": "不是现代的吧。这样啊。"
            }
        },
        "1_H200": {
            "name": "对话 7",
            "text": {
                "1_H200": "严令禁止严令禁止的烦死了，",
                "1_H201": "那个美女想干嘛呀？",
                "1_H202": "打扮比我更夸张的女孩相当不少吧？",
                "1_H203": "再说那个美女的泳装也很夸张吧……"
            }
        },
        "1_H210": {
            "name": "对话 8",
            "text": {
                "1_H210": "什么？　有谁……说了什么吗？",
                "1_H211": "肯定有人说了什么啦～。就从那附近……",
                "1_H212": "哎，酒吞童子？",
                "1_H213": "……虽然有个类似雾气般的存在，但那里没人吧。",
                "1_H214": "呵呵，真是的～不要说那种吓人的话啦～。"
            }
        },
        "1_H220": {
            "name": "对话 9",
            "text": {
                "1_H220": "找到了找到了。金发的小家伙！",
                "1_H221": "哎呀，还是那么帅气呢。",
                "1_H222": "不过是不是稍微有些变化？　是呢。多少有点……",
                "1_H223": "……变化？　人理的影子吗？",
                "1_H224": "怎么可能嘛。好了，该怎么玩呢……"
            }
        }
    },
    "ChrVoice_ids": [
        "104600"
    ]
}
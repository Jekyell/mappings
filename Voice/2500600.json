{
    "id": 2500600,
    "collectionNo": 295,
    "name": "梵高",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "涂抹涂抹～。"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "大手笔……！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "送上的话语乃是……",
                "0_B041": "从不（nooit），不行（neen），决不（nimmer）！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "非画不可。",
                "0_B051": "描绘星空之下，超越生死的丝柏。",
                "0_B052": "信仰、浪漫、视觉陷阱的彼方。",
                "0_B053": "来自永劫，依仗星旋，",
                "0_B054": "赠君握手。",
                "0_B055": "──星月夜！！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "这是我的得意之作。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "能承蒙指名，不胜惶恐……"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "差不多就到这里，送上握手……"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "莫夫老师，有没有看到呢……"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "我喜欢……痛苦……"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "我先走一步……去往无影之国……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "对不起……对不起……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "我会画……！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "好高兴……"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "各位，请闭上眼睛……！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "非画不可……"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "就像太阳……"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "梵高美工刀！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "在花圃里高呵呵……"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "虽然只是习作，但请收下吧……",
                "0_B441": "哎嘿嘿！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "太抬举了……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "这是梵高艰难时期创作的一幅作品……",
                "0_B801": "究竟会发生什么……",
                "0_B802": "作家也不能保证！",
                "0_B803": "习作，丝柏与村庄！",
                "0_B804": "又名，",
                "0_B805": "星月夜！！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "好吃好吃……",
                "0_G011": "啊，还想要更多。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "吃饱了吃饱了……",
                "0_G111": "哎，还有吗？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "那个，御主大人，其实，哎嘿嘿，今天好像是您生日……",
                "0_H011": "唔呵呵……如果您不介意，将这幅石版画……放在……",
                "0_H012": "放在身边的话！　好高兴！",
                "0_H013": "祝您今后的人生都充满了光明的祝福……",
                "0_H014": "画与话……接下来……跳……跳舞吧……",
                "0_H015": "梵高梵高，梵高～……"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "御主大人御主大人……",
                "0_H021": "唔呵呵，看来发生了什么事哦……",
                "0_H022": "画笔都已经保养好了……！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "唔唔，肚子饿了……",
                "0_H031": "御主大人，我们一起去食堂吧……",
                "0_H032": "顺便会去……去……会去……约，出任务吧！",
                "0_H033": "怎么样，梵高笑话！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "梵高将御主大人奉为名副其实的主人，并完全服从……",
                "0_H041": "御主迷茫的话，就一起迷茫吧……"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主大人坚定不移……",
                "0_H051": "虽然在不停摇摆却依然坚定不移的人……",
                "0_H052": "这种生活方式十分耀眼。",
                "0_H053": "对于想要坚定，却始终无法做到的梵高来说，太耀眼了。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的……东西吗……",
                "0_H061": "温暖的色调……",
                "0_H062": "那栋黄房子、咖啡香、谈话投机的朋友、充满刺激与平静的创作时光…… 您希望听到这种答案吧？",
                "0_H063": "就是平凡地喜欢吃东西……",
                "0_H064": "比如肉或是鱼……",
                "0_H065": "哎嘿嘿……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我什么都喜欢！",
                "0_H071": "什么都可以给我，我什么都可以接受！",
                "0_H072": "……唯一不能接受的，或许就是，这样的自己吧。",
                "0_H073": "哎嘿嘿，对不起。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "并没有想要向圣杯许的愿望……",
                "0_H081": "就算颠覆那别离那背叛又能如何？",
                "0_H082": "我，将会变成什么？",
                "0_H083": "我，会是什么……",
                "0_H084": "这个疑问肯定就是我的愿望，但假如向圣杯许这个愿，一定正中邪神的下怀吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "御主大人……请将梵高……",
                "0_H091": "以做牛做马粉身碎骨的状态进行差遣吧……",
                "0_H092": "虽然牛马是负责拉磨磨面粉的。",
                "0_H093": "哎嘿嘿，梵高笑话……"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我喜欢……迦勒底……",
                "0_H101": "虽然这么说或许有些亵渎……",
                "0_H102": "但因为这里始终陷于危机的状况，为此需要梵高……",
                "0_H103": "需……需要吗？　如果不是的话……"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "那个，御主大人……其实我有个不情之请……",
                "0_H111": "不，没……没什么，唔呼呼哎嘿嘿！",
                "0_H112": "今天晚饭吃什么？",
                "0_H113": "如果能吃到的话，今天一定也很幸福。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "……您应该……已经知道了吧……？　梵高只是个空虚的从者。",
                "0_H121": "双手没有建立过任何伟业，心中也没有任何愿望……",
                "0_H122": "即便如此……御主大人，梵高现在胸怀的小小愿望是……！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "唔呵呵……梵高，就是您所知的梵高。",
                "0_H131": "乃是在我的灵基终结前都要维持不辱梵高之名人生的，不断彷徨的诞生于虚数之异乡人。",
                "0_H132": "即便如此，我也希望能保持这样。",
                "0_H133": "若能如愿……希望能在御主大人的身旁。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "尼莫亲……船长大人吗？",
                "0_H171": "非常帅气，又聪明，还很可靠……",
                "0_H172": "但莫名让人有种亲近感。",
                "0_H173": "是某种在遥远世界里的缘分，或是存在形式接近的感觉。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "竟然能见到北斋！　没想到她竟然是个女孩子！",
                "0_H181": "啊，嗯，我听说她的灵基特性了。竟然是父女。",
                "0_H182": "梵高如果能和提奥一起现界就好了……"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "阿比亲……",
                "0_H191": "她的信仰是真诚的……所以才会被大家利用……无论哪个时代都是如此呢。",
                "0_H192": "但是，我不明白。",
                "0_H193": "守护她那纯洁无瑕的信仰。还是展现对成年人的肮脏与恶意的抗争。",
                "0_H194": "究竟哪个才是她所渴望的存在方式呢……"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "杨贵妃大人和梵高是一样的职阶。",
                "0_H201": "无论外形与形式如何变化，这点应该都是不会改变的。",
                "0_H202": "如果杨贵妃大人也这么想就好了……"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "弗兰亲对我说，同为东拼西凑的怪物，让我们和睦相处吧。",
                "0_H211": "令我非常……高兴。",
                "0_H212": "在面对存在意义的方式上，她也有很多我可以学习的地方……",
                "0_H213": "哎嘿嘿……但竟然比梵高还要我行我素，的确有点厉害……"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "XX大人吗……",
                "0_H221": "嗯……成熟的女性真是辛苦呢……",
                "0_H222": "我时不时会为她泡咖啡……",
                "0_H223": "她会露出非常困惑的表情……",
                "0_H224": "唔呵呵……大概不在一个频道上吧……！"
            }
        },
        "0_N010": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 1",
            "text": {
                "0_N010": "我是梵高。",
                "0_N011": "作为一文不值的从者也想帮上忙，所以我接下了航海计划进度管理的任务……",
                "0_N012": "唔呵呵，那就开始报告吧……"
            }
        },
        "0_N020": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 2",
            "text": {
                "0_N020": "修理舰艇、确保资源、警戒敌人和开发新技术……",
                "0_N021": "唔呵呵，遇难船的工作量太离谱了……！",
                "0_N022": "忙得头晕眼花！"
            }
        },
        "0_N030": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 3",
            "text": {
                "0_N030": "像这样工作时，就会令我想起在古皮尔公司时的事……",
                "0_N031": "有学识的梵高……想做就能做好的梵高……态度恶劣以至于被解雇的梵高……",
                "0_N032": "唔呵呵，想死！　救我，提奥……！！"
            }
        },
        "0_N040": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 4",
            "text": {
                "0_N040": "啊，御主大人！",
                "0_N041": "我……我并没有偷吃虚数性资源的碎片！",
                "0_N042": "要听报告吧，我会报告，这就是尽忠报告（尽忠报国）！",
                "0_N043": "开玩笑啦，梵高笑话！！"
            }
        },
        "0_N050": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 5",
            "text": {
                "0_N050": "船长下达的指示，必须认真阅读各种附加条款，有计划地惯例进度。",
                "0_N051": "基础知识的确很重要。无论是绘画还是船只都一样呢！",
                "0_N052": "计划性也……",
                "0_N053": "的……的确很重要呢，哎嘿嘿。"
            }
        },
        "0_N060": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 6",
            "text": {
                "0_N060": "无论是拼死的航海，还是疯狂的祭典，结束后都会成为虚无缥缈的梦……",
                "0_N061": "但是，御主大人的足迹不会消失。因为梵高会记住。",
                "0_N062": "在回忆中，随时可以见面。",
                "0_N063": "在终会到来的那天之前，让我们短暂握个手吧。"
            }
        },
        "0_N070": {
            "name": "虚数大海战 幻想急航\n～上浮吧鹦鹉螺号～ 7",
            "text": {
                "0_N070": "不过，这次航行还真是趟美食之旅呢……唔呵呵……",
                "0_N071": "接骨木花水（Elderflower cordial），是欧洲传统的饮料，有机会的话，请在迦勒底也试试看吧。",
                "0_N072": "名字有点像某种邪神或种族纯粹是多心了哦……！？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，Foreigner。",
                "0_S011": "如您所见，是梵高。",
                "0_S012": "让我们一起涂改这个世界吧……",
                "0_S013": "开……开玩笑啦，唔呵呵，哎嘿嘿……！"
            }
        },
        "1_B010": {
            "name": "攻击 1",
            "text": {
                "1_B010": "蜿蜒！！"
            }
        },
        "1_B020": {
            "name": "攻击 2",
            "text": {
                "1_B020": "重叠！！"
            }
        },
        "1_B030": {
            "name": "攻击 3",
            "text": {
                "1_B030": "染透！！"
            }
        },
        "1_B040": {
            "name": "额外攻击 1",
            "text": {
                "1_B040": "哎嘿嘿！　哎嘿嘿！",
                "1_B041": "好高兴啊！！"
            }
        },
        "1_B050": {
            "name": "宝具 1",
            "text": {
                "1_B050": "喂，提奥，听我说啊！",
                "1_B051": "身为神的存在竟然执着于如此如此如此无聊的东西！",
                "1_B052": "真是真是真是群无聊的家伙！",
                "1_B053": "所以要重塑！",
                "1_B054": "全部重塑！！",
                "1_B055": "——星月夜！！"
            }
        },
        "1_B080": {
            "name": "技能 1",
            "text": {
                "1_B080": "比平时更多一些！"
            }
        },
        "1_B090": {
            "name": "技能 2",
            "text": {
                "1_B090": "如果这样的光就可以的话……"
            }
        },
        "1_B100": {
            "name": "开始 1",
            "text": {
                "1_B100": "世界应该更明亮一些比较好！"
            }
        },
        "1_B110": {
            "name": "开始 2",
            "text": {
                "1_B110": "将一切都涂抹，然后……？"
            }
        },
        "1_B120": {
            "name": "胜利 1",
            "text": {
                "1_B120": "变得非常滑溜溜了呢……哎嘿嘿……"
            }
        },
        "1_B130": {
            "name": "胜利 2",
            "text": {
                "1_B130": "然而光，尚遥不可及……"
            }
        },
        "1_B150": {
            "name": "受击 1",
            "text": {
                "1_B150": "这个，忍受……"
            }
        },
        "1_B160": {
            "name": "无法战斗 1",
            "text": {
                "1_B160": "不要……还没……御主大人……"
            }
        },
        "1_B170": {
            "name": "无法战斗 2",
            "text": {
                "1_B170": "最后的……握手……"
            }
        },
        "1_B210": {
            "name": "宝具卡 1",
            "text": {
                "1_B210": "因而祈祷使星笑……"
            }
        },
        "1_B220": {
            "name": "宝具卡 2",
            "text": {
                "1_B220": "用异端对异端，交给我吧！"
            }
        },
        "1_B410": {
            "name": "攻击 4",
            "text": {
                "1_B410": "对不起对不起对不起……！"
            }
        },
        "1_B420": {
            "name": "攻击 5",
            "text": {
                "1_B420": "再多来点刺激……"
            }
        },
        "1_B430": {
            "name": "攻击 6",
            "text": {
                "1_B430": "播撒种子之神的过错是……"
            }
        },
        "1_B440": {
            "name": "额外攻击 2",
            "text": {
                "1_B440": "哪怕面目全非，",
                "1_B441": "也要继续描绘……"
            }
        },
        "1_B800": {
            "name": "宝具 2",
            "text": {
                "1_B800": "心念亦惶恐，水之祭司啊，风之贵公子啊……",
                "1_B801": "来自夏日星（火星）之硕花，化作一滴疯狂，",
                "1_B802": "现请再次将星辰带去永眠之座吧……",
                "1_B803": "wgah'nagl fhtagn （于居所沉睡）……！",
                "1_B804": "——星月夜！"
            }
        },
        "1_G010": {
            "name": "升级 1",
            "text": {
                "1_G010": "锻炼锻炼！",
                "1_G011": "哎嘿嘿，好开心啊！"
            }
        },
        "1_G020": {
            "name": "灵基再临 2",
            "text": {
                "1_G020": "没有外观能分辨出来的变化。",
                "1_G021": "然而似乎有种非常可怕的蠢动，在灵核那里蠕动，颤动……"
            }
        },
        "1_G030": {
            "name": "灵基再临 1",
            "text": {
                "1_G030": "提起兴致……绽放了……",
                "1_G031": "哎嘿嘿，哎嘿嘿！　请欣赏稍微成熟一些的梵高吧！"
            }
        },
        "1_G040": {
            "name": "灵基再临 3",
            "text": {
                "1_G040": "啊啊……欺瞒只能到此为止了……",
                "1_G041": "哎嘿嘿……御主大人，我能揭露这冒牌从者理应自称的名字吗？",
                "1_G042": "不行？",
                "1_G043": "怎么能这样啦！"
            }
        },
        "1_G050": {
            "name": "灵基再临 4",
            "text": {
                "1_G050": "漫长生长的最后，灵基终于稳定下来了。",
                "1_G051": "竟然留这种东拼西凑的怪物一起拯救人类，真是个奇特的御主大人。",
                "1_G052": "永远在一起。",
                "1_G053": "永远，一辈，子（死）！",
                "1_G054": "噗噗哎嘿嘿，梵高笑话……！"
            }
        },
        "1_G110": {
            "name": "升级 2",
            "text": {
                "1_G110": "受虐受虐……",
                "1_G111": "唔呵呵，再多一些……"
            }
        },
        "1_H010": {
            "name": "生日",
            "text": {
                "1_H010": "在这御主大人降生于世的无可替代的日子，梵高也想送您一件无可替代的东西……",
                "1_H011": "来吧，请用这把刀果断砍下这里。",
                "1_H012": "应该会很配醋或是酱油。要下狠心哦。",
                "1_H013": "呼哈呼哈。"
            }
        },
        "1_H020": {
            "name": "活动举行中",
            "text": {
                "1_H020": "似乎听到愉快的呼唤了……",
                "1_H021": "唔呵呵，我们走吧？"
            }
        },
        "1_H030": {
            "name": "对话 1",
            "text": {
                "1_H030": "在房间里锻炼虽然也不错，但我开始怀念阳光了……"
            }
        },
        "1_H040": {
            "name": "对话 2",
            "text": {
                "1_H040": "御主大人是天上的太阳，我是海中的月亮……",
                "1_H041": "毕竟是水母。"
            }
        },
        "1_H050": {
            "name": "对话 3",
            "text": {
                "1_H050": "赐予这东拼西凑怪物自我的人物。",
                "1_H051": "比起某个恶心的创造者可要令人敬仰上百上亿倍。"
            }
        },
        "1_H060": {
            "name": "喜欢的东西",
            "text": {
                "1_H060": "我喜欢疼痛……",
                "1_H061": "无论我这东拼西凑的灵基如何成长，我都渴望质问自己是否该是如此的那份痛楚……",
                "1_H062": "所以，请不要客气，尽情在前线差遣我吧。",
                "1_H063": "无论从梵高的角度来说，还是从我的角度来说，这点应该都是一致的。"
            }
        },
        "1_H070": {
            "name": "讨厌的东西",
            "text": {
                "1_H070": "讨厌的东西……",
                "1_H071": "我不太喜欢过度的批判……",
                "1_H072": "一针见血的评价我当然欢迎…… 但我如此认真地绘画……也不用……那样贬低吧……",
                "1_H073": "啊啊啊啊啊！！　吃土豆吧——！！"
            }
        },
        "1_H080": {
            "name": "关于圣杯",
            "text": {
                "1_H080": "听说圣杯是万能的愿望机……",
                "1_H081": "但说到底，这东西就像一大笔钱吧。",
                "1_H082": "一旦买了重要的东西，重要度就会下降……",
                "1_H083": "可收到别人赠送又会很高兴……",
                "1_H084": "嗯，如果愿意给我，我当然很高兴……",
                "1_H085": "差不多5个吧……"
            }
        },
        "1_H10090": {
            "name": "牵绊Lv.1",
            "text": {
                "1_H10090": "御主大人，需要使用绘画吗？",
                "1_H10091": "不用？",
                "1_H10092": "我想也是，那我告辞了！",
                "1_H10093": "哎，因为除此以外就没有梵高可以做的了……"
            }
        },
        "1_H10100": {
            "name": "牵绊Lv.2",
            "text": {
                "1_H10100": "啊，御主大人……",
                "1_H10101": "唔呵呵，问我这是什么画……？",
                "1_H10102": "这个嘛……梵高也不知道。",
                "1_H10103": "只知道梵高是不可能画这种画的……"
            }
        },
        "1_H10110": {
            "name": "牵绊Lv.3",
            "text": {
                "1_H10110": "…………哎嘿嘿，不要，一直盯着我……",
                "1_H10111": "这样子……这样子……明明不是任何人……",
                "1_H10112": "成形的罪与恶却毫无疑问，是属于我的……"
            }
        },
        "1_H10120": {
            "name": "牵绊Lv.4",
            "text": {
                "1_H10120": "深邃、深邃，悠远、悠远……",
                "1_H10121": "过去我发出的呼唤，这次似乎在呼唤我……",
                "1_H10122": "御主大人……虽然我的手长得这样……",
                "1_H10123": "但请您……请您……握住我的手……"
            }
        },
        "1_H10130": {
            "name": "牵绊Lv.5",
            "text": {
                "1_H10130": "我做梦都没想到自己竟会获得这样的存在形式，也没想到竟会有全盘接受这些的御主……",
                "1_H10131": "还请您永远多多关照哦？",
                "1_H10132": "可以靠近一点吗？",
                "1_H10133": "虽然有些滑溜溜的，但不要紧吧，哎嘿嘿……"
            }
        },
        "1_H170": {
            "name": "对话 7",
            "text": {
                "1_H170": "尼莫亲……船长大人吗？",
                "1_H171": "非常帅气，又聪明，还很可靠……",
                "1_H172": "但莫名让人有种亲近感。",
                "1_H173": "是某种在遥远世界里的缘分，或是存在形式接近的感觉。"
            }
        },
        "1_H180": {
            "name": "对话 8",
            "text": {
                "1_H180": "竟然能见到北斋！　没想到她竟然是个女孩子！",
                "1_H181": "啊，嗯，我听说她的灵基特性了。竟然是父女。",
                "1_H182": "梵高如果能和提奥一起现界就好了……"
            }
        },
        "1_H190": {
            "name": "对话 9",
            "text": {
                "1_H190": "阿比亲……",
                "1_H191": "她的信仰是真诚的……所以才会被大家利用……无论哪个时代都是如此呢。",
                "1_H192": "但是，我不明白。",
                "1_H193": "守护她那纯洁无瑕的信仰。还是展现对成年人的肮脏与恶意的抗争。",
                "1_H194": "究竟哪个才是她所渴望的存在方式呢……"
            }
        },
        "1_H200": {
            "name": "对话 10",
            "text": {
                "1_H200": "杨贵妃大人和梵高是一样的职阶。",
                "1_H201": "无论外形与形式如何变化，这点应该都是不会改变的。",
                "1_H202": "如果杨贵妃大人也这么想就好了……"
            }
        },
        "1_H210": {
            "name": "对话 11",
            "text": {
                "1_H210": "弗兰亲对我说，同为东拼西凑的怪物，让我们和睦相处吧。",
                "1_H211": "令我非常……高兴。",
                "1_H212": "在面对存在意义的方式上，她也有很多我可以学习的地方……",
                "1_H213": "哎嘿嘿……但竟然比梵高还要我行我素，的确有点厉害……"
            }
        },
        "1_H220": {
            "name": "对话 12",
            "text": {
                "1_H220": "XX大人吗……",
                "1_H221": "嗯……成熟的女性真是辛苦呢……",
                "1_H222": "我时不时会为她泡咖啡……",
                "1_H223": "她会露出非常困惑的表情……",
                "1_H224": "唔呵呵……大概不在一个频道上吧……！"
            }
        }
    },
    "ChrVoice_ids": [
        "2500600"
    ]
}
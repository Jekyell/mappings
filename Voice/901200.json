            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "要再次请你多关照啦！",
                "0_S011": "感谢你陪我一起冒险！",
                "0_S012": "啊啊，真是太开心了。",
                "0_S013": "一生的回忆肯定就是指这种事吧。",
                "0_S014": "我绝对不会忘记。",
                "0_S015": "你怎么样呢？"
            }
        },
        "0_S020": {

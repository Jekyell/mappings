{
    "id": 404900,
    "collectionNo": 366,
    "name": "黄飞虎",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "天化！ 【天化】见识我的银锤吧！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "天禄！ 【天禄】得手了！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "以人牛一体的枪术——贯穿！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "向青峰山紫阳洞洞主，",
                "0_B051": "清虚道德真君请愿！",
                "0_B052": "作为吾子天化的代理人，借璀璨星光一剑！",
                "0_B053": "莫邪宝剑！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "调整气息。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "儿子们啊！ 【天化】在此！ 【天禄】是。 【天爵】是。 【天祥】是！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "武成王，出击！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "吾之长枪不带一丝阴霾。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "令人昂首挺胸的战斗，还真是不错。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "走吧，去往我们该去的地方。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "不能退……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "这也是，天数，吗……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "我现在，就去你们那边……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "甚好！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "接受！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "突破！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "让你们见识黄家的牵绊吧。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "借我一用，天化啊。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "出击！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "金眼神莺啊，上吧！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "看来是不需要手下留情了！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "天爵啊，一起上！ 【天爵】是！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "当着我孩子们的面，可不能输！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "咆哮吧，五色神牛！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "赌上一族的尊严。"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "孩子们啊，助我一臂之力。",
                "0_B801": "【天化】没问题！",
                "0_B802": "【天禄】当然！",
                "0_B803": "【天爵】遵命。",
                "0_B804": "【天祥】是！",
                "0_B805": "此乃青峰山至宝，无形的断头之光！",
                "0_B806": "【全体】莫邪宝剑！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "【天禄】出场啦出场啦！",
                "0_B811": "【天祥】天祥要第一个上！",
                "0_B812": "【天爵】来晚了。",
                "0_B813": "【天化】原本应该是我的，你们别比老哥显眼啦。",
                "0_B814": "【天禄】不过～。／【天爵】但是啊。／【天祥】因为……",
                "0_B815": "你们几个！　给我适可而止！",
                "0_B816": "【兄弟四人】对，对不起，父亲大人！"
            }
        },
        "0_B910": {
            "name": "攻击 7",
            "text": {
                "0_B910": "【天祥】天祥也能行！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "是锻炼的成果吗。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "就算外表没有变化，内在也确实成长了。",
                "0_G021": "放心吧。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "这就是灵基再临……",
                "0_G031": "全新的装束吗。那在下就感激地收下了。",
                "0_G032": "嗯，很温暖。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "在下曾经觉得战斗时只要能动就好……",
                "0_G041": "但妻子总是发火说，一国大将军可不能这样。",
                "0_G042": "这身装束应该能同时兼顾方便行动与保持威严。",
                "0_G043": "人能够绷紧神经。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "其实，吾等并没有追随丞相的战斗到最后一刻。",
                "0_G051": "力有不逮，大家都战败了。",
                "0_G052": "存活的孩子也为了保留血脉，而被要求带着兄弟们的遗体返回西岐了。",
                "0_G053": "没能触及——该打倒的邪恶，该诛杀的敌人。",
                "0_G054": "但是，正因为如此，这次的战斗才不一样。",
                "0_G055": "我们黄家一门会陪伴主公到最后一刻。",
                "0_G056": "在下向您发誓，赌上武人的尊严。",
                "0_G057": "（哞～）",
                "0_G058": "呵呵，在下的灵兽似乎也有同感呢。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "并非孩子们，而是在下成长吗……"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "呼，出了一身汗。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "主公的生日吗。",
                "0_H011": "……唯独现在，还请允许在下像父亲一样看着您。",
                "0_H012": "对吾等这样的存在来说，守望孩子的成长是无与伦比的喜悦。",
                "0_H013": "如若可能……希望您能，永远健康茁壮。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "祭典和节日的时候才要格外小心哦，主公。",
                "0_H021": "因为酒醉的妖怪或许会趁着夜黑风高来吃人。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "五色神牛似乎缠着在下想去散步，怎么样？",
                "0_H031": "愿不愿意奉陪呢？"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "正所谓……『君不正，臣投外国』。",
                "0_H041": "而这个说法究竟是否正确。",
                "0_H042": "主公啊，您有没有想过呢？"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "虽说曾经有武成王这个称号，但身在此处的在下只是一介武人，一介从者。",
                "0_H051": "下令无需犹豫哦。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗。",
                "0_H061": "应该还是……家人吧。",
                "0_H062": "但正因为如此，在下才会失去妻子与妹妹这事……",
                "0_H063": "不。",
                "0_H064": "除此以外嘛，一般——",
                "0_H065": "还是酒啦！　在下酒量很大哦！"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "在下稍微有些不擅长应对夫妻对手呢。",
                "0_H071": "说起来有些不好意思，在下就是被张奎和高兰英夫妻杀掉的。",
                "0_H072": "不，当然，为了消除这种不擅长，在下也会勤奋锻炼！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "能实现所有愿望的杯子吗。",
                "0_H081": "与其说是厉害，更会觉得可怕呢。",
                "0_H082": "换作纣王的话，应该会不管牺牲多少人命都想要得到吧。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "哼，哈！",
                "0_H091": "……是主公吗。对不起，在下正在锻炼，浑身是汗。",
                "0_H092": "您还是不要靠近在下为好吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "假如主公坠入邪恶之路，真正的武人该作何选择。",
                "0_H101": "那时，该作何选择。",
                "0_H102": "在下……应该会不断寻找这个问题的答案吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "其实在下本应让孩子们也来正式向主公问候才对……",
                "0_H111": "但那是在下这个人理影子的，类似于影子的影子。",
                "0_H112": "在战场以外是很难召唤出来的。还请谅解。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "与在下同在的，当然不只是在下的孩子们。",
                "0_H121": "还有在下的父亲黄滚、弟弟飞彪与飞豹，以及在下的妻子……",
                "0_H122": "虽然能感受到他们的存在，但似乎始终无法获得形体。",
                "0_H123": "……或许总有一天，也能等到与他们对话的一天吧。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "在下身在这里，与您是一位贤主这件事。",
                "0_H131": "两者本质上恐怕没有任何关系。",
                "0_H132": "……武人有时也不得不侍奉走错道路的君王。",
                "0_H133": "但正因为如此——在下才会庆幸您是位贤主。",
                "0_H134": "由衷，这么认为。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "苏妲己……",
                "0_H171": "你这家伙……！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "不好意思，能让我看看您的那把剑吗？",
                "0_H181": "……唔，给人的感觉有点相似，但这是……？"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "这种美，以及神仙的气息……",
                "0_H192": "是来自某个洞府的仙姑大人吗？"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "这不是哪吒阁下吗，您也在这里啊！",
                "0_H201": "虽说是影子般的存在，但应该迟早也能见到天化吧。",
                "0_H202": "希望你们还能继续和睦相处……",
                "0_H203": "打架适可而止哦。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "这不是丞相阁下吗！",
                "0_H211": "没想到还能有机会和您并肩战斗。",
                "0_H212": "在下深知您的可怕之处……",
                "0_H213": "无论身为敌人，还是身为同伴。",
                "0_H214": "……太令人安心了。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "您是，殿下……！？",
                "0_H221": "不。应该，是搞错了人吧。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "唔，本以为只有在下呢，没想到这里还有其他带着牛的武将呢。",
                "0_H231": "（哞～）",
                "0_H232": "哈哈哈，五色神牛似乎也很高兴呢。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "哟，威廉·退尔阁下。",
                "0_H241": "我觉得在教育儿子的话题上，在下有很多话可以和您聊。",
                "0_H242": "当然，不会白聊的。在下准备了好酒哦。"
            }
        },
        "0_I010": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 1",
            "text": {
                "0_I010": "唔，来得好！",
                "0_I011": "虽然是第一次体验，但武成王黄飞虎定会出色地完成看店的任务！"
            }
        },
        "0_I020": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 2",
            "text": {
                "0_I020": "在下听传言说，在这种情况下首先应该免费提供一些东西。",
                "0_I021": "……那就是笑容！",
                "0_I022": "呵。",
                "0_I023": "……呵呵，稍微有些不好意思呢。"
            }
        },
        "0_I030": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 3",
            "text": {
                "0_I030": "呵呵，一脸高兴呢。",
                "0_I031": "让在下想起旅途归来，将礼物送给儿子们时的景象呢……"
            }
        },
        "0_I040": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 4",
            "text": {
                "0_I040": "抱歉，因为不太习惯，不太懂赠品的赠送方法。",
                "0_I041": "感觉一个不小心，库存都会瞬间清空……"
            }
        },
        "0_I050": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 5",
            "text": {
                "0_I050": "要再来一份吗？",
                "0_I051": "不用客气什么哦。"
            }
        },
        "0_I060": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 6",
            "text": {
                "0_I060": "忘记拿什么东西了吗？",
                "0_I061": "无妨，反正现在也无需那么着急。",
                "0_I062": "如果不介意的话，在下可以暂时奉陪。"
            }
        },
        "0_I070": {
            "name": "108人的万圣节·叛乱！\n～小龙娘水浒演义～ 7",
            "text": {
                "0_I070": "噢噢，来得好。",
                "0_I071": "没什么，即将关店的看店工作其实还挺无聊的。",
                "0_I072": "要不要来杯茶？",
                "0_I073": "不用担心，这是所谓的福利啦。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "在下乃武成王，名黄飞虎。",
                "0_S011": "是为奉您为主而来的。",
                "0_S012": "您究竟是文王那样的仁君，还是纣王那样的昏君……",
                "0_S013": "在下定会仔细评判。"
            }
        }
    },
    "ChrVoice_ids": [
        "404900"
    ]
}
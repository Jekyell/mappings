{
    "id": 404000,
    "collectionNo": 274,
    "name": "欧罗巴",
    "voices": {
        "0_B040": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B040": "塔罗斯灵血！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "来吧，可爱的孩子。",
                "0_B051": "那位大人的爱，那位大人的赠礼。",
                "0_B052": "守护我的青铜的你。",
                "0_B053": "——青铜巨人的超重锤！",
                "0_B054": "嗯嗯，真精神！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "唔嗯唔嗯……唔？　什么事？"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "宙斯大人，宙斯大人……"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "哎呀，接下来会发生什么呢？"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "…………呼。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "咦？　结束了吗？"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "大家，辛苦了。"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "好～。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "什么？"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "什么事？"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "啊，是叫塔罗斯吧。好的！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "塔罗斯！　塔罗斯，在吗！"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "轮到你出马啦，塔罗斯。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "上～上～！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "上吧。"
            }
        },
        "0_B440": {
            "name": "额外攻击 {0}",
            "text": {
                "0_B440": "哎呀！",
                "0_B441": "是灵血哦！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "来吧，让我们一起努力吧。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "吁，吁。呵呵呵。"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "打起精神前进吧！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "大家都是好孩子。"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "要吃糖吗？"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "真是的。"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "宙斯大人！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "宙斯大人……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "赫拉大人……出什么事了吗……"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "来吧，出现吧。",
                "0_B801": "守护我的青铜身躯。",
                "0_B802": "沸腾的血液，烧尽一切的炙热的你。",
                "0_B803": "——青铜巨人的超重锤！",
                "0_B804": "……干得漂亮！"
            }
        },
        "0_G010": {
            "name": "升级 {0}",
            "text": {
                "0_G010": "吁，吁。你看，我的公牛变得更强壮了！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "尽管外观没有变化……",
                "0_G021": "但灵基似乎发生了某种变化……",
                "0_G022": "真有意思呢……"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "哎呀，哎呀呀……！",
                "0_G031": "外观居然会变化呀。呵呵呵，真开心。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "哎呀……！",
                "0_G041": "这身打扮，就是骑在那位的背上，驰骋于大地……",
                "0_G042": "也就是驰骋于现在名为欧洲的土地上时的打扮呢。",
                "0_G043": "呵呵呵，居然会变成这样呀！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "辛苦你居然将我的灵基培养到这个程度了。",
                "0_G051": "我得好好道谢才行呢。",
                "0_G052": "是啊，作为给你的奖赏……",
                "0_G053": "呵呵，还是直接问你比较好吧。",
                "0_G054": "现在的孩子究竟喜欢什么呢？"
            }
        },
        "0_G110": {
            "name": "升级 {0}",
            "text": {
                "0_G110": "健康的秘诀呀，就是清晨锻炼！"
            }
        },
        "0_G120": {
            "name": "升级 {0}",
            "text": {
                "0_G120": "要品尝糖果吗？"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝你生日快乐。",
                "0_H011": "由我来代替宙斯大人和世界祝福你吧。",
                "0_H012": "诸神、世界，",
                "0_H013": "一定会回应不断前进的你。",
                "0_H014": "很抱歉一直让你忍受痛苦的回忆。",
                "0_H015": "……先不提这件事了，今天就由我来好好宠你吧！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "哎呀，哎呀！　是节日！",
                "0_H021": "我们去看看吧！",
                "0_H022": "呵呵，真期待。我给你买点什么吧。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "一起出门吧！",
                "0_H031": "呵呵，散步非常好哦，我最喜欢了。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "御主和从者……？",
                "0_H041": "我不是很了解这些……",
                "0_H042": "呵呵。对不起哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "啊，御主。",
                "0_H051": "今天也是好孩子吧？",
                "0_H052": "……来，好孩子好孩子。好孩子就该被摸摸头。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西？",
                "0_H061": "嗯，我喜欢晒太阳。",
                "0_H062": "也喜欢午睡。",
                "0_H063": "还喜欢散步。",
                "0_H064": "是呢，我呀……有好多喜欢的东西，如果继续说下去，太阳或许都要落山了。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西？",
                "0_H071": "是啊……",
                "0_H072": "不，还是不说了。",
                "0_H073": "因为一旦说出口，感觉就会成真。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯？",
                "0_H081": "是啊……",
                "0_H082": "如果对塔罗斯使用的话，会不会变得更加更加强大呢！",
                "0_H083": "不，我开玩笑的。塔罗斯已经足够强大了。",
                "0_H084": "我不需要什么圣杯。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "……呵呵。什么事？",
                "0_H091": "关于我吗？",
                "0_H092": "和其他英灵相比，似乎更为温和、悠闲？",
                "0_H093": "哎呀，是吗……",
                "0_H094": "不。嗯，或许是吧。",
                "0_H095": "因为我不是王，不是士兵，更不是战士。",
                "0_H096": "根本不是什么战斗的英灵吧。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "……呼……",
                "0_H101": "啊，对不起。我睡着了。",
                "0_H102": "总觉得今天非常舒服。",
                "0_H103": "呵呵，你也来这边吧。",
                "0_H104": "给你吃甜甜的糖果。",
                "0_H105": "啊啊，还是说，要我用梳子给你梳头发吗？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "你已经发现了吧？",
                "0_H111": "我自己是无法战斗的。",
                "0_H112": "如果没有了塔罗斯和公牛的话，我……",
                "0_H113": "只能成为你的盾牌了吧。",
                "0_H114": "——唔？　嗯，我当然可以当你的盾牌。",
                "0_H115": "呵呵，你想，所谓的从者呀，都是比普通人类更为顽强的哦。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我是获得了主神之爱的女人。",
                "0_H121": "即便成了英灵，那位大人也像这样赐予了我佑护。",
                "0_H122": "话说，你知道……这究竟是什么意思吗？",
                "0_H123": "呵呵。我知道是什么意思哦。",
                "0_H124": "那位大人在说，去守护，去守护那个人。",
                "0_H125": "…………指的是你哦。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "泛人类史现在正面临巨大的危机。",
                "0_H131": "而那位大人，宙斯大人非常清楚。",
                "0_H132": "即便成为了神灵，不再像神代那样存在，他也在为你们担心。",
                "0_H133": "没错，你，当然也包括玛修哦。",
                "0_H134": "……问我为什么？",
                "0_H135": "呵呵，这是因为呀……",
                "0_H136": "对我们来说，你们……就像是无比遥远的孙辈一样。",
                "0_H137": "所以当然想给你们糖果，想抚摸你们的头，为你们梳头发。",
                "0_H138": "好啦，来吧。欧罗巴奶奶来给你枕膝盖吧！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "哎呀，阿耳忒弥斯！　你是那位大人的女儿，那也相当于是我的女儿！",
                "0_H171": "好孩子，好孩子。",
                "0_H172": "哎哎，俄里翁阁下也和你在一起吗！？"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "哎呀，你……！",
                "0_H181": "阿斯忒里俄斯！",
                "0_H182": "你是弥诺斯的孩子吧？",
                "0_H183": "那你应该称呼我为奶奶。",
                "0_H184": "没有……血缘关系……？",
                "0_H185": "不，这种事无关紧要啦。好了，来吧……来吧……",
                "0_H186": "啊啊，好乖好乖，真是个好孩子……！",
                "0_H187": "我会紧紧拥抱你！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "俄里翁阁下！？　哎呀呀……！",
                "0_H191": "既然是我女儿的丈夫，就相当于我的儿子吧！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "因为你是阿波罗大人的孩子，",
                "0_H201": "所以你也是我的孙子。",
                "0_H202": "来吧，拥抱一下吧。来吧！"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "出生在欧罗巴的土地，欧洲大陆的孩子们都是我的后裔。",
                "0_H211": "好孩子，好孩子。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "出生在欧洲以外地区的孩子们？",
                "0_H221": "他们呀，嗯，就像是我后裔的朋友。",
                "0_H222": "来吧，大家都是好孩子哦！"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "好可爱～～！",
                "0_H231": "哎呀，哎呀，哎呀！　怎么会这样！",
                "0_H232": "居然有那么可爱的英灵……",
                "0_H233": "啊啊，真是的，抱紧～！",
                "0_H234": "……啊……啊啊，糖果完全不够了呢。",
                "0_H235": "借厨房用一下！　我要为你们做好多好吃的东西哦！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我是欧罗巴，您好。",
                "0_S011": "啊，呃……是啊，那个叫什么来着……",
                "0_S012": "Ri……Riden……！　对，Riden！",
                "0_S013": "以Riden职阶现界——",
                "0_S014": "嗯。什么事，塔罗斯？",
                "0_S015": "唔……错了？　不是Riden，而是Rider？",
                "0_S016": "………………哎呀。",
                "0_S017": "咳咳。那我再来一次。我是以Rider职阶现界的欧罗巴哦。",
                "0_S018": "是主神宙斯的妃子之一。",
                "0_S019": "请多指教啦，迦勒底的御主。"
            }
        }
    },
    "ChrVoice_ids": [
        "404000"
    ]
}
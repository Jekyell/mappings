{
    "id": 104800,
    "collectionNo": 301,
    "name": "迦尔纳〔圣诞〕",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "吃我这招，圣诞拳！"
            }
        },
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "能看破吗。"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "下巴！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "用猛击定胜负！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "终结组合技……要上了！",
                "0_B051": "用这连击必能K.O.。",
                "0_B052": "苏利耶的力量啊——",
                "0_B053": "寄宿在我的拳上吧！",
                "0_B054": "圣人连续拳！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "给你礼物。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "真正的圣诞老人眼睛会发光。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "回合开始。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "拳头已经预热好了。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "已经不需要站起来了。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "这就是所谓的……K.O.胜利吧。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "没用……！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "容我进入……休息时间吧……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "必须在倒计时十秒结束之前……站起来才行……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "就这么办。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "这战术组合不错。"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "一、二！"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "猛击吧。"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "就是说……轮到圣诞老人出场了吧。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "拳头的间距。"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "不会被捉到。"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "控制左边的人就能控制圣诞节！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "敲响铜锣吧！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "圣诞……快乐！"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "走黑暗的夜路也可以放心。"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "领教圣诞老人之拳吧。",
                "0_B801": "此乃一视同仁的施舍连击。",
                "0_B802": "不要慌张，按顺序来。",
                "0_B803": "——收下吧。",
                "0_B804": "这就是！",
                "0_B805": "圣诞礼物……"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "为了大家都期待的圣诞节……",
                "0_B811": "左、右、右、左、左、右！",
                "0_B812": "倾注情感，击打，击打！",
                "0_B813": "圣人连续拳！"
            }
        },
        "0_C010": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 15",
            "text": {
                "0_C010": "左、右！　 然后——"
            }
        },
        "0_C020": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 16",
            "text": {
                "0_C020": "现在我的拳头上承载着礼物。 也就是说，非常沉重哦。"
            }
        },
        "0_C030": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 17",
            "text": {
                "0_C030": "做好被击中的觉悟，耿直地切入。 ……啊。能击中。 能击中……！"
            }
        },
        "0_C040": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 18",
            "text": {
                "0_C040": "不要心急。 保持距离，想办法恢复体力也是优秀的战术。"
            }
        },
        "0_C050": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 19",
            "text": {
                "0_C050": "有种K.O.胜利的预感……！ 让我见证吧，御主！"
            }
        },
        "0_C060": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 20",
            "text": {
                "0_C060": "圣诞节统一战最终回合还剩1秒…… 最后，由我来击碎……！"
            }
        },
        "0_C110": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 21",
            "text": {
                "0_C110": "还没结束。 继续击打！"
            }
        },
        "0_C120": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 22",
            "text": {
                "0_C120": "就算没中，也要反复尝试直到命中…… 简直就像人生。就像人生。"
            }
        },
        "0_C130": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 23",
            "text": {
                "0_C130": "不是你想要的东西吗？ 这样啊…… 抱歉。我的圣诞老人修行还不够。"
            }
        },
        "0_C140": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 24",
            "text": {
                "0_C140": "没事，类似于为掌握节奏而使用的刺拳。 不用为此感到失落。继续。"
            }
        },
        "0_C150": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 25",
            "text": {
                "0_C150": "这手感不错。 不要忘记刚才的感觉。"
            }
        },
        "0_C160": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 26",
            "text": {
                "0_C160": "这就是传说中的右……！ 对方已经站不起来了。 我们胜利了！"
            }
        },
        "0_C210": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 27",
            "text": {
                "0_C210": "现在是猛击时间。 连续出击，御主！"
            }
        },
        "0_C220": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 28",
            "text": {
                "0_C220": "只要出手就能打中。无需迷茫。"
            }
        },
        "0_C230": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 29",
            "text": {
                "0_C230": "真奇妙。我有种所有攻击都会有效的预感。"
            }
        },
        "0_C240": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 30",
            "text": {
                "0_C240": "我试着提升了一个圣诞老人量级。 这个礼物盒……会很重哦？"
            }
        },
        "0_C250": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 31",
            "text": {
                "0_C250": "哼…… 好像掌握了。"
            }
        },
        "0_C260": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 32",
            "text": {
                "0_C260": "看好了。这就是我的终结技…… 也是我的终结礼物。"
            }
        },
        "0_C310": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 33",
            "text": {
                "0_C310": "可能有些朴实，但确实奏效了。继续吧。"
            }
        },
        "0_C320": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 34",
            "text": {
                "0_C320": "好礼物。 确实让我的脚步有些踉跄。有些踉跄。 稍微休息一回儿吧。"
            }
        },
        "0_C330": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 35",
            "text": {
                "0_C330": "圣诞快乐…… ……唔。这圣诞快乐的水平和以往大不一样呢。"
            }
        },
        "0_C340": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 36",
            "text": {
                "0_C340": "我有种越抽我就越…… 不，是我们就越强的感觉。"
            }
        },
        "0_C350": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 37",
            "text": {
                "0_C350": "虽说胜利的荣光是没有实体的…… 礼物真是好东西。"
            }
        },
        "0_C360": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 38",
            "text": {
                "0_C360": "这就是今年的圣诞节冠军腰带…… 或是与之相同的某种闪耀的存在。 我很高兴能把这个送给你。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "进入下个回合了呢。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "咻，咻……",
                "0_G021": "嗯，动作很轻巧，状态不错。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "虽然成长了，但量级似乎没变。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "能感受到身为圣诞老人的切实进步。",
                "0_G041": "御主，随时可以安排下一场比赛。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "看来，我已经达到了一个巅峰。",
                "0_G051": "但是，圣诞老人之路并没有就此结束。我发誓今后也会继续磨炼自己的拳头。",
                "0_G052": "虽然不知道最终等着我的会是什么……",
                "0_G053": "但至少，我对冠军腰带没有兴趣。",
                "0_G054": "我觉得那个应该被佩戴在一直陪伴我到现在的你的腰间才对。",
                "0_G055": "什么，不要？",
                "0_G056": "不，请务必戴上。我想看。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "看来力量也会进入拳头呢。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "感激不尽。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "哦……御主的生日啊。",
                "0_H011": "那身为圣诞老人的我必须准备生日用的圣诞礼物才行呢。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "圣诞节……或是类似此类的事件正在发生。",
                "0_H021": "该行动起来了，御主。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "在室内训练是有极限的。",
                "0_H031": "接下来去外面吧。御主，把陪练拳靶放在自行车上跟我来。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "圣诞老人的使命与御主的命令。",
                "0_H041": "这两者应该是可以兼顾的。",
                "0_H042": "……毕竟，两者都能带给人们喜悦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主，对于我不了解的圣诞节，你是我的顾问，你也相当于我擂台边的助手。",
                "0_H051": "虽然我的战斗与成长是属于我自己的，但你是非常可靠的存在。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "如蝴蝶般移动，像蜜蜂般出击。",
                "0_H061": "……多么美妙的格言，多么美妙的格言。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我讨厌被扔毛巾。",
                "0_H071": "只要我的身体还能动，我的拳头就会最优先击落那个吧。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯……那是你想要的礼物吗？",
                "0_H081": "好。对圣诞老人来说无可挑剔。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "是有急事吗？",
                "0_H091": "如果不是的话，我要去长跑训练了。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "听说圣诞老人总是会配备驯鹿，但我不需要。",
                "0_H101": "送礼物也是长跑训练的一环。",
                "0_H102": "这正好是一种修行。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "我不仅是从者，也是圣诞老人。",
                "0_H111": "虽然身为御主的你也会多一种职责……",
                "0_H112": "但支援就拜托你了。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "你觉得似乎有什么问题……？",
                "0_H121": "唔。如果你发现了什么问题就对我直说。",
                "0_H122": "我会尽全力修正。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "唔……原来如此，我对圣诞节和圣诞老人这些概念的了解又深入了一个阶段。",
                "0_H131": "问你果然是有意义的。",
                "0_H132": "感谢你，御主。"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "御主知道吗？",
                "0_H171": "那对双子……妹妹好像是拳击高手。",
                "0_H172": "很想好好和她较量一下，但那个哥哥总是来妨碍，导致比赛无法进行。真头疼。",
                "0_H173": "不，虽然我也觉得兄妹感情融洽是件非常好的事……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "说实话，我从罗马的神祖那里学到了不少。",
                "0_H181": "没错……男人的拳头里蕴含着无限的可能性。",
                "0_H182": "所以他是Lancer，而现在的我是Saber。",
                "0_H183": "有机会真想和他较量一下。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "阿周那吗……",
                "0_H191": "不知为何，我现在能清晰回忆起过去与他同在师父德罗纳处学习的事。",
                "0_H192": "真令人怀念啊。"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "虽然和我类型不同，但大家都是优秀的圣诞老人。得向他们好好学习才行。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "弗栗多啊。无论你夺走什么，无论你隐藏什么，人类都必然会克服那些苦难——",
                "0_H211": "哪怕没有神明或圣诞老人的帮助。",
                "0_H212": "这就是人类的坚强。"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "马嘶。他是每天都会用拳和我对战的陪练恶友。",
                "0_H221": "为了成为真正的圣诞老人，应该需要像他那样的朋友吧。",
                "0_H222": "我很幸运。"
            }
        },
        "0_N010": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 1",
            "text": {
                "0_N010": "圣诞老人的入场曲必定是这个。",
                "0_N011": "虽然我不知道曲名与歌词，但是这么哼的。",
                "0_N012": "……♪♪♪～，♪♪♪～，♪♪，♪♪♪……"
            }
        },
        "0_N020": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 2",
            "text": {
                "0_N020": "来得好。",
                "0_N021": "果然没你就没法开始了。",
                "0_N022": "拿好拳靶。"
            }
        },
        "0_N030": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 3",
            "text": {
                "0_N030": "你问我在这里干什么？",
                "0_N031": "哼，你不知道啊。",
                "0_N032": "其实，我也不清楚详情。"
            }
        },
        "0_N040": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 4",
            "text": {
                "0_N040": "眼神中充满了气势。",
                "0_N041": "类似那种只要还有体力就会……战斗下去的气概。",
                "0_N042": "这是……世界……冠军吗？"
            }
        },
        "0_N050": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 5",
            "text": {
                "0_N050": "比赛的日期、场地、比赛对手……",
                "0_N051": "所有信息应该都是公开的。",
                "0_N052": "千万不要看漏信息。"
            }
        },
        "0_N060": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 6",
            "text": {
                "0_N060": "现在开始也不晚。",
                "0_N061": "和我的拳头比起来，全部都很慢。",
                "0_N062": "抽到你心满意足为止吧。"
            }
        },
        "0_N070": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 7",
            "text": {
                "0_N070": "说实话，我还在担心你是不是忘了。你能来就好。",
                "0_N071": "……你能来……就好。"
            }
        },
        "0_N1010": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 8",
            "text": {
                "0_N1010": "已经完成热身了。",
                "0_N1011": "让你见识一下更强、更快的圣诞老人的英姿吧。"
            }
        },
        "0_N1020": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 9",
            "text": {
                "0_N1020": "凭现在我的实力，无论什么样的礼物，都能用这泡沫之拳打开。",
                "0_N1021": "交给我吧。"
            }
        },
        "0_N1030": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 10",
            "text": {
                "0_N1030": "提升频次直接关乎胜利。",
                "0_N1031": "无论是关于出拳，还是关于出拳以外的事。"
            }
        },
        "0_N1040": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 11",
            "text": {
                "0_N1040": "入场曲的歌词我也学会了。",
                "0_N1041": "叮叮当，叮叮当，铃儿响叮当嘿哟！",
                "0_N1042": "……圣诞节……令人回味无穷呢……"
            }
        },
        "0_N1050": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 12",
            "text": {
                "0_N1050": "就算在进攻时，也要小心谨慎。",
                "0_N1051": "如果不想吃防反攻击的话，一定要好好阅读说明事项。"
            }
        },
        "0_N1060": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 13",
            "text": {
                "0_N1060": "终场的铜锣还没响起。",
                "0_N1061": "在最后一刻到来之前都要不断出手。"
            }
        },
        "0_N1070": {
            "name": "荣光的圣诞老人之路\n～被封印的圣诞礼物～ 14",
            "text": {
                "0_N1070": "只要裁判没有制止，就不能停止追击。",
                "0_N1071": "在用尽全力之前，要连击、连击！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "虽然有所长进，但还差得远。",
                "0_S011": "圣诞老人之路漫长而艰险。",
                "0_S012": "御主，今后也请陪我一起长跑训练哦。"
            }
        },
        "0_S020": {
            "name": "期间限定加入",
            "text": {
                "0_S020": "我的名字是迦尔纳。职阶虽是圣诞老人，但也有可能是拳手。",
                "0_S021": "根据推测，我应该刚踏入圣诞老人这个领域吧。",
                "0_S022": "为保证自身能够成长，为成为独当一面的圣诞老人，今后我也会勤于锻炼。",
                "0_S023": "请多关照。"
            }
        }
    },

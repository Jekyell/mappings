        },
        "0_B930": {
            "name": "アタック 9",
            "text": {
                "0_B930": "Aurinko、Kuu（太阳、月亮）！"
            }
        },
        "0_G010": {
            "name": "レベルアップ 1",
            "text": {
                "0_G010": "Kiitos（谢谢）！　谢谢你，女婿。"
            }
        },
        "0_G020": {
            "name": "霊基再臨 2",
            "text": {
                "0_G020": "Mitä（什么）……？",
                "0_G021": "啊，这角是魔力的结晶啦。",
                "0_G022": "冬天的雌驯鹿用角来保护自己。",
                "0_G023": "如果贸然触碰的话，就会直接用这个顶穿肚子啦！",
                "0_G024": "Tota（呃）……轻一点的话，倒也行啦……"
            }
        },
        "0_G030": {
            "name": "霊基再臨 1",
            "text": {
                "0_G030": "Ihana（真棒）！　毛茸茸的厚衣服才是最强的！",
                "0_G031": "波赫约拉人就该这样才对！",
                "0_G032": "到睫毛都冻起来才刚要拿出真本事啦。",
                "0_G033": "好乖好乖，科科也状态绝佳吧。"
            }
        },
        "0_G110": {
            "name": "レベルアップ 2",
            "text": {
                "0_G110": "Ippon pippon pelista pois（点兵点将，你被淘汰啦）♪"
            }
        },
        "0_G120": {
            "name": "レベルアップ 3",
            "text": {
                "0_G120": "这是奖励的咸甘草糖哦。"
            }
        },
        "0_H010": {
            "name": "誕生日",
            "text": {
                "0_H010": "啊～～～～～～。",
                "0_H011": "讨厌啦，麻烦死了，真想变成萨卡班甲鱼～。",
                "0_H012": "……呜嘎。",
                "0_H013": "……不，这，这是因为……",
                "0_H014": "虽说平时总是被家人和外人说快去勤奋干活，或是振作点，一个劲地在背后督促……",
                "0_H015": "但娄希也希望每年至少能抛下一切逃跑一次啦。",
                "0_H016": "……哦？　女婿你今天过生日吗？",
                "0_H017": "那今天就和娄希一起尽情地偷懒吧！",
                "0_H018": "呵呵呵。"
            }
        },
        "0_H020": {
            "name": "イベント開催中",
            "text": {
                "0_H020": "真热闹啊。",
                "0_H021": "是哪里举行鲱鱼集市了吗？",
                "0_H022": "把三明治装进篮子，外出看看吧。"
            }
        },
        "0_H030": {
            "name": "会話 1",
            "text": {
                "0_H030": "Moi（哟）！　该去滑雪了啦，女婿。"
            }
        },
        "0_H040": {
            "name": "会話 2",
            "text": {
                "0_H040": "女婿和娄希的相遇是命运。",
                "0_H041": "是天神「乌戈」的旨意……",
                "0_H042": "你以为娄希会这么说吗！",
                "0_H043": "这当然是娄希自己争取来的啦！"
            }
        },
        "0_H050": {
            "name": "会話 3 男性主人公選択時",
            "text": {
                "0_H050": "女婿是娄希的女婿哦。",
                "0_H051": "娄希已经等不及女婿入赘的那天了啦。"
            }
        },
        "0_H060": {
            "name": "好きなこと",
            "text": {
                "0_H060": "世界上最难吃的糖……？",
                "0_H061": "这不可能啦。",
                "0_H062": "咸味甘草糖——",
                "0_H063": "虽然对人理什么的无效，但迟早会有效的啦。"
            }
        },
        "0_H070": {
            "name": "嫌いなこと",
            "text": {
                "0_H070": "……嗯嗯。",
                "0_H071": "科科说最讨厌雷鸟了。",
                "0_H072": "说讨厌那卡在喉咙里的羽毛，是吧～，noni-i（对吧）。",
                "0_H073": "娄希讨厌万奈摩宁。",
                "0_H074": "还有……其实……",
                "0_H075": "害害，害……害怕蛀牙啦……",
                "0_H076": ""
            }
        },
        "0_H080": {
            "name": "聖杯について",
            "text": {
                "0_H080": "娄希的愿望很微小。",
                "0_H081": "只想在那令人怀念的波赫约拉过着心情平静的生活。",
                "0_H082": "但是……这无论如何都无法实现。"
            }
        },
        "0_H090": {
            "name": "絆Lv.1",
            "text": {
                "0_H090": "现在已经成了只会在传说中提及的北国之名，那就是波赫约拉。",
                "0_H091": "如果对女婿说索米……向芬兰北侧延伸的北极圈，你能明白吗？",
                "0_H092": "被冰雪与极夜的黑暗封闭，但也有短暂而美好的夏天。",
                "0_H093": "那里虽然是这么一块偏僻、不尽如人意的不方便之地……",
                "0_H094": "但人的一生基本也是这样的啦。"
            }
        },
        "0_H100": {
            "name": "絆Lv.2",
            "text": {
                "0_H100": "这只雏鸟是炎之鹫科科。",
                "0_H101": "还有妖精童图们、洞穴巨人鬼佩科、海怪图里萨斯也一样。",
                "0_H102": "都是住在波赫约拉的娄希的家人。",
                "0_H103": "大家都在互相帮助着生活哦。"
            }
        },
        "0_H110": {
            "name": "絆Lv.3",
            "text": {
                "0_H110": "成为了从者，娄希也返老还童了不少。",
                "0_H111": "说不定是混入了一些女儿们的灵基吗……？",
                "0_H112": "总之，肩膀和腰都轻松多了。",
                "0_H113": "嘿呀！　啊呜。",
                "0_H114": "娄希引以为傲的牙齿锋利度也是绝佳状态哦！",
                "0_H115": "啊……抱歉，稍微出了点血呢。"
            }
        },
        "0_H120": {
            "name": "絆Lv.4",
            "text": {
                "0_H120": "哈哈～，原来如此。",
                "0_H121": "女婿也能靠礼装使用「Gandr」魔术啊。",
                "0_H122": "虽然不是波赫约拉的语言，但娄希知道北方民族擅长的诅咒被统称为「Gandr」，并受到人们的畏惧。",
                "0_H123": "那娄希干脆也这么称呼好了。",
                "0_H124": "倒不如说哦？",
                "0_H125": "应该说娄希才是Gandr啦。",
                "0_H126": "不管是受伤还是生病，总之如果女婿有想让其不幸的复仇对象，一定要告诉娄希。",
                "0_H127": "娄希会用令人无法翻身的一发招呼他哦。"
            }
        },
        "0_H130": {
            "name": "絆Lv.5",
            "text": {
                "0_H130": "Moi（哟）！　女婿。",
                "0_H131": "过来一下，帮娄希打磨一下角啦。",
                "0_H132": "……呼……对对，感觉不错。",
                "0_H133": "……唔？",
                "0_H134": "既坚硬又微微发光，就像宝石……触感又像天鹅绒？",
                "0_H135": "是，是吗。呵呵……咿嘻嘻嘻……",
                "0_H136": "娄希才不会夸你呢。"
            }
        },
        "0_H170": {
            "name": "会話 4",
            "text": {
                "0_H170": "哎，还有鹰之魔女？",
                "0_H171": "做麦粥的大魔女？",
                "0_H172": "特别想要猪圈……还会提供各种智慧。",
                "0_H173": "该怎么说呢，太狡……",
                "0_H174": "不，没什么。嗯，加油啦。",
                "0_H175": "同样麦子的话，娄希更擅长做啤酒哦。",
                "0_H176": "结婚宴席时娄希可是酿了满满一湖哦。",
                "0_H177": "需要准备庆祝活动时，就交给娄希吧。"
            }
        },
        "0_H180": {
            "name": "会話 5",
            "text": {
                "0_H180": "这些小人是「tonttu（童图）」。",
                "0_H181": "是娄希家的帮忙妖精哟。",
                "0_H182": "他们非常勤劳，很有帮助。",
                "0_H183": "但头疼的是，在这迦勒底这地方，每当他们看到Joulupukki（圣诞老人）……冒牌圣诞老人就会擅自跟着走。",
                "0_H184": "啊，童图们里还混着冰冷的霜妖精「Pakkanen（帕卡宁）」，一定要小心点哦，女婿。",
                "0_H185": "否则会像咸味甘草糖一样，手指变得乌黑掉下来哦。"
            }
        },
        "0_H190": {
            "name": "会話 6",
            "text": {
                "0_H190": "那个喜欢宝石的女神，是叫伊什塔尔吗？",
                "0_H191": "她相当有本事嘛……",
                "0_H192": "竟然能打出那种蓄力卓越的Gandr。",
                "0_H193": "要不是平日里就在不断积攒怨念，是打不出如此沉重的一击的啦。",
                "0_H194": "……有点可怕。"
            }
        },
        "0_H200": {
            "name": "会話 7",
            "text": {
                "0_H200": "Voi（哎哟）。",
                "0_H201": "志度内的内部也有娄希吗？",
                "0_H202": "真有意思！",
                "0_H203": "还有Otso（熊），熊啊。Otso（熊）真好……",
                "0_H204": "但是呀～，为了干掉那个老头子，娄希把关系要好的Otso（熊）长老送了进去……",
                "0_H205": "结果被彻底吃干抹净了，从那以后娄希就有点心理阴影了。",
                "0_H206": "呜呜呜，万奈摩宁那家伙～。"
            }
        },
        "0_H5050": {
            "name": "会話 3 女性主人公選択時",
            "text": {
                "0_H5050": "女婿是娄希的女婿哦。",
                "0_H5051": "是男是女都无所谓。",
                "0_H5052": "魔术师的话，这点小事总有办法解决啦。"
            }
        },
        "0_I010": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 1",
            "text": {
                "0_I010": "Hyvää joulua（圣诞快乐）！",
                "0_I011": "圣诞快乐！",
                "0_I012": "虽然不太明白，但既然负责看店，就要鼓起精神干哦！"
            }
        },
        "0_I020": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 2",
            "text": {
                "0_I020": "Moi（哟）！",
                "0_I021": "圣诞节才是冬天最令人期待的事！",
                "0_I022": "唔？　日本圣诞节很快就结束了？",
                "0_I023": "真浪费呢～。",
                "0_I024": "就算办上半年也可以吧！"
            }
        },
        "0_I030": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 3",
            "text": {
                "0_I030": "Hyvä（好）！",
                "0_I031": "好。"
            }
        },
        "0_I040": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 4",
            "text": {
                "0_I040": "辛苦啦。",
                "0_I041": "顺便，要不要吃圣诞童图烤的风车饼？"
            }
        },
        "0_I050": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 5",
            "text": {
                "0_I050": "Moi（哟）。",
                "0_I051": "还有附赠的咸甘草糖哦。",
                "0_I052": "啊？",
                "0_I053": "如果客气的话可是会增加的哦。"
            }
        },
        "0_I060": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 6",
            "text": {
                "0_I060": "在认真工作之后，",
                "0_I061": "当然应该坐在暖炉前",
                "0_I062": "Kalsarikännit（穿着内裤小酌一杯）最好啦……",
                "0_I063": "哎哟，在那之前，先打个招呼！",
                "0_I064": "虽然发生了很多事，但这次过得很开心哦。",
                "0_I065": "Hyvää uutta vuotta（新年快乐）！",
                "0_I066": "祝你新年快乐！"
            }
        },
        "0_I070": {
            "name": "ポホヨラのクリスマス・イブ\n～夢見るサンタとくるみ割り人形～ 7",
            "text": {
                "0_I070": "迦勒底真是的……",
                "0_I071": "到底是人手不足还是太闲了呢。",
                "0_I072": "怎么连北之女主人娄希也被拉来这里站着啊。",
                "0_I073": "既然这样，还不如把「Äteritsiputeritsipuolilautatsijänkä（艾泰里齐-普泰里齐-波利劳塔齐沼泽）」的「lentokonesuihkuturbiinimoottoriapumekaanikkoaliupseerioppilas（喷气飞机涡轮发动机机械助理实习生）」叫来呢！",
                "0_I074": "……啊……咬舌头了。"
            }
        },
        "0_S010": {
            "name": "召喚",
            "text": {
                "0_S010": "Moi（哟）！　是“娄希”哦～！",
                "0_S011": "鹫之魔女，也是统治极北之地“波赫约拉”的女主人！",
                "0_S012": "从今往后你也会成为这娄希的家人之一，",
                "0_S013": "一定要好好修行哦，女婿！"
            }
        },
        "2_B010": {
            "name": "アタック 1",
            "text": {
                "2_B010": "愚蠢的家伙。"
            }
        },
        "2_B020": {
            "name": "アタック 2",
            "text": {
                "2_B020": "Katala（邪恶）。"
            }
        },
        "2_B030": {
            "name": "アタック 3",
            "text": {
                "2_B030": "射击，童图！"
            }
        },
        "2_B040": {
            "name": "エクストラアタック 1",
            "text": {
                "2_B040": "图里萨斯！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "银之太阳，金之月亮，连火焰都在波赫约拉沉睡。",
                "2_B051": "被娄希拥抱着躺倒吧。",
                "2_B052": "而现在，轮到你们了……",
                "2_B053": "该横渡土奥乃拉之河了！",
                "2_B054": "极星天球壳循环！"
            }
        },
        "2_B080": {
            "name": "スキル 1",
            "text": {
                "2_B080": "Hei！　Hei！　Heippa！（嗨！　嗨！　你好！）"
            }
        },
        "2_B090": {
            "name": "スキル 2",
            "text": {
                "2_B090": "Ääliö（笨蛋）！！"
            }
        },
        "2_B100": {
            "name": "開始 1",
            "text": {
                "2_B100": "Heipä hei（你好）！"
            }
        },
        "2_B110": {
            "name": "開始 2",
            "text": {
                "2_B110": "娄希会好好疼你的。"
            }
        },
        "2_B120": {
            "name": "勝利 1",
            "text": {
                "2_B120": "干得好女婿！　就是这个状态。"
            }
        },
        "2_B130": {
            "name": "勝利 2",
            "text": {
                "2_B130": "桑拿预热就绪，白桦树枝就绪！"
            }
        },
        "2_B140": {
            "name": "ダメージ 2",
            "text": {
                "2_B140": "Ai（好痛）！"
            }
        },
        "2_B150": {
            "name": "ダメージ 1",
            "text": {
                "2_B150": "由波赫约拉的极夜招来……"
            }
        },
        "2_B160": {
            "name": "戦闘不能 1",
            "text": {
                "2_B160": "没有天生就是铁匠的啦……"
            }
        },
        "2_B170": {
            "name": "戦闘不能 2",
            "text": {
                "2_B170": "Harjoitus tekee mestarin（练习才能成就高手）……"
            }
        },
        "2_B180": {
            "name": "コマンドカード 1",
            "text": {
                "2_B180": "好。"
            }
        },
        "2_B190": {
            "name": "コマンドカード 2",
            "text": {
                "2_B190": "Niin（说得对）。"
            }
        },
        "2_B200": {
            "name": "コマンドカード 3",
            "text": {
                "2_B200": "OK。"
            }
        },
        "2_B2010": {
            "name": "アタック 10",
            "text": {
                "2_B2010": "Voi voi voi（诅咒）……"
            }
        },
        "2_B2020": {
            "name": "アタック 11",
            "text": {
                "2_B2020": "连滑雪板都当不成！"
            }
        },
        "2_B2030": {
            "name": "アタック 12",
            "text": {
                "2_B2030": "Oi（嗯）！"
            }
        },
        "2_B210": {
            "name": "宝具カード 1",
            "text": {
                "2_B210": "射击土奥乃拉的天鹅吧！"
            }
        },
        "2_B220": {
            "name": "宝具カード 2",
            "text": {
                "2_B220": "举起弓弩吧！"
            }
        },
        "2_B230": {
            "name": "宝具カード 3",
            "text": {
                "2_B230": "Totta kai（当然）！"
            }
        },
        "2_B2410": {
            "name": "アタック 13",
            "text": {
                "2_B2410": "回归乌有！"
            }
        },
        "2_B2420": {
            "name": "アタック 14",
            "text": {
                "2_B2420": "Tulta（火）！"
            }
        },
        "2_B2430": {
            "name": "アタック 15",
            "text": {
                "2_B2430": "Lumimyrsky（暴风雪）！"
            }
        },
        "2_B2440": {
            "name": "エクストラアタック 4",
            "text": {
                "2_B2440": "喂，好重好重好重。"
            }
        },
        "2_B250": {
            "name": "宝具カード 4",
            "text": {
                "2_B250": "noNIIN（好嘞）！"
            }
        },
        "2_B2810": {
            "name": "アタック 16",
            "text": {
                "2_B2810": "让你的雪橇载上死亡吧！"
            }
        },
        "2_B410": {
            "name": "アタック 4",
            "text": {
                "2_B410": "Nyt（就是现在）！"
            }
        },
        "2_B420": {
            "name": "アタック 5",
            "text": {
                "2_B420": "从科科的喉咙穿过吧！"
            }
        },
        "2_B430": {
            "name": "アタック 6",
            "text": {
                "2_B430": "Kehno（差劲）！"
            }
        },
        "2_B440": {
            "name": "エクストラアタック 2",
            "text": {
                "2_B440": "拖入阿赫托拉吧。"
            }
        },
        "2_B480": {
            "name": "スキル 3",
            "text": {
                "2_B480": "Perhe（家人）！"
            }
        },
        "2_B490": {
            "name": "スキル 4",
            "text": {
                "2_B490": "Hauska（开心）！"
            }
        },
        "2_B500": {
            "name": "開始 3",
            "text": {
                "2_B500": "去狩猎希西的鹿吧！"
            }
        },
        "2_B510": {
            "name": "開始 4",
            "text": {
                "2_B510": "北风将会诅咒你……"
            }
        },
        "2_B520": {
            "name": "勝利 3",
            "text": {
                "2_B520": "Eteenpäin（前进），前进前进！"
            }
        },
        "2_B530": {
            "name": "勝利 4",
            "text": {
                "2_B530": "娄希带你去滑雪吧，女婿。"
            }
        },
        "2_B540": {
            "name": "ダメージ 4",
            "text": {
                "2_B540": "Voi（啊啊）……"
            }
        },
        "2_B550": {
            "name": "ダメージ 3",
            "text": {
                "2_B550": "Ääliö älä lyö, ööliä läikkyy（别打了）……"
            }
        },
        "2_B560": {
            "name": "戦闘不能 3",
            "text": {
                "2_B560": "Meidän talo（我们的家）……"
            }
        },
        "2_B570": {
            "name": "戦闘不能 4",
            "text": {
                "2_B570": "听到康特勒琴的声音了……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "眺望西北的天空。",
                "2_B801": "燃烧之鹫整理着诅咒的羽翼。",
                "2_B802": "想保住生命的话就向乌戈祈祷，哭喊着体会魔女娄希的愤怒吧！",
                "2_B803": "极星天球壳循环！"
            }
        },
        "2_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "2_B840": "沉入海中旋涡吧！"
            }
        },
        "2_B880": {
            "name": "スキル 5",
            "text": {
                "2_B880": "狐狸跑吧！"
            }
        },
        "2_B890": {
            "name": "スキル 6",
            "text": {
                "2_B890": "Hurraa（万岁）！"
            }
        },
        "2_B910": {
            "name": "アタック 7",
            "text": {
                "2_B910": "让娄希温暖你吧！"
            }
        },
        "2_B920": {
            "name": "アタック 8",
            "text": {
                "2_B920": "Homekorva（发霉的耳朵）……"
            }
        },
        "2_B930": {
            "name": "アタック 9",
            "text": {
                "2_B930": "九种疾病之灵啊！"
            }
        },
        "2_G010": {
            "name": "レベルアップ 1",
            "text": {
                "2_G010": "Kiitti（谢谢），女婿。"
            }
        },
        "2_G040": {
            "name": "霊基再臨 1",
            "text": {
                "2_G040": "呀哈哈！　呼呜～！",
                "2_G041": "这下终于能发挥鹫之魔女的真本领了！",
                "2_G042": "这是与娄希之宿敌万奈摩宁的决战形态！",
                "2_G043": "与火焰之鹫科科合为一体，烧尽一切，吞噬一切！",
                "2_G044": "为女婿的敌人送去九种灾厄吧！"
            }
        },
        "2_G050": {
            "name": "霊基再臨 2",
            "text": {
                "2_G050": "呵呵……",
                "2_G051": "应该做好入赘的准备了吧，女婿？",
                "2_G052": "什么嘛，还没做决定啊。",
                "2_G053": "既然如此，就先来我家。",
                "2_G054": "边进行女婿修行边思考吧。",
                "2_G055": "反正迟早能找到女婿中意的新娘啦。"
            }
        },
        "2_G110": {
            "name": "レベルアップ 2",
            "text": {
                "2_G110": "日之恩惠，月之治愈，灶之热量。"
            }
        },
        "2_G120": {
            "name": "レベルアップ 3",
            "text": {
                "2_G120": "Tuota（呃）……",
                "2_G121": "这火种味道真奇怪呢。"
            }
        },
        "2_H010": {
            "name": "誕生日",
            "text": {
                "2_H010": "唔～？　生日？",
                "2_H011": "哪里有空特地庆祝那种……",
                "2_H012": "好像还真有呢？",
                "2_H013": "Hyvää syntymäpäivää（生日快乐）。",
                "2_H014": "生日快乐，女婿。",
                "2_H015": "礼物的话……",
                "2_H016": "是娄希的「新作空气吉他·通宵演唱会」还是「太太搬运竞速的参赛权」，",
                "2_H017": "喜欢哪个就选哪个吧。"
            }
        },
        "2_H020": {
            "name": "イベント開催中",
            "text": {
                "2_H020": "正在举办大型活动？",
                "2_H021": "Hyvä（真不错）！",
                "2_H022": "既然如此，娄希就去把珍藏的超巨大牛宰了吧。"
            }
        },
        "2_H030": {
            "name": "会話 1",
            "text": {
                "2_H030": "差不多该去采摘森林的珍馐了吧，女婿。"
            }
        },
        "2_H040": {
            "name": "会話 2",
            "text": {
                "2_H040": "娄希不需要体谅和温柔的话语。",
                "2_H041": "那些还是留给其他姑娘们吧。"
            }
        },
        "2_H050": {
            "name": "会話 3",
            "text": {
                "2_H050": "选择女婿的理由？",
                "2_H051": "只要看看这迦勒底里的人，不就一目了然了嘛。",
                "2_H052": "如果他们是女婿的家人，那对娄希来说也是家人。"
            }
        },
        "2_H060": {
            "name": "好きなこと",
            "text": {
                "2_H060": "不管怎么说，还是咱家最好啦。",
                "2_H061": "勤劳、珍惜家人。",
                "2_H062": "可以平凡，可以弱小。",
                "2_H063": "要诚实，犯错时认真道歉。",
                "2_H064": "要成为这样的女婿哦，女婿。"
            }
        },
        "2_H070": {
            "name": "嫌いなこと",
            "text": {
                "2_H070": "Voi voi voi（诅咒）……",
                "2_H071": "娄希最不能饶恕的，就是狡猾的混蛋老头啊……",
                "2_H072": "毕竟年轻男人都很傻嘛，搞点恶作剧也能睁一只眼闭一只眼。",
                "2_H073": "……但是！",
                "2_H074": "那个忘恩负义的万奈摩宁简直差劲透顶。",
                "2_H075": "唆使年轻人去干，自己装作不知情地弹着康特勒琴。",
                "2_H076": "偷走三宝，还把婴儿丢在山丘上。",
                "2_H077": "那个混蛋～！"
            }
        },
        "2_H080": {
            "name": "聖杯について",
            "text": {
                "2_H080": "只要有三宝……",
                "2_H081": "不是用魔力编织的宝具，而是有真正三宝的话。",
                "2_H082": "美丽的波赫约拉、大家，就都能回到咱们的家了啊……"
            }
        },
        "2_H20090": {
            "name": "絆Lv.1",
            "text": {
                "2_H20090": "被打得一败涂地，",
                "2_H20091": "甚至被运气抛弃，",
                "2_H20092": "像这样被彻底逼到绝境后才能看出一个人的活法！",
                "2_H20093": "这就是最棒的「SISU（毅力）」啦！"
            }
        },
        "2_H20100": {
            "name": "絆Lv.2",
            "text": {
                "2_H20100": "不请自来闯入宴席、虚张声势，把一切搞得一团糟后才自顾自地后悔。",
                "2_H20101": "男人就是这样愚蠢的生物啦。",
                "2_H20102": "另一方面，有些女人不管外表多么光鲜聪明，内在却犹如婴儿。",
                "2_H20103": "半斤八两啦。",
                "2_H20104": "要是每个人都精明世故的话，就不会出现什么诗歌和恋情了。"
            }
        },
        "2_H20110": {
            "name": "絆Lv.3",
            "text": {
                "2_H20110": "哒哒哒～哟　哒哒哒～哟　哒哒哒啦嘀啦 MoiMoiMoi♪",
                "2_H20112": "嗯嗯。",
                "2_H20113": "在波赫约拉基本都是用歌声和鼓声来诅咒的。",
                "2_H20114": "而最近娄希最喜欢，肯定就是「空气吉他」啦！",
                "2_H20115": "才不会输给那些吝啬老头的康特勒琴呢！",
                "2_H20116": "呼唔～！"
            }
        },
        "2_H20120": {
            "name": "絆Lv.4",
            "text": {
                "2_H20120": "家人……并非只有好的一面。",
                "2_H20121": "多半都是些麻烦事儿。",
                "2_H20122": "就像无法将雷鸟或松鼠活生生地平分一样，其实很难割舍。",
                "2_H20123": "心意无法传达、难以释怀也是家常便饭。",
                "2_H20124": "所以在挑选女婿时也要慎重。",
                "2_H20125": "不过……或许正因为如此，娄希的女儿们才吃了不少苦吧……"
            }
        },
        "2_H20130": {
            "name": "絆Lv.5",
            "text": {
                "2_H20130": "魔女娄希才是波赫约拉的主人！",
                "2_H20131": "……但是啊，这迦勒底，",
                "2_H20132": "女婿，娄希承认你才是御主。",
                "2_H20133": "娄希看出还是你最有胆识。",
                "2_H20134": "要保重身体，好好努力哦，女婿。"
            }
        },
        "2_H210": {
            "name": "会話 4",
            "text": {
                "2_H210": "嚯，阿斯特赖亚……？",
                "2_H211": "她的凭依之躯是俗话所说的卡莱瓦之民，索米的女孩吗。",
                "2_H212": "那对于波赫约拉的主人娄希来说，她与其说是同胞，更算是敌人啦。",
                "2_H213": "不过看在女婿的面子上，娄希倒是不在意陪她喝杯咖啡什么的。",
                "2_H214": "当然，如果想自命正义来场肉搏战，娄希随时愿意奉陪！"
            }
        },
        "2_H220": {
            "name": "会話 5",
            "text": {
                "2_H220": "唔哎～！　伽拉忒亚！　真了不起！",
                "2_H221": "获得了生命的象牙雕像啊。",

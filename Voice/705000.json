        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "转动，三宝。",
                "0_B801": "在九把锁，十道铰链之下，",
                "0_B802": "你将积蓄星夜之魔力。",
                "0_B803": "静静磨出生命的养分与宝物，",
                "0_B804": "化作天之钉星吧。",
                "0_B805": "极星天球壳循环。"
            }
        },
        "0_B840": {
            "name": "エクストラアタック 3",
            "text": {
                "0_B840": "嘿，多吃点～。"
            }
        },
        "0_B880": {
            "name": "スキル 5",
            "text": {
                "0_B880": "嘿咻～！"
            }
        },
        "0_B890": {
            "name": "スキル 6",
            "text": {
                "0_B890": "咸甘草咸甘草糖～！"

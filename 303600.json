{
    "id": 303600,
    "collectionNo": 232,
    "name": "布拉达曼特",
    "voices": {
        "0_B040": {
            "name": "额外攻击",
            "text": {
                "0_B040": "噢噢噢！——光啊！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "光啊！　化为螺旋！",
                "0_B051": "全力出击！",
                "0_B052": "——炫目的闪光魔盾！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "光啊，聚集吧！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "精力充沛！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "布拉达曼特，行使正义！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "代替查理曼大王，制裁汝等！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "圣骑士是不会被击败的！　只有胜利！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "赢了呢，御主！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "……失策了……对不起，御主……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "身体坚强……明明是我的长处……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "D'accord（明白了）！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "交给我吧！"
            }
        },
        "0_B210": {
            "name": "宝具卡",
            "text": {
                "0_B210": "Montjoie（战吼）！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "螺旋拘束！",
                "0_B801": "用尽！　全力！",
                "0_B802": "——炫目的闪光魔盾！！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "精力源源不断地涌出！"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "灵基得到了强化。",
                "0_G021": "布拉达曼特会进一步为您效力！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我穿上了外套！",
                "0_G031": "感觉防御力也提升了，御主！"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "完全武装！",
                "0_G041": "没关系，看上去没有铠甲防护的地方也覆盖着魔力。",
                "0_G042": "这铠甲就是这样的。",
                "0_G043": "来吧，我们出发！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "那个……这样真的好吗？",
                "0_G051": "竟然花费了那么多资源在我的身上……",
                "0_G052": "不，没有！",
                "0_G053": "既然备受您的期待，我布拉达曼特必会全力回应。",
                "0_G054": "今后也请您多指教，御主。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝您生日快乐！",
                "0_H011": "布拉达曼特会用足看家本领为您烤蛋糕的！",
                "0_H012": "哎？",
                "0_H013": "没关系，如此说来，虽然这是我第一次做，但不要紧。",
                "0_H014": "在做到好吃之前，我是不会放弃的！！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "布拉达曼特不会放弃做任何事！",
                "0_H021": "必定会全力以赴！",
                "0_H022": "活动？",
                "0_H023": "那也要全力以赴！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "我们走吧，御主！",
                "0_H031": "唯有永不懈怠地锻炼，才配称作骑士道！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我乃侍奉御主之身……",
                "0_H041": "所以请您尽情吩咐我布拉达曼特吧！　哎嘿！",
                "0_H042": "比方说……我很擅长寻找东西哦！",
                "0_H043": "您有没有什么遗失的物品？"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "御主和王不一样吧。",
                "0_H051": "尽管我对这方面的感觉不是很了解……",
                "0_H052": "但是，像这样辅佐他人前进的做法令人非常怀念呢。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "鲁杰罗不在啊。",
                "0_H061": "啊，御主！",
                "0_H062": "其实我有点线索……",
                "0_H063": "那只白色的野兽，就是散发着梅林大人气息的芙芙。",
                "0_H064": "那孩子该不会……",
                "0_H065": "是鲁杰罗的转世吧……？"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "我讨厌魔术师！",
                "0_H071": "所谓的魔术师，多半都是些冷酷、坏心眼、总在策划什么可怕主意的家伙们。",
                "0_H072": "啊，但御主另当别论！",
                "0_H073": "御主的身上有那位传说中的大魔术师，以花之魔术师著称的梅林大人的气息……",
                "0_H074": "既然是与梅林大人有关的人，嗯。",
                "0_H075": "那就没问题啦！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "所谓的圣杯，就是亚瑟王曾经追寻的东西吧。",
                "0_H081": "我听说是非常尊贵的东西。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "尽管我被作为Lancer召唤，但其实……",
                "0_H091": "我没有枪的宝具。对不起！",
                "0_H092": "我的宝具是这面盾。对。是聚集光的魔之盾。",
                "0_H093": "这曾经是邪恶魔术师装备于身上的聚集暗之盾……",
                "0_H094": "但在打倒了那家伙之后，得到了梅林大人的净化！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "除了盾以外，我还有其他宝具。",
                "0_H101": "就是这枚戒指。尽管平时戴着手套看不见——",
                "0_H102": "但这枚戒指曾帮助了我……",
                "0_H103": "帮了作为查理曼大王十二勇士，作为圣骑士战斗的我很大的忙。",
                "0_H104": "这是从某位公主殿下那里得到的重要的戒指。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "被作为其他职阶召唤？",
                "0_H111": "这个啊，我的话，应该可以被作为Rider职阶召唤吧。",
                "0_H112": "您知道骏鹰吗？",
                "0_H113": "我可以骑着那头可爱的幻兽在天空驰骋哦。",
                "0_H114": "哎嘿。",
                "0_H115": "在这个灵基状态下无法召唤它确实……令人有些寂寞呢。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "尽管我是查理曼十二勇士之一……",
                "0_H121": "哼哼哼，没想到吧！",
                "0_H122": "我也是那位传说中的大魔术师——梅林大人曾引导过的骑士哦！",
                "0_H123": "这是真的哦！",
                "0_H124": "过去梅林大人的巫女曾经为我的旅途……",
                "0_H125": "「你还是老样子呢，布拉达曼特。",
                "0_H126": "现在，我正在直接向你的内心说话。好久不见。」",
                "0_H127": "哎，哎，怎么回事？　您刚才有没有听到声音！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我曾是个不断追寻所爱之人鲁杰罗的圣骑士。",
                "0_H131": "那是一趟漫长的旅程。",
                "0_H132": "……而那些，至今仍在身为英灵显现的我的内心延续着。",
                "0_H133": "只要我还是我，就有必须追寻的东西，哪怕始终无法见到，也会不断追寻。",
                "0_H134": "但是，请放心吧，御主！",
                "0_H135": "更重要的是，我是名圣骑士，我定会为了夺回世界而全力以赴！",
                "0_H136": "「这就对了。",
                "0_H137": "现在，我正在直接向你的内心说话。",
                "0_H138": "没错，我是梅林。」",
                "0_H139": "咦，梅林大人！？"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "话说，小阿。",
                "0_H171": "那只骏鹰……该不会是我的吧？",
                "0_H172": "喂喂。",
                "0_H173": "我问你啊！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "圆……圆桌的骑士……！",
                "0_H181": "亚瑟王声名远播的骑士们……",
                "0_H182": "好厉害……这太厉害了啊！　御主！"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "梅……梅梅……梅林大人！",
                "0_H191": "是本人！？",
                "0_H192": "没想到居然能在当世获赐您的金言！",
                "0_H193": "布拉达曼特太感动了！",
                "0_H194": "但是那个……您是怎么离开那座塔的……"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "传说中的英雄赫克托耳大人！",
                "0_H201": "鲁杰罗和我的祖先……！",
                "0_H202": "啊哇哇哇哇……",
                "0_H203": "那……那……那个，初……初次见面！"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "圣剑的骑士王……亚瑟王……！",
                "0_H211": "那位著名的大人物……怎么会这样……",
                "0_H212": "啊哇哇……",
                "0_H213": "好想要签名啊……"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "啊！　该死的摩根！",
                "0_H221": "你难道还打算在当世作恶吗！",
                "0_H222": "……唔？　御主的从者？",
                "0_H223": "你也是吗？"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "我觉得罗兰应该多向里纳尔多哥哥学习才对。",
                "0_H231": "你在听吗？",
                "0_H232": "压根没在听吧。",
                "0_H233": "喂，我问你啦！"
            }
        },
        "0_I010": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 1",
            "text": {
                "0_I010": "交给我布拉达曼特吧！",
                "0_I011": "原来如此，这就是所谓的商店啊。",
                "0_I012": "稍微有些紧张呢……！"
            }
        },
        "0_I020": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 2",
            "text": {
                "0_I020": "这就是所谓的圣诞大促销！",
                "0_I021": "我觉得这非常棒！"
            }
        },
        "0_I030": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 3",
            "text": {
                "0_I030": "明白了！",
                "0_I031": "请这边走。",
                "0_I032": "呵呵呵！"
            }
        },
        "0_I040": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 4",
            "text": {
                "0_I040": "嗯嗯，原来如此……",
                "0_I041": "这是那种东西啊……"
            }
        },
        "0_I050": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 5",
            "text": {
                "0_I050": "祝您幸福！",
                "0_I051": "呵呵呵，怎么样？"
            }
        },
        "0_I060": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 6",
            "text": {
                "0_I060": "欢迎光临。",
                "0_I061": "商店也差不多要关门了。",
                "0_I062": "您觉得这次圣诞促销怎么样？",
                "0_I063": "能让您感到满意是我无上的喜悦！"
            }
        },
        "0_I070": {
            "name": "圣诞节2019 Holy·Samba·Night\n～降雪遗迹与少女骑士～ 7",
            "text": {
                "0_I070": "辛苦了。",
                "0_I071": "在下次战斗到来之前，还请好好休息。",
                "0_I072": "那么，回头见。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "查理曼十二勇士之一，",
                "0_S011": "白羽骑士布拉达曼特。",
                "0_S012": "作为Lancer被召唤至此。",
                "0_S013": "代替查理曼大王，行使正义！"
            }
        }
    },
    "ChrVoice_ids": [
        "303600"
    ]
}
{
    "id": 1000700,
    "collectionNo": 209,
    "name": "冲田总司〔Alter〕",
    "voices": {
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "休想逃跑。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "魔剑！　炼狱斩！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "穿透尘刹，",
                "0_B051": "以无边之光，斩断天命！",
                "0_B052": "绝剑！　无穹三段！！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "……我要动真格了哦！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "……（ZZZ)、……（啊！？） 没睡着，我没睡着哦，睡…… 睡着了！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "……我乃抑止的守护者。"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "吾乃撕裂魔，贯穿神，集结了人们祈祷的存在， ——魔·冲田总司！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "……结束了。没事吧，御主。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "魔小姐大胜利～，对吧。"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "快动啊，快动起来……！"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "结束了吗……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "嗯……"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "……上吧！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "是这样啊。"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "吾之光芒通达无穹。"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "醒醒，炼狱……轮到你出场了。"
            }
        },
        "0_B230": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B230": "……？　啊啊，是说我吗。"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "魔之魔意味着一刀两断！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "魔之意味着切成碎片！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "都结束了！　焰王炼狱斩！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "要我动真格吗？"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "魔力量全开！"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "……坠入无穹的狭缝间吧。"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "不痛不痛不痛……好痛啊！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "咳咳……！"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "……御……主。"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "无量、无碍、无边……",
                "0_B801": "三光集成就无穹！",
                "0_B802": "绝剑！　无穹三段！"
            }
        },
        "0_B810": {
            "name": "宝具 {0}",
            "text": {
                "0_B810": "呃，什么来着……",
                "0_B811": "……唔……忘掉了！",
                "0_B812": "必杀！　魔小姐光炮！"
            }
        },
        "0_B820": {
            "name": "宝具 {0}",
            "text": {
                "0_B820": "呃，什么来着……",
                "0_B821": "……唔……忘掉了！",
                "0_B822": "吃我这招！",
                "0_B823": "好像很厉害的光炮！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "又变强了哦，御主。 变强真是一件令人高兴的事。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "我之所以会变得那么强，都是多亏了御主。谢谢。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "这件羽织怎么样，御主？",
                "0_G031": "……这样啊。……嗯，我也很喜欢。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "吾之身抵达无穹——",
                "0_G041": "嗯，这毫无疑问是史上最强的我。",
                "0_G042": "换言之，就是超级魔冲田Alter小姐的状态。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "——我经历了漫长……漫长的时光。",
                "0_G051": "那一刻，我活出了超越所有时光的人生。",
                "0_G052": "但唯独刻印于我这灵基上的记忆是不会消失的吧。",
                "0_G053": "谢谢……真的，太感谢了，御主。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "魔小姐等级提升～，对吧。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "生日……？　是这样啊，是御主诞生的日子啊。",
                "0_H011": "这对我来说也是个美妙的日子吧。",
                "0_H012": "嗯……这么说来，我听说过，生日应该赠送礼物。",
                "0_H013": "该送什么……对了，这把刀……不行，既重，又容易搁到门，很碍事吧。",
                "0_H014": "好，那作为补偿，今天我会答应你任何要求。"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像很吵闹呢。是有意思的事情吗？",
                "0_H021": "那就应该去。因为要把握当下嘛。",
                "0_H022": "当然，我也会跟你一起去。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "发生什么事了吗？",
                "0_H031": "如果能有我出场的机会就好了……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我原本是作为一次性灵基显现的。",
                "0_H041": "本来与御主这种存在毫无关系才对。",
                "0_H042": "从这个角度来说，我很感谢与我缔结契约的御主。 这是真的哦。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "主从？　我原本是为抑止而被调整而成的灵基。",
                "0_H051": "并不会侍奉任何人。",
                "0_H052": "不过嘛，为了御主，我倒不介意使用这把剑。",
                "0_H053": "不，应该说我要为您使用。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "让我想想，上次在食堂吃的关东煮，那个很好吃。",
                "0_H062": "尤其是白色的，叫什么来着……对，是竹轮麸。",
                "0_H063": "那种柔韧的口感很棒。",
                "0_H064": "……但是，不需要芥末。辣。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西吗。倒是没什么……不，对了。",
                "0_H071": "之前在商店抽奖时全都没中。",
                "0_H072": "在抽奖前明明还不怎么想要，",
                "0_H073": "但实际没抽中后就感到非常不甘心。",
                "0_H074": "不甘心。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯吗……",
                "0_H081": "我几乎可以说是为了破坏圣杯而现界的存在，所以没有什么愿望。",
                "0_H082": "什么？　能实现任何愿望？",
                "0_H083": "我压根不知道这种事……",
                "0_H084": "真的什么都能实现吗……？",
                "0_H085": "这样啊……什么都能啊。",
                "0_H086": "难道我希望夏天能吃到关东煮也可以吗？　……可以啊。",
                "0_H087": "唔，那下次遇到这种事，我还是重新考虑一下是否要破坏好了。"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我乃不知何时就会消失的虚幻的灵基。",
                "0_H091": "还是不要和我扯上什么关系为好。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "和我这样的人扯上关系真的好吗……",
                "0_H101": "这样啊，那我也就随意行动了。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "今天要做什么？",
                "0_H111": "我吗？　我会跟着御主。",
                "0_H112": "唔？　你说你在意？",
                "0_H113": "我知道了，我会不留任何气息与脚步声跟着你。"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "我喜欢御主。",
                "0_H121": "……怎么了？　我的话有什么奇怪的吗？",
                "0_H122": "御主对待不知何时就会消失的我那么好，",
                "0_H123": "在这里度过的人生本应是我无法得到的。",
                "0_H124": "……我真的很感谢您。"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我能触碰您的面颊吗？　嗯，没错……",
                "0_H131": "御主给了我一切……",
                "0_H132": "给了原本不可能存在于世的我居所、生命、一切……",
                "0_H133": "我喜欢御主与御主生活的这个世界。",
                "0_H134": "哪怕此身的灵基粉碎，我也会拼上一切守护御主所在的这个世界。"
            }
        },
        "0_H160": {
            "name": "对话 4",
            "text": {
                "0_H160": "第六天魔王……吗？",
                "0_H161": "那种看上去就很冷的衣服是什么？",
                "0_H162": "泳装……？　原来如此，还有这种东西啊。",
                "0_H163": "御主……我也想穿那种……"
            }
        },
        "0_H170": {
            "name": "对话 5",
            "text": {
                "0_H170": "第六天魔王吗，因为她为我的剑起了炼狱之铭，所以我并不讨厌。",
                "0_H171": "话说不知为什么，我对她莫名有种亲近感。",
                "0_H172": "究竟是为什么呢。"
            }
        },
        "0_H180": {
            "name": "对话 6",
            "text": {
                "0_H180": "茶茶大人吗。唔？　问我为什么要尊称大人？",
                "0_H181": "确实，为什么呢。",
                "0_H182": "虽然我不是很明白，但似乎有什么促使我这么称呼。"
            }
        },
        "0_H190": {
            "name": "对话 7",
            "text": {
                "0_H190": "冲田总司……另一个我。",
                "0_H191": "因为我几乎没有身为冲田总司而生的记忆，",
                "0_H192": "所以有时也会搞不清楚自己究竟是不是冲田的另一个现身。",
                "0_H193": "说个完全无关的话题，不知为何，身高和胸部都是我更胜一筹。",
                "0_H194": "难道是因为我还在发育期吗。"
            }
        },
        "0_H200": {
            "name": "对话 8",
            "text": {
                "0_H200": "土方……那个温柔的人吗……",
                "0_H201": "什么？　你说其实他让人觉得非常可怕？",
                "0_H202": "不，我完全不害怕啊。",
                "0_H203": "……这样啊，害怕啊。真不可思议。"
            }
        },
        "0_H210": {
            "name": "对话 9",
            "text": {
                "0_H210": "坂本龙马和阿龙啊。",
                "0_H211": "在帝都承蒙他们关照了。",
                "0_H212": "看来他们现界的目的和我相同，从这个角度来说应该算是我的前辈吧？"
            }
        },
        "0_H220": {
            "name": "对话 10",
            "text": {
                "0_H220": "唔，是当时的人斩啊。",
                "0_H221": "以藏的剑术实力确实很天才，或许连我都不如他吧。",
                "0_H222": "不过，到时候只要用光炮解决他就行了吧。"
            }
        },
        "0_H230": {
            "name": "对话 11",
            "text": {
                "0_H230": "原来如此，御主的周围确实有很多Alter的存在。",
                "0_H231": "说实话令我吃了一惊。",
                "0_H232": "虽然我是新人Alter，但也请您多指教了。"
            }
        },
        "0_M010": {
            "name": "唠唠叨叨帝都圣杯奇谭 4",
            "text": {
                "0_M010": "太好了，御主。",
                "0_M011": "找到什么了哦，话说这是什么好东西吗？"
            }
        },
        "0_M030": {
            "name": "唠唠叨叨帝都圣杯奇谭 5",
            "text": {
                "0_M030": "下一份工作委托好像来了。 按顺序解决掉吧。 帝都的和平由魔小姐来守护！"
            }
        },
        "0_M040": {
            "name": "唠唠叨叨帝都圣杯奇谭 6",
            "text": {
                "0_M040": "是新的工作委托吧。 不过还真是络绎不绝呢…… 所谓的侦探真是太辛苦了。"
            }
        },
        "0_M050": {
            "name": "唠唠叨叨帝都圣杯奇谭 7",
            "text": {
                "0_M050": "唔，发现了可疑的场所啊。 小心地前去探索吧。 没关系，御主，有我跟着。"
            }
        },
        "0_M060": {
            "name": "唠唠叨叨帝都圣杯奇谭 8",
            "text": {
                "0_M060": "那两个人吗？ 这样啊，太好了。",
                "0_M061": "因为我有承蒙那两位照顾，还要再次请各位多多关照呢。"
            }
        },
        "0_M080": {
            "name": "唠唠叨叨帝都圣杯奇谭 9",
            "text": {
                "0_M080": "全任务完成。太好了，御主！",
                "0_M081": "能走到这一步都是多亏了御主的努力，我也很高兴。",
                "0_M082": "尽管时间短暂，但这阵子过得真愉快……",
                "0_M083": "嗯，非常……愉快……"
            }
        },
        "0_N010": {
            "name": "唠唠叨叨帝都圣杯奇谭 1",
            "text": {
                "0_N010": "如果御主高兴的话，我也就高兴了。",
                "0_N011": "继续搜集吧。"
            }
        },
        "0_N020": {
            "name": "唠唠叨叨帝都圣杯奇谭 2",
            "text": {
                "0_N020": "做好觉悟了吗，御主。",
                "0_N021": "准备好了以后就告诉我。"
            }
        },
        "0_N030": {
            "name": "唠唠叨叨帝都圣杯奇谭 3",
            "text": {
                "0_N030": "这样一来，在这帝都的工作也就结束了吧，御主。",
                "0_N031": "话虽如此，我随时都是万无一失的魔小姐状态。",
                "0_N032": "需要帮助的时候请随时呼唤我。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "吾之铭乃是魔·冲田总司，回应召唤现界。",
                "0_S011": "我会与您并肩战斗到……此身的灵基粉碎为止。"
            }
        },
        "11_B030": {
            "name": "攻击 3 炼狱Alter",
            "text": {
                "11_B030": "休想逃！"
            }
        },
        "11_B040": {
            "name": "额外攻击 1 炼狱Alter",
            "text": {
                "11_B040": "无称光域，极地展开！ 必杀一击！"
            }
        },
        "11_B050": {
            "name": "宝具 1 炼狱Alter",
            "text": {
                "11_B050": "就由我炼狱代替主人，",
                "11_B051": "担任抑止之业！",
                "11_B052": "绝剑！　无穹三段！！"
            }
        },
        "11_B080": {
            "name": "技能 1 炼狱Alter",
            "text": {
                "11_B080": "这不是自卖自夸，我可是非常锋利的哦。"
            }
        },
        "11_B090": {
            "name": "技能 2 炼狱Alter",
            "text": {
                "11_B090": "唔？　意识……差点飞了，抱歉抱歉。"
            }
        },
        "11_B100": {
            "name": "开始 1 炼狱Alter",
            "text": {
                "11_B100": "由我代替我的主人，挥舞抑止之业吧。"
            }
        },
        "11_B110": {
            "name": "开始 2 炼狱Alter",
            "text": {
                "11_B110": "话说在前头，我可不像主人那样温柔哦。"
            }
        },
        "11_B120": {
            "name": "胜利 1 炼狱Alter",
            "text": {
                "11_B120": "结束了吧…… 喂，主人，差不多可以起来啦～。"
            }
        },
        "11_B130": {
            "name": "胜利 2 炼狱Alter",
            "text": {
                "11_B130": "如今，我立于已然遥远的无穹彼方。"
            }
        },
        "11_B150": {
            "name": "受击 1 炼狱Alter",
            "text": {
                "11_B150": "可恶！一定要想办法撑住……！"
            }
        },
        "11_B160": {
            "name": "无法战斗 1 炼狱Alter",
            "text": {
                "11_B160": "不能，在这里停止……"
            }
        },
        "11_B170": {
            "name": "无法战斗 2 炼狱Alter",
            "text": {
                "11_B170": "主人啊……对不起……！"
            }
        },
        "11_B180": {
            "name": "指令卡 1 炼狱Alter",
            "text": {
                "11_B180": "好嘞。"
            }
        },
        "11_B190": {
            "name": "指令卡 2 炼狱Alter",
            "text": {
                "11_B190": "噢。"
            }
        },
        "11_B200": {
            "name": "指令卡 3 炼狱Alter",
            "text": {
                "11_B200": "交给我吧。"
            }
        },
        "11_B210": {
            "name": "宝具卡 1 炼狱Alter",
            "text": {
                "11_B210": "要认真上了！"
            }
        },
        "11_B220": {
            "name": "宝具卡 2 炼狱Alter",
            "text": {
                "11_B220": "主人，快醒醒。 不行吗？"
            }
        },
        "11_B230": {
            "name": "宝具卡 3 炼狱Alter",
            "text": {
                "11_B230": "啊，我吗？"
            }
        },
        "11_B410": {
            "name": "攻击 4 炼狱Alter",
            "text": {
                "11_B410": "炼狱的炼是…… 不，我又不知道！"
            }
        },
        "11_B420": {
            "name": "攻击 5 炼狱Alter",
            "text": {
                "11_B420": "消失吧！"
            }
        },
        "11_B440": {
            "name": "额外攻击 2 炼狱Alter",
            "text": {
                "11_B440": "那招数叫什么来着…… 算了，无关紧要啦！"
            }
        },
        "11_B480": {
            "name": "技能 3 炼狱Alter",
            "text": {
                "11_B480": "要打飞他们了哦！"
            }
        },
        "11_B490": {
            "name": "技能 4 炼狱Alter",
            "text": {
                "11_B490": "魔力量全开！ ……魔力量是什么？"
            }
        },
        "11_B520": {
            "name": "胜利 3 炼狱Alter",
            "text": {
                "11_B520": "主人的御主也没事吧？ 那太好了，总算不用被主人训斥了。"
            }
        },
        "11_B550": {
            "name": "受击 3 炼狱Alter",
            "text": {
                "11_B550": "这种东西对我无效！ ……不，稍微有点效果！"
            }
        },
        "11_B560": {
            "name": "无法战斗 3 炼狱Alter",
            "text": {
                "11_B560": "我竟然会……"
            }
        },
        "11_B570": {
            "name": "无法战斗 4 炼狱Alter",
            "text": {
                "11_B570": "太难看了……"
            }
        },
        "11_B800": {
            "name": "宝具 2 炼狱Alter",
            "text": {
                "11_B800": "无量、无碍、无边……",
                "11_B801": "三光集成就无穹！",
                "11_B802": "绝剑！　无穹三段！"
            }
        },
        "11_B810": {
            "name": "宝具 3 炼狱Alter",
            "text": {
                "11_B810": "不知我一个人能否控制住……",
                "11_B811": "算了，也只能动手了！",
                "11_B812": "打飞吧！　炼狱炮——！"
            }
        },
        "11_B820": {
            "name": "宝具 4 炼狱Alter",
            "text": {
                "11_B820": "将目标认定为歼灭对象，",
                "11_B821": "安全锁解除，魔力回路满负荷运转，",
                "11_B822": "这就是我和主人的，压轴大招——！！"
            }
        },
        "11_G010": {
            "name": "升级 1 炼狱Alter",
            "text": {
                "11_G010": "虽然我说这个或许有些奇怪，但谢啦。"
            }
        },
        "11_G110": {
            "name": "升级 2 炼狱Alter",
            "text": {
                "11_G110": "炼狱先生等级提升～，才不是呢，真是的。"
            }
        },
        "11_H010": {
            "name": "生日 炼狱Alter",
            "text": {
                "11_H010": "你说生日？",
                "11_H011": "人类总喜欢执着于这种毫无意义的时间坐标呢。",
                "11_H012": "哎？　主人，怎么突然觉醒了……什么，一定要好好祝贺？",
                "11_H013": "为什么一遇到这种事就认真起来了啦！？",
                "11_H014": "……好啦好啦，我知道了啦。",
                "11_H015": "主人的御主啊，倒也不是不能说正是多亏了你的诞生，才有了现在的主人，或者说的确这么说。",
                "11_H016": "从这种角度来说，今天果然是个特别的日子呢。",
                "11_H017": "嗯，说得对……祝你生日快乐。"
            }
        },
        "11_H020": {
            "name": "活动举行中 炼狱Alter",
            "text": {
                "11_H020": "活动？",
                "11_H021": "挺好的，能够开心玩耍的时候就该尽情玩。",
                "11_H022": "这也是为了无法开心玩的时候。",
                "11_H023": "这一定会成为支撑你的某种存在。"
            }
        },
        "11_H030": {
            "name": "对话 1 炼狱Alter",
            "text": {
                "11_H030": "好像发生了什么呢。",
                "11_H031": "但愿不需要主人出场……"
            }
        },
        "11_H040": {
            "name": "对话 2 炼狱Alter",
            "text": {
                "11_H040": "我归根结底还是主人的剑，没有理由或义务听你的命令。",
                "11_H041": "所以，不要对我抱有奇怪的期待哦。",
                "11_H042": "我终归只会为主人而行动啦。",
                "11_H043": "但主人是为你而战的，结果我再怎么唠叨也没有意义呢。",
                "11_H044": "所以啦，你就随便点用我吧，嗯。"
            }
        },
        "11_H050": {
            "name": "对话 3 炼狱Alter",
            "text": {
                "11_H050": "主从？",
                "11_H051": "我和主人并不是这样的关系，我只不过是支援主人的存在。",
                "11_H052": "哈？　我和你？",
                "11_H053": "这个嘛，该怎么说呢，毕竟你是主人的主人，所以，并不是不能说你也算我的主人……吗？"
            }
        },
        "11_H060": {
            "name": "喜欢的东西 炼狱Alter",
            "text": {
                "11_H060": "喜欢的东西？",
                "11_H061": "没有什么特别的。",
                "11_H062": "那么喜欢的食物？",
                "11_H063": "我其实并不需要食物。",
                "11_H064": "唔～，一定要说的话，应该是我们分成两个人时被喂的那个，叫什么来着。",
                "11_H065": "对对，橡皮糖。",
                "11_H066": "那个会让人上瘾呢。"
            }
        },
        "11_H070": {
            "name": "讨厌的东西 炼狱Alter",
            "text": {
                "11_H070": "讨厌的东西？",
                "11_H071": "呃，是什么呢？……一定要说的话，应该是人类，不，是世界吧？",
                "11_H072": "啊，不是指这类东西啊。",
                "11_H073": "那让我想想～……",
                "11_H074": "啊啊，那个啦，那个，主人偶尔会用洗洁精清洗我，但能不能让她别那么干？",
                "11_H075": "不行的话，至少让她换成肥皂啦。"
            }
        },
        "11_H080": {
            "name": "关于圣杯 炼狱Alter",
            "text": {
                "11_H080": "圣杯？",
                "11_H081": "啊，快毁掉快毁掉。",
                "11_H082": "迄今为止已经见识过很多次了，但能正常使用那玩意儿的人根本不存在。",
                "11_H083": "一个个都觉得自己能够正常使用，但那时还压根没用过，为什么能这么肯定啦，那些人类。",
                "11_H084": "真是的，怎么不多为收拾残局的人着想啦。"
            }
        },
        "11_H10090": {
            "name": "牵绊Lv.1 炼狱Alter",
            "text": {
                "11_H10090": "这具身体只是我代替主人暂时控制而已。",
                "11_H10091": "也就是说你现在向我搭话也没用，再见啦。"
            }
        },
        "11_H10100": {
            "name": "牵绊Lv.2 炼狱Alter",
            "text": {
                "11_H10100": "喂，别因为主人睡着了就做什么奇怪的事哦。",
                "11_H10101": "只要有我管理着，就不允许你动主人一根汗毛。"
            }
        },
        "11_H10110": {
            "name": "牵绊Lv.3 炼狱Alter",
            "text": {
                "11_H10110": "一个不注意主人就会拿别人的冰淇淋之类的吃，你也要注意啦。",
                "11_H10111": "老是吃那种零食的话，进食量就会减少，运转效率也会降低的。",
                "11_H10112": "什么……？　说我像老妈？　谁是老妈啊！"
            }
        },
        "11_H10120": {
            "name": "牵绊Lv.4 炼狱Alter",
            "text": {
                "11_H10120": "啊，主人的话，还在睡哦。",
                "11_H10121": "其实想这么一直睡下去也没问题。",
                "11_H10122": "……当然这是不可能的啦。",
                "11_H10123": "主人为了世界，为了人类而一直……不，是为了你吧？",
                "11_H10124": "虽然我不太懂，但只要这是主人的愿望，我也会这么做。"
            }
        },
        "11_H10130": {
            "name": "牵绊Lv.5 炼狱Alter",
            "text": {
                "11_H10130": "仔细想来，只要我就这样把所有一切全部消灭，主人就不用再干那些无聊的工作了吧，炼狱十分惊讶。",
                "11_H10131": "……什么的，开玩笑啦开玩笑，虽然有那么点认真。",
                "11_H10132": "我知道主人不愿见到这种事，你也不愿意吧。",
                "11_H10133": "没错，那就是主人，那就是你。",
                "11_H10134": "我会为此而战。一直战斗到，终有一天抵达那片天空之下为止……"
            }
        },
        "11_H160": {
            "name": "对话 4 炼狱Alter",
            "text": {
                "11_H160": "那是叫泳装信吗？",
                "11_H161": "虽然言行蠢到让大家都忘了，但那个状态说实话相当不妙呢。",
                "11_H162": "真的发生什么的时候会麻烦到令人绝望，能趁现在干掉她吗？　不行？　哦，这样啊。"
            }
        },
        "11_H170": {
            "name": "对话 5 炼狱Alter",
            "text": {
                "11_H170": "第六天魔王？",
                "11_H171": "姑且算是给我取名的人，首次启动时也多亏她提供魔力，我很感谢她啦。",
                "11_H172": "不过一码归一码，她迟早会和主人……",
                "11_H173": "不，还是算了，这种无可奈何的事再怎么担心也没用。"
            }
        },
        "11_H180": {
            "name": "对话 6 炼狱Alter",
            "text": {
                "11_H180": "茶茶？",
                "11_H181": "啊啊，是那个人吧。主人经常承蒙她的照顾。",
                "11_H182": "究竟是为什么呢，只要在那个人的身边，主人也莫名非常安定。",
                "11_H183": "虽然我不太明白，但人类间确实存在这种莫名其妙关系吧。"
            }
        },
        "11_H190": {
            "name": "对话 7 炼狱Alter",
            "text": {
                "11_H190": "主人源头的存在啊。",
                "11_H191": "就算源头一样，主人也已经走得相当遥远了呢。",
                "11_H192": "但是，根源或者说源头的部分还是始终没变呢，虽然完全看不出来。",
                "11_H193": "该怎么说呢，感觉很难形容，但有点没有限度，限度啦。",
                "11_H194": "嗯，不懂其实也没关系。就算明白了，也只会觉得那又如何。"
            }
        },
        "11_H200": {
            "name": "对话 8 炼狱Alter",
            "text": {
                "11_H200": "土方岁三啊……",
                "11_H201": "他莫名和主人很亲近呢。",
                "11_H202": "或许也是因为原本和主人认识吧。",
                "11_H203": "该怎么说呢，他和主人有点相似呢，我指存在方式。"
            }
        },
        "11_H210": {
            "name": "对话 9 炼狱Alter",
            "text": {
                "11_H210": "那个带着不好惹的蛇的家伙吗？",
                "11_H211": "该说是同行还是什么好呢，虽然严格来说感觉不太一样……",
                "11_H212": "不过算了。他们看起来不像是会成为主人的敌人。",
                "11_H213": "再说了，那个男人真的是人类吗？"
            }
        },
        "11_H220": {
            "name": "对话 10 炼狱Alter",
            "text": {
                "11_H220": "冈田……什么来着？",
                "11_H221": "没错，就是那个叫田冈的家伙。那家伙还是不要过于接近主人为好呢。",
                "11_H222": "因为一旦大意，主人就会模仿他的剑。",
                "11_H223": "那种剑术不可貌相，是以极高水准的术理构成的。",
                "11_H224": "不太适合主人的剑，或者说主人的话，一旦接纳了那个，反而会弱化。",
                "11_H225": "而且，我也不希望笨蛋传染给主人。"
            }
        },
        "11_H230": {
            "name": "对话 11 炼狱Alter",
            "text": {
                "11_H230": "你的周围有好多Alter分类的从者呢。",
                "11_H231": "所以主人才会说让你叫她Alter亲啊。",
                "11_H232": "总之，有这兴致的话就这么称呼吧，叫主人Alter亲。",
                "11_H233": "毕竟主人像这样说出自己的愿望是非常罕见的。"
            }
        },
        "11_H240": {
            "name": "对话 12 炼狱Alter",
            "text": {
                "11_H240": "泳装的主人？",
                "11_H241": "那个到底是什么啦。",
                "11_H242": "我也万万没想到自己竟会变成那种样子。",
                "11_H243": "哈？　可爱？",
                "11_H244": "主人姑且不论，我可不会因为你这么评价而感到高兴哦。",
                "11_H245": "你说声音也很可爱？　哎？　我现在的声音难道不可爱吗？"
            }
        }
    },
    "ChrVoice_ids": [
        "1000700"
    ]
}
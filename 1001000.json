        },
        "11_G060": {
            "name": "灵衣开放 牵牛花纹浴衣（咒）",
            "text": {
                "11_G060": "芦屋道满，换上夏日服装！",
                "11_G061": "原来如此，这就是所谓的浴衣啊。",
                "11_G062": "唔唔唔唔，这是何等凉爽而轻巧，应该非常适合贫僧吧，您觉得呢？",
                "11_G063": "如果方便的话，这个夏天期间贫僧就以这套服装随侍御主身侧吧。"
            }
        },
        "11_G110": {
            "name": "升级 2 牵牛花纹浴衣（咒）",
            "text": {
                "11_G110": "唔唔唔唔！　令人亢奋。"
            }
        },
        "11_G120": {
            "name": "升级 3 牵牛花纹浴衣（咒）",
            "text": {
                "11_G120": "侥幸，侥幸。"
            }
        },
        "11_H9000": {
            "name": "关于灵衣 牵牛花纹浴衣（咒）",
            "text": {
                "11_H9000": "请千万小心，请千万小心。",
                "11_H9001": "听好了哦，御主？",
                "11_H9002": "这身灵衣，可不是普通的浴衣，千万不要掉以轻心。",
                "11_H9003": "否则的话，牵牛就会当即盛开哦……？",
                "11_H9004": "看吧，一下就开了哦！"
            }
        },
        "2_B010": {
            "name": "攻击 1",
            "text": {
                "2_B010": "急急如律令！"
            }
        },
        "2_B020": {
            "name": "攻击 2",
            "text": {
                "2_B020": "去死吧——！"
            }
        },
        "2_B030": {
            "name": "攻击 3",
            "text": {
                "2_B030": "黑暗啊！"
            }
        },
        "2_B040": {
            "name": "额外攻击 1",
            "text": {
                "2_B040": "呵呵呵……哈哈哈哈哈哈哈！ 粉碎吧！"
            }
        },
        "2_B050": {
            "name": "宝具 1",
            "text": {
                "2_B050": "显光阁下，苏醒吧！",
                "2_B051": "黑暗的帷幕到来吧，",
                "2_B052": "太阳将在此重生！",
                "2_B053": "——『狂澜怒涛·恶灵左府』！",
                "2_B054": "呵呵呵呵哈哈哈哈哈哈哈！！"
            }
        },
        "2_B080": {
            "name": "技能 1",
            "text": {
                "2_B080": "吾之指尖，无所不达——"
            }
        },
        "2_B090": {
            "name": "技能 2",
            "text": {
                "2_B090": "黑色的力量，永无止尽。"
            }
        },
        "2_B100": {
            "name": "开始 1",
            "text": {
                "2_B100": "骷髅乌帽子芦屋道满，出击！"
            }
        },
        "2_B120": {
            "name": "胜利 1",
            "text": {
                "2_B120": "罗刹王永不败北！ 呵呵——哈哈哈！　哈哈哈哈哈哈！"
            }

        "2_B410": {
            "name": "攻击 4",
            "text": {
                "2_B410": "鬼神招来！　哈哈哈哈！"
            }
        },
        "2_B420": {
            "name": "攻击 5",
            "text": {
                "2_B420": "切尔诺伯格！"
            }
        },
        "2_B440": {
            "name": "额外攻击 2",
            "text": {
                "2_B440": "好好领教一下……罗刹王究竟为何吧！"
            }
        },
        "2_B480": {
            "name": "技能 3",
            "text": {
                "2_B480": "黑暗在呢喃。"
            }
        },
        "2_B500": {
            "name": "开始 3",
            "text": {
                "2_B500": "让你见识罗刹王的力量吧！"
            }
        },
        "2_B510": {
            "name": "开始 4",
            "text": {
                "2_B510": "人、神、兽，以及……！"
            }
        },
        "2_B520": {
            "name": "胜利 3",
            "text": {
                "2_B520": "魔力有没有消耗过度呢？"
            }
        },
        "2_B530": {
            "name": "胜利 4",
            "text": {
                "2_B530": "至此全部完结！"
            }
        },
        "2_B550": {
            "name": "受击 3",
            "text": {
                "2_B550": "此乃甘露……！"
            }
        },
        "2_B560": {
            "name": "无法战斗 3",
            "text": {
                "2_B560": "有些……过于奢望了吗……"
            }
        },
        "2_B800": {
            "name": "宝具 2",
            "text": {
                "2_B800": "光之刻至此结束。",
                "2_B801": "拟似神核并连，",
                "2_B802": "黑暗太阳临界！",
                "2_B803": "——『狂澜怒涛·恶灵左府』！",
                "2_B804": "唔唔唔唔唔唔唔多谢款待！！"
            }
        },
        "2_B840": {
            "name": "额外攻击 3",
            "text": {
                "2_B840": "快看，快看！ 晴明…… 晴明————！"
            }
        },
        "2_G010": {
            "name": "升级 1",
            "text": {
                "2_G010": "可不要……过度接触贫僧哦。"
            }
        },
        "2_G040": {
            "name": "灵基再临 1",
            "text": {
                "2_G040": "噢……噢噢……！",
                "2_G041": "这个形象连贫僧也是初次得见哦！",
                "2_G042": "一定要说的话，没错，骷髅乌帽子芦屋道满！",
                "2_G043": "适应到宛若与吸收入吾身的三个御灵真正融为了一体！",
                "2_G044": "比人、兽、神更进一步的存在……",
                "2_G045": "换言之，或许可以称之为罗刹王·骷髅乌帽子芦屋道满！",
                "2_G046": "诸如此类——",
                "2_G047": "当然只是个玩笑，御主。"
            }
        },
        "2_G050": {
            "name": "灵基再临 2",
            "text": {
                "2_G050": "在这边，在这边哦，御主。",
                "2_G051": "贫僧似乎有点亢奋……",
                "2_G052": "如何，您要不要也一起……",
                "2_G053": "什么？",
                "2_G054": "嗯，嗯，这的确是骷髅……",
                "2_G055": "但不用那么颤抖。",
                "2_G056": "当然！　当然，只是模拟出来的虚伪欺诈如梦似幻的存在哦……"
            }
        },
        "2_G110": {
            "name": "升级 2",
            "text": {
                "2_G110": "唔唔唔唔！　令人亢奋。"
            }
        },
        "2_G120": {
            "name": "升级 3",
            "text": {
                "2_G120": "侥幸，侥幸。"
            }
        },
        "2_H010": {
            "name": "生日",
            "text": {
                "2_H010": "唔唔唔唔，御主！",
                "2_H011": "为您的生日送上祝福！",
                "2_H012": "……光用话语来祝福未免不合适，就凝聚吾的术之精华为您祝福吧。",
                "2_H013": "不惜夺走他人的幸运，也要为您带去幸运。",
                "2_H014": "什么？　不喜欢这样？",
                "2_H015": "您真是位任性的人呢——"
            }
        },
        "2_H020": {
            "name": "活动举行中",
            "text": {
                "2_H020": "看来正在举办什么活动。",
                "2_H021": "我们走吧，御主。"
            }
        },
        "2_H030": {
            "name": "对话 1",
            "text": {
                "2_H030": "好了，我们出发吧。",
                "2_H031": "有贫僧在一旁守护着您哦……"
            }
        },
        "2_H040": {
            "name": "对话 2",
            "text": {
                "2_H040": "我虽身为影子却能为宫中效力，呵呵……",
                "2_H041": "简直太讽刺了。"
            }
        },
        "2_H050": {
            "name": "对话 3",
            "text": {
                "2_H050": "为了人理，您大可榨干我道满所有的剩余价值。",
                "2_H051": "请不要客气哦，御主。"
            }
        },
        "2_H070": {
            "name": "讨厌的东西",
            "text": {
                "2_H070": "讨厌的东西。",
                "2_H071": "贫僧虽然没有任何讨厌的东西……",
                "2_H072": "但晴明……安倍晴明……",
                "2_H073": "只有那家伙是例外……！",
                "2_H074": "唔唔唔唔唔唔……！！"
            }
        },
        "2_H080": {
            "name": "关于圣杯",
            "text": {
                "2_H080": "圣杯。圣杯啊……",
                "2_H081": "呵呵。呵呵呵呵呵呵呵。",
                "2_H082": "不不，贫僧什么也没说。",
                "2_H083": "唔呵呵呵呵呵呵……"
            }
        },
        "2_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "2_H090": "来吧，御主。",
                "2_H091": "让我们一起去夺回人理、世界、泛人类史吧！",
                "2_H092": "呵呵呵！"
            }
        },
        "2_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "2_H100": "御主。",
                "2_H101": "请不要过于埋头苦干。",
                "2_H102": "没有人可以代替您。",
                "2_H103": "贫僧十分理解您觉得正因如此才要这么做的心情，但依然要这么说。",
                "2_H104": "哎哎，事务办公就交给贫僧吧。",
                "2_H105": "贫僧好歹也是与宫廷走的比较近的人，从那些文书工作的超人那里依样学了不少。",
                "2_H106": "好了好了，交给贫僧吧，交给贫僧吧。"
            }
        },
        "2_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "2_H110": "御主，您……",
                "2_H111": "……唔唔……拇……",
                "2_H112": "看贫僧憋了那么久，呵呵，可话到嘴边突然忘了想说什么了。",
                "2_H113": "唔唔唔唔十分抱歉！",
                "2_H114": "什么？　您也会遇到——这种忘事的时候吗。",
                "2_H115": "哈哈您也是呀！",
                "2_H116": "呵呵。贫僧虽然是人理的影子，英灵之身……",
                "2_H117": "但或许与人类并没有什么差别呢……"
            }
        },
        "2_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "2_H120": "御主。我的主人。",
                "2_H121": "唔，唔唔……尽管贫僧心知肚明——",
                "2_H122": "但究竟该如何将这份内心的激动传达给您呢。",
                "2_H123": "御主，每当看到您的眼睛……",
                "2_H124": "或是您的侧脸时，贫僧……",
                "2_H125": "贫僧……就会被难以遏制的情感所囚禁。",
                "2_H126": "不停告诉自己要平静下来，要平静下来……",
                "2_H127": "不，请忘记这些话吧。贫僧只是头脑发昏……"
            }
        },
        "2_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "2_H130": "我的主人，请您下令。",
                "2_H131": "只要是为了您，贫僧会毫不犹豫地去做任何事。",
                "2_H132": "无论是杀戮，还是蹂躏。",
                "2_H133": "您会踌躇的一切，我都不会犹豫。",
                "2_H134": "我会做遍一切恶事，为您开辟前行的道路——"
            }
        },
        "2_H170": {
            "name": "对话 4",
            "text": {
                "2_H170": "玉藻前——",
                "2_H171": "过去、现在、未来，置身人世之外始终保持美丽的存在啊……",
                "2_H172": "你永远那么美丽，美丽到令人生厌。"
            }
        },
        "2_H180": {
            "name": "对话 5",
            "text": {
                "2_H180": "源赖光……",
                "2_H181": "没想到成为影子之后都未能消除这业障……"
            }
        },
        "2_H1800": {
            "name": "牵绊Lv.5 (地狱界曼荼罗 平安京通关后)",
            "text": {
                "2_H1800": "呵呵，呵呵呵。您是说，不用再撒谎了吗……？",
                "2_H1801": "呵呵，呵呵呵——唔唔唔唔唔唔唔，正是！　乃是戏言！　贫僧作为贫僧，",
                "2_H1802": "拥有所有身为Alterego、芦屋道满、Caster……或是Alterego Limbo的记录。",
                "2_H1803": "贫僧对于当时，您能悉数参与贫僧的游戏表示无比，无比诚挚的……",
                "2_H1804": "……什么？",
                "2_H1805": "啊……不不怎么会呢！　此灵基乃是沿着缘分，由迦勒底召唤式召唤而来的您的从者。",
                "2_H1806": "已经与「异星之神」切断了关系哦。",
                "2_H1807": "……假若您不相信的话，随时可以斩下贫僧的首级哦。",
                "2_H1808": "呵呵呵……但是，如若可能……既然被召唤，愿能陪伴您的左右……",
                "2_H1809": "——直到地狱尽头。我的，主人。"
            }
        },
        "2_H190": {
            "name": "对话 6",
            "text": {
                "2_H190": "噢噢，赖光四天王。",
                "2_H191": "知道吗。武士这种存在似乎在当代已经绝迹了哦！",
                "2_H192": "唔唔唔真遗憾！"
            }
        },
        "2_H200": {
            "name": "对话 7",
            "text": {
                "2_H200": "大陆的智者阁下！",
                "2_H201": "难道您会使用仙术、道术？",
                "2_H202": "颇有意思。实际上贫僧也……",
                "2_H203": "唔，咳咳，有机会再聊。"
            }
        },
        "2_H210": {
            "name": "对话 8",
            "text": {
                "2_H210": "加藤段藏阁下。",
                "2_H211": "您找贫僧……是有何贵干呢？",
                "2_H212": "被您这样狠狠地盯着，贫僧都无法动弹了。呵呵呵……"
            }
        },
        "2_H220": {
            "name": "对话 9",
            "text": {
                "2_H220": "风魔小太郎阁下。",
                "2_H221": "风魔忍群的头领。原来如此。这样一位……",
                "2_H222": "找贫僧有何贵干？　监视？",
                "2_H223": "哎呀，为何要这么做。贫僧毫无自觉……",
                "2_H224": "不过还请随意。"
            }
        },
        "2_H230": {
            "name": "对话 10",
            "text": {
                "2_H230": "原来如此。",
                "2_H231": "那个女人……终于实现……",
                "2_H232": "夙愿了吗……"
            }
        },
        "2_H240": {
            "name": "对话 11",
            "text": {
                "2_H240": "哎呀呀，藤原香子阁下。",
                "2_H241": "还是应该称呼您为紫式部阁下比较好呢？",
                "2_H242": "话说，香子阁下。",
                "2_H243": "晴明……有没有传授您什么……",
                "2_H244": "没有？　什么都没有？",
                "2_H245": "唔唔唔唔唔唔唔这样啊，这样啊——"
            }
        },
        "2_H250": {
            "name": "对话 12",
            "text": {
                "2_H250": "清少纳言阁下，请住手。",
                "2_H251": "请不要扒贫僧的衣服。",
                "2_H252": "贫僧不是变成人形的野兽！",
                "2_H253": "贫僧只不过是『美丽的肉食兽』……",
                "2_H254": "请住手！"
            }
        },
        "2_H260": {
            "name": "对话 13",
            "text": {
                "2_H260": "唔……怎么？　想和贫僧玩耍？",
                "2_H261": "可以！　先做个与人类分毫不差的式神送你们吧。让你们尽情解体……",
                "2_H262": "唔唔，为何要拉贫僧的头发，Caster 童谣。",
                "2_H263": "在迦勒底不能玩这种危险的游戏？　现在是玩扮家家的时间？",
                "2_H264": "……没办法。",
                "2_H265": "那贫僧希望扮演岳父的角色！"
            }
        },
        "2_H280": {
            "name": "对话 14",
            "text": {
                "2_H280": "唔嗯……",
                "2_H281": "机关人偶又多了不少呢。",
                "2_H282": "呵呵，呵呵呵呵呵呵呵。"
            }
        },
        "2_H290": {
            "name": "对话 15",
            "text": {
                "2_H290": "御主，能打扰一下吗。",
                "2_H291": "其实是关于尼基季奇阁下。",
                "2_H292": "每当贫僧想要放式神的时候，她总是怒发冲冠地攻击贫僧，唉，真是太令人头疼了……",
                "2_H293": "……什么？",
                "2_H294": "问贫僧用式神干什么？",
                "2_H295": "当然是对高扬斯卡娅阁下……",
                "2_H296": "哎哟，被勇士阁下发现了，那回头见！"
            }
        },
        "2_H300": {
            "name": "对话 16",
            "text": {
                "2_H300": "嚯嚯，太公望阁下！",
                "2_H301": "真是太棒了，务必允许贫僧与您好好探讨仙术的奥秘。",
                "2_H302": "虽然贫僧的思想键纹是稍微有些非法的那种，不过这种细节就不提了。"
            }
        },
        "2_H3060": {
            "name": "喜欢的东西 (地狱界曼荼罗 平安京通关后)",
            "text": {
                "2_H3060": "喜欢的东西。",
                "2_H3061": "一定要说的话，没错……",
                "2_H3062": "践踏诸如他人的尊严、信念之类的东西——真不错。非常美妙。犹如甘露的滋味。您要不要也试试？"
            }
        },
        "2_H310": {
            "name": "对话 17",
            "text": {
                "2_H310": "哎呀，高扬斯卡娅阁下，祝您心情愉快！",
                "2_H311": "哈哈，原来如此，这种表情啊……",
                "2_H312": "不不，不不不，怎么会呢。",
                "2_H313": "贫僧是芦屋道满，只是个微不足道的法师阴阳师。",
                "2_H314": "Limbo？　您在说什么呢？"
            }
        },
        "2_H330": {
            "name": "对话 18",
            "text": {
                "2_H330": "鬼一阁下，鬼一法眼阁下。",
                "2_H331": "您还真是，老样子呢……"
            }
        },
        "2_H340": {
            "name": "对话 19",
            "text": {
                "2_H340": "嗯。",
                "2_H341": "阿比盖尔阁下，找贫僧有何贵干？",
                "2_H342": "关于梦？　御主的？",
                "2_H343": "呵呵，贫僧并不擅长这类，请恕我婉拒！",
                "2_H344": "什么？",
                "2_H345": "你说是叔叔吩咐你来的。哦，这样啊。"
            }
        },
        "2_H350": {
            "name": "对话 20",
            "text": {
                "2_H350": "哎呀哎呀，",
                "2_H351": "变得有些空虚了呢……",
                "2_H352": "是的，指的是各位复仇者。",
                "2_H353": "他们的灵基已经相当于空壳了。",
                "2_H354": "呜呼，这是多么的空虚啊。"
            }
        },
        "2_H360": {
            "name": "对话 21",
            "text": {
                "2_H360": "卡利奥斯特罗伯爵？",
                "2_H361": "原来如此。",
                "2_H362": "贫僧不认识呢……"
            }
        },
        "2_H6060": {
            "name": "喜欢的东西",
            "text": {
                "2_H6060": "喜欢的东西……",
                "2_H6061": "还是不说了。",
                "2_H6062": "现在告诉您一定还为时过早。"
            }
        }
    },
    "ChrVoice_ids": [
        "1001000",
        "109824830"
    ]
}
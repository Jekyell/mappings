{
    "id": 1001000,
    "collectionNo": 297,
    "name": "芦屋道满",
    "voices": {
        "0_B020": {
            "name": "攻击 2",
            "text": {
                "0_B020": "看招！！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "怎么样！"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "贫僧……非常亢奋！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "显光阁下，苏醒吧！",
                "0_B051": "黑暗的帷幕到来吧，",
                "0_B052": "太阳将在此重生！",
                "0_B053": "——『狂澜怒涛·恶灵左府』！",
                "0_B054": "呵呵呵呵哈哈哈哈哈哈哈！！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "那么各位，敬请笑览——"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "道满要出击了哟。"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "唔唔唔唔唔唔！　您觉得如何，御主！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "将胜利献给您，我的御主。"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "怎么会……！？"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "那请容贫僧先行告退了……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "嗯，这灵基也要不行了呢……"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "可以吗？"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "唔唔唔唔真不错！！"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "当然没问题。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "随心所欲地贪求吧。"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "晴明……晴明————！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "最终会如何可不关贫僧什么事哦……？"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "用这招！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "还有你！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "贫僧，多才多艺——"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "唔唔唔唔唔唔唔，是呢……"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "多少，有些蠢蠢欲动呢？"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "要不要再做个新式神呢……"
            }
        },
        "0_B530": {
            "name": "胜利 4",
            "text": {
                "0_B530": "可喜可贺，可喜可贺……呵呵呵呵呵呵。"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "哎哟……"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "甘露，甘露……！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "不不，这绝不是因为御主的魔力不足！ 请您不要在意。"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "唔唔唔唔稍后再见……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "光之刻至此结束。",
                "0_B801": "拟似神核并连，",
                "0_B802": "黑暗太阳临界！",
                "0_B803": "——『狂澜怒涛·恶灵左府』！",
                "0_B804": "唔唔唔唔唔唔唔多谢款待！！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "那就……慷慨款待大家吧！"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "可不要……过度接触贫僧哦。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "灵基……没有变化呢。",
                "0_G021": "不过灵基似乎的确有所提升。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "灵基变化了！",
                "0_G031": "迦勒底召唤式，唔唔唔唔多么有意思的玩意儿啊。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "唔唔唔唔！　令人亢奋。"
            }
        },
        "0_G120": {
            "name": "升级 3",
            "text": {
                "0_G120": "侥幸，侥幸。"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "唔唔唔唔，御主！",
                "0_H011": "为您的生日送上祝福！",
                "0_H012": "……光用话语来祝福未免不合适，就凝聚吾的术之精华为您祝福吧。",
                "0_H013": "不惜夺走他人的幸运，也要为您带去幸运。",
                "0_H014": "什么？　不喜欢这样？",
                "0_H015": "您真是位任性的人呢——"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "看来正在举办什么活动。",
                "0_H021": "我们走吧，御主。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "好了，我们出发吧。",
                "0_H031": "有贫僧在一旁守护着您哦……"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "我虽身为影子却能为宫中效力，呵呵……",
                "0_H041": "简直太讽刺了。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "为了人理，您大可榨干我道满所有的剩余价值。",
                "0_H051": "请不要客气哦，御主。"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西……",
                "0_H061": "还是不说了。",
                "0_H062": "现在告诉您一定还为时过早。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西。",
                "0_H071": "贫僧虽然没有任何讨厌的东西……",
                "0_H072": "但晴明……安倍晴明……",
                "0_H073": "只有那家伙是例外……！",
                "0_H074": "唔唔唔唔唔唔……！！"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "圣杯。圣杯啊……",
                "0_H081": "呵呵。呵呵呵呵呵呵呵。",
                "0_H082": "不不，贫僧什么也没说。",
                "0_H083": "唔呵呵呵呵呵呵……"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "来吧，御主。",
                "0_H091": "让我们一起去夺回人理、世界、泛人类史吧！",
                "0_H092": "呵呵呵！"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "御主。",
                "0_H101": "请不要过于埋头苦干。",
                "0_H102": "没有人可以代替您。",
                "0_H103": "贫僧十分理解您觉得正因如此才要这么做的心情，但依然要这么说。",
                "0_H104": "哎哎，事务办公就交给贫僧吧。",
                "0_H105": "贫僧好歹也是与宫廷走的比较近的人，从那些文书工作的超人那里依样学了不少。",
                "0_H106": "好了好了，交给贫僧吧，交给贫僧吧。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "御主，您……",
                "0_H111": "……唔唔……拇……",
                "0_H112": "看贫僧憋了那么久，呵呵，可话到嘴边突然忘了想说什么了。",
                "0_H113": "唔唔唔唔十分抱歉！",
                "0_H114": "什么？　您也会遇到——这种忘事的时候吗。",
                "0_H115": "哈哈您也是呀！",
                "0_H116": "呵呵。贫僧虽然是人理的影子，英灵之身……",
                "0_H117": "但或许与人类并没有什么差别呢……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "御主。我的主人。",
                "0_H121": "唔，唔唔……尽管贫僧心知肚明——",
                "0_H122": "但究竟该如何将这份内心的激动传达给您呢。",
                "0_H123": "御主，每当看到您的眼睛……",
                "0_H124": "或是您的侧脸时，贫僧……",
                "0_H125": "贫僧……就会被难以遏制的情感所囚禁。",
                "0_H126": "不停告诉自己要平静下来，要平静下来……",
                "0_H127": "不，请忘记这些话吧。贫僧只是头脑发昏……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "我的主人，请您下令。",
                "0_H131": "只要是为了您，贫僧会毫不犹豫地去做任何事。",
                "0_H132": "无论是杀戮，还是蹂躏。",
                "0_H133": "您会踌躇的一切，我都不会犹豫。",
                "0_H134": "我会做遍一切恶事，为您开辟前行的道路——"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "玉藻前——",
                "0_H171": "过去、现在、未来，置身人世之外始终保持美丽的存在啊……",
                "0_H172": "你永远那么美丽，美丽到令人生厌。"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "源赖光……",
                "0_H181": "没想到成为影子之后都未能消除这业障……"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5 (地狱界曼荼罗 平安京通关后)",
            "text": {
                "0_H1800": "呵呵，呵呵呵。您是说，不用再撒谎了吗……？",
                "0_H1801": "呵呵，呵呵呵——唔唔唔唔唔唔唔，正是！　乃是戏言！　贫僧作为贫僧，",
                "0_H1802": "拥有所有身为Alterego、芦屋道满、Caster……或是Alterego Limbo的记录。",
                "0_H1803": "贫僧对于当时，您能悉数参与贫僧的游戏表示无比，无比诚挚的……",
                "0_H1804": "……什么？",
                "0_H1805": "啊……不不怎么会呢！　此灵基乃是沿着缘分，由迦勒底召唤式召唤而来的您的从者。",
                "0_H1806": "已经与「异星之神」切断了关系哦。",
                "0_H1807": "……假若您不相信的话，随时可以斩下贫僧的首级哦。",
                "0_H1808": "呵呵呵……但是，如若可能……既然被召唤，愿能陪伴您的左右……",
                "0_H1809": "——直到地狱尽头。我的，主人。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "噢噢，赖光四天王。",
                "0_H191": "知道吗。武士这种存在似乎在当代已经绝迹了哦！",
                "0_H192": "唔唔唔真遗憾！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "大陆的智者阁下！",
                "0_H201": "难道您会使用仙术、道术？",
                "0_H202": "颇有意思。实际上贫僧也……",
                "0_H203": "唔，咳咳，有机会再聊。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "加藤段藏阁下。",
                "0_H211": "您找贫僧……是有何贵干呢？",
                "0_H212": "被您这样狠狠地盯着，贫僧都无法动弹了。呵呵呵……"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "风魔小太郎阁下。",
                "0_H221": "风魔忍群的头领。原来如此。这样一位……",
                "0_H222": "找贫僧有何贵干？　监视？",
                "0_H223": "哎呀，为何要这么做。贫僧毫无自觉……",
                "0_H224": "不过还请随意。"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "原来如此。",
                "0_H231": "那个女人……终于实现……",
                "0_H232": "夙愿了吗……"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "哎呀呀，藤原香子阁下。",
                "0_H241": "还是应该称呼您为紫式部阁下比较好呢？",
                "0_H242": "话说，香子阁下。",
                "0_H243": "晴明……有没有传授您什么……",
                "0_H244": "没有？　什么都没有？",
                "0_H245": "唔唔唔唔唔唔唔这样啊，这样啊——"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "清少纳言阁下，请住手。",
                "0_H251": "请不要扒贫僧的衣服。",
                "0_H252": "贫僧不是变成人形的野兽！",
                "0_H253": "贫僧只不过是『美丽的肉食兽』……",
                "0_H254": "请住手！"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "唔……怎么？　想和贫僧玩耍？",
                "0_H261": "可以！　先做个与人类分毫不差的式神送你们吧。让你们尽情解体……",
                "0_H262": "唔唔，为何要拉贫僧的头发，Caster 童谣。",
                "0_H263": "在迦勒底不能玩这种危险的游戏？　现在是玩扮家家的时间？",
                "0_H264": "……没办法。",
                "0_H265": "那贫僧希望扮演岳父的角色！"
            }
        },
        "0_H270": {
            "name": "对话 14",
            "text": {
                "0_H270": "贫僧果然很习惯这身打扮。",
                "0_H271": "没什么，在平安京城里干这干那的时候，就是这身僧伽的打扮。",
                "0_H272": "曾保护过人类。",
                "0_H273": "也曾害过人类。",
                "0_H274": "那么，这次又将如何呢。"
            }
        },
        "0_H280": {
            "name": "对话 15",
            "text": {
                "0_H280": "唔嗯……",
                "0_H281": "机关人偶又多了不少呢。",
                "0_H282": "呵呵，呵呵呵呵呵呵呵。"
            }
        },
        "0_H290": {
            "name": "对话 16",
            "text": {
                "0_H290": "御主，能打扰一下吗。",
                "0_H291": "其实是关于尼基季奇阁下。",
                "0_H292": "每当贫僧想要放式神的时候，她总是怒发冲冠地攻击贫僧，唉，真是太令人头疼了……",
                "0_H293": "……什么？",
                "0_H294": "问贫僧用式神干什么？",
                "0_H295": "当然是对高扬斯卡娅阁下……",
                "0_H296": "哎哟，被勇士阁下发现了，那回头见！"
            }
        },
        "0_H300": {
            "name": "对话 17",
            "text": {
                "0_H300": "嚯嚯，太公望阁下！",
                "0_H301": "真是太棒了，务必允许贫僧与您好好探讨仙术的奥秘。",
                "0_H302": "虽然贫僧的思想键纹是稍微有些非法的那种，不过这种细节就不提了。"
            }
        },
        "0_H3060": {
            "name": "喜欢的东西 (地狱界曼荼罗 平安京通关后)",
            "text": {
                "0_H3060": "喜欢的东西。",
                "0_H3061": "一定要说的话，没错……",
                "0_H3062": "践踏诸如他人的尊严、信念之类的东西——真不错。非常美妙。犹如甘露的滋味。您要不要也试试？"
            }
        },
        "0_H310": {
            "name": "对话 18",
            "text": {
                "0_H310": "哎呀，高扬斯卡娅阁下，祝您心情愉快！",
                "0_H311": "哈哈，原来如此，这种表情啊……",
                "0_H312": "不不，不不不，怎么会呢。",
                "0_H313": "贫僧是芦屋道满，只是个微不足道的法师阴阳师。",
                "0_H314": "Limbo？　您在说什么呢？"
            }
        },
        "0_H330": {
            "name": "对话 19",
            "text": {
                "0_H330": "鬼一阁下，鬼一法眼阁下。",
                "0_H331": "您还真是，老样子呢……"
            }
        },
        "0_H340": {
            "name": "对话 20",
            "text": {
                "0_H340": "嗯。",
                "0_H341": "阿比盖尔阁下，找贫僧有何贵干？",
                "0_H342": "关于梦？　御主的？",
                "0_H343": "呵呵，贫僧并不擅长这类，请恕我婉拒！",
                "0_H344": "什么？",
                "0_H345": "你说是叔叔吩咐你来的。哦，这样啊。"
            }
        },
        "0_H350": {
            "name": "对话 21",
            "text": {
                "0_H350": "哎呀哎呀，",
                "0_H351": "变得有些空虚了呢……",
                "0_H352": "是的，指的是各位复仇者。",
                "0_H353": "他们的灵基已经相当于空壳了。",
                "0_H354": "呜呼，这是多么的空虚啊。"
            }
        },
        "0_H360": {
            "name": "对话 22",
            "text": {
                "0_H360": "卡利奥斯特罗伯爵？",
                "0_H361": "原来如此。",
                "0_H362": "贫僧不认识呢……"
            }
        },
        "0_I010": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 1",
            "text": {
                "0_I010": "哈啊～，您竟然来了！",
                "0_I011": "欢迎光临！",
                "0_I012": "此处乃是价值相同之物相互交换之地。",
                "0_I013": "这里准备了这些那些各种物件，您大可尽情挑选！"
            }
        },
        "0_I020": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 2",
            "text": {
                "0_I020": "唔唔唔唔，御主！",
                "0_I021": "您竟然会特地来看望贫僧……",
                "0_I022": "啊啊～这是何等的……",
                "0_I023": "啊，不，不不不，不用解释了，您完全可以——",
                "0_I024": "不用解释了哦？"
            }
        },
        "0_I030": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 4",
            "text": {
                "0_I030": "哈哈，您眼光真不错！",
                "0_I031": "这件品质相当好哦……"
            }
        },
        "0_I040": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 5",
            "text": {
                "0_I040": "您确定……真的要选这件吗？",
                "0_I041": "不不什么事都没有哦，不不不。",
                "0_I042": "感谢您的光顾！"
            }
        },
        "0_I050": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 6",
            "text": {
                "0_I050": "贫僧这就去办！",
                "0_I051": "唔唔唔～这可不能重新来过，还请见谅。"
            }
        },
        "0_I060": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 7",
            "text": {
                "0_I060": "噢噢，御主！",
                "0_I061": "如您所见，贫僧正在善后收拾，但您可以慢慢来。",
                "0_I062": "啊啊，这个贫僧可不是式神。",
                "0_I063": "是货真价实侍奉您的从者！",
                "0_I064": "他人格·芦屋道满是也！",
                "0_I065": "您不必担心哦？"
            }
        },
        "0_I070": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 8",
            "text": {
                "0_I070": "是的……嗯……没错……",
                "0_I071": "贫僧道满遭到了鬼一法眼阁下罄竹难书的惩戒……",
                "0_I072": "甚至遭到了似曾相识的术式攻击……",
                "0_I073": "不断逼近的惩罚！　呜呼！",
                "0_I074": "刻在贫僧灵魂上的罪之刻印！　邪恶之路只能走到这里了！",
                "0_I075": "受此教训，贫僧再也不会，为恶……",
                "0_I076": "哈？　哈啊？",
                "0_I077": "您说，戏就不用演了，今后适可而止就行了？",
                "0_I078": "唔唔唔唔唔唔！　呵呵呵呵呵！　啊哈哈哈哈哈！",
                "0_I079": "御主，这种手段，您是从哪儿学来的？"
            }
        },
        "0_I080": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 3",
            "text": {
                "0_I080": "贫僧只能很遗憾地告诉您，贫僧背叛了。",
                "0_I081": "所以，嗯，是的，这般形象实乃式神，或者说，是形同幻觉的存在。",
                "0_I082": "就算攻击这个贫僧也毫无意义。",
                "0_I083": "没事，贫僧不会害您的。",
                "0_I084": "难得的机会，您大可以在这里兑换珍贵的道具。",
                "0_I085": "哪怕贫僧背叛了。",
                "0_I086": "归根结底，归根结底哦，贫僧的灵魂还是会伴随在您的身边哦？",
                "0_I087": "贫僧的御主。"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "初次见面！",
                "0_S011": "贫僧乃是真名为芦屋道满的法师兼阴阳师——",
                "0_S012": "职阶是！　Alterego！",
                "0_S013": "咦？",
                "0_S014": "Alterego。不是Caster？",
                "0_S015": "……哎呀，这还真奇怪。",
                "0_S016": "不管怎么说。今后请多关照。",
                "0_S017": "御主。"
            }
        },
        "0_X030": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 9",
            "text": {
                "0_X030": "您解除了第三封印吧！ 恭喜，贫僧的御主…… 虽然势单力薄，但贫僧也会为您尽一份心力。 噢噢，您客气了，这不是什么值得夸奖的事…… 不，不，不不不……唔呵呵呵！ 来吧来吧，今后也让我们集大家的力量，惩罚极北的女王吧！"
            }
        },
        "0_X050": {
            "name": "北极·夏日世界！\n～迦勒底盛夏魔园观光～ 10",
            "text": {
                "0_X050": "恭喜您攻略了武则天阁下的区域，贫僧的御主。 说是如此，还请原谅贫僧无礼从远处通过符咒向您发言…… 您有没有充分体验贫僧打工的成果呢？ 贫僧会在远方祈祷御主发愤图强哦？ 唔唔唔唔唔唔～嗦嗦嗦嗦嗦嗦。"
            }
        },

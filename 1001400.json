{
    "id": 1001400,
    "collectionNo": 352,
    "name": "徐福",
    "voices": {
        "0_B010": {
            "name": "攻击 1",
            "text": {
                "0_B010": "看招～。"
            }
        },
        "0_B040": {
            "name": "额外攻击 1",
            "text": {
                "0_B040": "急急如律令！"
            }
        },
        "0_B050": {
            "name": "宝具 1",
            "text": {
                "0_B050": "四神、无限、黄龙、时平、彼方、螺旋。",
                "0_B051": "跨越两千两百时光，吾要在此上奏。",
                "0_B052": "即为，此乃不老不死的灵药。",
                "0_B053": "虽然是谎言。"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "好麻烦好麻烦。"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "踏踏实实，踏踏实实。"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "我，我会加油的～。 希望你不要抱什么期待～……"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "讨厌～！　还是不做了啦～！ 不行？　讨厌啦——！！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "赢了。那就这样…… 匆忙离去～。"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "哇～哈～哈～。 你～活该～。"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "我要回去！"
            }
        },
        "0_B150": {
            "name": "受击 1",
            "text": {
                "0_B150": "好痛好痛好痛！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "对，对不起……先去……休息……一会儿……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "由于过劳……稍微睡一会儿……"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "唔～。"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "哎……"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "想放弃……"
            }
        },
        "0_B210": {
            "name": "宝具卡 1",
            "text": {
                "0_B210": "好麻烦……"
            }
        },
        "0_B220": {
            "name": "宝具卡 2",
            "text": {
                "0_B220": "我会做～。"
            }
        },
        "0_B230": {
            "name": "宝具卡 3",
            "text": {
                "0_B230": "好～，要做啦～。"
            }
        },
        "0_B2440": {
            "name": "额外攻击 4",
            "text": {
                "0_B2440": "包围包围——！"
            }
        },
        "0_B250": {
            "name": "宝具卡 4",
            "text": {
                "0_B250": "虞大人万岁～！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "扣杀！"
            }
        },
        "0_B420": {
            "name": "攻击 5",
            "text": {
                "0_B420": "樱花千变！"
            }
        },
        "0_B430": {
            "name": "攻击 6",
            "text": {
                "0_B430": "炸飞吧！"
            }
        },
        "0_B440": {
            "name": "额外攻击 2",
            "text": {
                "0_B440": "全速前进～！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "嘿咻。"
            }
        },
        "0_B490": {
            "name": "技能 4",
            "text": {
                "0_B490": "消散吧，绚烂的诅咒。"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "做，做给你看做给你看！　呜啊！"
            }
        },
        "0_B510": {
            "name": "开始 4",
            "text": {
                "0_B510": "——那么，认真开始杀害。 我会稍微认真一点的。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "虞大人，您有看见吗，虞大人！ 赢了，胜利！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "喂，真是的，讨厌啦——！"
            }
        },
        "0_B560": {
            "name": "无法战斗 3",
            "text": {
                "0_B560": "我变鬼也不会放过你啦……"
            }
        },
        "0_B570": {
            "name": "无法战斗 4",
            "text": {
                "0_B570": "怨念应该消除吗……"
            }
        },
        "0_B800": {
            "name": "宝具 2",
            "text": {
                "0_B800": "不老和不死都是人类的究极愿望。",
                "0_B801": "保持青春继续生存。",
                "0_B802": "不想变老不想死去。",
                "0_B803": "因此，此乃不老不死的灵药。",
                "0_B804": "来，一口喝下这瓶吧！"
            }
        },
        "0_B810": {
            "name": "宝具 3",
            "text": {
                "0_B810": "不可能有什么不老不死啦。",
                "0_B811": "我不断追寻的归根结底",
                "0_B812": "都是为了虞大人啊！",
                "0_B813": "不过啦，还是有点像样东西的哦？",
                "0_B814": "如果不介意的话来一份吧！"
            }
        },
        "0_B840": {
            "name": "额外攻击 3",
            "text": {
                "0_B840": "无法生存！"
            }
        },
        "0_C010": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 8",
            "text": {
                "0_C010": "转～转～转～，嗡～嗡～嗡～。 噗咚咚～。 哎，旋转不就是这样的吗？"
            }
        },
        "0_C020": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 9",
            "text": {
                "0_C020": "旋，旋转旋转旋转…… 那，我要转了哦。 骨碌骨碌骨碌骨碌……"
            }
        },
        "0_C030": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 10",
            "text": {
                "0_C030": "问我要不要将我的这个虞大人玩偶放进抽奖盒？ 不给你，绝对不给你。 不～给～你～啦～。"
            }
        },
        "0_C040": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 11",
            "text": {
                "0_C040": "人为什么要转这个。 虞大人为什么那么尊贵。 太空徐福亲为什么会在这里。 每一个都是深远的谜题……谜题……"
            }
        },
        "0_C050": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 12",
            "text": {
                "0_C050": "啊，感觉中了什么。 不是大吉的大杀凶，不祥的人生。"
            }
        },
        "0_C060": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 13",
            "text": {
                "0_C060": "最后的大奖。 为必然的奇迹而颤抖吧～。"
            }
        },
        "0_C110": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 14",
            "text": {
                "0_C110": "虞大人虞大人……哎嘿嘿。 啊，结果是这样的。"
            }
        },
        "0_C120": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 15",
            "text": {
                "0_C120": "这是大宇宙冒险小子分享的。 感激吧～。"
            }
        },
        "0_C130": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 16",
            "text": {
                "0_C130": "太空徐福是银河的徐福♪ 出发吧冒险银河♪ 哼哼哼～♪"
            }
        },
        "0_C140": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 17",
            "text": {
                "0_C140": "呼哈啊啊啊……困…… 啊，是这样的。"
            }
        },
        "0_C150": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 18",
            "text": {
                "0_C150": "啊唔啊唔…… 结果……啊唔……怎么……啊唔……样呢…… 咕嘟。"
            }
        },
        "0_C160": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 19",
            "text": {
                "0_C160": "中大奖～。 真好～，我连银河棒冰都没有中过…… 据说只要抽中一根就能兑换一颗行星，真棒。"
            }
        },
        "0_C170": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 20",
            "text": {
                "0_C170": "这就是最后的大奖吧。 难得的机会，唱一首恭喜歌曲吧。 曲名叫银河黑猫与银河烤薄饼。 哎，不行？ 怎么能这样……"
            }
        },
        "0_C180": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 21",
            "text": {
                "0_C180": "啊唔啊唔…… 结果……啊唔……怎么……啊唔……样呢…… 咕嘟。"
            }
        },
        "0_C190": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 22",
            "text": {
                "0_C190": "啊唔啊唔…… 结果……啊唔……怎么……啊唔……样呢…… 咕嘟。"
            }
        },
        "0_G010": {
            "name": "升级 1",
            "text": {
                "0_G010": "从异国之神分享的火焰中获得力量什么的，真奇妙呢～。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "笔记笔记，这次再临麻烦保持太空徐福亲的样子～。",
                "0_G021": "明白啦～。",
                "0_G022": "以上，是确认设计风格的对话。"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "通过再临，灵衣变化了。",
                "0_G031": "这个的外号是太空徐福亲。",
                "0_G032": "……问我太空这个词是从哪儿冒出来的？",
                "0_G033": "这我也不知道。",
                "0_G034": "不～知～道～。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "啊啊，这是……我从嬴逃……出发时的超豪华孤品灵衣呢。真怀念。",
                "0_G041": "经费充裕的时候，我总会不停去弄那种多余的东西呢～……",
                "0_G042": "不过，与之相对，强度也非同一般，嗯哼。"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "于是我现在身在此处。",
                "0_G051": "寻求不老不死的道士其实是在寻找杀害的方法。",
                "0_G052": "但如今这种欺瞒的力量也被需要了吧。",
                "0_G053": "虽然偏偏被你差遣这事真令我火大……",
                "0_G054": "做就做吧～。"
            }
        },
        "0_G110": {
            "name": "升级 2",
            "text": {
                "0_G110": "真适合做下酒菜呢……"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "哎，今天是御主的生日？",
                "0_H011": "……我好歹也是个老道士，所以提醒你一句。",
                "0_H012": "还是不要过于暴露自己的名字或是生日之类接近自身本质的东西比较好哦～。",
                "0_H013": "因为诅咒一个人的方法有很多。",
                "0_H014": "……需要我诅咒你一次吗？　不行？"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "是祭典活动哦～。",
                "0_H021": "大吉大吉。"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "虽然我可能没资格这么说。",
                "0_H031": "快出门啦，出门。"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "主从？",
                "0_H041": "是的，我是在黑心企业工作的使魔～。",
                "0_H042": "吐舌头～。"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "和御主的关系？",
                "0_H051": "唔……虞大人的……熟人……之类的……？"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西是虞大人和虞大人喜欢的东西。 就这些，结束。"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西是虞大人讨厌的东西。 就这些，结束。"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "用圣杯……",
                "0_H081": "让虞大人心满意足的方法～！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "好啦，快回去～。我还在工作啦～。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "怎么？　有事？",
                "0_H101": "战斗的话能麻烦你另找高明吗？",
                "0_H102": "哎，不行？"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "呼……哎嘿嘿……",
                "0_H111": "虞大人……虞大人……",
                "0_H112": "呼噜……"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "啊，稍微等一下！",
                "0_H121": "把那瓶子慢慢地放回桌子上！　慢一点哦！",
                "0_H122": "太好了……",
                "0_H123": "唔？　慢着。",
                "0_H124": "干脆摔破瓶子或许也很有趣？",
                "0_H125": "好，收回前言！　夸张地砸碎瓶子！　夸张点！"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "好，我就原谅你吧。那个夏天，那场骚动，我愿意原谅你。",
                "0_H131": "嗯～，宽宏大量的我真了不起！",
                "0_H132": "呜啊！？",
                "0_H133": "脸！　不要扯我的脸啦～！！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "呀啊————！　虞大人——！",
                "0_H171": "欢迎光临这种邋遢地方——！",
                "0_H172": "啊啊，我死也甘愿了，消灭也甘愿了。",
                "0_H173": "加个音效也没问题！"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "呀啊————！　那个夏日的虞大人——！",
                "0_H181": "我做梦都祈祷希望能够见到您——！",
                "0_H182": "今天月亮已经美到我死也心甘情愿了～！",
                "0_H183": "这个暂且不谈，虞大人，您的泳装还是那么高品位……！",
                "0_H184": "Beautiful……Beautiful虞大人……"
            }
        },
        "0_H1800": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H1800": "徐福亲现在正在休息中～。",
                "0_H1801": "如果你有空的话，麻烦给我按摩一下肩膀、腰部和足底～。",
                "0_H1802": "眼睛也很疲劳，麻烦也帮忙刺激一下眼皮～。",
                "0_H1803": "啊～，好倦怠……",
                "0_H1804": "喂，拜托了啦，御主～，拜～托～了～啦～。"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "恶，项羽……大人。",
                "0_H191": "……如果你不能让虞大人获得幸福，我会让你不幸哦！",
                "0_H192": "你这混蛋——！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "呜哇，是兰陵王！",
                "0_H201": "……姑且话说在前头哦？　是我？　资格比较老哦？",
                "0_H202": "排位已经固定了哦？",
                "0_H203": "干嘛啦，不要无视我啦～。",
                "0_H204": "不～要～无～视～我～啦～。",
                "0_H205": "我～们～好～好～聊～聊～啦～。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "那个夏天的记忆？　虽然没有记忆，但还是有记录的哦？",
                "0_H211": "从者就是这样的存在。",
                "0_H212": "唔～……用简单易懂的比喻来形容，就是浏览从未见过的自己的自拍相册吧？",
                "0_H213": "话说回来，我率领的三千人真努力呢～！　拍手喝彩！"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "啊，是玛修亲！",
                "0_H221": "……啊，不。",
                "0_H222": "那个……是玛修小姐……吧。",
                "0_H223": "呃，那个，就当我们是初次见面……",
                "0_H224": "请多关照……"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "喂，我说御主！　你有没有对玛修亲说什么！？",
                "0_H231": "哎，什么都没说？　那她为什么对我态度那么亲切！？",
                "0_H232": "虽然很高兴！　虽然我非～常～高～兴～啦！",
                "0_H233": "……我夏天写的日记？　她读了？",
                "0_H234": "啊啊啊啊哇哇呜呜呜呜……"
            }
        },
        "0_H2300": {
            "name": "对话 3(牵绊Lv.4以上）",
            "text": {
                "0_H2300": "和御主的关系？",
                "0_H2301": "嗯，还算是比较正确的关系啦。健全健全。"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "感谢您平日里的惠顾，这里是徐福艺术工作室。",
                "0_H241": "此次由于敝司的缘故，实在非常抱歉，但我们不得不关闭不老不死的探索业务。",
                "0_H242": "基于以上情况，基于，以上，情况…………",
                "0_H243": "求您饶我一条小命吧～！"
            }
        },
        "0_N010": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 1",
            "text": {
                "0_N010": "欢迎光临抽奖区域。",
                "0_N011": "话说……为什么我会在这里呢……",
                "0_N012": "哎，旋转？　将盒子骨碌骨碌旋转？",
                "0_N013": "莫名其妙吧……"
            }
        },
        "0_N020": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 2",
            "text": {
                "0_N020": "您，您好～。",
                "0_N021": "我是宇宙第一秘书，联邦军的偶像，太空徐福亲……",
                "0_N022": "哎嘿嘿，开玩笑的……",
                "0_N023": "稍微夸大了一些……",
                "0_N024": "其实差不多只是银河第一而已……"
            }
        },
        "0_N030": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 3",
            "text": {
                "0_N030": "我是心被虞大人囚禁的徐福。",
                "0_N031": "现在担任总司令的秘书。",
                "0_N032": "啊，关于欧米伽始皇帝大人的事，对不起，我没能找到不老不死……",
                "0_N033": "作为赔礼……要不要转一下这个盒子……抽奖？"
            }
        },
        "0_N040": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 4",
            "text": {
                "0_N040": "分明是初次见面，你莫名让我感觉有些可恨呢。",
                "0_N041": "……打打打～。"
            }
        },
        "0_N050": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 5",
            "text": {
                "0_N050": "情报是非常重要的哦。",
                "0_N051": "据说遥远东方银河的行星上有不老不死的灵药，你知道吗？",
                "0_N052": "如果可以的话，我可以去拿来哦。",
                "0_N053": "当然，前提是要给我一点零花钱……"
            }
        },
        "0_N060": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 6",
            "text": {
                "0_N060": "活动结束了。",
                "0_N061": "各位，辛苦啦～。",
                "0_N062": "那我就回去继续执行疼爱虞大人玩偶的任务啦……哎嘿嘿嘿。"
            }
        },
        "0_N070": {
            "name": "BATTLE IN NEWYORK 2023\n～太空·奥德修斯对尼古拉·特斯拉～ 7",
            "text": {
                "0_N070": "太空雪之太空萤火虫开始流动了。",
                "0_N071": "也就是说，店快要关门了吧，嗯。",
                "0_N072": "我真是辛苦了！",
                "0_N073": "虽然一开始嫌麻烦……",
                "0_N074": "但你时喜时忧的样子稍微有些有趣。",
                "0_N075": "不，准确地说，喜悦的样子令人恼火，只有失望的时候令我高兴哦……",
                "0_N076": "太空徐福的秘书做得非常完美吧？",
                "0_N077": "多多夸奖我吧！"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "从者，魔术师转为他人格。",
                "0_S011": "真名是方…………",
                "0_S012": "呜哇啊啊啊！？",
                "0_S013": "什么，骗人吧，这里是，迦勒底……！？",
                "0_S014": "干嘛啦，这是和我有什么仇怨吗！",
                "0_S015": "咦？　等一下。",
                "0_S016": "……虞大人。",
                "0_S017": "喂，虞大人在这里吧！？　在吧！？",
                "0_S018": "好啦，快把她交出来！　快～点～啦～！"
            }
        }
    },

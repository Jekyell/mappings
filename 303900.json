{
    "id": 303900,
    "collectionNo": 256,
    "name": "加雷斯",
    "voices": {
        "0_B010": {
            "name": "额外攻击 2",
            "text": {
                "0_B010": "嘿，呀！",
                "0_B440": "模仿的缚锁全断·过重湖光！"
            }
        },
        "0_B020": {
            "name": "额外攻击 4",
            "text": {
                "0_B020": "呀！",
                "0_B2440": "模仿的缚锁全断·过重湖光！"
            }
        },
        "0_B030": {
            "name": "攻击 3",
            "text": {
                "0_B030": "上！！"
            }
        },
        "0_B050": {
            "name": "宝具 {0}",
            "text": {
                "0_B050": "我要上了！",
                "0_B051": "嘿哎哎哎——！",
                "0_B052": "唔噢噢噢噢噢噢！",
                "0_B053": "最后一击！",
                "0_B054": "——狂暴少女狼！"
            }
        },
        "0_B080": {
            "name": "技能 1",
            "text": {
                "0_B080": "我对体力很有自信！"
            }
        },
        "0_B090": {
            "name": "技能 2",
            "text": {
                "0_B090": "经常有人夸奖我的手哦，哎嘿嘿！"
            }
        },
        "0_B100": {
            "name": "开始 1",
            "text": {
                "0_B100": "圆桌第七席！　加雷斯出击！"
            }
        },
        "0_B110": {
            "name": "开始 2",
            "text": {
                "0_B110": "我加雷斯，一定会为您赢得胜利！"
            }
        },
        "0_B120": {
            "name": "胜利 1",
            "text": {
                "0_B120": "Victory！！　我赢了哦，御主！"
            }
        },
        "0_B130": {
            "name": "胜利 2",
            "text": {
                "0_B130": "呵呵呵。当我是个无名小卒，掉以轻心了吧！"
            }
        },
        "0_B140": {
            "name": "受击 2",
            "text": {
                "0_B140": "没什么大不了的！"
            }
        },
        "0_B160": {
            "name": "无法战斗 1",
            "text": {
                "0_B160": "不甘心……"
            }
        },
        "0_B170": {
            "name": "无法战斗 2",
            "text": {
                "0_B170": "如果我能更为……！"
            }
        },
        "0_B180": {
            "name": "指令卡 1",
            "text": {
                "0_B180": "是！"
            }
        },
        "0_B190": {
            "name": "指令卡 2",
            "text": {
                "0_B190": "原来如此！"
            }
        },
        "0_B200": {
            "name": "指令卡 3",
            "text": {
                "0_B200": "嗯嗯！"
            }
        },
        "0_B210": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B210": "就这样没问题吧！"
            }
        },
        "0_B220": {
            "name": "宝具卡 {0}",
            "text": {
                "0_B220": "Absolutely！！"
            }
        },
        "0_B410": {
            "name": "攻击 4",
            "text": {
                "0_B410": "预备！　咚！！"
            }
        },
        "0_B480": {
            "name": "技能 3",
            "text": {
                "0_B480": "戒指啊！　赐予我力量！"
            }
        },
        "0_B500": {
            "name": "开始 3",
            "text": {
                "0_B500": "我要动真格了。没问题吧。"
            }
        },
        "0_B520": {
            "name": "胜利 3",
            "text": {
                "0_B520": "我好歹也是圆桌骑士！　我是不会输的！"
            }
        },
        "0_B540": {
            "name": "受击 4",
            "text": {
                "0_B540": "还差得远！"
            }
        },
        "0_B550": {
            "name": "受击 3",
            "text": {
                "0_B550": "这是什么———！"
            }
        },
        "0_B800": {
            "name": "宝具 {0}",
            "text": {
                "0_B800": "干掉敌人！",
                "0_B801": "吾乃狼！",
                "0_B802": "吾之枪乃必杀之牙！",
                "0_B803": "上！",
                "0_B804": "——狂暴少女狼！"
            }
        },
        "0_G010": {
            "name": "升级",
            "text": {
                "0_G010": "开始适应这魔力了！　噢噢。"
            }
        },
        "0_G020": {
            "name": "灵基再临 2",
            "text": {
                "0_G020": "唔？　有什么变化吗？",
                "0_G021": "尽管我不太清楚，但非常感谢！"
            }
        },
        "0_G030": {
            "name": "灵基再临 1",
            "text": {
                "0_G030": "我还是比较习惯这身打扮！",
                "0_G031": "因为只有武装起来，才能与您并肩战斗！",
                "0_G032": "顺便一提，这个或许是我修行时代的铠甲呢。"
            }
        },
        "0_G040": {
            "name": "灵基再临 3",
            "text": {
                "0_G040": "噢噢，好厉害！",
                "0_G041": "这是我圆桌时代的铠甲哦，御主！",
                "0_G042": "好厉害好厉害！"
            }
        },
        "0_G050": {
            "name": "灵基再临 4",
            "text": {
                "0_G050": "曾为吾王亚瑟·潘德拉贡夸奖过的吾之枪——",
                "0_G051": "愿服从您的命令，贯穿任何人的躯体。",
                "0_G052": "……然而若能如愿。",
                "0_G053": "还请您。",
                "0_G054": "为了世界的安宁差遣我。",
                "0_G055": "…………开玩笑的啦，我只是稍微装模作样了一下！",
                "0_G056": "哎嘿嘿！"
            }
        },
        "0_H010": {
            "name": "生日",
            "text": {
                "0_H010": "祝您生日快乐。",
                "0_H011": "那在这值得庆贺的日子，我久违地下个厨吧。",
                "0_H012": "交给我吧！",
                "0_H013": "毕竟我以前模仿过别人打杂！"
            }
        },
        "0_H020": {
            "name": "活动举行中",
            "text": {
                "0_H020": "好像正在举办有趣的节日呢！",
                "0_H021": "活动，哈哈，原来是这样的啊！",
                "0_H022": "那我们快走吧，御主！"
            }
        },
        "0_H030": {
            "name": "对话 1",
            "text": {
                "0_H030": "再这么待着不动的话，都要生根了。",
                "0_H031": "御主还年轻！　我也年轻！",
                "0_H032": "所以我们要多活动活动啦！"
            }
        },
        "0_H040": {
            "name": "对话 2",
            "text": {
                "0_H040": "所谓的御主和从者……",
                "0_H041": "是类似……王和骑士吗？",
                "0_H042": "稍微有些不一样吗？",
                "0_H043": "唔……哎嘿嘿，真难懂呢！"
            }
        },
        "0_H050": {
            "name": "对话 3",
            "text": {
                "0_H050": "如果说是王与骑士的话，我有亚瑟王……",
                "0_H051": "说前辈和后辈就更好理解了，但您的后辈是玛修阁下……唔，唔……",
                "0_H052": "好难啊！"
            }
        },
        "0_H060": {
            "name": "喜欢的东西",
            "text": {
                "0_H060": "喜欢的东西吗？",
                "0_H061": "枪和盾！　只有枪也行哦！",
                "0_H062": "有没有人愿意陪我来一场马上骑枪比试呢。",
                "0_H063": "但是室内好像又不方便骑马到处跑……"
            }
        },
        "0_H070": {
            "name": "讨厌的东西",
            "text": {
                "0_H070": "讨厌的东西是油污。",
                "0_H071": "那个很难擦掉呢……"
            }
        },
        "0_H080": {
            "name": "关于圣杯",
            "text": {
                "0_H080": "魔力块？　我对这种圣杯没有兴趣。",
                "0_H081": "倘若是真正的圣杯，吾王曾经追寻的存在的话，或许……",
                "0_H082": "不，如果真的存在，凭我这样的是无法获得的。",
                "0_H083": "圆桌中唯一能获得圣杯的只有加拉哈德卿。",
                "0_H084": "假如还有其他可能性——",
                "0_H085": "啊，没错，珀西瓦尔卿的话！或许！"
            }
        },
        "0_H090": {
            "name": "牵绊Lv.1",
            "text": {
                "0_H090": "我是洛特王与摩根之子，加雷斯！",
                "0_H091": "曾经侍奉亚瑟王，获赐圆桌第七席的位置。",
                "0_H092": "啊，当然不是初代的第七席哦。",
                "0_H093": "因为圆桌骑士并非始终是同一批成员啦。"
            }
        },
        "0_H100": {
            "name": "牵绊Lv.2",
            "text": {
                "0_H100": "我可以算是……加入圆桌的最后一位骑士吧。",
                "0_H101": "尽管还不成熟，但其他各位骑士，尤其是陛下和兰斯洛特非常照顾我。",
                "0_H102": "啊，我有提过吗？",
                "0_H103": "我曾经类似兰斯洛特卿的随从啦。"
            }
        },
        "0_H110": {
            "name": "牵绊Lv.3",
            "text": {
                "0_H110": "「总有一天将会成为最为优秀的骑士。」",
                "0_H111": "「迟早能成为足以与所有兄弟匹敌的真正骑士。」",
                "0_H112": "大家是这么评价我的……",
                "0_H113": "我实在是太愧不敢当了。",
                "0_H114": "哎，压力？",
                "0_H115": "当……当然有过，现在没有了！"
            }
        },
        "0_H120": {
            "name": "牵绊Lv.4",
            "text": {
                "0_H120": "当阿格规文哥哥决心告发兰斯洛特卿的时候，我非常悲伤。",
                "0_H121": "非常非常悲伤。",
                "0_H122": "我当然表示了反对。",
                "0_H123": "……毕竟，那是兰斯洛特卿。是正式叙任我为骑士的存在。",
                "0_H124": "对于我来说，是我一直想成为的理想的骑士……"
            }
        },
        "0_H130": {
            "name": "牵绊Lv.5",
            "text": {
                "0_H130": "对，过去的我被兰斯洛特卿杀害了。",
                "0_H131": "我连发生了什么都不清楚吧……",
                "0_H132": "我只是看到了为救出桂妮薇儿大人而跑来的兰斯洛特卿的身影，然后就被那位大人——",
                "0_H133": "……不，我执着于过去了呢。",
                "0_H134": "事到如今无论再说什么，既无法阻止那天的我，也无法改变他一句话都没对我说，就亲手劈碎了我头骨的过去。",
                "0_H135": "他的眼中丝毫没有映照出我的身影吧。我明白。",
                "0_H136": "事到如今我也不会再后悔了。",
                "0_H137": "只不过，既然以曾经挥枪时的状态被召唤……",
                "0_H138": "就要与当时一样，全力以赴地战斗！",
                "0_H139": "我会努力的！"
            }
        },
        "0_H170": {
            "name": "对话 4",
            "text": {
                "0_H170": "吾等的骑士王！　啊啊，亚瑟陛下！！",
                "0_H171": "没想到居然能见到您……",
                "0_H172": "呜呜……呜呜……呜呜呜……",
                "0_H173": "对不起，对不起，在最后一战时没能追随您左右……"
            }
        },
        "0_H180": {
            "name": "对话 5",
            "text": {
                "0_H180": "贝德维尔卿！",
                "0_H181": "哇，是真的……",
                "0_H182": "咦？　您的手臂是怎么了……？",
                "0_H183": "严格来说不是本人？　您在说什么啊？？"
            }
        },
        "0_H190": {
            "name": "对话 6",
            "text": {
                "0_H190": "哥哥大人，您好。",
                "0_H191": "高文大人。",
                "0_H192": "……呃，哈哈哈哈，不要把我头发揉乱啦，我要生气啦。",
                "0_H193": "但是哥哥大人，我很高兴能见到您。",
                "0_H194": "我最喜欢您了！"
            }
        },
        "0_H200": {
            "name": "对话 7",
            "text": {
                "0_H200": "莫德雷德的气息……",
                "0_H201": "唔唔唔。我很想和他多聊聊，但没有机会呢。"
            }
        },
        "0_H210": {
            "name": "对话 8",
            "text": {
                "0_H210": "我觉得花心是不好的。",
                "0_H211": "崔斯坦卿。",
                "0_H212": "崔斯坦卿？",
                "0_H213": "不要移开视线啦，崔斯坦卿？"
            }
        },
        "0_H220": {
            "name": "对话 9",
            "text": {
                "0_H220": "其……其实我不是很擅长应付梅林大人。",
                "0_H221": "我喜欢他，但可能是有点可怕吧……",
                "0_H222": "我也不知道究竟是为什么……",
                "0_H223": "御主，请您保密，不要告诉梅林大人哦……"
            }
        },
        "0_H230": {
            "name": "对话 10",
            "text": {
                "0_H230": "咦？",
                "0_H231": "加拉哈德卿？",
                "0_H232": "是……加拉哈德卿吧？",
                "0_H233": "为什么变成女孩子了！？",
                "0_H234": "……啊，搞错了，是玛修阁下啊。",
                "0_H235": "对不起！",
                "0_H236": "唔……一不小心……"
            }
        },
        "0_H240": {
            "name": "对话 11",
            "text": {
                "0_H240": "呀啊！",
                "0_H241": "骗……骗人的，骗人的骗人的。",
                "0_H242": "怎么会这样！",
                "0_H243": "不可能！",
                "0_H244": "当……当……当时的兰斯洛特卿居然在这里……！",
                "0_H245": "这……这……怎么可能！",
                "0_H246": "啊哇哇，啊哇哇哇，啊哇哇哇哇……",
                "0_H247": "我……我该怎么办。"
            }
        },
        "0_H250": {
            "name": "对话 12",
            "text": {
                "0_H250": "这不祥的样子……啊啊……",
                "0_H251": "和杀害我那天的样子如此相似……",
                "0_H252": "兰斯洛特大人——",
                "0_H253": "…………如今您仍然无法听到我的声音吧。"
            }
        },
        "0_H260": {
            "name": "对话 13",
            "text": {
                "0_H260": "嗯嗯！",
                "0_H261": "法兰克王国的骑士！",
                "0_H262": "那请务必和我来一场马上长枪比试吧！"
            }
        },
        "0_H270": {
            "name": "对话 14",
            "text": {
                "0_H270": "让我们来一场！",
                "0_H271": "马上长枪比试吧！"
            }
        },
        "0_H280": {
            "name": "对话 15",
            "text": {
                "0_H280": "哎……",
                "0_H281": "亚瑟王……陛下……？ 咦？　感觉各种不太一样……",
                "0_H282": "虽说，那个，这话有些难以启齿……",
                "0_H283": "但我觉得……室内似乎不太应该骑马……"
            }
        },
        "0_H290": {
            "name": "对话 16",
            "text": {
                "0_H290": "是……陛下吧？ 应该……没错吧？",
                "0_H291": "唔唔唔？"
            }
        },
        "0_S010": {
            "name": "召唤",
            "text": {
                "0_S010": "我是加雷斯。",
                "0_S011": "——是圆桌第七席，侍奉亚瑟王的骑士！"
            }
        }
    },
    "ChrVoice_ids": [
        "303900"
    ]
}